INSERT INTO quranLatin VALUES (3295, '28:43', 28, 43, ' walaqad <u>aa</u>tayn<u>aa</u> muus<u>aa</u> <b>a</b>lkit<u>aa</u>ba min ba\'di m<u>aa</u> ahlakn<u>aa</u> <b>a</b>lquruuna <b>a</b>l-uul<u>aa</u> ba<u>shaa</u>-ira li<b>l</b>nn<u>aa</u>si wahudan wara<u>h</u>matan la\'allahum yata<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3296, '28:44', 28, 44, ' wam<u>aa</u> kunta bij<u>aa</u>nibi <b>a</b>lgharbiyyi i<u>dz</u> qa<u>dh</u>ayn<u>aa</u> il<u>aa</u> muus<u>aa</u> <b>a</b>l-amra wam<u>aa</u> kunta mina <b>al</b>sysy<u>aa</u>hidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3297, '28:45', 28, 45, ' wal<u>aa</u>kinn<u>aa</u> ansya-n<u>aa</u> quruunan fata<u>th</u><u>aa</u>wala \'alayhimu <b>a</b>l\'umuru wam<u>aa</u> kunta ts<u>aa</u>wiyan fii ahli madyana tatluu \'alayhim <u>aa</u>y<u>aa</u>tin<u>aa</u> wal<u>aa</u>kinn<u>aa</u> kunn<u>aa</u> mursiliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3298, '28:46', 28, 46, ' wam<u>aa</u> kunta bij<u>aa</u>nibi <b>al</b><u>ththh</u>uuri i<u>dz</u> n<u>aa</u>dayn<u>aa</u> wal<u>aa</u>kin ra<u>h</u>matan min rabbika litun<u>dz</u>ira qawman m<u>aa</u> at<u>aa</u>hum min na<u>dz</u>iirin min qablika la\'allahum yata<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3299, '28:47', 28, 47, ' walawl<u>aa</u> an tu<u>sh</u>iibahum mu<u>sh</u>iibatun bim<u>aa</u> qaddamat aydiihim fayaquuluu rabban<u>aa</u> lawl<u>aa</u> arsalta ilayn<u>aa</u> rasuulan fanattabi\'a <u>aa</u>y<u>aa</u>tika wanakuuna mina <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3300, '28:48', 28, 48, ' falamm<u>aa</u> j<u>aa</u>-ahumu <b>a</b>l<u>h</u>aqqu min \'indin<u>aa</u> q<u>aa</u>luu lawl<u>aa</u> uutiya mitsla m<u>aa</u> uutiya muus<u>aa</u> awa lam yakfuruu bim<u>aa</u> uutiya muus<u>aa</u> min qablu q<u>aa</u>luu si<u>h</u>r<u>aa</u>ni ta<u>zhaa</u>har<u>aa</u> waq<u>aa</u>luu inn<u>aa</u> bikullin k<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3301, '28:49', 28, 49, ' qul fa/tuu bikit<u>aa</u>bin min \'indi <b>al</b>l<u>aa</u>hi huwa ahd<u>aa</u> minhum<u>aa</u> attabi\'hu in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3302, '28:50', 28, 50, ' fa-in lam yastajiibuu laka fa<b>i</b>\'lam annam<u>aa</u> yattabi\'uuna ahw<u>aa</u>-ahum waman a<u>dh</u>allu mimmani ittaba\'a haw<u>aa</u>hu bighayri hudan mina <b>al</b>l<u>aa</u>hi inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yahdii <b>a</b>lqawma <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3303, '28:51', 28, 51, ' walaqad wa<u>shsh</u>aln<u>aa</u> lahumu <b>a</b>lqawla la\'allahum yata<u>dz</u>akkaruun<b>a</b>\r\r\n');
