INSERT INTO quranLatin VALUES (3308, '28:56', 28, 56, ' innaka l<u>aa</u> tahdii man a<u>h</u>babta wal<u>aa</u>kinna <b>al</b>l<u>aa</u>ha yahdii man yasy<u>aa</u>u wahuwa a\'lamu bi<b>a</b>lmuhtadiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3309, '28:57', 28, 57, ' waq<u>aa</u>luu in nattabi\'i <b>a</b>lhud<u>aa</u> ma\'aka nutakha<u>ththh</u>af min ar<u>dh</u>in<u>aa</u> awa lam numakkin lahum <u>h</u>araman <u>aa</u>minan yujb<u>aa</u> ilayhi tsamar<u>aa</u>tu kulli syay-in rizqan min ladunn<u>aa</u> wal<u>aa</u>kinna aktsarahum l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
