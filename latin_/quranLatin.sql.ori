INSERT INTO quranLatin VALUES (2236, '18:96', 18, 96, ' <u>aa</u>tuunii zubara <b>a</b>l<u>h</u>adiidi <u>h</u>att<u>aa</u> i<u>dzaa</u> s<u>aa</u>w<u>aa</u> bayna <b>al</b><u>sh</u>adafayni q<u>aa</u>la unfukhuu <u>h</u>att<u>aa</u> i<u>dzaa</u> ja\'alahu n<u>aa</u>ran q<u>aa</u>la <u>aa</u>tuunii ufrigh \'alayhi qi<u>th</u>r<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2237, '18:97', 18, 97, ' fam<u>aa</u> is<u>th</u><u>aa</u>\'uu an ya<u>zh</u>haruuhu wam<u>aa</u> ista<u>th</u><u>aa</u>\'uu lahu naqb<u>aa</u><b>n</b>\r\r\n');
