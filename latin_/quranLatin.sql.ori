INSERT INTO quranLatin VALUES (2317, '19:67', 19, 67, ' awa l<u>aa</u> ya<u>dz</u>kuru <b>a</b>l-ins<u>aa</u>nu ann<u>aa</u> khalaqn<u>aa</u>hu min qablu walam yaku syay-<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2318, '19:68', 19, 68, ' fawarabbika lana<u>h</u>syurannahum wa<b>al</b>sysyay<u>aath</u>iina tsumma lanu<u>hd</u>irannahum <u>h</u>awla jahannama jitsiyy<u>aa</u><b>n</b>\r\r\n');
