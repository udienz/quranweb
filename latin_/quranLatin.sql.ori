INSERT INTO quranLatin VALUES (637, '4:144', 4, 144, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tattakhi<u>dz</u>uu <b>a</b>lk<u>aa</u>firiina awliy<u>aa</u>-a min duuni <b>a</b>lmu/miniina aturiiduuna an taj\'aluu lill<u>aa</u>hi \'alaykum sul<u>th</u><u>aa</u>nan mubiin<u>aa</u><b>n</b>\r\r\n');
