INSERT INTO quranLatin VALUES (3165, '27:6', 27, 6, ' wa-innaka latulaqq<u>aa</u> <b>a</b>lqur-<u>aa</u>na min ladun <u>h</u>akiimin \'aliim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3166, '27:7', 27, 7, ' i<u>dz</u> q<u>aa</u>la muus<u>aa</u> li-ahlihi innii <u>aa</u>nastu n<u>aa</u>ran sa<u>aa</u>tiikum minh<u>aa</u> bikhabarin aw <u>aa</u>tiikum bisyih<u>aa</u>bin qabasin la\'allakum ta<u>sth</u>aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3167, '27:8', 27, 8, ' falamm<u>aa</u> j<u>aa</u>-ah<u>aa</u> nuudiya an buurika man fii <b>al</b>nn<u>aa</u>ri waman <u>h</u>awlah<u>aa</u> wasub<u>haa</u>na <b>al</b>l<u>aa</u>hi rabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3168, '27:9', 27, 9, ' y<u>aa</u> muus<u>aa</u> innahu an<u>aa</u> <b>al</b>l<u>aa</u>hu <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3169, '27:10', 27, 10, ' wa-alqi \'a<u>shaa</u>ka falamm<u>aa</u> ra<u>aa</u>h<u>aa</u> tahtazzu ka-annah<u>aa</u> j<u>aa</u>nnun wall<u>aa</u> mudbiran walam yu\'aqqib y<u>aa</u> muus<u>aa</u> l<u>aa</u> takhaf innii l<u>aa</u> yakh<u>aa</u>fu ladayya <b>a</b>lmursaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3170, '27:11', 27, 11, ' ill<u>aa</u> man <u>zh</u>alama tsumma baddala <u>h</u>usnan ba\'da suu-in fa-innii ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3171, '27:12', 27, 12, ' wa-adkhil yadaka fii jaybika takhruj bay<u>dhaa</u>-a min ghayri suu-in fii tis\'i <u>aa</u>y<u>aa</u>tin il<u>aa</u> fir\'awna waqawmihi innahum k<u>aa</u>nuu qawman f<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3172, '27:13', 27, 13, ' falamm<u>aa</u> j<u>aa</u>-at-hum <u>aa</u>y<u>aa</u>tun<u>aa</u> mub<u>sh</u>iratan q<u>aa</u>luu h<u>aadzaa</u> si<u>h</u>run mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3173, '27:14', 27, 14, ' waja<u>h</u>aduu bih<u>aa</u> wa<b>i</b>stayqanat-h<u>aa</u> anfusuhum <u>zh</u>ulman wa\'uluwwan fa<b>u</b>n<u>zh</u>ur kayfa k<u>aa</u>na \'<u>aa</u>qibatu <b>a</b>lmufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3174, '27:15', 27, 15, ' walaqad <u>aa</u>tayn<u>aa</u> d<u>aa</u>wuuda wasulaym<u>aa</u>na \'ilman waq<u>aa</u>l<u>aa</u> <b>a</b>l<u>h</u>amdu lill<u>aa</u>hi <b>al</b>la<u>dz</u>ii fa<u>dhdh</u>alan<u>aa</u> \'al<u>aa</u> katsiirin min \'ib<u>aa</u>dihi <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3175, '27:16', 27, 16, ' wawaritsa sulaym<u>aa</u>nu d<u>aa</u>wuuda waq<u>aa</u>la y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su \'ullimn<u>aa</u> man<u>th</u>iqa <b>al</b><u>ththh</u>ayri wauutiin<u>aa</u> min kulli syay-in inna h<u>aadzaa</u> lahuwa <b>a</b>lfa<u>dh</u>lu <b>a</b>lmubiin<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3176, '27:17', 27, 17, ' wa<u>h</u>usyira lisulaym<u>aa</u>na junuuduhu mina <b>a</b>ljinni wa<b>a</b>l-insi wa<b>al</b><u>ththh</u>ayri fahum yuuza\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3177, '27:18', 27, 18, ' <u>h</u>att<u>aa</u> i<u>dzaa</u> ataw \'al<u>aa</u> w<u>aa</u>di <b>al</b>nnamli q<u>aa</u>lat namlatun y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nnamlu udkhuluu mas<u>aa</u>kinakum l<u>aa</u> ya<u>hth</u>imannakum sulaym<u>aa</u>nu wajunuuduhu wahum l<u>aa</u> yasy\'uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3178, '27:19', 27, 19, ' fatabassama <u>dah</u>ikan min qawlih<u>aa</u> waq<u>aa</u>la rabbi awzi\'nii an asykura ni\'mataka <b>al</b>latii an\'amta \'alayya wa\'al<u>aa</u> w<u>aa</u>lidayya wa-an a\'mala <u>shaa</u>li<u>h</u>an tar<u>daa</u>hu wa-adkhilnii bira<u>h</u>matika fii \'ib<u>aa</u>dika <b>al</b><u>shshaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3179, '27:20', 27, 20, ' watafaqqada <b>al</b><u>ththh</u>ayra faq<u>aa</u>la m<u>aa</u> liya l<u>aa</u> ar<u>aa</u> <b>a</b>lhudhuda am k<u>aa</u>na mina <b>a</b>lgh<u>aa</u>-ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3180, '27:21', 27, 21, ' lau\'a<u>dzdz</u>ibannahu \'a<u>dzaa</u>ban syadiidan aw la-a<u>dz</u>ba<u>h</u>annahu aw laya/tiyannii bisul<u>th</u><u>aa</u>nin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3181, '27:22', 27, 22, ' famakatsa ghayra ba\'iidin faq<u>aa</u>la a<u>hath</u>tu bim<u>aa</u> lam tu<u>h</u>i<u>th</u> bihi waji/tuka min saba-in binaba-in yaqiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3182, '27:23', 27, 23, ' innii wajadtu imra-atan tamlikuhum wauutiyat min kulli syay-in walah<u>aa</u> \'arsyun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3183, '27:24', 27, 24, ' wajadtuh<u>aa</u> waqawmah<u>aa</u> yasjuduuna li<b>l</b>sysyamsi min duuni <b>al</b>l<u>aa</u>hi wazayyana lahumu <b>al</b>sysyay<u>th</u><u>aa</u>nu a\'m<u>aa</u>lahum fa<u>sh</u>addahum \'ani <b>al</b>ssabiili fahum l<u>aa</u> yahtaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3184, '27:25', 27, 25, ' <b>al</b>l<u>aa</u> yasjuduu lill<u>aa</u>hi <b>al</b>la<u>dz</u>ii yukhriju <b>a</b>lkhaba-a fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i waya\'lamu m<u>aa</u> tukhfuuna wam<u>aa</u> tu\'linuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3185, '27:26', 27, 26, ' <b>al</b>l<u>aa</u>hu l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa rabbu <b>a</b>l\'arsyi <b>a</b>l\'a<u>zh</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3186, '27:27', 27, 27, ' q<u>aa</u>la sanan<u>zh</u>uru a<u>sh</u>adaqta am kunta mina <b>a</b>lk<u>aadz</u>ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3187, '27:28', 27, 28, ' i<u>dz</u>hab bikit<u>aa</u>bii h<u>aadzaa</u> fa-alqih ilayhim tsumma tawalla \'anhum fa<b>u</b>n<u>zh</u>ur m<u>aatsaa</u> yarji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3188, '27:29', 27, 29, ' q<u>aa</u>lat y<u>aa</u> ayyuh<u>aa</u> <b>a</b>lmalau innii ulqiya ilayya kit<u>aa</u>bun kariim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3189, '27:30', 27, 30, ' innahu min sulaym<u>aa</u>na wa-innahu bismi <b>al</b>l<u>aa</u>hi <b>al</b>rra<u>h</u>m<u>aa</u>ni <b>al</b>rra<u>h</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3190, '27:31', 27, 31, ' <b>al</b>l<u>aa</u> ta\'luu \'alayya wa/tuunii muslimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3191, '27:32', 27, 32, ' q<u>aa</u>lat y<u>aa</u> ayyuh<u>aa</u> <b>a</b>lmalau aftuunii fii amrii m<u>aa</u> kuntu q<u>aath</u>i\'atan amran <u>h</u>att<u>aa</u> tasyhaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3192, '27:33', 27, 33, ' q<u>aa</u>luu na<u>h</u>nu uluu quwwatin wauluu ba/sin syadiidin wa<b>a</b>l-amru ilayki fa<b>u</b>n<u>zh</u>urii m<u>aatsaa</u> ta/muriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3193, '27:34', 27, 34, ' q<u>aa</u>lat inna <b>a</b>lmuluuka i<u>dzaa</u> dakhaluu qaryatan afsaduuh<u>aa</u> waja\'aluu a\'izzata ahlih<u>aa</u> a<u>dz</u>illatan waka<u>dzaa</u>lika yaf\'aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3194, '27:35', 27, 35, ' wa-innii mursilatun ilayhim bihadiyyatin fan<u>aats</u>iratun bima yarji\'u <b>a</b>lmursaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3195, '27:36', 27, 36, ' falamm<u>aa</u> j<u>aa</u>-a sulaym<u>aa</u>na q<u>aa</u>la atumidduunani bim<u>aa</u>lin fam<u>aa</u> <u>aa</u>t<u>aa</u>niya <b>al</b>l<u>aa</u>hu khayrun mimm<u>aa</u> <u>aa</u>t<u>aa</u>kum bal antum bihadiyyatikum tafra<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3196, '27:37', 27, 37, ' irji\' ilayhim falana/tiyannahum bijunuudin l<u>aa</u> qibala lahum bih<u>aa</u> walanukhrijannahum minh<u>aa</u> a<u>dz</u>illatan wahum <u>shaa</u>ghiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3197, '27:38', 27, 38, ' q<u>aa</u>la y<u>aa</u> ayyuh<u>aa</u> <b>a</b>lmalau ayyukum ya/tiinii bi\'arsyih<u>aa</u> qabla an ya/tuunii muslimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3198, '27:39', 27, 39, ' q<u>aa</u>la \'ifriitun mina <b>a</b>ljinni an<u>aa</u> <u>aa</u>tiika bihi qabla an taquuma min maq<u>aa</u>mika wa-inne \'alayhi laqawiyyun amiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3199, '27:40', 27, 40, ' q<u>aa</u>la <b>al</b>la<u>dz</u>ii \'indahu \'ilmun mina <b>a</b>lkit<u>aa</u>bi an<u>aa</u> <u>aa</u>tiika bihi qabla an yartadda ilayka <u>th</u>arfuka falamm<u>aa</u> ra<u>aa</u>hu mustaqirran \'indahu q<u>aa</u>la h<u>aadzaa</u> min fa<u>dh</u>li rabbii liyabluwanii a-asykuru am akfuru waman syakara fa-innam<u>aa</u> yasykuru linafsihi waman kafara fa-inna rabbii ghaniyyun kariim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3200, '27:41', 27, 41, ' q<u>aa</u>la nakkiruu lah<u>aa</u> \'arsyah<u>aa</u> nan<u>zh</u>ur atahtadii am takuunu mina <b>al</b>la<u>dz</u>iina l<u>aa</u> yahtaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3201, '27:42', 27, 42, ' falamm<u>aa</u> j<u>aa</u>-at qiila ah<u>aa</u>ka<u>dzaa</u> \'arsyuki q<u>aa</u>lat ka-annahu huwa wauutiin<u>aa</u> <b>a</b>l\'ilma min qablih<u>aa</u> wakunn<u>aa</u> muslimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3202, '27:43', 27, 43, ' wa<u>sh</u>addah<u>aa</u> m<u>aa</u> k<u>aa</u>nat ta\'budu min duuni <b>al</b>l<u>aa</u>hi innah<u>aa</u> k<u>aa</u>nat min qawmin k<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3203, '27:44', 27, 44, ' qiila lah<u>aa</u> udkhulii <b>al</b><u>shsh</u>ar<u>h</u>a falamm<u>aa</u> ra-at-hu <u>h</u>asibat-hu lujjatan wakasyafat \'an s<u>aa</u>qayh<u>aa</u> q<u>aa</u>la innahu <u>sh</u>ar<u>h</u>un mumarradun min qaw<u>aa</u>riira q<u>aa</u>lat rabbi innii <u>zh</u>alamtu nafsii wa-aslamtu ma\'a sulaym<u>aa</u>na lill<u>aa</u>hi rabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3204, '27:45', 27, 45, ' walaqad arsaln<u>aa</u> il<u>aa</u> tsamuuda akh<u>aa</u>hum <u>shaa</u>li<u>h</u>an ani u\'buduu <b>al</b>l<u>aa</u>ha fa-i<u>dzaa</u> hum fariiq<u>aa</u>ni yakhta<u>sh</u>imuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3205, '27:46', 27, 46, ' q<u>aa</u>la y<u>aa</u> qawmi lima tasta\'jiluuna bi<b>al</b>ssayyi-ati qabla <b>a</b>l<u>h</u>asanati lawl<u>aa</u> tastaghfiruuna <b>al</b>l<u>aa</u>ha la\'allakum tur<u>h</u>amuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3206, '27:47', 27, 47, ' q<u>aa</u>luu i<u>ththh</u>ayyarn<u>aa</u> bika wabiman ma\'aka q<u>aa</u>la <u>th</u><u>aa</u>-irukum \'inda <b>al</b>l<u>aa</u>hi bal antum qawmun tuftanuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3207, '27:48', 27, 48, ' wak<u>aa</u>na fii <b>a</b>lmadiinati tis\'atu rah<u>th</u>in yufsiduuna fii <b>a</b>l-ar<u>dh</u>i wal<u>aa</u> yu<u>sh</u>li<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3208, '27:49', 27, 49, ' q<u>aa</u>luu taq<u>aa</u>samuu bi<b>al</b>l<u>aa</u>hi lanubayyitannahu wa-ahlahu tsumma lanaquulanna liwaliyyihi m<u>aa</u> syahidn<u>aa</u> mahlika ahlihi wa-inn<u>aa</u> la<u>shaa</u>diquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3209, '27:50', 27, 50, ' wamakaruu makran wamakarn<u>aa</u> makran wahum l<u>aa</u> yasy\'uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3210, '27:51', 27, 51, ' fa<b>u</b>n<u>zh</u>ur kayfa k<u>aa</u>na \'<u>aa</u>qibatu makrihim ann<u>aa</u> dammarn<u>aa</u>hum waqawmahum ajma\'iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3211, '27:52', 27, 52, ' fatilka buyuutuhum kh<u>aa</u>wiyatan bim<u>aa</u> <u>zh</u>alamuu inna fii <u>dzaa</u>lika la<u>aa</u>yatan liqawmin ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3212, '27:53', 27, 53, ' wa-anjayn<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wak<u>aa</u>nuu yattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3213, '27:54', 27, 54, ' waluu<u>th</u>an i<u>dz</u> q<u>aa</u>la liqawmihi ata/tuuna <b>a</b>lf<u>aah</u>isyata wa-antum tub<u>sh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3214, '27:55', 27, 55, ' a-innakum lata/tuuna <b>al</b>rrij<u>aa</u>la syahwatan min duuni <b>al</b>nnis<u>aa</u>-i bal antum qawmun tajhaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3215, '27:56', 27, 56, ' fam<u>aa</u> k<u>aa</u>na jaw<u>aa</u>ba qawmihi ill<u>aa</u> an q<u>aa</u>luu akhrijuu <u>aa</u>la luu<u>th</u>in min qaryatikum innahum un<u>aa</u>sun yata<u>th</u>ahharuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3216, '27:57', 27, 57, ' fa-anjayn<u>aa</u>hu wa-ahlahu ill<u>aa</u> imra-atahu qaddarn<u>aa</u>h<u>aa</u> mina <b>a</b>lgh<u>aa</u>biriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3217, '27:58', 27, 58, ' wa-am<u>th</u>arn<u>aa</u> \'alayhim ma<u>th</u>aran fas<u>aa</u>-a ma<u>th</u>aru <b>a</b>lmun<u>dz</u>ariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3218, '27:59', 27, 59, ' quli <b>a</b>l<u>h</u>amdu lill<u>aa</u>hi wasal<u>aa</u>mun \'al<u>aa</u> \'ib<u>aa</u>dihi <b>al</b>la<u>dz</u>iina i<u>sth</u>af<u>aa</u> <u>aa</u>ll<u>aa</u>hu khayrun amm<u>aa</u> yusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3219, '27:60', 27, 60, ' amman khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a wa-anzala lakum mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an fa-anbatn<u>aa</u> bihi <u>h</u>ad<u>aa</u>-iqa <u>dzaa</u>ta bahjatin m<u>aa</u> k<u>aa</u>na lakum an tunbituu syajarah<u>aa</u> a-il<u>aa</u>hun ma\'a <b>al</b>l<u>aa</u>hi bal hum qawmun ya\'diluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3220, '27:61', 27, 61, ' amman ja\'ala <b>a</b>l-ar<u>dh</u>a qar<u>aa</u>ran waja\'ala khil<u>aa</u>lah<u>aa</u> anh<u>aa</u>ran waja\'ala lah<u>aa</u> raw<u>aa</u>siya waja\'ala bayna <b>a</b>lba<u>h</u>rayni <u>haa</u>jizan a-il<u>aa</u>hun ma\'a <b>al</b>l<u>aa</u>hi bal aktsaruhum l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3221, '27:62', 27, 62, ' amman yujiibu <b>a</b>lmu<u>dth</u>arra i<u>dzaa</u> da\'<u>aa</u>hu wayaksyifu <b>al</b>ssuu-a wayaj\'alukum khulaf<u>aa</u>-a <b>a</b>l-ar<u>dh</u>i a-il<u>aa</u>hun ma\'a <b>al</b>l<u>aa</u>hi qaliilan m<u>aa</u> ta<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3222, '27:63', 27, 63, ' amman yahdiikum fii <u>zh</u>ulum<u>aa</u>ti <b>a</b>lbarri wa<b>a</b>lba<u>h</u>ri waman yursilu <b>al</b>rriy<u>aah</u>a busyran bayna yaday ra<u>h</u>matihi a-il<u>aa</u>hun ma\'a <b>al</b>l<u>aa</u>hi ta\'<u>aa</u>l<u>aa</u> <b>al</b>l<u>aa</u>hu \'amm<u>aa</u> yusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3223, '27:64', 27, 64, ' amman yabdau <b>a</b>lkhalqa tsumma yu\'iiduhu waman yarzuqukum mina <b>al</b>ssam<u>aa</u>-i wa<b>a</b>l-ar<u>dh</u>i a-il<u>aa</u>hun ma\'a <b>al</b>l<u>aa</u>hi qul h<u>aa</u>tuu burh<u>aa</u>nakum in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3224, '27:65', 27, 65, ' qul l<u>aa</u> ya\'lamu man fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i <b>a</b>lghayba ill<u>aa</u> <b>al</b>l<u>aa</u>hu wam<u>aa</u> yasy\'uruuna ayy<u>aa</u>na yub\'atsuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3225, '27:66', 27, 66, ' bali idd<u>aa</u>raka \'ilmuhum fii <b>a</b>l-<u>aa</u>khirati bal hum fii syakkin minh<u>aa</u> bal hum minh<u>aa</u> \'amuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3226, '27:67', 27, 67, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu a-i<u>dzaa</u> kunn<u>aa</u> tur<u>aa</u>ban wa<u>aa</u>b<u>aa</u>un<u>aa</u> a-inn<u>aa</u> lamukhrajuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3227, '27:68', 27, 68, ' laqad wu\'idn<u>aa</u> h<u>aadzaa</u> na<u>h</u>nu wa<u>aa</u>b<u>aa</u>un<u>aa</u> min qablu in h<u>aadzaa</u> ill<u>aa</u> as<u>aath</u>iiru <b>a</b>l-awwaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3228, '27:69', 27, 69, ' qul siiruu fii <b>a</b>l-ar<u>dh</u>i fa<b>u</b>n<u>zh</u>uruu kayfa k<u>aa</u>na \'<u>aa</u>qibatu <b>a</b>lmujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3229, '27:70', 27, 70, ' wal<u>aa</u> ta<u>h</u>zan \'alayhim wal<u>aa</u> takun fii <u>dh</u>ayqin mimm<u>aa</u> yamkuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3230, '27:71', 27, 71, ' wayaquuluuna mat<u>aa</u> h<u>aadzaa</u> <b>a</b>lwa\'du in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3231, '27:72', 27, 72, ' qul \'as<u>aa</u> an yakuuna radifa lakum ba\'<u>dh</u>u <b>al</b>la<u>dz</u>ii tasta\'jiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3232, '27:73', 27, 73, ' wa-inna rabbaka la<u>dz</u>uu fa<u>dh</u>lin \'al<u>aa</u> <b>al</b>nn<u>aa</u>si wal<u>aa</u>kinna aktsarahum l<u>aa</u> yasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3233, '27:74', 27, 74, ' wa-inna rabbaka laya\'lamu m<u>aa</u> tukinnu <u>sh</u>uduuruhum wam<u>aa</u> yu\'linuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3234, '27:75', 27, 75, ' wam<u>aa</u> min gh<u>aa</u>-ibatin fii <b>al</b>ssam<u>aa</u>-i wa<b>a</b>l-ar<u>dh</u>i ill<u>aa</u> fii kit<u>aa</u>bin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3235, '27:76', 27, 76, ' inna h<u>aadzaa</u> <b>a</b>lqur-<u>aa</u>na yaqu<u>shsh</u>u \'al<u>aa</u> banii isr<u>aa</u>-iila aktsara <b>al</b>la<u>dz</u>ii hum fiihi yakhtalifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3236, '27:77', 27, 77, ' wa-innahu lahudan wara<u>h</u>matun lilmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3237, '27:78', 27, 78, ' inna rabbaka yaq<u>dh</u>ii baynahum bi<u>h</u>ukmihi wahuwa <b>a</b>l\'aziizu <b>a</b>l\'aliim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3238, '27:79', 27, 79, ' fatawakkal \'al<u>aa</u> <b>al</b>l<u>aa</u>hi innaka \'al<u>aa</u> <b>a</b>l<u>h</u>aqqi <b>a</b>lmubiin<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3239, '27:80', 27, 80, ' innaka l<u>aa</u> tusmi\'u <b>a</b>lmawt<u>aa</u> wal<u>aa</u> tusmi\'u <b>al</b><u>shsh</u>umma <b>al</b>ddu\'<u>aa</u>-a i<u>dzaa</u> wallaw mudbiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3240, '27:81', 27, 81, ' wam<u>aa</u> anta bih<u>aa</u>dii <b>a</b>l\'umyi \'an <u>dh</u>al<u>aa</u>latihim in tusmi\'u ill<u>aa</u> man yu/minu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> fahum muslimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3241, '27:82', 27, 82, ' wa-i<u>dzaa</u> waqa\'a <b>a</b>lqawlu \'alayhim akhrajn<u>aa</u> lahum d<u>aa</u>bbatan mina <b>a</b>l-ar<u>dh</u>i tukallimuhum anna <b>al</b>nn<u>aa</u>sa k<u>aa</u>nuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> l<u>aa</u> yuuqinuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3242, '27:83', 27, 83, ' wayawma na<u>h</u>syuru min kulli ummatin fawjan mimman yuka<u>dzdz</u>ibu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> fahum yuuza\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3243, '27:84', 27, 84, ' <u>h</u>att<u>aa</u> i<u>dzaa</u> j<u>aa</u>uu q<u>aa</u>la aka<u>dzdz</u>abtum bi-<u>aa</u>y<u>aa</u>tii walam tu<u>h</u>ii<u>th</u>uu bih<u>aa</u> \'ilman amm<u>aatsaa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3244, '27:85', 27, 85, ' wawaqa\'a <b>a</b>lqawlu \'alayhim bim<u>aa</u> <u>zh</u>alamuu fahum l<u>aa</u> yan<u>th</u>iquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3245, '27:86', 27, 86, ' alam yaraw ann<u>aa</u> ja\'aln<u>aa</u> <b>al</b>layla liyaskunuu fiihi wa<b>al</b>nnah<u>aa</u>ra mub<u>sh</u>iran inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin liqawmin yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3246, '27:87', 27, 87, ' wayawma yunfakhu fii <b>al</b><u>shsh</u>uuri fafazi\'a man fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti waman fii <b>a</b>l-ar<u>dh</u>i ill<u>aa</u> man sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu wakullun atawhu d<u>aa</u>khiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3247, '27:88', 27, 88, ' watar<u>aa</u> <b>a</b>ljib<u>aa</u>la ta<u>h</u>sabuh<u>aa</u> j<u>aa</u>midatan wahiya tamurru marra <b>al</b>ssa<u>haa</u>bi <u>sh</u>un\'a <b>al</b>l<u>aa</u>hi <b>al</b>la<u>dz</u>ii atqana kulla syay-in innahu khabiirun bim<u>aa</u> taf\'aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3248, '27:89', 27, 89, ' man j<u>aa</u>-a bi<b>a</b>l<u>h</u>asanati falahu khayrun minh<u>aa</u> wahum min faza\'in yawma-i<u>dz</u>in <u>aa</u>minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3249, '27:90', 27, 90, ' waman j<u>aa</u>-a bi<b>al</b>ssayyi-ati fakubbat wujuuhuhum fii <b>al</b>nn<u>aa</u>ri hal tujzawna ill<u>aa</u> m<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3250, '27:91', 27, 91, ' innam<u>aa</u> umirtu an a\'buda rabba h<u>aadz</u>ihi <b>a</b>lbaldati <b>al</b>la<u>dz</u>ii <u>h</u>arramah<u>aa</u> walahu kullu syay-in waumirtu an akuuna mina <b>a</b>lmuslimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3251, '27:92', 27, 92, ' wa-an atluwa <b>a</b>lqur-<u>aa</u>na famani ihtad<u>aa</u> fa-innam<u>aa</u> yahtadii linafsihi waman <u>dh</u>alla faqul innam<u>aa</u> an<u>aa</u> mina <b>a</b>lmun<u>dz</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3252, '27:93', 27, 93, ' waquli <b>a</b>l<u>h</u>amdu lill<u>aa</u>hi sayuriikum <u>aa</u>y<u>aa</u>tihi fata\'rifuunah<u>aa</u> wam<u>aa</u> rabbuka bigh<u>aa</u>filin \'amm<u>aa</u> ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3253, '28:1', 28, 1, ' <u>th</u><u>aa</u>-siin-miim</u>\r\r\n');
INSERT INTO quranLatin VALUES (3254, '28:2', 28, 2, ' tilka <u>aa</u>y<u>aa</u>tu <b>a</b>lkit<u>aa</u>bi <b>a</b>lmubiin<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3255, '28:3', 28, 3, ' natluu \'alayka min naba-i muus<u>aa</u> wafir\'awna bi<b>a</b>l<u>h</u>aqqi liqawmin yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3256, '28:4', 28, 4, ' inna fir\'awna \'al<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i waja\'ala ahlah<u>aa</u> syiya\'an yasta<u>dh</u>\'ifu <u>th</u><u>aa</u>-ifatan minhum yu<u>dz</u>abbi<u>h</u>u abn<u>aa</u>-ahum wayasta<u>h</u>yii nis<u>aa</u>-ahum innahu k<u>aa</u>na mina <b>a</b>lmufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3257, '28:5', 28, 5, ' wanuriidu an namunna \'al<u>aa</u> <b>al</b>la<u>dz</u>iina istu<u>dh</u>\'ifuu fii <b>a</b>l-ar<u>dh</u>i wanaj\'alahum a-immatan wanaj\'alahumu <b>a</b>lw<u>aa</u>ritsiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3258, '28:6', 28, 6, ' wanumakkina lahum fii <b>a</b>l-ar<u>dh</u>i wanuriya fir\'awna wah<u>aa</u>m<u>aa</u>na wajunuudahum<u>aa</u> minhum m<u>aa</u> k<u>aa</u>nuu ya<u>hts</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3259, '28:7', 28, 7, ' wa-aw<u>h</u>ayn<u>aa</u> il<u>aa</u> ummi muus<u>aa</u> an ar<u>dh</u>i\'iihi fa-i<u>dzaa</u> khifti \'alayhi fa-alqiihi fii <b>a</b>lyammi wal<u>aa</u> takh<u>aa</u>fii wal<u>aa</u> ta<u>h</u>zanii inn<u>aa</u> r<u>aa</u>dduuhu ilayki waj<u>aa</u>\'iluuhu mina <b>a</b>lmursaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3260, '28:8', 28, 8, ' fa<b>i</b>ltaqa<u>th</u>ahu <u>aa</u>lu fir\'awna liyakuuna lahum \'aduwwan wa<u>h</u>azanan inna fir\'awna wah<u>aa</u>m<u>aa</u>na wajunuudahum<u>aa</u> k<u>aa</u>nuu kh<u>aath</u>i-iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3261, '28:9', 28, 9, ' waq<u>aa</u>lati imra-atu fir\'awna qurratu \'aynin lii walaka l<u>aa</u> taqtuluuhu \'as<u>aa</u> an yanfa\'an<u>aa</u> aw nattakhi<u>dz</u>ahu waladan wahum l<u>aa</u> yasy\'uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3262, '28:10', 28, 10, ' wa-a<u>sh</u>ba<u>h</u>a fu-<u>aa</u>du ummi muus<u>aa</u> f<u>aa</u>righan in k<u>aa</u>dat latubdii bihi lawl<u>aa</u> an raba<u>th</u>n<u>aa</u> \'al<u>aa</u> qalbih<u>aa</u> litakuuna mina <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3263, '28:11', 28, 11, ' waq<u>aa</u>lat li-ukhtihi qu<u>shsh</u>iihi faba<u>sh</u>urat bihi \'an junubin wahum l<u>aa</u> yasy\'uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3264, '28:12', 28, 12, ' wa<u>h</u>arramn<u>aa</u> \'alayhi <b>a</b>lmar<u>aad</u>i\'a min qablu faq<u>aa</u>lat hal adullukum \'al<u>aa</u> ahli baytin yakfuluunahu lakum wahum lahu n<u>aas</u>i<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3265, '28:13', 28, 13, ' faradadn<u>aa</u>hu il<u>aa</u> ummihi kay taqarra \'aynuh<u>aa</u> wal<u>aa</u> ta<u>h</u>zana walita\'lama anna wa\'da <b>al</b>l<u>aa</u>hi <u>h</u>aqqun wal<u>aa</u>kinna aktsarahum l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3266, '28:14', 28, 14, ' walamm<u>aa</u> balagha asyuddahu wa<b>i</b>staw<u>aa</u> <u>aa</u>tayn<u>aa</u>hu <u>h</u>ukman wa\'ilman waka<u>dzaa</u>lika najzii <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3267, '28:15', 28, 15, ' wadakhala <b>a</b>lmadiinata \'al<u>aa</u> <u>h</u>iini ghaflatin min ahlih<u>aa</u> fawajada fiih<u>aa</u> rajulayni yaqtatil<u>aa</u>ni h<u>aadzaa</u> min syii\'atihi wah<u>aadzaa</u> min \'aduwwihi fa<b>i</b>stagh<u>aa</u>tsahu <b>al</b>la<u>dz</u>ii min syii\'atihi \'al<u>aa</u> <b>al</b>la<u>dz</u>ii min \'aduwwihi fawakazahu muus<u>aa</u> faqa<u>daa</u> \'alayhi q<u>aa</u>la h<u>aadzaa</u> min \'amali <b>al</b>sysyay<u>th</u><u>aa</u>ni innahu \'aduwwun mu<u>dh</u>illun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3268, '28:16', 28, 16, ' q<u>aa</u>la rabbi innii <u>zh</u>alamtu nafsii fa<b>i</b>ghfir lii faghafara lahu innahu huwa <b>a</b>lghafuuru <b>al</b>rra<u>h</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3269, '28:17', 28, 17, ' q<u>aa</u>la rabbi bim<u>aa</u> an\'amta \'alayya falan akuuna <u>zh</u>ahiiran lilmujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3270, '28:18', 28, 18, ' fa-a<u>sh</u>ba<u>h</u>a fii <b>a</b>lmadiinati kh<u>aa</u>-ifan yataraqqabu fa-i<u>dzaa</u> <b>al</b>la<u>dz</u>ii istan<u>sh</u>arahu bi<b>a</b>l-amsi yasta<u>sh</u>rikhuhu q<u>aa</u>la lahu muus<u>aa</u> innaka laghawiyyun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3271, '28:19', 28, 19, ' falamm<u>aa</u> an ar<u>aa</u>da an yab<u>th</u>isya bi<b>a</b>lla<u>dz</u>ii huwa \'aduwwun lahum<u>aa</u> q<u>aa</u>la y<u>aa</u> muus<u>aa</u> aturiidu an taqtulanii kam<u>aa</u> qatalta nafsan bi<b>a</b>l-amsi in turiidu ill<u>aa</u> an takuuna jabb<u>aa</u>ran fii <b>a</b>l-ar<u>dh</u>i wam<u>aa</u> turiidu an takuuna mina <b>a</b>lmu<u>sh</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3272, '28:20', 28, 20, ' waj<u>aa</u>-a rajulun min aq<u>shaa</u> <b>a</b>lmadiinati yas\'<u>aa</u> q<u>aa</u>la y<u>aa</u> muus<u>aa</u> inna <b>a</b>lmala-a ya/tamiruuna bika liyaqtuluuka fa<b>u</b>khruj innii laka mina <b>al</b>nn<u>aas</u>i<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3273, '28:21', 28, 21, ' fakharaja minh<u>aa</u> kh<u>aa</u>-ifan yataraqqabu q<u>aa</u>la rabbi najjinii mina <b>a</b>lqawmi <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3274, '28:22', 28, 22, ' walamm<u>aa</u> tawajjaha tilq<u>aa</u>-a madyana q<u>aa</u>la \'as<u>aa</u> rabbii an yahdiyanii saw<u>aa</u>-a <b>al</b>ssabiil<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3275, '28:23', 28, 23, ' walamm<u>aa</u> warada m<u>aa</u>-a madyana wajada \'alayhi ummatan mina <b>al</b>nn<u>aa</u>si yasquuna wawajada min duunihimu imra-atayni ta<u>dz</u>uud<u>aa</u>ni q<u>aa</u>la m<u>aa</u> kha<u>th</u>bukum<u>aa</u> q<u>aa</u>lat<u>aa</u> l<u>aa</u> nasqii <u>h</u>att<u>aa</u> yu<u>sh</u>dira <b>al</b>rri\'<u>aa</u>u wa-abuun<u>aa</u> syaykhun kabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3276, '28:24', 28, 24, ' fasaq<u>aa</u> lahum<u>aa</u> tsumma tawall<u>aa</u> il<u>aa</u> <b>al</b><u>zhzh</u>illi faq<u>aa</u>la rabbi innii lim<u>aa</u> anzalta ilayya min khayrin faqiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3277, '28:25', 28, 25, ' faj<u>aa</u>-at-hu i<u>h</u>d<u>aa</u>hum<u>aa</u> tamsyii \'al<u>aa</u> isti<u>h</u>y<u>aa</u>-in q<u>aa</u>lat inna abii yad\'uuka liyajziyaka ajra m<u>aa</u> saqayta lan<u>aa</u> falamm<u>aa</u> j<u>aa</u>-ahu waqa<u>shsh</u>a \'alayhi <b>a</b>lqa<u>sh</u>a<u>sh</u>a q<u>aa</u>la l<u>aa</u> takhaf najawta mina <b>a</b>lqawmi <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3278, '28:26', 28, 26, ' q<u>aa</u>lat i<u>h</u>d<u>aa</u>hum<u>aa</u> y<u>aa</u> abati ista/jirhu inna khayra mani ista/jarta <b>a</b>lqawiyyu <b>a</b>l-amiin<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3279, '28:27', 28, 27, ' q<u>aa</u>la innii uriidu an unki<u>h</u>aka i<u>h</u>d<u>aa</u> ibnatayya h<u>aa</u>tayni \'al<u>aa</u> an ta/juranii tsam<u>aa</u>niya <u>h</u>ijajin fa-in atmamta \'asyran famin \'indika wam<u>aa</u> uriidu an asyuqqa \'alayka satajidunii in sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu mina <b>al</b><u>shshaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3280, '28:28', 28, 28, ' q<u>aa</u>la <u>dzaa</u>lika baynii wabaynaka ayyam<u>aa</u> <b>a</b>l-ajalayni qa<u>dh</u>aytu fal<u>aa</u> \'udw<u>aa</u>na \'alayya wa<b>al</b>l<u>aa</u>hu \'al<u>aa</u> m<u>aa</u> naquulu wakiil<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3281, '28:29', 28, 29, ' falamm<u>aa</u> qa<u>daa</u> muus<u>aa</u> <b>a</b>l-ajala was<u>aa</u>ra bi-ahlihi <u>aa</u>nasa min j<u>aa</u>nibi <b>al</b><u>ththh</u>uuri n<u>aa</u>ran q<u>aa</u>la li-ahlihi umkutsuu innii <u>aa</u>nastu n<u>aa</u>ran la\'allii <u>aa</u>tiikum minh<u>aa</u> bikhabarin aw ja<u>dz</u>watin mina <b>al</b>nn<u>aa</u>ri la\'allakum ta<u>sth</u>aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3282, '28:30', 28, 30, ' falamm<u>aa</u> at<u>aa</u>h<u>aa</u> nuudiya min sy<u>aath</u>i-i <b>a</b>lw<u>aa</u>di <b>a</b>l-aymani fii <b>a</b>lbuq\'ati <b>a</b>lmub<u>aa</u>rakati mina <b>al</b>sysyajarati an y<u>aa</u> muus<u>aa</u> innii an<u>aa</u> <b>al</b>l<u>aa</u>hu rabbu <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3283, '28:31', 28, 31, ' wa-an <b>a</b>lqi \'a<u>shaa</u>ka falamm<u>aa</u> ra<u>aa</u>h<u>aa</u> tahtazzu ka-annah<u>aa</u> j<u>aa</u>nnun wall<u>aa</u> mudbiran walam yu\'aqqib y<u>aa</u> muus<u>aa</u> aqbil wal<u>aa</u> takhaf innaka mina <b>a</b>l-<u>aa</u>miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3284, '28:32', 28, 32, ' usluk yadaka fii jaybika takhruj bay<u>dhaa</u>-a min ghayri suu-in wa<b>u</b><u>dh</u>mum ilayka jan<u>aah</u>aka mina <b>al</b>rrahbi fa<u>dzaa</u>nika burh<u>aa</u>n<u>aa</u>ni min rabbika il<u>aa</u> fir\'awna wamala-ihi innahum k<u>aa</u>nuu qawman f<u>aa</u>siqiin<b>a</b>\r\r\n');
