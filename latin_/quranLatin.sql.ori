INSERT INTO quranLatin VALUES (3286, '28:34', 28, 34, ' wa-akhii h<u>aa</u>ruunu huwa af<u>sh</u>a<u>h</u>u minnii lis<u>aa</u>nan fa-arsilhu ma\'iya rid-an yu<u>sh</u>addiqunii innii akh<u>aa</u>fu an yuka<u>dzdz</u>ibuun<b>i</b>\r\r\n');
