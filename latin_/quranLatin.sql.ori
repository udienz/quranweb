INSERT INTO quranLatin VALUES (611, '4:118', 4, 118, ' la\'anahu <b>al</b>l<u>aa</u>hu waq<u>aa</u>la la-attakhi<u>dz</u>anna min \'ib<u>aa</u>dika na<u>sh</u>iiban mafruu<u>daa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (612, '4:119', 4, 119, ' walau<u>dh</u>illannahum walaumanniyannahum wala<u>aa</u>murannahum falayubattikunna <u>aatsaa</u>na <b>a</b>l-an\'<u>aa</u>mi wala<u>aa</u>murannahum falayughayyirunna khalqa <b>al</b>l<u>aa</u>hi waman yattakhi<u>dz</u>i <b>al</b>sysyay<u>th</u><u>aa</u>na waliyyan min duuni <b>al</b>l<u>aa</u>hi faqad khasira khusr<u>aa</u>nan mubiin<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (613, '4:120', 4, 120, ' ya\'iduhum wayumanniihim wam<u>aa</u> ya\'iduhumu <b>al</b>sysyay<u>th</u><u>aa</u>nu ill<u>aa</u> ghuruur<u>aa</u><b>n</b>\r\r\n');
