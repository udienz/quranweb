INSERT INTO quranLatin VALUES (3316, '28:64', 28, 64, ' waqiila ud\'uu syurak<u>aa</u>-akum fada\'awhum falam yastajiibuu lahum wara-awuu <b>a</b>l\'a<u>dzaa</u>ba law annahum k<u>aa</u>nuu yahtaduun<b>a</b>\r\r\n');
