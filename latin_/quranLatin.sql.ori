INSERT INTO quranLatin VALUES (4477, '45:4', 45, 4, ' wafii khalqikum wam<u>aa</u> yabutstsu min d<u>aa</u>bbatin <u>aa</u>y<u>aa</u>tun liqawmin yuuqinuun<b>a</b>\r\r\n');
