INSERT INTO quranLatin VALUES (3325, '28:73', 28, 73, ' wamin ra<u>h</u>matihi ja\'ala lakumu <b>al</b>layla wa<b>al</b>nnah<u>aa</u>ra litaskunuu fiihi walitabtaghuu min fa<u>dh</u>lihi wala\'allakum tasykuruun<b>a</b>\r\r\n');
