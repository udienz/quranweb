INSERT INTO quranLatin VALUES (5081, '57:6', 57, 6, ' yuuliju <b>al</b>layla fii <b>al</b>nnah<u>aa</u>ri wayuuliju <b>al</b>nnah<u>aa</u>ra fii <b>al</b>layi wahuwa \'aliimun bi<u>dzaa</u>ti <b>al</b><u>shsh</u>uduur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5082, '57:7', 57, 7, ' <u>aa</u>minuu bi<b>al</b>l<u>aa</u>hi warasuulihi wa-anfiquu mimm<u>aa</u> ja\'alakum mustakhlafiina fiihi fa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu minkum wa-anfaquu lahum ajrun kabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5083, '57:8', 57, 8, ' wam<u>aa</u> lakum l<u>aa</u> tu/minuuna bi<b>al</b>l<u>aa</u>hi wa<b>al</b>rrasuulu yad\'uukum litu/minuu birabbikum waqad akha<u>dz</u>a miits<u>aa</u>qakum in kuntum mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5084, '57:9', 57, 9, ' huwa <b>al</b>la<u>dz</u>ii yunazzilu \'al<u>aa</u> \'abdihi <u>aa</u>y<u>aa</u>tin bayyin<u>aa</u>tin liyukhrijakum mina <b>al</b><u>zhzh</u>ulum<u>aa</u>ti il<u>aa</u> <b>al</b>nnuuri wa-inna <b>al</b>l<u>aa</u>ha bikum larauufun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5085, '57:10', 57, 10, ' wam<u>aa</u> lakum <b>al</b>l<u>aa</u> tunfiquu fii sabiili <b>al</b>l<u>aa</u>hi walill<u>aa</u>hi miir<u>aa</u>tsu <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i l<u>aa</u> yastawii minkum man anfaqa min qabli <b>a</b>lfat<u>h</u>i waq<u>aa</u>tala ul<u>aa</u>-ika a\'<u>zh</u>amu darajatan mina <b>al</b>la<u>dz</u>iina anfaquu min ba\'du waq<u>aa</u>taluu wakullan wa\'ada <b>al</b>l<u>aa</u>hu <b>a</b>l<u>h</u>usn<u>aa</u> wa<b>al</b>l<u>aa</u>hu bim<u>aa</u> ta\'maluuna khabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5086, '57:11', 57, 11, ' man <u>dzaa</u> <b>al</b>la<u>dz</u>ii yuqri<u>dh</u>u <b>al</b>l<u>aa</u>ha qar<u>dh</u>an <u>h</u>asanan fayu<u>daa</u>\'ifahu lahu walahu ajrun kariim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5087, '57:12', 57, 12, ' yawma tar<u>aa</u> <b>a</b>lmu/miniina wa<b>a</b>lmu/min<u>aa</u>ti yas\'<u>aa</u> nuuruhum bayna aydiihim wabi-aym<u>aa</u>nihim busyr<u>aa</u>kumu <b>a</b>lyawma jann<u>aa</u>tun tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru kh<u>aa</u>lidiina fiih<u>aa</u> <u>dzaa</u>lika huwa <b>a</b>lfawzu <b>a</b>l\'a<u>zh</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5088, '57:13', 57, 13, ' yawma yaquulu <b>a</b>lmun<u>aa</u>fiquuna wa<b>a</b>lmun<u>aa</u>fiq<u>aa</u>tu lilla<u>dz</u>iina <u>aa</u>manuu un<u>zh</u>uruun<u>aa</u> naqtabis min nuurikum qiila irji\'uu war<u>aa</u>-akum fa<b>i</b>ltamisuu nuuran fa<u>dh</u>uriba baynahum bisuurin lahu b<u>aa</u>bun b<u>aath</u>inuhu fiihi <b>al</b>rra<u>h</u>matu wa<u>zhaa</u>hiruhu min qibalihi <b>a</b>l\'a<u>dzaa</u>b<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5089, '57:14', 57, 14, ' yun<u>aa</u>duunahum alam nakun ma\'akum q<u>aa</u>luu bal<u>aa</u> wal<u>aa</u>kinnakum fatantum anfusakum watarabba<u>sh</u>tum wa<b>i</b>rtabtum wagharratkumu al-am<u>aa</u>niyyu <u>h</u>att<u>aa</u> j<u>aa</u>-a amru <b>al</b>l<u>aa</u>hi wagharrakum bi<b>al</b>l<u>aa</u>hi algharuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5090, '57:15', 57, 15, ' fa<b>a</b>lyawma l<u>aa</u> yu/kha<u>dz</u>u minkum fidyatun wal<u>aa</u> mina <b>al</b>la<u>dz</u>iina kafaruu ma/w<u>aa</u>kumu <b>al</b>nn<u>aa</u>ru hiya mawl<u>aa</u>kum wabi/sa <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5091, '57:16', 57, 16, ' alam ya/ni lilla<u>dz</u>iina <u>aa</u>manuu an takhsya\'a quluubuhum li<u>dz</u>ikri <b>al</b>l<u>aa</u>hi wam<u>aa</u> nazala mina <b>a</b>l<u>h</u>aqqi wal<u>aa</u> yakuunuu ka<b>a</b>lla<u>dz</u>iina uutuu <b>a</b>lkit<u>aa</u>ba min qablu fa<u>th</u><u>aa</u>la \'alayhimu <b>a</b>l-amadu faqasat quluubuhum wakatsiirun minhum f<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5092, '57:17', 57, 17, ' i\'lamuu anna <b>al</b>l<u>aa</u>ha yu<u>h</u>yii <b>a</b>l-ar<u>dh</u>a ba\'da mawtih<u>aa</u> qad bayyann<u>aa</u> lakumu <b>a</b>l-<u>aa</u>y<u>aa</u>ti la\'allakum ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5093, '57:18', 57, 18, ' inna <b>a</b>lmu<u>shsh</u>addiqiina wa<b>a</b>lmu<u>shsh</u>addiq<u>aa</u>ti wa-aqra<u>dh</u>uu <b>al</b>l<u>aa</u>ha qar<u>dh</u>an <u>h</u>asanan yu<u>daa</u>\'afu lahum walahum ajrun kariim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5094, '57:19', 57, 19, ' wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu bi<b>al</b>l<u>aa</u>hi warusulihi ul<u>aa</u>-ika humu <b>al</b><u>shsh</u>iddiiquuna wa<b>al</b>sysyuhad<u>aa</u>u \'inda rabbihim lahum ajruhum wanuuruhum wa<b>a</b>lla<u>dz</u>iina kafaruu waka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> ul<u>aa</u>-ika a<u>sh</u>-<u>haa</u>bu <b>a</b>lja<u>h</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5095, '57:20', 57, 20, ' i\'lamuu annam<u>aa</u> <b>a</b>l<u>h</u>ay<u>aa</u>tu <b>al</b>dduny<u>aa</u> la\'ibun walahwun waziinatun wataf<u>aa</u>khurun baynakum watak<u>aa</u>tsurun fii <b>a</b>l-amw<u>aa</u>li wa<b>a</b>l-awl<u>aa</u>di kamatsali ghaytsin a\'jaba <b>a</b>lkuff<u>aa</u>ra nab<u>aa</u>tuhu tsumma yahiiju fatar<u>aa</u>hu mu<u>sh</u>farran tsumma yakuunu <u>h</u>u<u>th</u><u>aa</u>man wafii <b>a</b>l-<u>aa</u>khirati \'a<u>dzaa</u>bun syadiidun wamaghfiratun mina <b>al</b>l<u>aa</u>hi wari<u>dh</u>w<u>aa</u>nun wam<u>aa</u> <b>a</b>l<u>h</u>ay<u>aa</u>tu <b>al</b>dduny<u>aa</u> ill<u>aa</u> mat<u>aa</u>\'u <b>a</b>lghuruur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5096, '57:21', 57, 21, ' s<u>aa</u>biquu il<u>aa</u> maghfiratin min rabbikum wajannatin \'ar<u>dh</u>uh<u>aa</u> ka\'ar<u>dh</u>i <b>al</b>ssam<u>aa</u>-i wa<b>a</b>l-ar<u>dh</u>i u\'iddat lilla<u>dz</u>iina <u>aa</u>manuu bi<b>al</b>l<u>aa</u>hi warusulihi <u>dzaa</u>lika fa<u>dh</u>lu <b>al</b>l<u>aa</u>hi yu/tiihi man yasy<u>aa</u>u wa<b>al</b>l<u>aa</u>hu <u>dz</u>uu <b>a</b>lfa<u>dh</u>li <b>a</b>l\'a<u>zh</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5097, '57:22', 57, 22, ' m<u>aa</u> a<u>shaa</u>ba min mu<u>sh</u>iibatin fii <b>a</b>l-ar<u>dh</u>i wal<u>aa</u> fii anfusikum ill<u>aa</u> fii kit<u>aa</u>bin min qabli an nabra-ah<u>aa</u> inna <u>dzaa</u>lika \'al<u>aa</u> <b>al</b>l<u>aa</u>hi yasiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5098, '57:23', 57, 23, ' likayl<u>aa</u> ta/saw \'al<u>aa</u> m<u>aa</u> f<u>aa</u>takum wal<u>aa</u> tafra<u>h</u>uu bim<u>aa</u> <u>aa</u>t<u>aa</u>kum wa<b>al</b>l<u>aa</u>hu l<u>aa</u> yu<u>h</u>ibbu kulla mukht<u>aa</u>lin fakhuur<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (5099, '57:24', 57, 24, ' <b>al</b>la<u>dz</u>iina yabkhaluuna waya/muruuna <b>al</b>nn<u>aa</u>sa bi<b>a</b>lbukhli waman yatawalla fa-inna <b>al</b>l<u>aa</u>ha huwa <b>a</b>lghaniyyu <b>a</b>l<u>h</u>amiid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5100, '57:25', 57, 25, ' laqad arsaln<u>aa</u> rusulan<u>aa</u> bi<b>a</b>lbayyin<u>aa</u>ti wa-anzaln<u>aa</u> ma\'ahumu <b>a</b>lkit<u>aa</u>ba wa<b>a</b>lmiiz<u>aa</u>na liyaquuma <b>al</b>nn<u>aa</u>su bi<b>a</b>lqis<u>th</u>i wa-anzaln<u>aa</u> <b>a</b>l<u>h</u>adiida fiihi ba/sun syadiidun waman<u>aa</u>fi\'u li<b>l</b>nn<u>aa</u>si waliya\'lama <b>al</b>l<u>aa</u>hu man yan<u>sh</u>uruhu warusulahu bi<b>a</b>lghaybi inna <b>al</b>l<u>aa</u>ha qawiyyun \'aziiz<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5101, '57:26', 57, 26, ' walaqad arsaln<u>aa</u> nuu<u>h</u>an wa-ibr<u>aa</u>hiima waja\'aln<u>aa</u> fii <u>dz</u>urriyyatihim<u>aa</u> <b>al</b>nnubuwwata wa<b>a</b>lkit<u>aa</u>ba faminhum muhtadin wakatsiirun minhum f<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5102, '57:27', 57, 27, ' tsumma qaffayn<u>aa</u> \'al<u>aa</u> <u>aa</u>ts<u>aa</u>rihim birusulin<u>aa</u> waqaffayn<u>aa</u> bi\'iis<u>aa</u> ibni maryama wa<u>aa</u>tayn<u>aa</u>hu <b>a</b>l-injiila waja\'aln<u>aa</u> fii quluubi <b>al</b>la<u>dz</u>iina ittaba\'uuhu ra/fatan wara<u>h</u>matan warahb<u>aa</u>niyyatan ibtada\'uuh<u>aa</u> m<u>aa</u> katabn<u>aa</u>h<u>aa</u> \'alayhim ill<u>aa</u> ibtigh<u>aa</u>-a ri<u>dh</u>w<u>aa</u>ni <b>al</b>l<u>aa</u>hi fam<u>aa</u> ra\'awh<u>aa</u> <u>h</u>aqqa ri\'<u>aa</u>yatih<u>aa</u> fa<u>aa</u>tayn<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu minhum ajrahum wakatsiirun minhum f<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5103, '57:28', 57, 28, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu ittaquu <b>al</b>l<u>aa</u>ha wa<u>aa</u>minuu birasuulihi yu/tikum kiflayni min ra<u>h</u>matihi wayaj\'al lakum nuuran tamsyuuna bihi wayaghfir lakum wa<b>al</b>l<u>aa</u>hu ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5104, '57:29', 57, 29, ' li-all<u>aa</u> ya\'lama ahlu <b>a</b>lkit<u>aa</u>bi <b>al</b>l<u>aa</u> yaqdiruuna \'al<u>aa</u> syay-in min fa<u>dh</u>li <b>al</b>l<u>aa</u>hi wa-anna <b>a</b>lfa<u>dh</u>la biyadi <b>al</b>l<u>aa</u>hi yu/tiihi man yasy<u>aa</u>u wa<b>al</b>l<u>aa</u>hu <u>dz</u>uu <b>a</b>lfa<u>dh</u>li <b>a</b>l\'a<u>zh</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5105, '58:1', 58, 1, ' qad sami\'a <b>al</b>l<u>aa</u>hu qawla <b>al</b>latii tuj<u>aa</u>diluka fii zawjih<u>aa</u> watasytakii il<u>aa</u> <b>al</b>l<u>aa</u>hi wa<b>al</b>l<u>aa</u>hu yasma\'u ta<u>haa</u>wurakum<u>aa</u> inna <b>al</b>l<u>aa</u>ha samii\'un ba<u>sh</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5106, '58:2', 58, 2, ' <b>al</b>la<u>dz</u>iina yu<u>zhaa</u>hiruuna minkum min nis<u>aa</u>-ihim m<u>aa</u> hunna ummah<u>aa</u>tihim in ummah<u>aa</u>tuhum ill<u>aa</u> <b>al</b>l<u>aa</u>-ii waladnahum wa-innahum layaquuluuna munkaran mina <b>a</b>lqawli wazuuran wa-inna <b>al</b>l<u>aa</u>ha la\'afuwwun ghafuur<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5107, '58:3', 58, 3, ' wa<b>a</b>lla<u>dz</u>iina yu<u>zhaa</u>hiruuna min nis<u>aa</u>-ihim tsumma ya\'uuduuna lim<u>aa</u> q<u>aa</u>luu fata<u>h</u>riiru raqabatin min qabli an yatam<u>aa</u>ss<u>aa</u> <u>dzaa</u>likum tuu\'a<u>zh</u>uuna bihi wa<b>al</b>l<u>aa</u>hu bim<u>aa</u> ta\'maluuna khabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5108, '58:4', 58, 4, ' faman lam yajid fa<u>sh</u>iy<u>aa</u>mu syahrayni mutat<u>aa</u>bi\'ayni min qabli an yatam<u>aa</u>ss<u>aa</u> faman lam yasta<u>th</u>i\' fa-i<u>th</u>\'<u>aa</u>mu sittiina miskiinan <u>dzaa</u>lika litu/minuu bi<b>al</b>l<u>aa</u>hi warasuulihi watilka <u>h</u>uduudu <b>al</b>l<u>aa</u>hi walilk<u>aa</u>firiina \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5109, '58:5', 58, 5, ' inna <b>al</b>la<u>dz</u>iina yu<u>haa</u>dduuna <b>al</b>l<u>aa</u>ha warasuulahu kubituu kam<u>aa</u> kubita <b>al</b>la<u>dz</u>iina min qablihim waqad anzaln<u>aa</u> <u>aa</u>y<u>aa</u>tin bayyin<u>aa</u>tin walilk<u>aa</u>firiina \'a<u>dzaa</u>bun muhiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5110, '58:6', 58, 6, ' yawma yab\'atsuhumu <b>al</b>l<u>aa</u>hu jamii\'an fayunabbi-uhum bim<u>aa</u> \'amiluu a<u>hsaa</u>hu <b>al</b>l<u>aa</u>hu wanasuuhu wa<b>al</b>l<u>aa</u>hu \'al<u>aa</u> kulli syay-in syahiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5111, '58:7', 58, 7, ' alam tara anna <b>al</b>l<u>aa</u>ha ya\'lamu m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i m<u>aa</u> yakuunu min najw<u>aa</u> tsal<u>aa</u>tsatin ill<u>aa</u> huwa r<u>aa</u>bi\'uhum wal<u>aa</u> khamsatin ill<u>aa</u> huwa s<u>aa</u>disuhum wal<u>aa</u> adn<u>aa</u> min <u>dzaa</u>lika wal<u>aa</u> aktsara ill<u>aa</u> huwa ma\'ahum ayna m<u>aa</u> k<u>aa</u>nuu tsumma yunabbi-uhum bim<u>aa</u> \'amiluu yawma <b>a</b>lqiy<u>aa</u>mati inna <b>al</b>l<u>aa</u>ha bikulli syay-in \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5112, '58:8', 58, 8, ' alam tara il<u>aa</u> <b>al</b>la<u>dz</u>iina nuhuu \'ani <b>al</b>nnajw<u>aa</u> tsumma ya\'uuduuna lim<u>aa</u> nuhuu \'anhu wayatan<u>aa</u>jawna bi<b>a</b>l-itsmi wa<b>a</b>l\'udw<u>aa</u>ni wama\'<u>sh</u>iyati <b>al</b>rrasuuli wa-i<u>dzaa</u> j<u>aa</u>uuka <u>h</u>ayyawka bim<u>aa</u> lam yu<u>h</u>ayyika bihi <b>al</b>l<u>aa</u>hu wayaquuluuna fii anfusihim lawl<u>aa</u> yu\'a<u>dzdz</u>ibun<u>aa</u> <b>al</b>l<u>aa</u>hu bim<u>aa</u> naquulu <u>h</u>asbuhum jahannamu ya<u>sh</u>lawnah<u>aa</u> fabi/sa <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5113, '58:9', 58, 9, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu i<u>dzaa</u> tan<u>aa</u>jaytum fal<u>aa</u> tatan<u>aa</u>jaw bi<b>a</b>l-itsmi wa<b>a</b>l\'udw<u>aa</u>ni wama\'<u>sh</u>iyati <b>al</b>rrasuuli watan<u>aa</u>jaw bi<b>a</b>lbirri wa<b>al</b>ttaqw<u>aa</u> wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha <b>al</b>la<u>dz</u>ii ilayhi tu<u>h</u>syaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5114, '58:10', 58, 10, ' innam<u>aa</u> <b>al</b>nnajw<u>aa</u> mina <b>al</b>sysyay<u>th</u><u>aa</u>ni liya<u>h</u>zuna <b>al</b>la<u>dz</u>iina <u>aa</u>manuu walaysa bi<u>daa</u>rrihim syay-an ill<u>aa</u> bi-i<u>dz</u>ni <b>al</b>l<u>aa</u>hi wa\'al<u>aa</u> <b>al</b>l<u>aa</u>hi falyatawakkali <b>a</b>lmu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5115, '58:11', 58, 11, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu i<u>dzaa</u> qiila lakum tafassa<u>h</u>uu fii <b>a</b>lmaj<u>aa</u>lisi fa<b>i</b>fsa<u>h</u>uu yafsa<u>h</u>i <b>al</b>l<u>aa</u>hu lakum wa-i<u>dzaa</u> qiila unsyuzuu fa<b>u</b>nsyuzuu yarfa\'i <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>iina <u>aa</u>manuu minkum wa<b>a</b>lla<u>dz</u>iina uutuu <b>a</b>l\'ilma daraj<u>aa</u>tin wa<b>al</b>l<u>aa</u>hu bim<u>aa</u> ta\'maluuna khabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5116, '58:12', 58, 12, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu i<u>dzaa</u> n<u>aa</u>jaytumu <b>al</b>rrasuula faqaddimuu bayna yaday najw<u>aa</u>kum <u>sh</u>adaqatan <u>dzaa</u>lika khayrun lakum wa-a<u>th</u>haru fa-in lam tajiduu fa-inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5117, '58:13', 58, 13, ' a-asyfaqtum an tuqaddimuu bayna yaday najw<u>aa</u>kum <u>sh</u>adaq<u>aa</u>tin fa-i<u>dz</u> lam taf\'aluu wat<u>aa</u>ba <b>al</b>l<u>aa</u>hu \'alaykum fa-aqiimuu <b>al</b><u>shsh</u>al<u>aa</u>ta wa<u>aa</u>tuu <b>al</b>zzak<u>aa</u>ta wa-a<u>th</u>ii\'uu <b>al</b>l<u>aa</u>ha warasuulahu wa<b>al</b>l<u>aa</u>hu khabiirun bim<u>aa</u> ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5118, '58:14', 58, 14, ' alam tara il<u>aa</u> <b>al</b>la<u>dz</u>iina tawallaw qawman gha<u>dh</u>iba <b>al</b>l<u>aa</u>hu \'alayhim m<u>aa</u> hum minkum wal<u>aa</u> minhum waya<u>h</u>lifuuna \'al<u>aa</u> <b>a</b>lka<u>dz</u>ibi wahum ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5119, '58:15', 58, 15, ' a\'adda <b>al</b>l<u>aa</u>hu lahum \'a<u>dzaa</u>ban syadiidan innahum s<u>aa</u>-a m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5120, '58:16', 58, 16, ' ittakha<u>dz</u>uu aym<u>aa</u>nahum junnatan fa<u>sh</u>adduu \'an sabiili <b>al</b>l<u>aa</u>hi falahum \'a<u>dzaa</u>bun muhiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5121, '58:17', 58, 17, ' lan tughniya \'anhum amw<u>aa</u>luhum wal<u>aa</u> awl<u>aa</u>duhum mina <b>al</b>l<u>aa</u>hi syay-an ul<u>aa</u>-ika a<u>sh</u>-<u>haa</u>bu <b>al</b>nn<u>aa</u>ri hum fiih<u>aa</u> kh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5122, '58:18', 58, 18, ' yawma yab\'atsuhumu <b>al</b>l<u>aa</u>hu jamii\'an faya<u>h</u>lifuuna lahu kam<u>aa</u> ya<u>h</u>lifuuna lakum waya<u>h</u>sabuuna annahum \'al<u>aa</u> syay-in <b>a</b>l<u>aa</u> innahum humu <b>a</b>lk<u>aadz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5123, '58:19', 58, 19, ' ista<u>h</u>wa<u>dz</u>a \'alayhimu <b>al</b>sysyay<u>th</u><u>aa</u>nu fa-ans<u>aa</u>hum <u>dz</u>ikra <b>al</b>l<u>aa</u>hi ul<u>aa</u>-ika <u>h</u>izbu <b>al</b>sysyay<u>th</u><u>aa</u>ni <b>a</b>l<u>aa</u> inna <u>h</u>izba <b>al</b>sysyay<u>th</u><u>aa</u>ni humu <b>a</b>lkh<u>aa</u>siruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5124, '58:20', 58, 20, ' inna <b>al</b>la<u>dz</u>iina yu<u>haa</u>dduuna <b>al</b>l<u>aa</u>ha warasuulahu ul<u>aa</u>-ika fii <b>a</b>l-a<u>dz</u>alliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5125, '58:21', 58, 21, ' kataba <b>al</b>l<u>aa</u>hu la-aghlibanna an<u>aa</u> warusulii inna <b>al</b>l<u>aa</u>ha qawiyyun \'aziiz<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5126, '58:22', 58, 22, ' l<u>aa</u> tajidu qawman yu/minuuna bi<b>al</b>l<u>aa</u>hi wa<b>a</b>lyawmi <b>a</b>l-<u>aa</u>khiri yuw<u>aa</u>dduuna man <u>haa</u>dda <b>al</b>l<u>aa</u>ha warasuulahu walaw k<u>aa</u>nuu <u>aa</u>b<u>aa</u>-ahum aw abn<u>aa</u>-ahum aw ikhw<u>aa</u>nahum aw \'asyiiratahum ul<u>aa</u>-ika kataba fii quluubihimu <b>a</b>l-iim<u>aa</u>na wa-ayyadahum biruu<u>h</u>in minhu wayudkhiluhum jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru kh<u>aa</u>lidiina fiih<u>aa</u> ra<u>dh</u>iya <b>al</b>l<u>aa</u>hu \'anhum wara<u>dh</u>uu \'anhu ul<u>aa</u>-ika <u>h</u>izbu <b>al</b>l<u>aa</u>hi <b>a</b>l<u>aa</u> inna <u>h</u>izba <b>al</b>l<u>aa</u>hi humu <b>a</b>lmufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5127, '59:1', 59, 1, ' sabba<u>h</u>a lill<u>aa</u>hi m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i wahuwa <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5128, '59:2', 59, 2, ' huwa <b>al</b>la<u>dz</u>ii akhraja <b>al</b>la<u>dz</u>iina kafaruu min ahli <b>a</b>lkit<u>aa</u>bi min diy<u>aa</u>rihim li-awwali <b>a</b>l<u>h</u>asyri m<u>aa</u> <u>zh</u>anantum an yakhrujuu wa<u>zh</u>annuu annahum m<u>aa</u>ni\'atuhum <u>h</u>u<u>sh</u>uunuhum mina <b>al</b>l<u>aa</u>hi fa-at<u>aa</u>humu <b>al</b>l<u>aa</u>hu min <u>h</u>aytsu lam ya<u>h</u>tasibuu waqa<u>dz</u>afa fii quluubihimu <b>al</b>rru\'ba yukhribuuna buyuutahum bi-aydiihim wa-aydii <b>a</b>lmu/miniina fa<b>i</b>\'tabiruu y<u>aa</u> ulii <b>a</b>l-ab<u>shaa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5129, '59:3', 59, 3, ' walawl<u>aa</u> an kataba <b>al</b>l<u>aa</u>hu \'alayhimu <b>a</b>ljal<u>aa</u>-a la\'a<u>dzdz</u>abahum fii <b>al</b>dduny<u>aa</u> walahum fii <b>a</b>l-<u>aa</u>khirati \'a<u>dzaa</u>bu <b>al</b>nn<u>aa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5130, '59:4', 59, 4, ' <u>dzaa</u>lika bi-annahum sy<u>aa</u>qquu <b>al</b>l<u>aa</u>ha warasuulahu waman yusy<u>aa</u>qqi <b>al</b>l<u>aa</u>ha fa-inna <b>al</b>l<u>aa</u>ha syadiidu <b>a</b>l\'iq<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5131, '59:5', 59, 5, ' m<u>aa</u> qa<u>th</u>a\'tum min liinatin aw taraktumuuh<u>aa</u> q<u>aa</u>-imatan \'al<u>aa</u> u<u>sh</u>uulih<u>aa</u> fabi-i<u>dz</u>ni <b>al</b>l<u>aa</u>hi waliyukhziya <b>a</b>lf<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5132, '59:6', 59, 6, ' wam<u>aa</u> af<u>aa</u>-a <b>al</b>l<u>aa</u>hu \'al<u>aa</u> rasuulihi minhum fam<u>aa</u> awjaftum \'alayhi min khaylin wal<u>aa</u> rik<u>aa</u>bin wal<u>aa</u>kinna <b>al</b>l<u>aa</u>ha yusalli<u>th</u>u rusulahu \'al<u>aa</u> man yasy<u>aa</u>u wa<b>al</b>l<u>aa</u>hu \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5133, '59:7', 59, 7, ' m<u>aa</u> af<u>aa</u>-a <b>al</b>l<u>aa</u>hu \'al<u>aa</u> rasuulihi min ahli <b>a</b>lqur<u>aa</u> falill<u>aa</u>hi wali<b>l</b>rrasuuli wali<u>dz</u>ii <b>a</b>lqurb<u>aa</u> wa<b>a</b>lyat<u>aa</u>m<u>aa</u> wa<b>a</b>lmas<u>aa</u>kiini wa<b>i</b>bni <b>al</b>ssabiili kay l<u>aa</u> yakuuna duulatan bayna <b>a</b>l-aghniy<u>aa</u>-i minkum wam<u>aa</u> <u>aa</u>t<u>aa</u>kumu <b>al</b>rrasuulu fakhu<u>dz</u>uuhu wam<u>aa</u> nah<u>aa</u>kum \'anhu fa<b>i</b>ntahuu wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha inna <b>al</b>l<u>aa</u>ha syadiidu <b>a</b>l\'iq<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5134, '59:8', 59, 8, ' lilfuqar<u>aa</u>-i <b>a</b>lmuh<u>aa</u>jiriina <b>al</b>la<u>dz</u>iina ukhrijuu min diy<u>aa</u>rihim wa-amw<u>aa</u>lihim yabtaghuuna fa<u>dh</u>lan mina <b>al</b>l<u>aa</u>hi wari<u>dh</u>w<u>aa</u>nan wayan<u>sh</u>uruuna <b>al</b>l<u>aa</u>ha warasuulahu ul<u>aa</u>-ika humu <b>al</b><u>shshaa</u>diquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5135, '59:9', 59, 9, ' wa<b>a</b>lla<u>dz</u>iina tabawwauu <b>al</b>dd<u>aa</u>ra wa<b>a</b>l-iim<u>aa</u>na min qablihim yu<u>h</u>ibbuuna man h<u>aa</u>jara ilayhim wal<u>aa</u> yajiduuna fii <u>sh</u>uduurihim <u>haa</u>jatan mimm<u>aa</u> uutuu wayu/tsiruuna \'al<u>aa</u> anfusihim walaw k<u>aa</u>na bihim kha<u>sas</u>atun waman yuuqa syu<u>hh</u>a nafsihi faul<u>aa</u>-ika humu <b>a</b>lmufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5136, '59:10', 59, 10, ' wa<b>a</b>lla<u>dz</u>iina j<u>aa</u>uu min ba\'dihim yaquuluuna rabban<u>aa</u> ighfir lan<u>aa</u> wali-ikhw<u>aa</u>nin<u>aa</u> <b>al</b>la<u>dz</u>iina sabaquun<u>aa</u> bi<b>a</b>l-iim<u>aa</u>ni wal<u>aa</u> taj\'al fii quluubin<u>aa</u> ghillan lilla<u>dz</u>iina <u>aa</u>manuu rabban<u>aa</u> innaka rauufun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5137, '59:11', 59, 11, ' alam tara il<u>aa</u> <b>al</b>la<u>dz</u>iina n<u>aa</u>faquu yaquuluuna li-ikhw<u>aa</u>nihimu <b>al</b>la<u>dz</u>iina kafaruu min ahli <b>a</b>lkit<u>aa</u>bi la-in ukhrijtum lanakhrujanna ma\'akum wal<u>aa</u> nu<u>th</u>ii\'u fiikum a<u>h</u>adan abadan wa-in quutiltum lanan<u>sh</u>urannakum wa<b>al</b>l<u>aa</u>hu yasyhadu innahum lak<u>aadz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5138, '59:12', 59, 12, ' la-in ukhrijuu l<u>aa</u> yakhrujuuna ma\'ahum wala-in quutiluu l<u>aa</u> yan<u>sh</u>uruunahum wala-in na<u>sh</u>aruuhum layuwallunna <b>a</b>l-adb<u>aa</u>ra tsumma l<u>aa</u> yun<u>sh</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5139, '59:13', 59, 13, ' la-antum asyaddu rahbatan fii <u>sh</u>uduurihim mina <b>al</b>l<u>aa</u>hi <u>dzaa</u>lika bi-annahum qawmun l<u>aa</u> yafqahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5140, '59:14', 59, 14, ' l<u>aa</u> yuq<u>aa</u>tiluunakum jamii\'an ill<u>aa</u> fii quran mu<u>h</u>a<u>shsh</u>anatin aw min war<u>aa</u>-i judurin ba/suhum baynahum syadiidun ta<u>h</u>sabuhum jamii\'an waquluubuhum syatt<u>aa</u> <u>dzaa</u>lika bi-annahum qawmun l<u>aa</u> ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5141, '59:15', 59, 15, ' kamatsali <b>al</b>la<u>dz</u>iina min qablihim qariiban <u>dzaa</u>quu wab<u>aa</u>la amrihim walahum \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5142, '59:16', 59, 16, ' kamatsali <b>al</b>sysyay<u>th</u><u>aa</u>ni i<u>dz</u> q<u>aa</u>la lil-ins<u>aa</u>ni ukfur falamm<u>aa</u> kafara q<u>aa</u>la innii barii-un minka innii akh<u>aa</u>fu <b>al</b>l<u>aa</u>ha rabba <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5143, '59:17', 59, 17, ' fak<u>aa</u>na \'<u>aa</u>qibatahum<u>aa</u> annahum<u>aa</u> fii <b>al</b>nn<u>aa</u>ri kh<u>aa</u>lidayni fiih<u>aa</u> wa<u>dzaa</u>lika jaz<u>aa</u>u <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5144, '59:18', 59, 18, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu ittaquu <b>al</b>l<u>aa</u>ha waltan<u>zh</u>ur nafsun m<u>aa</u> qaddamat lighadin wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha inna <b>al</b>l<u>aa</u>ha khabiirun bim<u>aa</u> ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5145, '59:19', 59, 19, ' wal<u>aa</u> takuunuu ka<b>a</b>lla<u>dz</u>iina nasuu <b>al</b>l<u>aa</u>ha fa-ans<u>aa</u>hum anfusahum ul<u>aa</u>-ika humu <b>a</b>lf<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5146, '59:20', 59, 20, ' l<u>aa</u> yastawii a<u>sh</u>-<u>haa</u>bu <b>al</b>nn<u>aa</u>ri wa-a<u>sh</u>-<u>haa</u>bu <b>a</b>ljannati a<u>sh</u>-<u>haa</u>bu <b>a</b>ljannati humu <b>a</b>lf<u>aa</u>-izuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5147, '59:21', 59, 21, ' law anzaln<u>aa</u> h<u>aadzaa</u> <b>a</b>lqur-<u>aa</u>na \'al<u>aa</u> jabalin lara-aytahu kh<u>aa</u>syi\'an muta<u>sh</u>addi\'an min khasyyati <b>al</b>l<u>aa</u>hi watilka <b>a</b>l-amts<u>aa</u>lu na<u>dh</u>ribuh<u>aa</u> li<b>l</b>nn<u>aa</u>si la\'allahum yatafakkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5148, '59:22', 59, 22, ' huwa <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa \'<u>aa</u>limu <b>a</b>lghaybi wa<b>al</b>sysyah<u>aa</u>dati huwa <b>al</b>rra<u>h</u>m<u>aa</u>nu <b>al</b>rra<u>h</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5149, '59:23', 59, 23, ' huwa <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa <b>a</b>lmaliku <b>a</b>lqudduusu <b>al</b>ssal<u>aa</u>mu <b>a</b>lmu/minu <b>a</b>lmuhayminu <b>a</b>l\'aziizu <b>a</b>ljabb<u>aa</u>ru <b>a</b>lmutakabbiru sub<u>haa</u>na <b>al</b>l<u>aa</u>hi \'amm<u>aa</u> yusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5150, '59:24', 59, 24, ' huwa <b>al</b>l<u>aa</u>hu <b>a</b>lkh<u>aa</u>liqu <b>a</b>lb<u>aa</u>ri-u <b>a</b>lmu<u>sh</u>awwiru lahu <b>a</b>l-asm<u>aa</u>u <b>a</b>l<u>h</u>usn<u>aa</u> yusabbi<u>h</u>u lahu m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wahuwa <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5151, '60:1', 60, 1, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tattakhi<u>dz</u>uu \'aduwwii wa\'aduwwakum awliy<u>aa</u>-a tulquuna ilayhim bi<b>a</b>lmawaddati waqad kafaruu bim<u>aa</u> j<u>aa</u>-akum mina <b>a</b>l<u>h</u>aqqi yukhrijuuna <b>al</b>rrasuula wa-iyy<u>aa</u>kum an tu/minuu bi<b>al</b>l<u>aa</u>hi rabbikum in kuntum kharajtum jih<u>aa</u>dan fii sabiilii wa<b>i</b>btigh<u>aa</u>-a mar<u>daa</u>tii tusirruuna ilayhim bi<b>a</b>lmawaddati wa-an<u>aa</u> a\'lamu bim<u>aa</u> akhfaytum wam<u>aa</u> a\'lantum waman yaf\'alhu minkum faqad <u>dh</u>alla saw<u>aa</u>-a <b>al</b>ssabiil<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5152, '60:2', 60, 2, ' in yatsqafuukum yakuunuu lakam a\'d<u>aa</u>-an wayabsu<u>th</u>uu ilaykum aydiyahum wa-alsinatahum bi<b>al</b>ssuu-i wawadduu law takfuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5153, '60:3', 60, 3, ' lan tanfa\'akum ar<u>haa</u>mukum wal<u>aa</u> awl<u>aa</u>dukum yawma <b>a</b>lqiy<u>aa</u>mati yaf<u>sh</u>ilu baynakum wa<b>al</b>l<u>aa</u>hu bim<u>aa</u> ta\'maluuna ba<u>sh</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5154, '60:4', 60, 4, ' qad k<u>aa</u>nat lakum uswatun <u>h</u>asanatun fii ibr<u>aa</u>hiima wa<b>a</b>lla<u>dz</u>iina ma\'ahu i<u>dz</u> q<u>aa</u>luu liqawmihim inn<u>aa</u> bura<u>aa</u>u minkum wamimm<u>aa</u> ta\'buduuna min duuni <b>al</b>l<u>aa</u>hi kafarn<u>aa</u> bikum wabad<u>aa</u> baynan<u>aa</u> wabaynakumu <b>a</b>l\'ad<u>aa</u>watu wa<b>a</b>lbagh<u>daa</u>u abadan <u>h</u>att<u>aa</u> tu/minuu bi<b>al</b>l<u>aa</u>hi wa<u>h</u>dahu ill<u>aa</u> qawla ibr<u>aa</u>hiima li-abiihi la-astaghfiranna laka wam<u>aa</u> amliku laka mina <b>al</b>l<u>aa</u>hi min syay-in rabban<u>aa</u> \'alayka tawakkaln<u>aa</u> wa-ilayka anabn<u>aa</u> wa-ilayka <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5155, '60:5', 60, 5, ' rabban<u>aa</u> l<u>aa</u> taj\'aln<u>aa</u> fitnatan lilla<u>dz</u>iina kafaruu wa<b>i</b>ghfir lan<u>aa</u> rabban<u>aa</u> innaka anta <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5156, '60:6', 60, 6, ' laqad k<u>aa</u>na lakum fiihim uswatun <u>h</u>asanatun liman k<u>aa</u>na yarjuu <b>al</b>l<u>aa</u>ha wa<b>a</b>lyawma <b>a</b>l-<u>aa</u>khira waman yatawalla fa-inna <b>al</b>l<u>aa</u>ha huwa <b>a</b>lghanniyyu <b>a</b>l<u>h</u>amiid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5157, '60:7', 60, 7, ' \'as<u>aa</u> <b>al</b>l<u>aa</u>hu an yaj\'ala baynakum wabayna <b>al</b>la<u>dz</u>iina \'<u>aa</u>daytum minhum mawaddatan wa<b>al</b>l<u>aa</u>hu qadiirun wa<b>al</b>l<u>aa</u>hu ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5158, '60:8', 60, 8, ' l<u>aa</u> yanh<u>aa</u>kumu <b>al</b>l<u>aa</u>hu \'ani <b>al</b>la<u>dz</u>iina lam yuq<u>aa</u>tiluukum fii <b>al</b>ddiini walam yukhrijuukum min diy<u>aa</u>rikum an tabarruuhum watuqsi<u>th</u>uu ilayhim inna <b>al</b>l<u>aa</u>ha yu<u>h</u>ibbu <b>a</b>lmuqsi<u>th</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5159, '60:9', 60, 9, ' innam<u>aa</u> yanh<u>aa</u>kumu <b>al</b>l<u>aa</u>hu \'ani <b>al</b>la<u>dz</u>iina q<u>aa</u>taluukum fii <b>al</b>ddiini wa-akhrajuukum min diy<u>aa</u>rikum wa<u>zhaa</u>haruu \'al<u>aa</u> ikhr<u>aa</u>jikum an tawallawhum waman yatawallahum faul<u>aa</u>-ika humu <b>al</b><u>zhzhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5160, '60:10', 60, 10, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu i<u>dzaa</u> j<u>aa</u>-akumu <b>a</b>lmu/min<u>aa</u>tu muh<u>aa</u>jir<u>aa</u>tin fa<b>i</b>mta<u>h</u>inuuhunna <b>al</b>l<u>aa</u>hu a\'lamu bi-iim<u>aa</u>nihinna fa-in \'alimtumuuhunna mu/min<u>aa</u>tin fal<u>aa</u> tarji\'uuhunna il<u>aa</u> <b>a</b>lkuff<u>aa</u>ri l<u>aa</u> hunna <u>h</u>illun lahum wal<u>aa</u> hum ya<u>h</u>illuuna lahunna wa<u>aa</u>tuuhum m<u>aa</u> anfaquu wal<u>aa</u> jun<u>aah</u>a \'alaykum an tanki<u>h</u>uuhunna i<u>dzaa</u> <u>aa</u>taytumuuhunna ujuurahunna wal<u>aa</u> tumsikuu bi\'i<u>sh</u>ami <b>a</b>lkaw<u>aa</u>firi wa<b>i</b>s-aluu m<u>aa</u> anfaqtum walyas-aluu m<u>aa</u> anfaquu <u>dzaa</u>likum <u>h</u>ukmu <b>al</b>l<u>aa</u>hi ya<u>h</u>kumu baynakum wa<b>al</b>l<u>aa</u>hu \'aliimun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5161, '60:11', 60, 11, ' wa-in f<u>aa</u>takum syay-un min azw<u>aa</u>jikum il<u>aa</u> <b>a</b>lkuff<u>aa</u>ri fa\'<u>aa</u>qabtum fa<u>aa</u>tuu <b>al</b>la<u>dz</u>iina <u>dz</u>ahabat azw<u>aa</u>juhum mitsla m<u>aa</u> anfaquu wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha <b>al</b>la<u>dz</u>ii antum bihi mu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5162, '60:12', 60, 12, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nnabiyyu i<u>dzaa</u> j<u>aa</u>-aka <b>a</b>lmu/min<u>aa</u>tu yub<u>aa</u>yi\'naka \'al<u>aa</u> an l<u>aa</u> yusyrikna bi<b>al</b>l<u>aa</u>hi syay-an wal<u>aa</u> yasriqna wal<u>aa</u> yazniina wal<u>aa</u> yaqtulna awl<u>aa</u>dahunna wal<u>aa</u> ya/tiina bibuht<u>aa</u>nin yaftariinahu bayna aydiihinna wa-arjulihinna wal<u>aa</u> ya\'<u>sh</u>iinaka fii ma\'ruufin fab<u>aa</u>yi\'hunna wa<b>i</b>staghfir lahunna <b>al</b>l<u>aa</u>ha inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5163, '60:13', 60, 13, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tatawallaw qawman gha<u>dh</u>iba <b>al</b>l<u>aa</u>hu \'alayhim qad ya-isuu mina <b>a</b>l-<u>aa</u>khirati kam<u>aa</u> ya-isa <b>a</b>lkuff<u>aa</u>ru min a<u>sh</u>-<u>haa</u>bi <b>a</b>lqubuur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5164, '61:1', 61, 1, ' sabba<u>h</u>a lill<u>aa</u>hi m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i wahuwa <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5165, '61:2', 61, 2, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu lima taquuluuna m<u>aa</u> l<u>aa</u> taf\'aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5166, '61:3', 61, 3, ' kabura maqtan \'inda <b>al</b>l<u>aa</u>hi an taquuluu m<u>aa</u> l<u>aa</u> taf\'aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5167, '61:4', 61, 4, ' inna <b>al</b>l<u>aa</u>ha yu<u>h</u>ibbu <b>al</b>la<u>dz</u>iina yuq<u>aa</u>tiluuna fii sabiilihi <u>sh</u>affan ka-annahum buny<u>aa</u>nun mar<u>sh</u>uu<u>sh</u><b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5168, '61:5', 61, 5, ' wa-i<u>dz</u> q<u>aa</u>la muus<u>aa</u> liqawmihi y<u>aa</u> qawmi lima tu/<u>dz</u>uunanii waqad ta\'lamuuna annii rasuulu <b>al</b>l<u>aa</u>hi ilaykum falamm<u>aa</u> z<u>aa</u>ghuu az<u>aa</u>gha <b>al</b>l<u>aa</u>hu quluubahum wa<b>al</b>l<u>aa</u>hu l<u>aa</u> yahdii <b>a</b>lqawma <b>a</b>lf<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5169, '61:6', 61, 6, ' wa-i<u>dz</u> q<u>aa</u>la \'iis<u>aa</u> ibnu maryama y<u>aa</u> banii isr<u>aa</u>-iila innii rasuulu <b>al</b>l<u>aa</u>hi ilaykum mu<u>sh</u>addiqan lim<u>aa</u> bayna yadayya mina <b>al</b>ttawr<u>aa</u>ti wamubasysyiran birasuulin ya/tii min ba\'dii ismuhu a<u>h</u>madu falamm<u>aa</u> j<u>aa</u>-ahum bi<b>a</b>lbayyin<u>aa</u>ti q<u>aa</u>luu h<u>aadzaa</u> si<u>h</u>run mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5170, '61:7', 61, 7, ' waman a<u>zh</u>lamu mimmani iftar<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi <b>a</b>lka<u>dz</u>iba wahuwa yud\'<u>aa</u> il<u>aa</u> <b>a</b>l-isl<u>aa</u>mi wa<b>al</b>l<u>aa</u>hu l<u>aa</u> yahdii <b>a</b>lqawma <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5171, '61:8', 61, 8, ' yuriiduuna liyu<u>th</u>fi-uu nuura <b>al</b>l<u>aa</u>hi bi-afw<u>aa</u>hihim wa<b>al</b>l<u>aa</u>hu mutimmu nuurihi walaw kariha <b>a</b>lk<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5172, '61:9', 61, 9, ' huwa <b>al</b>la<u>dz</u>ii arsala rasuulahu bi<b>a</b>lhud<u>aa</u> wadiini <b>a</b>l<u>h</u>aqqi liyu<u>zh</u>hirahu \'al<u>aa</u> <b>al</b>ddiini kullihi walaw kariha <b>a</b>lmusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5173, '61:10', 61, 10, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu hal adullukum \'al<u>aa</u> tij<u>aa</u>ratin tunjiikum min \'a<u>dzaa</u>bin <b>a</b>liim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (5174, '61:11', 61, 11, ' tu/minuuna bi<b>al</b>l<u>aa</u>hi warasuulihi watuj<u>aa</u>hiduuna fii sabiili <b>al</b>l<u>aa</u>hi bi-amw<u>aa</u>likum wa-anfusikum <u>dzaa</u>likum khayrun lakum in kuntum ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5175, '61:12', 61, 12, ' yaghfir lakum <u>dz</u>unuubakum wayudkhilkum jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru wamas<u>aa</u>kina <u>th</u>ayyibatan fii jann<u>aa</u>ti \'adnin <u>dzaa</u>lika <b>a</b>lfawzu <b>a</b>l\'a<u>zh</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5176, '61:13', 61, 13, ' waukhr<u>aa</u> tu<u>h</u>ibbuunah<u>aa</u> na<u>sh</u>run mina <b>al</b>l<u>aa</u>hi wafat<u>h</u>un qarriibun wabasysyiri <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5177, '61:14', 61, 14, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu kuunuu an<u>shaa</u>ra <b>al</b>l<u>aa</u>hi kam<u>aa</u> q<u>aa</u>la \'iis<u>aa</u> ibnu maryama lil<u>h</u>aw<u>aa</u>riyyiina man an<u>shaa</u>rii il<u>aa</u> <b>al</b>l<u>aa</u>hi q<u>aa</u>la <b>a</b>l<u>h</u>aw<u>aa</u>riyyuuna na<u>h</u>nu an<u>shaa</u>ru <b>al</b>l<u>aa</u>hi fa<u>aa</u>manat <u>th</u><u>aa</u>-ifatun min banii isr<u>aa</u>-iila wakafarat <u>th</u><u>aa</u>-ifatun fa-ayyadn<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu \'al<u>aa</u> \'aduwwihim fa-a<u>sh</u>ba<u>h</u>uu <u>zhaa</u>hiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5178, '62:1', 62, 1, ' yusabbi<u>h</u>u lill<u>aa</u>hi m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i <b>a</b>lmaliki <b>a</b>lqudduusi <b>a</b>l\'aziizi <b>a</b>l<u>h</u>akiim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5179, '62:2', 62, 2, ' huwa <b>al</b>la<u>dz</u>ii ba\'atsa fii <b>a</b>l-ummiyyiina rasuulan minhum yatluu \'alayhim <u>aa</u>y<u>aa</u>tihi wayuzakkiihim wayu\'allimuhumu <b>a</b>lkit<u>aa</u>ba wa<b>a</b>l<u>h</u>ikmata wa-in k<u>aa</u>nuu min qablu lafii <u>dh</u>al<u>aa</u>lin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (5180, '62:3', 62, 3, ' wa<u>aa</u>khariina minhum lamm<u>aa</u> yal<u>h</u>aquu bihim wahuwa <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5181, '62:4', 62, 4, ' <u>dzaa</u>lika fa<u>dh</u>lu <b>al</b>l<u>aa</u>hi yu/tiihi man yasy<u>aa</u>u wa<b>al</b>l<u>aa</u>hu <u>dz</u>uu <b>a</b>lfa<u>dh</u>li <b>a</b>l\'a<u>zh</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5182, '62:5', 62, 5, ' matsalu <b>al</b>la<u>dz</u>iina <u>h</u>ummiluu <b>al</b>ttawr<u>aa</u>ta tsumma lam ya<u>h</u>miluuh<u>aa</u> kamatsali <b>a</b>l<u>h</u>im<u>aa</u>ri ya<u>h</u>milu asf<u>aa</u>ran bi/sa matsalu <b>a</b>lqawmi <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi wa<b>al</b>l<u>aa</u>hu l<u>aa</u> yahdii <b>a</b>lqawma <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5183, '62:6', 62, 6, ' qul y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina h<u>aa</u>duu in za\'amtum annakum awliy<u>aa</u>u lill<u>aa</u>hi min duuni <b>al</b>nn<u>aa</u>si fatamannawuu <b>a</b>lmawta in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5184, '62:7', 62, 7, ' wal<u>aa</u> yatamannawnahu abadan bim<u>aa</u> qaddamat aydiihim wa<b>al</b>l<u>aa</u>hu \'aliimun bi<b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5185, '62:8', 62, 8, ' qul inna <b>a</b>lmawta <b>al</b>la<u>dz</u>ii tafirruuna minhu fa-innahu mul<u>aa</u>qiikum tsumma turadduuna il<u>aa</u> \'<u>aa</u>limi <b>a</b>lghaybi wa<b>al</b>sysyah<u>aa</u>dati fayunabbi-ukum bim<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5186, '62:9', 62, 9, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu i<u>dzaa</u> nuudiya li<b>l</b><u>shsh</u>al<u>aa</u>ti min yawmi <b>a</b>ljumu\'ati fa<b>i</b>s\'aw il<u>aa</u> <u>dz</u>ikri <b>al</b>l<u>aa</u>hi wa<u>dz</u>aruu <b>a</b>lbay\'a <u>dzaa</u>likum khayrun lakum in kuntum ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5187, '62:10', 62, 10, ' fa-i<u>dzaa</u> qu<u>dh</u>iyati <b>al</b><u>shsh</u>al<u>aa</u>tu fa<b>i</b>ntasyiruu fii <b>a</b>l-ar<u>dh</u>i wa<b>i</b>btaghuu min fa<u>dh</u>li <b>al</b>l<u>aa</u>hi wa<b>u</b><u>dz</u>kuruu <b>al</b>l<u>aa</u>ha katsiiran la\'allakum tufli<u>h</u>uun<b>a </b>\r\r\n');
INSERT INTO quranLatin VALUES (5188, '62:11', 62, 11, ' wa-i<u>dzaa</u> ra-aw tij<u>aa</u>ratan aw lahwan infa<u>dhdh</u>uu ilayh<u>aa</u> watarakuuka q<u>aa</u>-iman qul m<u>aa</u> \'inda <b>al</b>l<u>aa</u>hi khayrun mina <b>al</b>lahwi wamina <b>al</b>ttij<u>aa</u>rati wa<b>al</b>l<u>aa</u>hu khayru <b>al</b>rr<u>aa</u>ziqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5189, '63:1', 63, 1, ' i<u>dzaa</u> j<u>aa</u>-aka <b>a</b>lmun<u>aa</u>fiquuna q<u>aa</u>luu nasyhadu innaka larasuulu <b>al</b>l<u>aa</u>hi wa<b>al</b>l<u>aa</u>hu ya\'lamu innaka larasuuluhu wa<b>al</b>l<u>aa</u>hu yasyhadu inna <b>a</b>lmun<u>aa</u>fiqiina lak<u>aadz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5190, '63:2', 63, 2, ' ittakha<u>dz</u>uu aym<u>aa</u>nahum junnatan fa<u>sh</u>adduu \'an sabiili <b>al</b>l<u>aa</u>hi innahum s<u>aa</u>-a m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5191, '63:3', 63, 3, ' <u>dzaa</u>lika bi-annahum <u>aa</u>manuu tsumma kafaruu fa<u>th</u>ubi\'a \'al<u>aa</u> quluubihim fahum l<u>aa</u> yafqahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5192, '63:4', 63, 4, ' wa-i<u>dzaa</u> ra-aytahum tu\'jibuka ajs<u>aa</u>muhum wa-in yaquuluu tasma\' liqawlihim ka-annahum khusyubun musannadatun ya<u>h</u>sabuuna kulla <u>sh</u>ay<u>h</u>atin \'alayhim humu <b>a</b>l\'aduwwu fa<b>i</b><u>hts</u>arhum q<u>aa</u>talahumu <b>al</b>l<u>aa</u>hu ann<u>aa</u> yu/fakuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5193, '63:5', 63, 5, ' wa-i<u>dzaa</u> qiila lahum ta\'<u>aa</u>law yastaghfir lakum rasuulu <b>al</b>l<u>aa</u>hi lawwaw ruuusahum wara-aytahum ya<u>sh</u>udduuna wahum mustakbiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5194, '63:6', 63, 6, ' saw<u>aa</u>un \'alayhim astaghfarta lahum am lam tastaghfir lahum lan yaghfira <b>al</b>l<u>aa</u>hu lahum inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yahdii <b>a</b>lqawma <b>a</b>lf<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5195, '63:7', 63, 7, ' humu <b>al</b>la<u>dz</u>iina yaquuluuna l<u>aa</u> tunfiquu \'al<u>aa</u> man \'inda rasuuli <b>al</b>l<u>aa</u>hi <u>h</u>att<u>aa</u> yanfa<u>dhdh</u>uu walill<u>aa</u>hi khaz<u>aa</u>-inu <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wal<u>aa</u>kinna <b>a</b>lmun<u>aa</u>fiqiina l<u>aa</u> yafqahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5196, '63:8', 63, 8, ' yaquuluuna la-in raja\'n<u>aa</u> il<u>aa</u> <b>a</b>lmadiinati layukhrijanna <b>a</b>l-a\'azzu minh<u>aa</u> <b>a</b>l-a<u>dz</u>alla walill<u>aa</u>hi <b>a</b>l\'izzatu walirasuulihi walilmu/miniina wal<u>aa</u>kinna <b>a</b>lmun<u>aa</u>fiqiina l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5197, '63:9', 63, 9, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tulhikum amw<u>aa</u>lukum wal<u>aa</u> awl<u>aa</u>dukum \'an <u>dz</u>ikri <b>al</b>l<u>aa</u>hi waman yaf\'al <u>dzaa</u>lika faul<u>aa</u>-ika humu <b>a</b>lkh<u>aa</u>siruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5198, '63:10', 63, 10, ' wa-anfiquu min m<u>aa</u> razaqn<u>aa</u>kum min qabli an ya/tiya a<u>h</u>adakumu <b>a</b>lmawtu fayaquula rabbi lawl<u>aa</u> akhkhartanii il<u>aa</u> ajalin qariibin fa-a<u>shsh</u>addaqa wa-akun mina <b>al</b><u>shshaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5199, '63:11', 63, 11, ' walan yu-akhkhira <b>al</b>l<u>aa</u>hu nafsan i<u>dzaa</u> j<u>aa</u>-a ajaluh<u>aa</u> wa<b>al</b>l<u>aa</u>hu khabiirun bim<u>aa</u> ta\'maluun<b>a</b><dir>\r\r\n');
INSERT INTO quranLatin VALUES (5200, '64:1', 64, 1, ' yusabbi<u>h</u>u lill<u>aa</u>hi m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i lahu <b>a</b>lmulku walahu <b>a</b>l<u>h</u>amdu wahuwa \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5201, '64:2', 64, 2, ' huwa <b>al</b>la<u>dz</u>ii khalaqakum faminkum k<u>aa</u>firun waminkum mu/minun wa<b>al</b>l<u>aa</u>hu bim<u>aa</u> ta\'maluuna ba<u>sh</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5202, '64:3', 64, 3, ' khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a bi<b>a</b>l<u>h</u>aqqi wa<u>sh</u>awwarakum fa-a<u>h</u>sana <u>sh</u>uwarakum wa-ilayhi <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5203, '64:4', 64, 4, ' ya\'lamu m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i waya\'lamu m<u>aa</u> tusirruuna wam<u>aa</u> tu\'linuuna wa<b>al</b>l<u>aa</u>hu \'aliimun bi<u>dzaa</u>ti <b>al</b><u>shsh</u>uduur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5204, '64:5', 64, 5, ' alam ya/tikum nabau <b>al</b>la<u>dz</u>iina kafaruu min qablu fa<u>dzaa</u>quu wab<u>aa</u>la amrihim walahum \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5205, '64:6', 64, 6, ' <u>dzaa</u>lika bi-annahu k<u>aa</u>nat ta/tiihim rusuluhum bi<b>a</b>lbayyin<u>aa</u>ti faq<u>aa</u>luu abasyarun yahduunan<u>aa</u> fakafaruu watawallaw wa<b>i</b>staghn<u>aa</u> <b>al</b>l<u>aa</u>hu wa<b>al</b>l<u>aa</u>hu ghaniyyun <u>h</u>amiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5206, '64:7', 64, 7, ' za\'ama <b>al</b>la<u>dz</u>iina kafaruu an lan yub\'atsuu qul bal<u>aa</u> warabbii latub\'atsunna tsumma latunabbaunna bim<u>aa</u> \'amiltum wa<u>dzaa</u>lika \'al<u>aa</u> <b>al</b>l<u>aa</u>hi yasiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5207, '64:8', 64, 8, ' fa<u>aa</u>minuu bi<b>al</b>l<u>aa</u>hi warasuulihi wa<b>al</b>nnuuri <b>al</b>la<u>dz</u>ii anzaln<u>aa</u> wa<b>al</b>l<u>aa</u>hu bim<u>aa</u> ta\'maluuna khabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5208, '64:9', 64, 9, ' yawma yajma\'ukum liyawmi <b>a</b>ljam\'i <u>dzaa</u>lika yawmu <b>al</b>ttagh<u>aa</u>buni waman yu/min bi<b>al</b>l<u>aa</u>hi waya\'mal <u>shaa</u>li<u>h</u>an yukaffir \'anhu sayyi-<u>aa</u>tihi wayudkhilhu jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru kh<u>aa</u>lidiina fiih<u>aa</u> abadan <u>dzaa</u>lika <b>a</b>lfawzu <b>a</b>l\'a<u>zh</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5209, '64:10', 64, 10, ' wa<b>a</b>lla<u>dz</u>iina kafaruu waka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> ul<u>aa</u>-ika a<u>sh</u>-<u>haa</u>bu <b>al</b>nn<u>aa</u>ri kh<u>aa</u>lidiina fiih<u>aa</u> wabi/sa <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5210, '64:11', 64, 11, ' m<u>aa</u> a<u>shaa</u>ba min mu<u>sh</u>iibatin ill<u>aa</u> bi-i<u>dz</u>ni <b>al</b>l<u>aa</u>hi waman yu/min bi<b>al</b>l<u>aa</u>hi yahdi qalbahu wa<b>al</b>l<u>aa</u>hu bikulli syay-in \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5211, '64:12', 64, 12, ' wa-a<u>th</u>ii\'uu <b>al</b>l<u>aa</u>ha wa-a<u>th</u>ii\'uu <b>al</b>rrasuula fa-in tawallaytum fa-innam<u>aa</u> \'al<u>aa</u> rasuulin<u>aa</u> <b>a</b>lbal<u>aa</u>ghu <b>a</b>lmubiin<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5212, '64:13', 64, 13, ' <b>al</b>l<u>aa</u>hu l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa wa\'al<u>aa</u> <b>al</b>l<u>aa</u>hi falyatawakkali <b>a</b>lmu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5213, '64:14', 64, 14, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu inna min azw<u>aa</u>jikum wa-awl<u>aa</u>dikum \'aduwwan lakum fa<b>i</b><u>hts</u>aruuhum wa-in ta\'fuu wata<u>sh</u>fa<u>h</u>uu wataghfiruu fa-inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5214, '64:15', 64, 15, ' innam<u>aa</u> amw<u>aa</u>lukum wa-awl<u>aa</u>dukum fitnatun wa<b>al</b>l<u>aa</u>hu \'indahu ajrun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5215, '64:16', 64, 16, ' fa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha m<u>aa</u> ista<u>th</u>a\'tum wa<b>i</b>sma\'uu wa-a<u>th</u>ii\'uu wa-anfiquu khayran li-anfusikum waman yuuqa syu<u>hh</u>a nafsihi faul<u>aa</u>-ika humu <b>a</b>lmufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5216, '64:17', 64, 17, ' in tuqri<u>dh</u>uu <b>al</b>l<u>aa</u>ha qar<u>dh</u>an <u>h</u>asanan yu<u>daa</u>\'ifhu lakum wayaghfir lakum wa<b>al</b>l<u>aa</u>hu syakuurun <u>h</u>aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5217, '64:18', 64, 18, ' \'<u>aa</u>limu <b>a</b>lghaybi wa<b>al</b>sysyah<u>aa</u>dati <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5218, '65:1', 65, 1, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nnabiyyu i<u>dzaa</u> <u>th</u>allaqtumu <b>al</b>nnis<u>aa</u>-a fa<u>th</u>alliquuhunna li\'iddatihinna wa-a<u>hs</u>uu <b>a</b>l\'iddata wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha rabbakum l<u>aa</u> tukhrijuuhunna min buyuutihinna wal<u>aa</u> yakhrujna ill<u>aa</u> an ya/tiina bif<u>aah</u>isyatin mubayyinatin watilka <u>h</u>uduudu <b>al</b>l<u>aa</u>hi waman yata\'adda <u>h</u>uduuda <b>al</b>l<u>aa</u>hi faqad <u>zh</u>alama nafsahu l<u>aa</u> tadrii la\'alla <b>al</b>l<u>aa</u>ha yu<u>h</u>ditsu ba\'da <u>dzaa</u>lika amr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (5219, '65:2', 65, 2, ' fa-i<u>dzaa</u> balaghna ajalahunna fa-amsikuuhunna bima\'ruufin aw f<u>aa</u>riquuhunna bima\'ruufin wa-asyhiduu <u>dz</u>away \'adlin minkum wa-aqiimuu <b>al</b>sysyah<u>aa</u>data lill<u>aa</u>hi <u>dzaa</u>likum yuu\'a<u>zh</u>u bihi man k<u>aa</u>na yu/minu bi<b>al</b>l<u>aa</u>hi wa<b>a</b>lyawmi <b>a</b>l-<u>aa</u>khiri waman yattaqi <b>al</b>l<u>aa</u>ha yaj\'al lahu makhraj<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (5220, '65:3', 65, 3, ' wayarzuqhu min <u>h</u>aytsu l<u>aa</u> ya<u>h</u>tasibu waman yatawakkal \'al<u>aa</u> <b>al</b>l<u>aa</u>hi fahuwa <u>h</u>asbuhu inna <b>al</b>l<u>aa</u>ha b<u>aa</u>lighu amrihi qad ja\'ala <b>al</b>l<u>aa</u>hu likulli syay-in qadr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (5221, '65:4', 65, 4, ' wa<b>a</b>ll<u>aa</u>-ii ya-isna mina <b>a</b>lma<u>h</u>ii<u>dh</u>i min nis<u>aa</u>-ikum ini irtabtum fa\'iddatuhunna tsal<u>aa</u>tsatu asyhurin wa<b>a</b>ll<u>aa</u>-ii lam ya<u>h</u>i<u>dh</u>na waul<u>aa</u>tu <b>a</b>l-a<u>h</u>m<u>aa</u>li ajaluhunna an ya<u>dh</u>a\'na <u>h</u>amlahunna waman yattaqi <b>al</b>l<u>aa</u>ha yaj\'al lahu min amrihi yusr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (5222, '65:5', 65, 5, ' <u>dzaa</u>lika amru <b>al</b>l<u>aa</u>hi anzalahu ilaykum waman yattaqi <b>al</b>l<u>aa</u>ha yukaffir \'anhu sayyi-<u>aa</u>tihi wayu\'<u>zh</u>im lahu ajr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (5223, '65:6', 65, 6, ' askinuuhunna min <u>h</u>aytsu sakantum min wujdikum wal<u>aa</u> tu<u>daa</u>rruuhunna litu<u>dh</u>ayyiquu \'alayhinna wa-in kunna ul<u>aa</u>ti <u>h</u>amlin fa-anfiquu \'alayhinna <u>h</u>att<u>aa</u> ya<u>dh</u>a\'na <u>h</u>amlahunna fa-in ar<u>dh</u>a\'na lakum fa<u>aa</u>tuuhunna ujuurahunna wa/tamiruu baynakum bima\'ruufin wa-in ta\'<u>aa</u>sartum fasatur<u>dh</u>i\'u lahu ukhr<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (5224, '65:7', 65, 7, ' liyunfiq <u>dz</u>uu sa\'atin min sa\'atihi waman qudira \'alayhi rizquhu falyunfiq mimm<u>aa</u> <u>aa</u>t<u>aa</u>hu <b>al</b>l<u>aa</u>hu l<u>aa</u> yukallifu <b>al</b>l<u>aa</u>hu nafsan ill<u>aa</u> m<u>aa</u> <u>aa</u>t<u>aa</u>h<u>aa</u> sayaj\'alu <b>al</b>l<u>aa</u>hu ba\'da \'usrin yusr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (5225, '65:8', 65, 8, ' waka-ayyin min qaryatin \'atat \'an amri rabbih<u>aa</u> warusulihi fa<u>haa</u>sabn<u>aa</u>h<u>aa</u> <u>h</u>is<u>aa</u>ban syadiidan wa\'a<u>dzdz</u>abn<u>aa</u>h<u>aa</u> \'a<u>dzaa</u>ban nukr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (5226, '65:9', 65, 9, '  fa<u>dzaa</u>qat wab<u>aa</u>la amrih<u>aa</u> wak<u>aa</u>na \'<u>aa</u>qibatu amrih<u>aa</u> khusr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (5227, '65:10', 65, 10, ' a\'adda <b>al</b>l<u>aa</u>hu lahum \'a<u>dzaa</u>ban syadiidan fa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha y<u>aa</u> ulii <b>a</b>l-alb<u>aa</u>bi <b>al</b>la<u>dz</u>iina <u>aa</u>manuu qad anzala <b>al</b>l<u>aa</u>hu ilaykum <u>dz</u>ikr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (5228, '65:11', 65, 11, ' rasuulan yatluu \'alaykum <u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi mubayyin<u>aa</u>tin liyukhrija <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti mina <b>al</b><u>zhzh</u>ulum<u>aa</u>ti il<u>aa</u> <b>al</b>nnuuri waman yu/min bi<b>al</b>l<u>aa</u>hi waya\'mal <u>shaa</u>li<u>h</u>an yudkhilhu jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru kh<u>aa</u>lidiina fiih<u>aa</u> abadan qad a<u>h</u>sana <b>al</b>l<u>aa</u>hu lahu rizq<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (5229, '65:12', 65, 12, ' <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii khalaqa sab\'a sam<u>aa</u>w<u>aa</u>tin wamina <b>a</b>l-ar<u>dh</u>i mitslahunna yatanazzalu <b>a</b>l-amru baynahunna lita\'lamuu anna <b>al</b>l<u>aa</u>ha \'al<u>aa</u> kulli syay-in qadiirun wa-anna <b>al</b>l<u>aa</u>ha qad a<u>hath</u>a bikulli syay-in \'ilm<u>aa</u><b>n</b><dir>\r\r\n');
INSERT INTO quranLatin VALUES (5230, '66:1', 66, 1, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nnabiyyu lima tu<u>h</u>arrimu m<u>aa</u> a<u>h</u>alla <b>al</b>l<u>aa</u>hu laka tabtaghii mar<u>daa</u>ta azw<u>aa</u>jika wa<b>al</b>l<u>aa</u>hu ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5231, '66:2', 66, 2, ' qad fara<u>dh</u>a <b>al</b>l<u>aa</u>hu lakum ta<u>h</u>illata aym<u>aa</u>nikum wa<b>al</b>l<u>aa</u>hu mawl<u>aa</u>kum wahuwa <b>a</b>l\'aliimu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5232, '66:3', 66, 3, ' wa-i<u>dz</u> asarra <b>al</b>nnabiyyu il<u>aa</u> ba\'<u>dh</u>i azw<u>aa</u>jihi <u>h</u>adiitsan falamm<u>aa</u> nabba-at bihi wa-a<u>zh</u>harahu <b>al</b>l<u>aa</u>hu \'alayhi \'arrafa ba\'<u>dh</u>ahu wa-a\'ra<u>dh</u>a \'an ba\'<u>dh</u>in falamm<u>aa</u> nabba-ah<u>aa</u> bihi q<u>aa</u>lat man anba-aka h<u>aadzaa</u> q<u>aa</u>la nabba-aniya <b>a</b>l\'aliimu <b>a</b>lkhabiir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5233, '66:4', 66, 4, ' in tatuub<u>aa</u> il<u>aa</u> <b>al</b>l<u>aa</u>hi faqad <u>sh</u>aghat quluubukum<u>aa</u> wa-in ta<u>zhaa</u>har<u>aa</u> \'alayhi fa-inna <b>al</b>l<u>aa</u>ha huwa mawl<u>aa</u>hu wajibriilu wa<u>shaa</u>li<u>h</u>u <b>a</b>lmu/miniina wa<b>a</b>lmal<u>aa</u>-ikatu ba\'da <u>dzaa</u>lika <u>zh</u>ahiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5234, '66:5', 66, 5, ' \'as<u>aa</u> rabbuhu in <u>th</u>allaqakunna an yubdilahu azw<u>aa</u>jan khayran minkunna muslim<u>aa</u>tin mu/min<u>aa</u>tin q<u>aa</u>nit<u>aa</u>tin t<u>aa</u>-ib<u>aa</u>tin \'<u>aa</u>bid<u>aa</u>tin s<u>aa</u>-i<u>haa</u>tin tsayyib<u>aa</u>tin wa-abk<u>aa</u>r<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (5235, '66:6', 66, 6, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu quu anfusakum wa-ahliikum n<u>aa</u>ran waquuduh<u>aa</u> <b>al</b>nn<u>aa</u>su wa<b>a</b>l<u>h</u>ij<u>aa</u>ratu \'alayh<u>aa</u> mal<u>aa</u>-ikatun ghil<u>aats</u>un syid<u>aa</u>dun l<u>aa</u> ya\'<u>sh</u>uuna <b>al</b>l<u>aa</u>ha m<u>aa</u> amarahum wayaf\'aluuna m<u>aa</u> yu/maruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5236, '66:7', 66, 7, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina kafaruu l<u>aa</u> ta\'ta<u>dz</u>iruu <b>a</b>lyawma innam<u>aa</u> tujzawna m<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5237, '66:8', 66, 8, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu tuubuu il<u>aa</u> <b>al</b>l<u>aa</u>hi tawbatan na<u>sh</u>uu<u>h</u>an \'as<u>aa</u> rabbukum an yukaffira \'ankum sayyi-<u>aa</u>tikum wayudkhilakum jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru yawma l<u>aa</u> yukhzii <b>al</b>l<u>aa</u>hu <b>al</b>nnabiyya wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu ma\'ahu nuuruhum yas\'<u>aa</u> bayna aydiihim wabi-aym<u>aa</u>nihim yaquuluuna rabban<u>aa</u> atmim lan<u>aa</u> nuuran<u>aa</u> wa<b>i</b>ghfir lan<u>aa</u> innaka \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5238, '66:9', 66, 9, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nnabiyyu j<u>aa</u>hidi <b>a</b>lkuff<u>aa</u>ra wa<b>a</b>lmun<u>aa</u>fiqiina wa<b>u</b>ghlu<u>zh</u> \'alayhim wama/w<u>aa</u>hum jahannamu wabi/sa <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5239, '66:10', 66, 10, ' <u>dh</u>araba <b>al</b>l<u>aa</u>hu matsalan lilla<u>dz</u>iina kafaruu imra-ata nuu<u>h</u>in wa<b>i</b>mra-ata luu<u>th</u>in k<u>aa</u>nat<u>aa</u> ta<u>h</u>ta \'abdayni min \'ib<u>aa</u>din<u>aa</u> <u>shaa</u>li<u>h</u>ayni fakh<u>aa</u>nat<u>aa</u>hum<u>aa</u> falam yughniy<u>aa</u> \'anhum<u>aa</u> mina <b>al</b>l<u>aa</u>hi syay-an waqiila udkhul<u>aa</u> <b>al</b>nn<u>aa</u>ra ma\'a <b>al</b>dd<u>aa</u>khiliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5240, '66:11', 66, 11, ' wa<u>dh</u>araba <b>al</b>l<u>aa</u>hu matsalan lilla<u>dz</u>iina <u>aa</u>manuu imra-ata fir\'awna i<u>dz</u> q<u>aa</u>lat rabbi ibni lii \'indaka baytan fii <b>a</b>ljannati wanajjinii min fir\'awna wa\'amalihi wanajjinii mina <b>a</b>lqawmi <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5241, '66:12', 66, 12, ' wamaryama ibnata \'imr<u>aa</u>na <b>al</b>latii a<u>hs</u>anat farjah<u>aa</u> fanafakhn<u>aa</u> fiihi min ruu<u>h</u>in<u>aa</u> wa<u>sh</u>addaqat bikalim<u>aa</u>ti rabbih<u>aa</u> wakutubihi wak<u>aa</u>nat mina <b>a</b>lq<u>aa</u>nitiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5242, '67:1', 67, 1, ' tab<u>aa</u>raka <b>al</b>la<u>dz</u>ii biyadihi <b>a</b>lmulku wahuwa \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5243, '67:2', 67, 2, ' <b>al</b>la<u>dz</u>ii khalaqa <b>a</b>lmawta wa<b>a</b>l<u>h</u>ay<u>aa</u>ta liyabluwakum ayyukum a<u>h</u>sanu \'amalan wahuwa <b>a</b>l\'aziizu <b>a</b>lghafuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5244, '67:3', 67, 3, ' <b>al</b>la<u>dz</u>ii khalaqa sab\'a sam<u>aa</u>w<u>aa</u>tin <u>th</u>ib<u>aa</u>qan m<u>aa</u> tar<u>aa</u> fii khalqi <b>al</b>rra<u>h</u>m<u>aa</u>ni min taf<u>aa</u>wutin fa<b>i</b>rji\'i <b>a</b>lba<u>sh</u>ara hal tar<u>aa</u> min fu<u>th</u>uur<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (5245, '67:4', 67, 4, ' tsumma irji\'i <b>a</b>lba<u>sh</u>ara karratayni yanqalib ilayka <b>a</b>lba<u>sh</u>aru kh<u>aa</u>si-an wahuwa <u>h</u>asiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5246, '67:5', 67, 5, ' walaqad zayyann<u>aa</u> <b>al</b>ssam<u>aa</u>-a <b>al</b>dduny<u>aa</u> bima<u>shaa</u>bii<u>h</u>a waja\'aln<u>aa</u>h<u>aa</u> rujuuman li<b>l</b>sysyay<u>aath</u>iini wa-a\'tadn<u>aa</u> lahum \'a<u>dzaa</u>ba <b>al</b>ssa\'iir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5247, '67:6', 67, 6, ' walilla<u>dz</u>iina kafaruu birabbihim \'a<u>dzaa</u>bu jahannama wabi/sa <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5248, '67:7', 67, 7, ' i<u>dzaa</u> ulquu fiih<u>aa</u> sami\'uu lah<u>aa</u> syahiiqan wahiya tafuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5249, '67:8', 67, 8, ' tak<u>aa</u>du tamayyazu mina alghay<u>zh</u>i kullam<u>aa</u> ulqiya fiih<u>aa</u> fawjun sa-alahum khazanatuh<u>aa</u> alam ya/tikum na<u>dz</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5250, '67:9', 67, 9, ' q<u>aa</u>luu bal<u>aa</u> qad j<u>aa</u>-an<u>aa</u> na<u>dz</u>iirun faka<u>dzdz</u>abn<u>aa</u> waquln<u>aa</u> m<u>aa</u> nazzala <b>al</b>l<u>aa</u>hu min syay-in in antum ill<u>aa</u> fii <u>dh</u>al<u>aa</u>lin kabiir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (5251, '67:10', 67, 10, ' waq<u>aa</u>luu law kunn<u>aa</u> nasma\'u aw na\'qilu m<u>aa</u> kunn<u>aa</u> fii a<u>sh</u>-<u>haa</u>bi <b>al</b>ssa\'iir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5252, '67:11', 67, 11, ' fa<b>i</b>\'tarafuu bi<u>dz</u>anbihim fasu<u>h</u>qan li-a<u>sh</u>-<u>haa</u>bi <b>al</b>ssa\'iir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5253, '67:12', 67, 12, ' inna <b>al</b>la<u>dz</u>iina yakhsyawna rabbahum bi<b>a</b>lghaybi lahum maghfiratun wa-ajrun kabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5254, '67:13', 67, 13, ' wa-asirruu qawlakum awi ijharuu bihi innahu \'aliimun bi<u>dzaa</u>ti <b>al</b><u>shsh</u>uduur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5255, '67:14', 67, 14, ' al<u>aa</u> ya\'lamu man khalaqa wahuwa <b>al</b>la<u>th</u>iifu <b>a</b>lkhabiir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5256, '67:15', 67, 15, ' huwa <b>al</b>la<u>dz</u>ii ja\'ala lakumu <b>a</b>l-ar<u>dh</u>a <u>dz</u>aluulan fa<b>i</b>msyuu fii man<u>aa</u>kibih<u>aa</u> wakuluu min rizqihi wa-ilayhi <b>al</b>nnusyuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5257, '67:16', 67, 16, ' a-amintum man fii <b>al</b>ssam<u>aa</u>-i an yakhsifa bikumu <b>a</b>l-ar<u>dh</u>a fa-i<u>dzaa</u> hiya tamuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (5258, '67:17', 67, 17, ' am amintum man fii <b>al</b>ssam<u>aa</u>-i an yursila \'alaykum <u>has</u>iban fasata\'lamuuna kayfa na<u>dz</u>iir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5259, '67:18', 67, 18, ' walaqad ka<u>dzdz</u>aba <b>al</b>la<u>dz</u>iina min qablihim fakayfa k<u>aa</u>na nakiir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (5260, '67:19', 67, 19, ' awa lam yaraw il<u>aa</u> <b>al</b><u>ththh</u>ayri fawqahum <u>shaa</u>ff<u>aa</u>tin wayaqbi<u>dh</u>na m<u>aa</u> yumsikuhunna ill<u>aa</u> <b>al</b>rra<u>h</u>m<u>aa</u>nu innahu bikulli syay-in ba<u>sh</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (5261, '67:20', 67, 20, ' amman h<u>aadzaa</u> <b>al</b>la<u>dz</u>ii huwa jundun lakum yan<u>sh</u>urukum min duuni <b>al</b>rra<u>h</u>m<u>aa</u>ni ini <b>a</b>lk<u>aa</u>firuuna ill<u>aa</u> fii ghuruur<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (5262, '67:21', 67, 21, ' amman h<u>aadzaa</u> <b>al</b>la<u>dz</u>ii yarzuqukum in amsaka rizqahu bal lajjuu fii \'utuwwin wanufuur<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (5263, '67:22', 67, 22, ' afaman yamsyii mukibban \'al<u>aa</u> wajhihi ahd<u>aa</u> amman yamsyii sawiyyan \'al<u>aa</u> <u>sh</u>ir<u>aath</u>in mustaqiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (5264, '67:23', 67, 23, ' qul huwa <b>al</b>la<u>dz</u>ii ansya-akum waja\'ala lakumu <b>al</b>ssam\'a wa<b>a</b>l-ab<u>shaa</u>ra wa<b>a</b>l-af-idata qaliilan m<u>aa</u> tasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5265, '67:24', 67, 24, ' qul huwa <b>al</b>la<u>dz</u>ii <u>dz</u>ara-akum fii <b>a</b>l-ar<u>dh</u>i wa-ilayhi tu<u>h</u>syaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (5266, '67:25', 67, 25, ' wayaquuluuna mat<u>aa</u> h<u>aadzaa</u> <b>a</b>lwa\'du in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
