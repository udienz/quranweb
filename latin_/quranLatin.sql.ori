INSERT INTO quranLatin VALUES (5079, '57:4', 57, 4, ' huwa <b>al</b>la<u>dz</u>ii khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a fii sittati ayy<u>aa</u>min tsumma istaw<u>aa</u> \'al<u>aa</u> <b>a</b>l\'arsyi ya\'lamu m<u>aa</u> yaliju fii <b>a</b>l-ar<u>dh</u>i wam<u>aa</u> yakhruju minh<u>aa</u> wam<u>aa</u> yanzilu mina <b>al</b>ssam<u>aa</u>-i wam<u>aa</u> ya\'ruju fiih<u>aa</u> wahuwa ma\'akum ayna m<u>aa</u> kuntum wa<b>al</b>l<u>aa</u>hu bim<u>aa</u> ta\'maluuna ba<u>sh</u>iir<b>un</b>\r\r\n');
