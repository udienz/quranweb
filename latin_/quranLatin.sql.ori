INSERT INTO quranLatin VALUES (616, '4:123', 4, 123, ' laysa bi-am<u>aa</u>niyyikum wal<u>aa</u> am<u>aa</u>niyyi ahli <b>a</b>lkit<u>aa</u>bi man ya\'mal suu-an yujza bihi wal<u>aa</u> yajid lahu min duuni <b>al</b>l<u>aa</u>hi waliyyan wal<u>aa</u> na<u>sh</u>iir<u>aa</u><b>n</b>\r\r\n');
