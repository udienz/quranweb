INSERT INTO quranLatin VALUES (4479, '45:6', 45, 6, ' tilka <u>aa</u>y<u>aa</u>tu <b>al</b>l<u>aa</u>hi natluuh<u>aa</u> \'alayka bi<b>a</b>l<u>h</u>aqqi fabi-ayyi <u>h</u>adiitsin ba\'da <b>al</b>l<u>aa</u>hi wa<u>aa</u>y<u>aa</u>tihi yu/minuun<b>a</b>\r\r\n');
