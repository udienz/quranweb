INSERT INTO quranLatin VALUES (3293, '28:41', 28, 41, ' waja\'aln<u>aa</u>hum a-immatan yad\'uuna il<u>aa</u> <b>al</b>nn<u>aa</u>ri wayawma <b>a</b>lqiy<u>aa</u>mati l<u>aa</u> yun<u>sh</u>aruun<b>a</b>\r\r\n');
