INSERT INTO quranLatin VALUES (3311, '28:59', 28, 59, ' wam<u>aa</u> k<u>aa</u>na rabbuka muhlika <b>a</b>lqur<u>aa</u> <u>h</u>att<u>aa</u> yab\'atsa fii ummih<u>aa</u> rasuulan yatluu \'alayhim <u>aa</u>y<u>aa</u>tin<u>aa</u> wam<u>aa</u> kunn<u>aa</u> muhlikii <b>a</b>lqur<u>aa</u> ill<u>aa</u> wa-ahluh<u>aa</u> <u>zhaa</u>limuun<b>a</b>\r\r\n');
