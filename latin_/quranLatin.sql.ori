INSERT INTO quranLatin VALUES (3609, '34:3', 34, 3, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu l<u>aa</u> ta/tiin<u>aa</u> <b>al</b>ss<u>aa</u>\'atu qul bal<u>aa</u> warabbii lata/tiyannakum \'<u>aa</u>limi <b>a</b>lghaybi l<u>aa</u> ya\'zubu \'anhu mitsq<u>aa</u>lu <u>dz</u>arratin fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wal<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i wal<u>aa</u> a<u>sh</u>gharu min <u>dzaa</u>lika wal<u>aa</u> akbaru ill<u>aa</u> fii kit<u>aa</u>bin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3610, '34:4', 34, 4, ' liyajziya <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti ul<u>aa</u>-ika lahum maghfiratun warizqun kariim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3611, '34:5', 34, 5, ' wa<b>a</b>lla<u>dz</u>iina sa\'aw fii <u>aa</u>y<u>aa</u>tin<u>aa</u> mu\'<u>aa</u>jiziina ul<u>aa</u>-ika lahum \'a<u>dzaa</u>bun min rijzin <b>a</b>liim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3612, '34:6', 34, 6, ' wayar<u>aa</u> <b>al</b>la<u>dz</u>iina uutuu <b>a</b>l\'ilma <b>al</b>la<u>dz</u>ii unzila ilayka min rabbika huwa <b>a</b>l<u>h</u>aqqa wayahdii il<u>aa</u> <u>sh</u>ir<u>aath</u>i <b>a</b>l\'aziizi <b>a</b>l<u>h</u>amiid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3613, '34:7', 34, 7, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu hal nadullukum \'al<u>aa</u> rajulin yunabbi-ukum i<u>dzaa</u> muzziqtum kulla mumazzaqin innakum lafii khalqin jadiid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3614, '34:8', 34, 8, ' aftar<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ka<u>dz</u>iban am bihi jinnatun bali <b>al</b>la<u>dz</u>iina l<u>aa</u> yu/minuuna bi<b>a</b>l-<u>aa</u>khirati fii <b>a</b>l\'a<u>dzaa</u>bi wa<b>al</b><u>dhdh</u>al<u>aa</u>li <b>a</b>lba\'iid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3615, '34:9', 34, 9, ' afalam yaraw il<u>aa</u> m<u>aa</u> bayna aydiihim wam<u>aa</u> khalfahum mina <b>al</b>ssam<u>aa</u>-i wa<b>a</b>l-ar<u>dh</u>i in nasya/ nakhsif bihimu <b>a</b>l-ar<u>dh</u>a aw nusqi<u>th</u> \'alayhim kisafan mina <b>al</b>ssam<u>aa</u>-i inna fii <u>dzaa</u>lika la<u>aa</u>yatan likulli \'abdin muniib<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3616, '34:10', 34, 10, ' walaqad <u>aa</u>tayn<u>aa</u> d<u>aa</u>wuuda minn<u>aa</u> fa<u>dh</u>lan y<u>aa</u> jib<u>aa</u>lu awwibii ma\'ahu wa<b>al</b><u>ththh</u>ayra wa-alann<u>aa</u> lahu <b>a</b>l<u>h</u>adiid<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3617, '34:11', 34, 11, ' ani i\'mal s<u>aa</u>bigh<u>aa</u>tin waqaddir fii <b>al</b>ssardi wa<b>i</b>\'maluu <u>shaa</u>li<u>h</u>an innii bim<u>aa</u> ta\'maluuna ba<u>sh</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3618, '34:12', 34, 12, ' walisulaym<u>aa</u>na <b>al</b>rrii<u>h</u>a ghuduwwuh<u>aa</u> syahrun waraw<u>aah</u>uh<u>aa</u> syahrun wa-asaln<u>aa</u> lahu \'ayna <b>a</b>lqi<u>th</u>ri wamina <b>a</b>ljinni man ya\'malu bayna yadayhi bi-i<u>dz</u>ni rabbihi waman yazigh minhum \'an amrin<u>aa</u> nu<u>dz</u>iqhu min \'a<u>dzaa</u>bi <b>al</b>ssa\'iir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3619, '34:13', 34, 13, ' ya\'maluuna lahu m<u>aa</u> yasy<u>aa</u>u min ma<u>haa</u>riiba watam<u>aa</u>tsiila wajif<u>aa</u>nin ka<b>a</b>ljaw<u>aa</u>bi waquduurin r<u>aa</u>siy<u>aa</u>tin i\'maluu <u>aa</u>la d<u>aa</u>wuuda syukran waqaliilun min \'ib<u>aa</u>diya <b>al</b>sysyakuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3620, '34:14', 34, 14, ' falamm<u>aa</u> qa<u>dh</u>ayn<u>aa</u> \'alayhi <b>a</b>lmawta m<u>aa</u> dallahum \'al<u>aa</u> mawtihi ill<u>aa</u> d<u>aa</u>bbatu <b>a</b>l-ar<u>dh</u>i ta/kulu minsa-atahu falamm<u>aa</u> kharra tabayyanati <b>a</b>ljinnu an law k<u>aa</u>nuu ya\'lamuuna <b>a</b>lghayba m<u>aa</u> labitsuu fii <b>a</b>l\'a<u>dzaa</u>bi <b>a</b>lmuhiin<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3621, '34:15', 34, 15, ' laqad k<u>aa</u>na lisaba-in fii maskanihim <u>aa</u>yatun jannat<u>aa</u>ni \'an yamiinin wasyim<u>aa</u>lin kuluu min rizqi rabbikum wa<b>u</b>sykuruu lahu baldatun <u>th</u>ayyibatun warabbun ghafuur<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3622, '34:16', 34, 16, ' fa-a\'ra<u>dh</u>uu fa-arsaln<u>aa</u> \'alayhim sayla <b>a</b>l\'arimi wabaddaln<u>aa</u>hum bijannatayhim jannatayni <u>dz</u>aw<u>aa</u>tay ukulin kham<u>th</u>in wa-atslin wasyay-in min sidrin qaliil<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3623, '34:17', 34, 17, ' <u>dzaa</u>lika jazayn<u>aa</u>hum bim<u>aa</u> kafaruu wahal nuj<u>aa</u>zii ill<u>aa</u> <b>a</b>lkafuur<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3624, '34:18', 34, 18, ' waja\'aln<u>aa</u> baynahum wabayna <b>a</b>lqur<u>aa</u> <b>al</b>latii b<u>aa</u>rakn<u>aa</u> fiih<u>aa</u> quran <u>zhaa</u>hiratan waqaddarn<u>aa</u> fiih<u>aa</u> <b>al</b>ssayra siiruu fiih<u>aa</u> lay<u>aa</u>liya wa-ayy<u>aa</u>man <u>aa</u>miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3625, '34:19', 34, 19, ' faq<u>aa</u>luu rabban<u>aa</u> b<u>aa</u>\'id bayna asf<u>aa</u>rin<u>aa</u> wa<u>zh</u>alamuu anfusahum faja\'aln<u>aa</u>hum a<u>haa</u>diitsa wamazzaqn<u>aa</u>hum kulla mumazzaqin inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin likulli <u>sh</u>abb<u>aa</u>rin syakuur<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3626, '34:20', 34, 20, ' walaqad <u>sh</u>addaqa \'alayhim ibliisu <u>zh</u>annahu fa<b>i</b>ttaba\'uuhu ill<u>aa</u> fariiqan mina <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3627, '34:21', 34, 21, ' wam<u>aa</u> k<u>aa</u>na lahu \'alayhim min sul<u>th</u><u>aa</u>nin ill<u>aa</u> lina\'lama man yu/minu bi<b>a</b>l-<u>aa</u>khirati mimman huwa minh<u>aa</u> fii syakkin warabbuka \'al<u>aa</u> kulli syay-in <u>h</u>afii<u>zh</u><b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3628, '34:22', 34, 22, ' quli ud\'uu <b>al</b>la<u>dz</u>iina za\'amtum min duuni <b>al</b>l<u>aa</u>hi l<u>aa</u> yamlikuuna mitsq<u>aa</u>la <u>dz</u>arratin fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wal<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i wam<u>aa</u> lahum fiihim<u>aa</u> min syirkin wam<u>aa</u> lahu minhum min <u>zh</u>ahiir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3629, '34:23', 34, 23, ' wal<u>aa</u> tanfa\'u <b>al</b>sysyaf<u>aa</u>\'atu \'indahu ill<u>aa</u> liman a<u>dz</u>ina lahu <u>h</u>att<u>aa</u> i<u>dzaa</u> fuzzi\'a \'an quluubihim q<u>aa</u>luu m<u>aatsaa</u> q<u>aa</u>la rabbukum q<u>aa</u>luu <b>a</b>l<u>h</u>aqqa wahuwa <b>a</b>l\'aliyyu <b>a</b>lkabiir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3630, '34:24', 34, 24, ' qul man yarzuqukum mina <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i quli <b>al</b>l<u>aa</u>hu wa-inn<u>aa</u> aw iyy<u>aa</u>kum la\'al<u>aa</u> hudan aw fii <u>dh</u>al<u>aa</u>lin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3631, '34:25', 34, 25, ' qul l<u>aa</u> tus-aluuna \'amm<u>aa</u> ajramn<u>aa</u> wal<u>aa</u> nus-alu \'amm<u>aa</u> ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3632, '34:26', 34, 26, ' qul yajma\'u baynan<u>aa</u> rabbun<u>aa</u> tsumma yafta<u>h</u>u baynan<u>aa</u> bi<b>a</b>l<u>h</u>aqqi wahuwa <b>a</b>lfatt<u>aah</u>u <b>a</b>l\'aliim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3633, '34:27', 34, 27, ' qul aruuniya <b>al</b>la<u>dz</u>iina <b>a</b>l<u>h</u>aqtum bihi syurak<u>aa</u>-a kall<u>aa</u> bal huwa <b>al</b>l<u>aa</u>hu <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3634, '34:28', 34, 28, ' wam<u>aa</u> arsaln<u>aa</u>ka ill<u>aa</u> k<u>aa</u>ffatan li<b>l</b>nn<u>aa</u>si basyiiran wana<u>dz</u>iiran wal<u>aa</u>kinna aktsara <b>al</b>nn<u>aa</u>si l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3635, '34:29', 34, 29, ' wayaquuluuna mat<u>aa</u> h<u>aadzaa</u> <b>a</b>lwa\'du in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3636, '34:30', 34, 30, ' qul lakum mii\'<u>aa</u>du yawmin l<u>aa</u> tasta/khiruuna \'anhu s<u>aa</u>\'atan wal<u>aa</u> tastaqdimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3637, '34:31', 34, 31, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu lan nu/mina bih<u>aadzaa</u> <b>a</b>lqur-<u>aa</u>ni wal<u>aa</u> bi<b>a</b>lla<u>dz</u>ii bayna yadayhi walaw tar<u>aa</u> i<u>dz</u>i <b>al</b><u>zhzhaa</u>limuuna mawquufuuna \'inda rabbihim yarji\'u ba\'<u>dh</u>uhum il<u>aa</u> ba\'<u>dh</u>in <b>a</b>lqawla yaquulu <b>al</b>la<u>dz</u>iina istu<u>dh</u>\'ifuu lilla<u>dz</u>iina istakbaruu lawl<u>aa</u> antum lakunn<u>aa</u> mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3638, '34:32', 34, 32, ' q<u>aa</u>la <b>al</b>la<u>dz</u>iina istakbaruu lilla<u>dz</u>iina istu<u>dh</u>\'ifuu ana<u>h</u>nu <u>sh</u>adadn<u>aa</u>kum \'ani <b>a</b>lhud<u>aa</u> ba\'da i<u>dz</u> j<u>aa</u>-akum bal kuntum mujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3639, '34:33', 34, 33, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>iina istu<u>dh</u>\'ifuu lilla<u>dz</u>iina istakbaruu bal makru <b>al</b>layli wa<b>al</b>nnah<u>aa</u>ri i<u>dz</u> ta/muruunan<u>aa</u> an nakfura bi<b>al</b>l<u>aa</u>hi wanaj\'ala lahu and<u>aa</u>dan wa-asarruu <b>al</b>nnad<u>aa</u>mata lamm<u>aa</u> ra-awuu <b>a</b>l\'a<u>dzaa</u>ba waja\'aln<u>aa</u> <b>a</b>l-aghl<u>aa</u>la fii a\'n<u>aa</u>qi <b>al</b>la<u>dz</u>iina kafaruu hal yujzawna ill<u>aa</u> m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3640, '34:34', 34, 34, ' wam<u>aa</u> arsaln<u>aa</u> fii qaryatin min na<u>dz</u>iirin ill<u>aa</u> q<u>aa</u>la mutrafuuh<u>aa</u> inn<u>aa</u> bim<u>aa</u> ursiltum bihi k<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3641, '34:35', 34, 35, ' waq<u>aa</u>luu na<u>h</u>nu aktsaru amw<u>aa</u>lan wa-awl<u>aa</u>dan wam<u>aa</u> na<u>h</u>nu bimu\'a<u>dzdz</u>abiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3642, '34:36', 34, 36, ' qul inna rabbii yabsu<u>th</u>u <b>al</b>rrizqa liman yasy<u>aa</u>u wayaqdiru wal<u>aa</u>kinna aktsara <b>al</b>nn<u>aa</u>si l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3643, '34:37', 34, 37, ' wam<u>aa</u> amw<u>aa</u>lukum wal<u>aa</u> awl<u>aa</u>dukum bi<b>a</b>llatii tuqarribukum \'indan<u>aa</u> zulf<u>aa</u> ill<u>aa</u> man <u>aa</u>mana wa\'amila <u>shaa</u>li<u>h</u>an faul<u>aa</u>-ika lahum jaz<u>aa</u>u <b>al</b><u>dhdh</u>i\'fi bim<u>aa</u> \'amiluu wahum fii <b>a</b>lghuruf<u>aa</u>ti <u>aa</u>minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3644, '34:38', 34, 38, ' wa<b>a</b>lla<u>dz</u>iina yas\'awna fii <u>aa</u>y<u>aa</u>tin<u>aa</u> mu\'<u>aa</u>jiziina ul<u>aa</u>-ika fii <b>a</b>l\'a<u>dzaa</u>bi mu<u>hd</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3645, '34:39', 34, 39, ' qul inna rabbii yabsu<u>th</u>u <b>al</b>rrizqa liman yasy<u>aa</u>u min \'ib<u>aa</u>dihi wayaqdiru lahu wam<u>aa</u> anfaqtum min syay-in fahuwa yukhlifuhu wahuwa khayru <b>al</b>rr<u>aa</u>ziqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3646, '34:40', 34, 40, ' wayawma ya<u>h</u>syuruhum jamii\'an tsumma yaquulu lilmal<u>aa</u>-ikati ah<u>aa</u>ul<u>aa</u>-i iyy<u>aa</u>kum k<u>aa</u>nuu ya\'buduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3647, '34:41', 34, 41, ' q<u>aa</u>luu sub<u>haa</u>naka anta waliyyun<u>aa</u> min duunihim bal k<u>aa</u>nuu ya\'buduuna <b>a</b>ljinna aktsaruhum bihim mu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3648, '34:42', 34, 42, ' fa<b>a</b>lyawma l<u>aa</u> yamliku ba\'<u>dh</u>ukum liba\'<u>dh</u>in naf\'an wal<u>aa</u> <u>dh</u>arran wanaquulu lilla<u>dz</u>iina <u>zh</u>alamuu <u>dz</u>uuquu \'a<u>dzaa</u>ba <b>al</b>nn<u>aa</u>ri <b>al</b>latii kuntum bih<u>aa</u> tuka<u>dzdz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3649, '34:43', 34, 43, ' wa-i<u>dzaa</u> tutl<u>aa</u> \'alayhim <u>aa</u>y<u>aa</u>tun<u>aa</u> bayyin<u>aa</u>tin q<u>aa</u>luu m<u>aa</u> h<u>aadzaa</u> ill<u>aa</u> rajulun yuriidu an ya<u>sh</u>uddakum \'amm<u>aa</u> k<u>aa</u>na ya\'budu <u>aa</u>b<u>aa</u>ukum waq<u>aa</u>luu m<u>aa</u> h<u>aadzaa</u> ill<u>aa</u> ifkun muftaran waq<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu lil<u>h</u>aqqi lamm<u>aa</u> j<u>aa</u>-ahum in h<u>aadzaa</u> ill<u>aa</u> si<u>h</u>run mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3650, '34:44', 34, 44, ' wam<u>aa</u> <u>aa</u>tayn<u>aa</u>hum min kutubin yadrusuunah<u>aa</u> wam<u>aa</u> arsaln<u>aa</u> ilayhim qablaka min na<u>dz</u>iir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3651, '34:45', 34, 45, ' waka<u>dzdz</u>aba <b>al</b>la<u>dz</u>iina min qablihim wam<u>aa</u> balaghuu mi\'sy<u>aa</u>ra m<u>aa</u> <u>aa</u>tayn<u>aa</u>hum faka<u>dzdz</u>abuu rusulii fakayfa k<u>aa</u>na nakiir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3652, '34:46', 34, 46, ' qul innam<u>aa</u> a\'i<u>zh</u>ukum biw<u>aah</u>idatin an taquumuu lill<u>aa</u>hi matsn<u>aa</u> wafur<u>aa</u>d<u>aa</u> tsumma tatafakkaruu m<u>aa</u> bi<u>shaah</u>ibikum min jinnatin in huwa ill<u>aa</u> na<u>dz</u>iirun lakum bayna yaday \'a<u>dzaa</u>bin syadiid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3653, '34:47', 34, 47, ' qul m<u>aa</u> sa-altukum min ajrin fahuwa lakum in ajriya ill<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi wahuwa \'al<u>aa</u> kulli syay-in syahiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3654, '34:48', 34, 48, ' qul inna rabbii yaq<u>dz</u>ifu bi<b>a</b>l<u>h</u>aqqi \'all<u>aa</u>mu <b>a</b>lghuyuub<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3655, '34:49', 34, 49, ' qul j<u>aa</u>-a <b>a</b>l<u>h</u>aqqu wam<u>aa</u> yubdi-u <b>a</b>lb<u>aath</u>ilu wam<u>aa</u> yu\'iid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3656, '34:50', 34, 50, ' qul in <u>dh</u>alaltu fa-innam<u>aa</u> a<u>dh</u>illu \'al<u>aa</u> nafsii wa-ini ihtadaytu fabim<u>aa</u> yuu<u>h</u>ii ilayya rabbii innahu samii\'un qariib<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3657, '34:51', 34, 51, ' walaw tar<u>aa</u> i<u>dz</u> fazi\'uu fal<u>aa</u> fawta waukhi<u>dz</u>uu min mak<u>aa</u>nin qariib<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3658, '34:52', 34, 52, ' waq<u>aa</u>luu <u>aa</u>mann<u>aa</u> bihi wa-ann<u>aa</u> lahumu <b>al</b>ttan<u>aa</u>wusyu min mak<u>aa</u>nin ba\'iid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3659, '34:53', 34, 53, ' waqad kafaruu bihi min qablu wayaq<u>dz</u>ifuuna bi<b>a</b>lghaybi min mak<u>aa</u>nin ba\'iid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3660, '34:54', 34, 54, ' wa<u>h</u>iila baynahum wabayna m<u>aa</u> yasytahuuna kam<u>aa</u> fu\'ila bi-asyy<u>aa</u>\'ihim min qablu innahum k<u>aa</u>nuu fii syakkin muriib<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3661, '35:1', 35, 1, ' al<u>h</u>amdu lill<u>aa</u>hi f<u>aath</u>iri <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i j<u>aa</u>\'ili <b>a</b>lmal<u>aa</u>-ikati rusulan ulii ajni<u>h</u>atin matsn<u>aa</u> watsul<u>aa</u>tsa warub<u>aa</u>\'a yaziidu fii <b>a</b>lkhalqi m<u>aa</u> yasy<u>aa</u>u inna <b>al</b>l<u>aa</u>ha \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3662, '35:2', 35, 2, ' m<u>aa</u> yafta<u>h</u>i <b>al</b>l<u>aa</u>hu li<b>l</b>nn<u>aa</u>si min ra<u>h</u>matin fal<u>aa</u> mumsika lah<u>aa</u> wam<u>aa</u> yumsik fal<u>aa</u> mursila lahu min ba\'dihi wahuwa <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3663, '35:3', 35, 3, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su u<u>dz</u>kuruu ni\'mata <b>al</b>l<u>aa</u>hi \'alaykum hal min kh<u>aa</u>liqin ghayru <b>al</b>l<u>aa</u>hi yarzuqukum mina <b>al</b>ssam<u>aa</u>-i wa<b>a</b>l-ar<u>dh</u>i l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa fa-ann<u>aa</u> tu/fakuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3664, '35:4', 35, 4, ' wa-in yuka<u>dzdz</u>ibuuka faqad kut<u>hts</u>ibat rusulun min qablika wa-il<u>aa</u> <b>al</b>l<u>aa</u>hi turja\'u <b>a</b>l-umuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3665, '35:5', 35, 5, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su inna wa\'da <b>al</b>l<u>aa</u>hi <u>h</u>aqqun fal<u>aa</u> taghurrannakumu <b>a</b>l<u>h</u>ay<u>aa</u>tu <b>al</b>dduny<u>aa</u> wal<u>aa</u> yaghurrannakum bi<b>al</b>l<u>aa</u>hi <b>a</b>lgharuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3666, '35:6', 35, 6, ' inna <b>al</b>sysyay<u>th</u><u>aa</u>na lakum \'aduwwun fa<b>i</b>ttakhi<u>dz</u>uuhu \'aduwwan innam<u>aa</u> yad\'uu <u>h</u>izbahu liyakuunuu min a<u>sh</u>-<u>haa</u>bi <b>al</b>ssa\'iir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3667, '35:7', 35, 7, ' <b>al</b>la<u>dz</u>iina kafaruu lahum \'a<u>dzaa</u>bun syadiidun wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti lahum maghfiratun wa-ajrun kabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3668, '35:8', 35, 8, ' afaman zuyyina lahu suu-u \'amalihi fara<u>aa</u>hu <u>h</u>asanan fa-inna <b>al</b>l<u>aa</u>ha yu<u>dh</u>illu man yasy<u>aa</u>u wayahdii man yasy<u>aa</u>u fal<u>aa</u> ta<u>dz</u>hab nafsuka \'alayhim <u>h</u>asar<u>aa</u>tin inna <b>al</b>l<u>aa</u>ha \'aliimun bim<u>aa</u> ya<u>sh</u>na\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3669, '35:9', 35, 9, ' wa<b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii arsala <b>al</b>rriy<u>aah</u>a fatutsiiru sa<u>haa</u>ban fasuqn<u>aa</u>hu il<u>aa</u> baladin mayyitin fa-a<u>h</u>yayn<u>aa</u> bihi <b>a</b>l-ar<u>dh</u>a ba\'da mawtih<u>aa</u> ka<u>dzaa</u>lika <b>al</b>nnusyuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3670, '35:10', 35, 10, ' man k<u>aa</u>na yuriidu <b>a</b>l\'izzata falill<u>aa</u>hi <b>a</b>l\'izzatu jamii\'an ilayhi ya<u>sh</u>\'adu <b>a</b>lkalimu <b>al</b><u>ththh</u>ayyibu wa<b>a</b>l\'amalu <b>al</b><u>shshaa</u>li<u>h</u>u yarfa\'uhu wa<b>a</b>lla<u>dz</u>iina yamkuruuna <b>al</b>ssayyi-<u>aa</u>ti lahum \'a<u>dzaa</u>bun syadiidun wamakru ul<u>aa</u>-ika huwa yabuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3671, '35:11', 35, 11, ' wa<b>al</b>l<u>aa</u>hu khalaqakum min tur<u>aa</u>bin tsumma min nu<u>th</u>fatin tsumma ja\'alakum azw<u>aa</u>jan wam<u>aa</u> ta<u>h</u>milu min unts<u>aa</u> wal<u>aa</u> ta<u>dh</u>a\'u ill<u>aa</u> bi\'ilmihi wam<u>aa</u> yu\'ammaru min mu\'ammarin wal<u>aa</u> yunqa<u>sh</u>u min \'umurihi ill<u>aa</u> fii kit<u>aa</u>bin inna <u>dzaa</u>lika \'al<u>aa</u> <b>al</b>l<u>aa</u>hi yasiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3672, '35:12', 35, 12, ' wam<u>aa</u> yastawii <b>a</b>lba<u>h</u>r<u>aa</u>ni h<u>aadzaa</u> \'a<u>dz</u>bun fur<u>aa</u>tun s<u>aa</u>-ighun syar<u>aa</u>buhu wah<u>aadzaa</u> mil<u>h</u>un uj<u>aa</u>jun wamin kullin ta/kuluuna la<u>h</u>man <u>th</u>ariyyan watastakhrijuuna <u>h</u>ilyatan talbasuunah<u>aa</u> watar<u>aa</u> <b>a</b>lfulka fiihi maw<u>aa</u>khira litabtaghuu min fa<u>dh</u>lihi wala\'allakum tasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3673, '35:13', 35, 13, ' yuuliju <b>al</b>layla fii <b>al</b>nnah<u>aa</u>ri wayuuliju <b>al</b>nnah<u>aa</u>ra fii <b>al</b>layli wasakhkhara <b>al</b>sysyamsa wa<b>a</b>lqamara kullun yajrii li-ajalin musamman <u>dzaa</u>likumu <b>al</b>l<u>aa</u>hu rabbukum lahu <b>a</b>lmulku wa<b>a</b>lla<u>dz</u>iina tad\'uuna min duunihi m<u>aa</u> yamlikuuna min qi<u>th</u>miir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3674, '35:14', 35, 14, ' in tad\'uuhum l<u>aa</u> yasma\'uu du\'<u>aa</u>-akum walaw sami\'uu m<u>aa</u> istaj<u>aa</u>buu lakum wayawma <b>a</b>lqiy<u>aa</u>mati yakfuruuna bisyirkikum wal<u>aa</u> yunabbi-uka mitslu khabiir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3675, '35:15', 35, 15, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su antumu <b>a</b>lfuqar<u>aa</u>u il<u>aa</u> <b>al</b>l<u>aa</u>hi wa<b>al</b>l<u>aa</u>hu huwa <b>a</b>lghaniyyu <b>a</b>l<u>h</u>amiid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3676, '35:16', 35, 16, ' in yasya/ yu<u>dz</u>hibkum waya/ti bikhalqin jadiid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3677, '35:17', 35, 17, ' wam<u>aa</u> <u>dzaa</u>lika \'al<u>aa</u> <b>al</b>l<u>aa</u>hi bi\'aziiz<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3678, '35:18', 35, 18, ' wal<u>aa</u> taziru w<u>aa</u>ziratun wizra ukhr<u>aa</u> wa-in tad\'u mutsqalatun il<u>aa</u> <u>h</u>imlih<u>aa</u> l<u>aa</u> yu<u>h</u>mal minhu syay-un walaw k<u>aa</u>na <u>dzaa</u> qurb<u>aa</u> innam<u>aa</u> tun<u>dz</u>iru <b>al</b>la<u>dz</u>iina yakhsyawna rabbahum bi<b>a</b>lghaybi wa-aq<u>aa</u>muu <b>al</b><u>shsh</u>al<u>aa</u>ta waman tazakk<u>aa</u> fa-innam<u>aa</u> yatazakk<u>aa</u> linafsihi wa-il<u>aa</u> <b>al</b>l<u>aa</u>hi <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3679, '35:19', 35, 19, ' wam<u>aa</u> yastawii <b>a</b>l-a\'m<u>aa</u> wa<b>a</b>lba<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3680, '35:20', 35, 20, ' wal<u>aa</u> <b>al</b><u>zhzh</u>ulum<u>aa</u>tu wal<u>aa</u> <b>al</b>nnuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3681, '35:21', 35, 21, ' wal<u>aa</u> <b>al</b><u>zhzh</u>illu wal<u>aa</u> <b>a</b>l<u>h</u>aruur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3682, '35:22', 35, 22, ' wam<u>aa</u> yastawii <b>a</b>l-a<u>h</u>y<u>aa</u>u wal<u>aa</u> <b>a</b>l-amw<u>aa</u>tu inna <b>al</b>l<u>aa</u>ha yusmi\'u man yasy<u>aa</u>u wam<u>aa</u> anta bimusmi\'in man fii <b>a</b>lqubuur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3683, '35:23', 35, 23, ' in anta ill<u>aa</u> na<u>dz</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3684, '35:24', 35, 24, ' inn<u>aa</u> arsaln<u>aa</u>ka bi<b>a</b>l<u>h</u>aqqi basyiiran wana<u>dz</u>iiran wa-in min ummatin ill<u>aa</u> khal<u>aa</u> fiih<u>aa</u> na<u>dz</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3685, '35:25', 35, 25, ' wa-in yuka<u>dzdz</u>ibuuka faqad ka<u>dzdz</u>aba <b>al</b>la<u>dz</u>iina min qablihim j<u>aa</u>-at-hum rusuluhum bi<b>a</b>lbayyin<u>aa</u>ti wabi<b>al</b>zzuburi wabi<b>a</b>lkit<u>aa</u>bi <b>a</b>lmuniir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3686, '35:26', 35, 26, ' tsumma akha<u>dz</u>tu <b>al</b>la<u>dz</u>iina kafaruu fakayfa k<u>aa</u>na nakiir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3687, '35:27', 35, 27, ' alam tara anna <b>al</b>l<u>aa</u>ha anzala mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an fa-akhrajn<u>aa</u> bihi tsamar<u>aa</u>tin mukhtalifan <b>a</b>lw<u>aa</u>nuh<u>aa</u> wamina <b>a</b>ljib<u>aa</u>li judadun bii<u>dh</u>un wa<u>h</u>umrun mukhtalifun <b>a</b>lw<u>aa</u>nuh<u>aa</u> waghar<u>aa</u>biibu suud<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3688, '35:28', 35, 28, ' wamina <b>al</b>nn<u>aa</u>si wa<b>al</b>ddaw<u>aa</u>bbi wa<b>a</b>l-an\'<u>aa</u>mi mukhtalifun <b>a</b>lw<u>aa</u>nuhu ka<u>dzaa</u>lika innam<u>aa</u> yakhsy<u>aa</u> <b>al</b>l<u>aa</u>ha min \'ib<u>aa</u>dihi <b>a</b>l\'ulam<u>aa</u>u inna <b>al</b>l<u>aa</u>ha \'aziizun ghafuur<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3689, '35:29', 35, 29, ' inna <b>al</b>la<u>dz</u>iina yatluuna kit<u>aa</u>ba <b>al</b>l<u>aa</u>hi wa-aq<u>aa</u>muu <b>al</b><u>shsh</u>al<u>aa</u>ta wa-anfaquu mimm<u>aa</u> razaqn<u>aa</u>hum sirran wa\'al<u>aa</u>niyatan yarjuuna tij<u>aa</u>ratan lan tabuur<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3690, '35:30', 35, 30, ' liyuwaffiyahum ujuurahum wayaziidahum min fa<u>dh</u>lihi innahu ghafuurun syakuur<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3691, '35:31', 35, 31, ' wa<b>a</b>lla<u>dz</u>ii aw<u>h</u>ayn<u>aa</u> ilayka mina <b>a</b>lkit<u>aa</u>bi huwa <b>a</b>l<u>h</u>aqqu mu<u>sh</u>addiqan lim<u>aa</u> bayna yadayhi inna <b>al</b>l<u>aa</u>ha bi\'ib<u>aa</u>dihi lakhabiirun ba<u>sh</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3692, '35:32', 35, 32, ' tsumma awratsn<u>aa</u> <b>a</b>lkit<u>aa</u>ba <b>al</b>la<u>dz</u>iina i<u>sth</u>afayn<u>aa</u> min \'ib<u>aa</u>din<u>aa</u> faminhum <u>zhaa</u>limun linafsihi waminhum muqta<u>sh</u>idun waminhum s<u>aa</u>biqun bi<b>a</b>lkhayr<u>aa</u>ti bi-i<u>dz</u>ni <b>al</b>l<u>aa</u>hi <u>dzaa</u>lika huwa <b>a</b>lfa<u>dh</u>lu <b>a</b>lkabiir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3693, '35:33', 35, 33, ' jann<u>aa</u>tu \'adnin yadkhuluunah<u>aa</u> yu<u>h</u>allawna fiih<u>aa</u> min as<u>aa</u>wira min <u>dz</u>ahabin walu/lu-an walib<u>aa</u>suhum fiih<u>aa</u> <u>h</u>ariir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3694, '35:34', 35, 34, ' waq<u>aa</u>luu <b>a</b>l<u>h</u>amdu lill<u>aa</u>hi <b>al</b>la<u>dz</u>ii a<u>dz</u>haba \'ann<u>aa</u> <b>a</b>l<u>h</u>azana inna rabban<u>aa</u> laghafuurun syakuur<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3695, '35:35', 35, 35, ' <b>al</b>la<u>dz</u>ii a<u>h</u>allan<u>aa</u> d<u>aa</u>ra <b>a</b>lmuq<u>aa</u>mati min fa<u>dh</u>lihi l<u>aa</u> yamassun<u>aa</u> fiih<u>aa</u> na<u>sh</u>abun wal<u>aa</u> yamassun<u>aa</u> fiih<u>aa</u> lughuub<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3696, '35:36', 35, 36, ' wa<b>a</b>lla<u>dz</u>iina kafaruu lahum n<u>aa</u>ru jahannama l<u>aa</u> yuq<u>daa</u> \'alayhim fayamuutuu wal<u>aa</u> yukhaffafu \'anhum min \'a<u>dzaa</u>bih<u>aa</u> ka<u>dzaa</u>lika najzii kulla kafuur<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3697, '35:37', 35, 37, ' wahum ya<u>sth</u>arikhuuna fiih<u>aa</u> rabban<u>aa</u> akhrijn<u>aa</u> na\'mal <u>shaa</u>li<u>h</u>an ghayra <b>al</b>la<u>dz</u>ii kunn<u>aa</u> na\'malu awa lam nu\'ammirkum m<u>aa</u> yata<u>dz</u>akkaru fiihi man ta<u>dz</u>akkara waj<u>aa</u>-akumu <b>al</b>nna<u>dz</u>iiru fa<u>dz</u>uuquu fam<u>aa</u> li<b>l</b><u>zhzhaa</u>limiina min na<u>sh</u>iir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3698, '35:38', 35, 38, ' inna <b>al</b>l<u>aa</u>ha \'<u>aa</u>limu ghaybi <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i innahu \'aliimun bi<u>dzaa</u>ti <b>al</b><u>shsh</u>uduur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3699, '35:39', 35, 39, ' huwa <b>al</b>la<u>dz</u>ii ja\'alakum khal<u>aa</u>-ifa fii <b>a</b>l-ar<u>dh</u>i faman kafara fa\'alayhi kufruhu wal<u>aa</u> yaziidu <b>a</b>lk<u>aa</u>firiina kufruhum \'inda rabbihim ill<u>aa</u> maqtan wal<u>aa</u> yaziidu <b>a</b>lk<u>aa</u>firiina kufruhum ill<u>aa</u> khas<u>aa</u>r<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3700, '35:40', 35, 40, ' qul ara-aytum syurak<u>aa</u>-akumu <b>al</b>la<u>dz</u>iina tad\'uuna min duuni <b>al</b>l<u>aa</u>hi aruunii m<u>aatsaa</u> khalaquu mina <b>a</b>l-ar<u>dh</u>i am lahum syirkun fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti am <u>aa</u>tayn<u>aa</u>hum kit<u>aa</u>ban fahum \'al<u>aa</u> bayyinatin minhu bal in ya\'idu <b>al</b><u>zhzhaa</u>limuuna ba\'<u>dh</u>uhum ba\'<u>dh</u>an ill<u>aa</u> ghuruur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3701, '35:41', 35, 41, ' inna <b>al</b>l<u>aa</u>ha yumsiku <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a an tazuul<u>aa</u> wala-in z<u>aa</u>lat<u>aa</u> in amsakahum<u>aa</u> min a<u>h</u>adin min ba\'dihi innahu k<u>aa</u>na <u>h</u>aliiman ghafuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3702, '35:42', 35, 42, ' wa-aqsamuu bi<b>al</b>l<u>aa</u>hi jahda aym<u>aa</u>nihim la-in j<u>aa</u>-ahum na<u>dz</u>iirun layakuununna ahd<u>aa</u> min i<u>h</u>d<u>aa</u> <b>a</b>l-umami falamm<u>aa</u> j<u>aa</u>-ahum na<u>dz</u>iirun m<u>aa</u> z<u>aa</u>dahum ill<u>aa</u> nufuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3703, '35:43', 35, 43, ' istikb<u>aa</u>ran fii <b>a</b>l-ar<u>dh</u>i wamakra <b>al</b>ssayyi-i wal<u>aa</u> ya<u>h</u>iiqu <b>a</b>lmakru <b>al</b>ssayyi-u ill<u>aa</u> bi-ahlihi fahal yan<u>zh</u>uruuna ill<u>aa</u> sunnata <b>a</b>l-awwaliina falan tajida lisunnati <b>al</b>l<u>aa</u>hi tabdiilan walan tajida lisunnati <b>al</b>l<u>aa</u>hi ta<u>h</u>wiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3704, '35:44', 35, 44, ' awa lam yasiiruu fii <b>a</b>l-ar<u>dh</u>i fayan<u>zh</u>uruu kayfa k<u>aa</u>na \'<u>aa</u>qibatu <b>al</b>la<u>dz</u>iina min qablihim wak<u>aa</u>nuu asyadda minhum quwwatan wam<u>aa</u> k<u>aa</u>na <b>al</b>l<u>aa</u>hu liyu\'jizahu min syay-in fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wal<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i innahu k<u>aa</u>na \'aliiman qadiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3705, '35:45', 35, 45, ' walaw yu-<u>aa</u>khi<u>dz</u>u <b>al</b>l<u>aa</u>hu <b>al</b>nn<u>aa</u>sa bim<u>aa</u> kasabuu m<u>aa</u> taraka \'al<u>aa</u> <u>zh</u>ahrih<u>aa</u> min d<u>aa</u>bbatin wal<u>aa</u>kin yu-akhkhiruhum il<u>aa</u> ajalin musamman fa-i<u>dzaa</u> j<u>aa</u>-a ajaluhum fa-inna <b>al</b>l<u>aa</u>ha k<u>aa</u>na bi\'ib<u>aa</u>dihi ba<u>sh</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3706, '36:1', 36, 1, ' y<u>aa</u>-siin</u>\r\r\n');
INSERT INTO quranLatin VALUES (3707, '36:2', 36, 2, ' wa<b>a</b>lqur-<u>aa</u>ni <b>a</b>l<u>h</u>akiim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3708, '36:3', 36, 3, ' innaka lamina <b>a</b>lmursaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3709, '36:4', 36, 4, ' \'al<u>aa</u> <u>sh</u>ir<u>aath</u>in mustaqiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3710, '36:5', 36, 5, ' tanziila <b>a</b>l\'aziizi <b>al</b>rra<u>h</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3711, '36:6', 36, 6, ' litun<u>dz</u>ira qawman m<u>aa</u> un<u>dz</u>ira <u>aa</u>b<u>aa</u>uhum fahum gh<u>aa</u>filuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3712, '36:7', 36, 7, ' laqad <u>h</u>aqqa <b>a</b>lqawlu \'al<u>aa</u> aktsarihim fahum l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3713, '36:8', 36, 8, ' inn<u>aa</u> ja\'aln<u>aa</u> fii a\'n<u>aa</u>qihim aghl<u>aa</u>lan fahiya il<u>aa</u> <b>a</b>l-a<u>dz</u>q<u>aa</u>ni fahum muqma<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3714, '36:9', 36, 9, ' waja\'aln<u>aa</u> min bayni aydiihim saddan wamin khalfihim saddan fa-aghsyayn<u>aa</u>hum fahum l<u>aa</u> yub<u>sh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3715, '36:10', 36, 10, ' wasaw<u>aa</u>un \'alayhim a-an<u>dz</u>artahum am lam tun<u>dz</u>irhum l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3716, '36:11', 36, 11, ' innam<u>aa</u> tun<u>dz</u>iru mani ittaba\'a <b>al</b><u>dzdz</u>ikra wakhasyiya <b>al</b>rra<u>h</u>m<u>aa</u>na bi<b>a</b>lghaybi fabasysyirhu bimaghfiratin wa-ajrin kariim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3717, '36:12', 36, 12, ' inn<u>aa</u> na<u>h</u>nu nu<u>h</u>yii <b>a</b>lmawt<u>aa</u> wanaktubu m<u>aa</u> qaddamuu wa<u>aa</u>ts<u>aa</u>rahum wakulla syay-in a<u>hs</u>ayn<u>aa</u>hu fii im<u>aa</u>min mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3718, '36:13', 36, 13, ' wa<b>i</b><u>dh</u>rib lahum matsalan a<u>sh</u>-<u>haa</u>ba <b>a</b>lqaryati i<u>dz</u> j<u>aa</u>-ah<u>aa</u> <b>a</b>lmursaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3719, '36:14', 36, 14, ' i<u>dz</u> arsaln<u>aa</u> ilayhimu itsnayni faka<u>dzdz</u>abuuhum<u>aa</u> fa\'azzazn<u>aa</u> bits<u>aa</u>litsin faq<u>aa</u>luu inn<u>aa</u> ilaykum mursaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3720, '36:15', 36, 15, ' q<u>aa</u>luu m<u>aa</u> antum ill<u>aa</u> basyarun mitslun<u>aa</u> wam<u>aa</u> anzala <b>al</b>rra<u>h</u>m<u>aa</u>nu min syay-in in antum ill<u>aa</u> tak<u>dz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3721, '36:16', 36, 16, ' q<u>aa</u>luu rabbun<u>aa</u> ya\'lamu inn<u>aa</u> ilaykum lamursaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3722, '36:17', 36, 17, ' wam<u>aa</u> \'alayn<u>aa</u> ill<u>aa</u> <b>a</b>lbal<u>aa</u>ghu <b>a</b>lmubiin<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3723, '36:18', 36, 18, ' q<u>aa</u>luu inn<u>aa</u> ta<u>th</u>ayyarn<u>aa</u> bikum la-in lam tantahuu lanarjumannakum walayamassannakum minn<u>aa</u> \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3724, '36:19', 36, 19, ' q<u>aa</u>luu <u>th</u><u>aa</u>-irukum ma\'akum a-in <u>dz</u>ukkirtum bal antum qawmun musrifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3725, '36:20', 36, 20, ' waj<u>aa</u>-a min aq<u>shaa</u> <b>a</b>lmadiinati rajulun yas\'<u>aa</u> q<u>aa</u>la y<u>aa</u> qawmi ittabi\'uu <b>a</b>lmursaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3726, '36:21', 36, 21, ' ittabi\'uu man l<u>aa</u> yas-alukum ajran wahum muhtaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3727, '36:22', 36, 22, ' wam<u>aa</u> liya l<u>aa</u> a\'budu <b>al</b>la<u>dz</u>ii fa<u>th</u>aranii wa-ilayhi turja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3728, '36:23', 36, 23, ' a-attakhi<u>dz</u>u min duunihi <u>aa</u>lihatan in yuridni <b>al</b>rra<u>h</u>m<u>aa</u>nu bi<u>dh</u>urrin l<u>aa </u>tughni \'annii syaf<u>aa</u>\'atuhum syay-an wal<u>aa</u> yunqi<u>dz</u>uun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3729, '36:24', 36, 24, ' innii i<u>dz</u>an lafii <u>dh</u>al<u>aa</u>lin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3730, '36:25', 36, 25, ' innii <u>aa</u>mantu birabbikum fa<b>i</b>sma\'uun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3731, '36:26', 36, 26, ' qiila udkhuli <b>a</b>ljannata q<u>aa</u>la y<u>aa</u> layta qawmii ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3732, '36:27', 36, 27, ' bim<u>aa</u> ghafara lii rabbii waja\'alanii mina <b>a</b>lmukramiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3733, '36:28', 36, 28, ' wam<u>aa</u> anzaln<u>aa</u> \'al<u>aa</u> qawmihi min ba\'dihi min jundin mina <b>al</b>ssam<u>aa</u>-i wam<u>aa</u> kunn<u>aa</u> munziliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3734, '36:29', 36, 29, ' in k<u>aa</u>nat ill<u>aa</u> <u>sh</u>ay<u>h</u>atan w<u>aah</u>idatan fa-i<u>dzaa</u> hum kh<u>aa</u>miduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3735, '36:30', 36, 30, ' y<u>aa</u> <u>h</u>asratan \'al<u>aa</u> <b>a</b>l\'ib<u>aa</u>di m<u>aa</u> ya/tiihim min rasuulin ill<u>aa</u> k<u>aa</u>nuu bihi yastahzi-uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3736, '36:31', 36, 31, ' alam yaraw kam ahlakn<u>aa</u> qablahum mina <b>a</b>lquruuni annahum ilayhim l<u>aa</u> yarji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3737, '36:32', 36, 32, ' wa-in kullun lamm<u>aa</u> jamii\'un ladayn<u>aa</u> mu<u>hd</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3738, '36:33', 36, 33, ' wa<u>aa</u>yatun lahumu <b>a</b>l-ar<u>dh</u>u <b>a</b>lmaytatu a<u>h</u>yayn<u>aa</u>h<u>aa</u> wa-akhrajn<u>aa</u> minh<u>aa</u> <u>h</u>abban faminhu ya/kuluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3739, '36:34', 36, 34, ' waja\'aln<u>aa</u> fiih<u>aa</u> jann<u>aa</u>tin min nakhiilin wa-a\'n<u>aa</u>bin wafajjarn<u>aa</u> fiih<u>aa</u> mina <b>a</b>l\'uyuun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3740, '36:35', 36, 35, ' liya/kuluu min tsamarihi wam<u>aa</u> \'amilat-hu aydiihim afal<u>aa</u> yasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3741, '36:36', 36, 36, ' sub<u>haa</u>na <b>al</b>la<u>dz</u>ii khalaqa <b>a</b>l-azw<u>aa</u>ja kullah<u>aa</u> mimm<u>aa</u> tunbitu <b>a</b>l-ar<u>dh</u>u wamin anfusihim wamimm<u>aa</u> l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3742, '36:37', 36, 37, ' wa<u>aa</u>yatun lahumu <b>al</b>laylu naslakhu minhu <b>al</b>nnah<u>aa</u>ra fa-i<u>dzaa</u> hum mu<u>zh</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3743, '36:38', 36, 38, ' wa<b>al</b>sysyamsu tajrii limustaqarrin lah<u>aa</u> <u>dzaa</u>lika taqdiiru <b>a</b>l\'aziizi <b>a</b>l\'aliim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3744, '36:39', 36, 39, ' wa<b>a</b>lqamara qaddarn<u>aa</u>hu man<u>aa</u>zila <u>h</u>att<u>aa</u> \'<u>aa</u>da ka<b>a</b>l\'urjuuni <b>a</b>lqadiim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3745, '36:40', 36, 40, ' l<u>aa</u> <b>al</b>sysyamsu yanbaghii lah<u>aa</u> an tudrika <b>a</b>lqamara wal<u>aa</u> <b>al</b>laylu s<u>aa</u>biqu <b>al</b>nnah<u>aa</u>ri wakullun fii falakin yasba<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3746, '36:41', 36, 41, ' wa<u>aa</u>yatun lahum ann<u>aa</u> <u>h</u>amaln<u>aa</u> <u>dz</u>urriyyatahum fii <b>a</b>lfulki <b>a</b>lmasy<u>h</u>uun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3747, '36:42', 36, 42, ' wakhalaqn<u>aa</u> lahum min mitslihi m<u>aa</u> yarkabuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3748, '36:43', 36, 43, ' wa-in nasya/ nughriqhum fal<u>aa</u> <u>sh</u>ariikha lahum wal<u>aa</u> hum yunqa<u>dz</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3749, '36:44', 36, 44, ' ill<u>aa</u> ra<u>h</u>matan minn<u>aa</u> wamat<u>aa</u>\'an il<u>aa</u> <u>h</u>iin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3750, '36:45', 36, 45, ' wa-i<u>dzaa</u> qiila lahumu ittaquu m<u>aa</u> bayna aydiikum wam<u>aa</u> khalfakum la\'allakum tur<u>h</u>amuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3751, '36:46', 36, 46, ' wam<u>aa</u> ta/tiihim min <u>aa</u>yatin min <u>aa</u>y<u>aa</u>ti rabbihim ill<u>aa</u> k<u>aa</u>nuu \'anh<u>aa</u> mu\'ri<u>dh</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3752, '36:47', 36, 47, ' wa-i<u>dzaa</u> qiila lahum anfiquu mimm<u>aa</u> razaqakumu <b>al</b>l<u>aa</u>hu q<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu lilla<u>dz</u>iina <u>aa</u>manuu anu<u>th</u>\'imu man law yasy<u>aa</u>u <b>al</b>l<u>aa</u>hu a<u>th</u>\'amahu in antum ill<u>aa</u> fii <u>dh</u>al<u>aa</u>lin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3753, '36:48', 36, 48, ' wayaquuluuna mat<u>aa</u> h<u>aadzaa</u> <b>a</b>lwa\'du in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3754, '36:49', 36, 49, ' m<u>aa</u> yan<u>zh</u>uruuna ill<u>aa</u> <u>sh</u>ay<u>h</u>atan w<u>aah</u>idatan ta/khu<u>dz</u>uhum wahum yakhi<u>shsh</u>imuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3755, '36:50', 36, 50, ' fal<u>aa</u> yasta<u>th</u>ii\'uuna taw<u>sh</u>iyatan wal<u>aa</u> il<u>aa</u> ahlihim yarji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3756, '36:51', 36, 51, ' wanufikha fii <b>al</b><u>shsh</u>uuri fa-i<u>dzaa</u> hum mina <b>a</b>l-ajd<u>aa</u>tsi il<u>aa</u> rabbihim yansiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3757, '36:52', 36, 52, ' q<u>aa</u>luu y<u>aa</u> waylan<u>aa</u> man ba\'atsan<u>aa</u> min marqadin<u>aa</u> h<u>aadzaa</u> m<u>aa</u> wa\'ada <b>al</b>rra<u>h</u>m<u>aa</u>nu wa<u>sh</u>adaqa <b>a</b>lmursaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3758, '36:53', 36, 53, ' in k<u>aa</u>nat ill<u>aa</u> <u>sh</u>ay<u>h</u>atan w<u>aah</u>idatan fa-i<u>dzaa</u> hum jamii\'un ladayn<u>aa</u> mu<u>hd</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3759, '36:54', 36, 54, ' fa<b>a</b>lyawma l<u>aa</u> tu<u>zh</u>lamu nafsun syay-an wal<u>aa</u> tujzawna ill<u>aa</u> m<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3760, '36:55', 36, 55, ' inna a<u>sh</u>-<u>haa</u>ba <b>a</b>ljannati <b>a</b>lyawma fii syughulin f<u>aa</u>kihuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3761, '36:56', 36, 56, ' hum wa-azw<u>aa</u>juhum fii <u>zh</u>il<u>aa</u>lin \'al<u>aa</u> <b>a</b>l-ar<u>aa</u>-iki muttaki-uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3762, '36:57', 36, 57, ' lahum fiih<u>aa</u> f<u>aa</u>kihatun walahum m<u>aa</u> yadda\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3763, '36:58', 36, 58, ' sal<u>aa</u>mun qawlan min rabbin ra<u>h</u>iim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3764, '36:59', 36, 59, ' wa<b>i</b>mt<u>aa</u>zuu <b>a</b>lyawma ayyuh<u>aa</u> <b>a</b>lmujrimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3765, '36:60', 36, 60, ' alam a\'had ilaykum y<u>aa</u> banii <u>aa</u>dama an l<u>aa</u> ta\'buduu <b>al</b>sysyay<u>th</u><u>aa</u>na innahu lakum \'aduwwun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3766, '36:61', 36, 61, ' wa-ani u\'buduunii h<u>aadzaa</u> <u>sh</u>ir<u>aath</u>un mustaqiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3767, '36:62', 36, 62, ' walaqad a<u>dh</u>alla minkum jibillan katsiiran afalam takuunuu ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3768, '36:63', 36, 63, ' h<u>aadz</u>ihi jahannamu <b>al</b>latii kuntum tuu\'aduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3769, '36:64', 36, 64, ' i<u>sh</u>lawh<u>aa</u> <b>a</b>lyawma bim<u>aa</u> kuntum takfuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3770, '36:65', 36, 65, ' alyawma nakhtimu \'al<u>aa</u> afw<u>aa</u>hihim watukallimun<u>aa</u> aydiihim watasyhadu arjuluhum bim<u>aa</u> k<u>aa</u>nuu yaksibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3771, '36:66', 36, 66, ' walaw nasy<u>aa</u>u la<u>th</u>amasn<u>aa</u> \'al<u>aa</u> a\'yunihim fa<b>i</b>stabaquu <b>al</b><u>shsh</u>ir<u>aath</u>a fa-ann<u>aa</u> yub<u>sh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3772, '36:67', 36, 67, ' walaw nasy<u>aa</u>u lamasakhn<u>aa</u>hum \'al<u>aa</u> mak<u>aa</u>natihim fam<u>aa</u> ista<u>th</u><u>aa</u>\'uu mu<u>dh</u>iyyan wal<u>aa</u> yarji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3773, '36:68', 36, 68, ' waman nu\'ammirhu nunakkis-hu fii <b>a</b>lkhalqi afal<u>aa</u> ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3774, '36:69', 36, 69, ' wam<u>aa</u> \'allamn<u>aa</u>hu <b>al</b>sysyi\'ra wam<u>aa</u> yanbaghii lahu in huwa ill<u>aa</u> <u>dz</u>ikrun waqur-<u>aa</u>nun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3775, '36:70', 36, 70, ' liyun<u>dz</u>ira man k<u>aa</u>na <u>h</u>ayyan waya<u>h</u>iqqa <b>a</b>lqawlu \'al<u>aa</u> <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
