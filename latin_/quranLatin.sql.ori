INSERT INTO quranLatin VALUES (642, '4:149', 4, 149, ' in tubduu khayran aw tukhfuuhu aw ta\'fuu \'an suu-in fa-inna <b>al</b>l<u>aa</u>ha k<u>aa</u>na \'afuwwan qadiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (643, '4:150', 4, 150, ' inna <b>al</b>la<u>dz</u>iina yakfuruuna bi<b>al</b>l<u>aa</u>hi warusulihi wayuriiduuna an yufarriquu bayna <b>al</b>l<u>aa</u>hi warusulihi wayaquuluuna nu/minu biba\'<u>dh</u>in wanakfuru biba\'<u>dh</u>in wayuriiduuna an yattakhi<u>dz</u>uu bayna <u>dzaa</u>lika sabiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (644, '4:151', 4, 151, ' ul<u>aa</u>-ika humu <b>a</b>lk<u>aa</u>firuuna <u>h</u>aqqan wa-a\'tadn<u>aa</u> lilk<u>aa</u>firiina \'a<u>dzaa</u>ban muhiin<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (645, '4:152', 4, 152, ' wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu bi<b>al</b>l<u>aa</u>hi warusulihi walam yufarriquu bayna a<u>h</u>adin minhum ul<u>aa</u>-ika sawfa yu/tiihim ujuurahum wak<u>aa</u>na <b>al</b>l<u>aa</u>hu ghafuuran ra<u>h</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (646, '4:153', 4, 153, ' yas-aluka ahlu <b>a</b>lkit<u>aa</u>bi an tunazzila \'alayhim kit<u>aa</u>ban mina <b>al</b>ssam<u>aa</u>-i faqad sa-aluu muus<u>aa</u> akbara min <u>dzaa</u>lika faq<u>aa</u>luu arin<u>aa</u> <b>al</b>l<u>aa</u>ha jahratan fa-akha<u>dz</u>at-humu <b>al</b><u>shshaa</u>\'iqatu bi<u>zh</u>ulmihim tsumma ittakha<u>dz</u>uu <b>a</b>l\'ijla min ba\'di m<u>aa</u> j<u>aa</u>-at-humu <b>a</b>lbayyin<u>aa</u>tu fa\'afawn<u>aa</u> \'an <u>dzaa</u>lika wa<u>aa</u>tayn<u>aa</u> muus<u>aa</u> sul<u>th</u><u>aa</u>nan mubiin<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (647, '4:154', 4, 154, ' warafa\'n<u>aa</u> fawqahumu <b>al</b><u>ththh</u>uura bimiits<u>aa</u>qihim waquln<u>aa</u> lahumu udkhuluu <b>a</b>lb<u>aa</u>ba sujjadan waquln<u>aa</u> lahum l<u>aa</u> ta\'duu fii <b>al</b>ssabti wa-akha<u>dz</u>n<u>aa</u> minhum miits<u>aa</u>qan ghalii<u>zhaa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (648, '4:155', 4, 155, ' fabim<u>aa</u> naq<u>dh</u>ihim miits<u>aa</u>qahum wakufrihim bi-<u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi waqatlihimu <b>a</b>l-anbiy<u>aa</u>-a bighayri <u>h</u>aqqin waqawlihim quluubun<u>aa</u> ghulfun bal <u>th</u>aba\'a <b>al</b>l<u>aa</u>hu \'alayh<u>aa</u> bikufrihim fal<u>aa</u> yu/minuuna ill<u>aa</u> qaliil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (649, '4:156', 4, 156, ' wabikufrihim waqawlihim \'al<u>aa</u> maryama buht<u>aa</u>nan \'a<u>zh</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (650, '4:157', 4, 157, ' waqawlihim inn<u>aa</u> qataln<u>aa</u> <b>a</b>lmasii<u>h</u>a \'iis<u>aa</u> ibna maryama rasuula <b>al</b>l<u>aa</u>hi wam<u>aa</u> qataluuhu wam<u>aa</u> <u>sh</u>alabuuhu wal<u>aa</u>kin syubbiha lahum wa-inna <b>al</b>la<u>dz</u>iina ikhtalafuu fiihi lafii syakkin minhu m<u>aa</u> lahum bihi min \'ilmin ill<u>aa</u> ittib<u>aa</u>\'a <b>al</b><u>zhzh</u>anni wam<u>aa</u> qataluuhu yaqiin<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (651, '4:158', 4, 158, ' bal rafa\'ahu <b>al</b>l<u>aa</u>hu ilayhi wak<u>aa</u>na <b>al</b>l<u>aa</u>hu \'aziizan <u>h</u>akiim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (652, '4:159', 4, 159, ' wa-in min ahli <b>a</b>lkit<u>aa</u>bi ill<u>aa</u> layu/minanna bihi qabla mawtihi wayawma <b>a</b>lqiy<u>aa</u>mati yakuunu \'alayhim syahiid<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (653, '4:160', 4, 160, ' fabi<u>zh</u>ulmin mina <b>al</b>la<u>dz</u>iina h<u>aa</u>duu <u>h</u>arramn<u>aa</u> \'alayhim <u>th</u>ayyib<u>aa</u>tin u<u>h</u>illat lahum wabi<u>sh</u>addihim \'an sabiili <b>al</b>l<u>aa</u>hi katsiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (654, '4:161', 4, 161, ' wa-akh<u>dz</u>ihimu <b>al</b>rrib<u>aa</u> waqad nuhuu \'anhu wa-aklihim amw<u>aa</u>la <b>al</b>nn<u>aa</u>si bi<b>a</b>lb<u>aath</u>ili wa-a\'tadn<u>aa</u> lilk<u>aa</u>firiina minhum \'a<u>dzaa</u>ban <b>a</b>liim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (655, '4:162', 4, 162, ' l<u>aa</u>kini <b>al</b>rr<u>aa</u>sikhuuna fii <b>a</b>l\'ilmi minhum wa<b>a</b>lmu/minuuna yu/minuuna bim<u>aa</u> unzila ilayka wam<u>aa</u> unzila min qablika wa<b>a</b>lmuqiimiina <b>al</b><u>shsh</u>al<u>aa</u>ta wa<b>a</b>lmu/tuuna <b>al</b>zzak<u>aa</u>ta wa<b>a</b>lmu/minuuna bi<b>al</b>l<u>aa</u>hi wa<b>a</b>lyawmi <b>a</b>l-<u>aa</u>khiri ul<u>aa</u>-ika sanu/tiihim ajran \'a<u>zh</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (656, '4:163', 4, 163, ' inn<u>aa</u> aw<u>h</u>ayn<u>aa</u> ilayka kam<u>aa</u> aw<u>h</u>ayn<u>aa</u> il<u>aa</u> nuu<u>h</u>in wa<b>al</b>nnabiyyiina min ba\'dihi wa-aw<u>h</u>ayn<u>aa</u> il<u>aa</u> ibr<u>aa</u>hiima wa-ism<u>aa</u>\'iila wa-is<u>haa</u>qa waya\'quuba wa<b>a</b>l-asb<u>aath</u>i wa\'iis<u>aa</u> wa-ayyuuba wayuunusa wah<u>aa</u>ruuna wasulaym<u>aa</u>na wa<u>aa</u>tayn<u>aa</u> d<u>aa</u>wuuda zabuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (657, '4:164', 4, 164, ' warusulan qad qa<u>sh</u>a<u>sh</u>n<u>aa</u>hum \'alayka min qablu warusulan lam naq<u>sh</u>u<u>sh</u>hum \'alayka wakallama <b>al</b>l<u>aa</u>hu muus<u>aa</u> takliim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (658, '4:165', 4, 165, ' rusulan mubasysyiriina wamun<u>dz</u>iriina li-all<u>aa</u> yakuuna li<b>l</b>nn<u>aa</u>si \'al<u>aa</u> <b>al</b>l<u>aa</u>hi <u>h</u>ujjatun ba\'da <b>al</b>rrusuli wak<u>aa</u>na <b>al</b>l<u>aa</u>hu \'aziizan <u>h</u>akiim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (659, '4:166', 4, 166, ' l<u>aa</u>kini <b>al</b>l<u>aa</u>hu yasyhadu bim<u>aa</u> anzala ilayka anzalahu bi\'ilmihi wa<b>a</b>lmal<u>aa</u>-ikatu yasyhaduuna wakaf<u>aa</u> bi<b>al</b>l<u>aa</u>hi syahiid<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (660, '4:167', 4, 167, ' inna <b>al</b>la<u>dz</u>iina kafaruu wa<u>sh</u>adduu \'an sabiili <b>al</b>l<u>aa</u>hi qad <u>dh</u>alluu <u>dh</u>al<u>aa</u>lan ba\'iid<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (661, '4:168', 4, 168, ' inna <b>al</b>la<u>dz</u>iina kafaruu wa<u>zh</u>alamuu lam yakuni <b>al</b>l<u>aa</u>hu liyaghfira lahum wal<u>aa</u> liyahdiyahum <u>th</u>ariiq<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (662, '4:169', 4, 169, ' ill<u>aa</u> <u>th</u>ariiqa jahannama kh<u>aa</u>lidiina fiih<u>aa</u> abadan wak<u>aa</u>na <u>dzaa</u>lika \'al<u>aa</u> <b>al</b>l<u>aa</u>hi yasiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (663, '4:170', 4, 170, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su qad j<u>aa</u>-akumu <b>al</b>rrasuulu bi<b>a</b>l<u>h</u>aqqi min rabbikum fa<u>aa</u>minuu khayran lakum wa-in takfuruu fa-inna lill<u>aa</u>hi m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wak<u>aa</u>na <b>al</b>l<u>aa</u>hu \'aliiman <u>h</u>akiim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (664, '4:171', 4, 171, ' y<u>aa</u> ahla <b>a</b>lkit<u>aa</u>bi l<u>aa</u> taghluu fii diinikum wal<u>aa</u> taquuluu \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ill<u>aa</u> <b>a</b>l<u>h</u>aqqa innam<u>aa</u> <b>a</b>lmasii<u>h</u>u \'iis<u>aa</u> ibnu maryama rasuulu <b>al</b>l<u>aa</u>hi wakalimatuhu <b>a</b>lq<u>aa</u>h<u>aa</u> il<u>aa</u> maryama waruu<u>h</u>un minhu fa<u>aa</u>minuu bi<b>al</b>l<u>aa</u>hi warusulihi wal<u>aa</u> taquuluu tsal<u>aa</u>tsatun intahuu khayran lakum innam<u>aa</u> <b>al</b>l<u>aa</u>hu il<u>aa</u>hun w<u>aah</u>idun sub<u>haa</u>nahu an yakuuna lahu waladun lahu m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i wakaf<u>aa</u> bi<b>al</b>l<u>aa</u>hi wakiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (665, '4:172', 4, 172, ' lan yastankifa <b>a</b>lmasii<u>h</u>u an yakuuna \'abdan lill<u>aa</u>hi wal<u>aa</u> <b>a</b>lmal<u>aa</u>-ikatu <b>a</b>lmuqarrabuuna waman yastankif \'an \'ib<u>aa</u>datihi wayastakbir fasaya<u>h</u>syuruhum ilayhi jamii\'<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (666, '4:173', 4, 173, ' fa-amm<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti fayuwaffiihim ujuurahum wayaziiduhum min fa<u>dh</u>lihi wa-amm<u>aa</u> <b>al</b>la<u>dz</u>iina istankafuu wa<b>i</b>stakbaruu fayu\'a<u>dzdz</u>ibuhum \'a<u>dzaa</u>ban <b>a</b>liiman wal<u>aa</u> yajiduuna lahum min duuni <b>al</b>l<u>aa</u>hi waliyyan wal<u>aa</u> na<u>sh</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (667, '4:174', 4, 174, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su qad j<u>aa</u>-akum burh<u>aa</u>nun min rabbikum wa-anzaln<u>aa</u> ilaykum nuuran mubiin<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (668, '4:175', 4, 175, ' fa-amm<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu bi<b>al</b>l<u>aa</u>hi wa<b>i</b>\'ta<u>sh</u>amuu bihi fasayudkhiluhum fii ra<u>h</u>matin minhu wafa<u>dh</u>lin wayahdiihim ilayhi <u>sh</u>ir<u>aath</u>an mustaqiim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (669, '4:176', 4, 176, ' yastaftuunaka quli <b>al</b>l<u>aa</u>hu yuftiikum fii <b>a</b>lkal<u>aa</u>lati ini imruun halaka laysa lahu waladun walahu ukhtun falah<u>aa</u> ni<u>sh</u>fu m<u>aa</u> taraka wahuwa yaritsuh<u>aa</u> in lam yakun lah<u>aa</u> waladun fa-in k<u>aa</u>nat<u>aa</u> itsnatayni falahum<u>aa</u> <b>al</b>tstsuluts<u>aa</u>ni mimm<u>aa</u> taraka wa-in k<u>aa</u>nuu ikhwatan rij<u>aa</u>lan wanis<u>aa</u>-an fali<b>l</b><u>dzdz</u>akari mitslu <u>h</u>a<u>zhzh</u>i <b>a</b>luntsayayni yubayyinu <b>al</b>l<u>aa</u>hu lakum an ta<u>dh</u>illuu wa<b>al</b>l<u>aa</u>hu bikulli syay-in \'aliim<b>un</b><dir>\r\r\n');
INSERT INTO quranLatin VALUES (670, '5:1', 5, 1, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu awfuu bi<b>a</b>l\'uquudi u<u>h</u>illat lakum bahiimatu <b>a</b>l-an\'<u>aa</u>mi ill<u>aa</u> m<u>aa</u> yutl<u>aa</u> \'alaykum ghayra mu<u>h</u>illii <b>al</b><u>shsh</u>aydi wa-antum <u>h</u>urumun inna <b>al</b>l<u>aa</u>ha ya<u>h</u>kumu m<u>aa</u> yuriid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (671, '5:2', 5, 2, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tu<u>h</u>illuu sya\'<u>aa</u>-ira <b>al</b>l<u>aa</u>hi wal<u>aa</u> <b>al</b>sysyahra <b>a</b>l<u>h</u>ar<u>aa</u>ma wal<u>aa</u> <b>a</b>lhadya wal<u>aa</u> <b>a</b>lqal<u>aa</u>-ida wal<u>aa</u> <u>aa</u>mmiina <b>a</b>lbayta <b>a</b>l<u>h</u>ar<u>aa</u>ma yabtaghuuna fa<u>dh</u>lan min rabbihim wari<u>dh</u>w<u>aa</u>nan wa-i<u>dzaa</u> <u>h</u>alaltum fa<b>i</b><u>staa</u>duu wal<u>aa</u> yajrimannakum syana<u>aa</u>nu qawmin an <u>sh</u>adduukum \'ani <b>a</b>lmasjidi <b>a</b>l<u>h</u>ar<u>aa</u>mi an ta\'taduu wata\'<u>aa</u>wanuu \'al<u>aa</u> <b>a</b>lbirri wa<b>al</b>ttaqw<u>aa</u> wal<u>aa</u> ta\'<u>aa</u>wanuu \'al<u>aa</u> <b>a</b>l-itsmi wa<b>a</b>l\'udw<u>aa</u>ni wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha inna <b>al</b>l<u>aa</u>ha syadiidu <b>a</b>l\'iq<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (672, '5:3', 5, 3, ' <u>h</u>urrimat \'alaykumu <b>a</b>lmaytatu wa<b>al</b>ddamu wala<u>h</u>mu <b>a</b>lkhinziiri wam<u>aa</u> uhilla lighayri <b>al</b>l<u>aa</u>hi bihi wa<b>a</b>lmunkhaniqatu wa<b>a</b>lmawquu<u>dz</u>atu wa<b>a</b>lmutaraddiyatu wa<b>al</b>nna<u>th</u>ii<u>h</u>atu wam<u>aa</u> akala <b>al</b>ssabu\'u ill<u>aa</u> m<u>aa ts</u>akkaytum wam<u>aa</u> <u>dz</u>ubi<u>h</u>a \'al<u>aa</u> <b>al</b>nnu<u>sh</u>ubi wa-an tastaqsimuu bi<b>a</b>l-azl<u>aa</u>mi <u>dzaa</u>likum fisqun <b>a</b>lyawma ya-isa <b>al</b>la<u>dz</u>iina kafaruu min diinikum fal<u>aa</u> takhsyawhum wa<b>i</b>khsyawni <b>a</b>lyawma akmaltu lakum diinakum wa-atmamtu \'alaykum ni\'matii wara<u>dh</u>iitu lakumu <b>a</b>l-isl<u>aa</u>ma diinan famani i<u>dth</u>urra fii makhma<u>sh</u>atin ghayra mutaj<u>aa</u>nifin li-itsmin fa-inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (673, '5:4', 5, 4, ' yas-aluunaka m<u>aatsaa</u> u<u>h</u>illa lahum qul u<u>h</u>illa lakumu <b>al</b><u>ththh</u>ayyib<u>aa</u>tu wam<u>aa</u> \'allamtum mina <b>a</b>ljaw<u>aa</u>ri<u>h</u>i mukallibiina tu\'allimuunahunna mimm<u>aa</u> \'allamakumu <b>al</b>l<u>aa</u>hu fakuluu mimm<u>aa</u> amsakna \'alaykum wa<b>u</b><u>dz</u>kuruu isma <b>al</b>l<u>aa</u>hi \'alayhi wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha inna <b>al</b>l<u>aa</u>ha sarii\'u <b>a</b>l<u>h</u>is<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (674, '5:5', 5, 5, ' alyawma u<u>h</u>illa lakumu <b>al</b><u>ththh</u>ayyib<u>aa</u>tu wa<u>th</u>a\'<u>aa</u>mu <b>al</b>la<u>dz</u>iina uutuu <b>a</b>lkit<u>aa</u>ba <u>h</u>illun lakum wa<u>th</u>a\'<u>aa</u>mukum <u>h</u>illun lahum wa<b>a</b>lmu<u>hs</u>an<u>aa</u>tu mina <b>a</b>lmu/min<u>aa</u>ti wa<b>a</b>lmu<u>hs</u>an<u>aa</u>tu mina <b>al</b>la<u>dz</u>iina uutuu <b>a</b>lkit<u>aa</u>ba min qablikum i<u>dzaa</u> <u>aa</u>taytumuuhunna ujuurahunna mu<u>hs</u>iniina ghayra mus<u>aa</u>fi<u>h</u>iina wal<u>aa</u> muttakhi<u>dz</u>ii akhd<u>aa</u>nin waman yakfur bi<b>a</b>l-iim<u>aa</u>ni faqad <u>h</u>abi<u>th</u>a \'amaluhu wahuwa fii <b>a</b>l-<u>aa</u>khirati mina <b>a</b>lkh<u>aa</u>siriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (675, '5:6', 5, 6, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu i<u>dzaa</u> qumtum il<u>aa</u> <b>al</b><u>shsh</u>al<u>aa</u>ti fa<b>i</b>ghsiluu wujuuhakum wa-aydiyakum il<u>aa</u> <b>a</b>lmar<u>aa</u>fiqi wa<b>i</b>msa<u>h</u>uu biruuusikum wa-arjulakum il<u>aa</u> <b>a</b>lka\'bayni wa-in kuntum junuban fa<b>i</b><u>ththh</u>ahharuu wa-in kuntum mar<u>daa</u> aw \'al<u>aa</u> safarin aw j<u>aa</u>-a a<u>h</u>adun minkum mina <b>a</b>lgh<u>aa</u>-i<u>th</u>i aw l<u>aa</u>mastumu <b>al</b>nnis<u>aa</u>-a falam tajiduu m<u>aa</u>-an fatayammamuu <u>sh</u>a\'iidan <u>th</u>ayyiban fa<b>i</b>msa<u>h</u>uu biwujuuhikum wa-aydiikum minhu m<u>aa</u> yuriidu <b>al</b>l<u>aa</u>hu liyaj\'ala \'alaykum min <u>h</u>arajin wal<u>aa</u>kin yuriidu liyu<u>th</u>ahhirakum waliyutimma ni\'matahu \'alaykum la\'allakum tasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (676, '5:7', 5, 7, ' wa<b>u</b><u>dz</u>kuruu ni\'mata <b>al</b>l<u>aa</u>hi \'alaykum wamiits<u>aa</u>qahu <b>al</b>la<u>dz</u>ii w<u>aa</u>tsaqakum bihi i<u>dz</u> qultum sami\'n<u>aa</u> wa-a<u>th</u>a\'n<u>aa</u> wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha inna <b>al</b>l<u>aa</u>ha \'aliimun bi<u>dzaa</u>ti <b>al</b><u>shsh</u>uduur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (677, '5:8', 5, 8, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu kuunuu qaww<u>aa</u>miina lill<u>aa</u>hi syuhad<u>aa</u>-a bi<b>a</b>lqis<u>th</u>i wal<u>aa</u> yajrimannakum syana<u>aa</u>nu qawmin \'al<u>aa</u> <b>al</b>l<u>aa</u> ta\'diluu i\'diluu huwa aqrabu li<b>l</b>ttaqw<u>aa</u> wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha inna <b>al</b>l<u>aa</u>ha khabiirun bim<u>aa</u> ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (678, '5:9', 5, 9, ' wa\'ada <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti lahum maghfiratun wa-ajrun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (679, '5:10', 5, 10, ' wa<b>a</b>lla<u>dz</u>iina kafaruu waka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> ul<u>aa</u>-ika a<u>sh</u>-<u>haa</u>bu <b>a</b>lja<u>h</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (680, '5:11', 5, 11, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu u<u>dz</u>kuruu ni\'mata <b>al</b>l<u>aa</u>hi \'alaykum i<u>dz</u> hamma qawmun an yabsu<u>th</u>uu ilaykum aydiyahum fakaffa aydiyahum \'ankum wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha wa\'al<u>aa</u> <b>al</b>l<u>aa</u>hi falyatawakkali <b>a</b>lmu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (681, '5:12', 5, 12, ' walaqad akha<u>dz</u>a <b>al</b>l<u>aa</u>hu miits<u>aa</u>qa banii isr<u>aa</u>-iila waba\'atsn<u>aa</u> minhumu itsnay \'asyara naqiiban waq<u>aa</u>la <b>al</b>l<u>aa</u>hu innii ma\'akum la-in aqamtumu <b>al</b><u>shsh</u>al<u>aa</u>ta wa<u>aa</u>taytumu <b>al</b>zzak<u>aa</u>ta wa<u>aa</u>mantum birusulii wa\'azzartumuuhum wa-aqra<u>dh</u>tumu <b>al</b>l<u>aa</u>ha qar<u>dh</u>an <u>h</u>asanan laukaffiranna \'ankum sayyi-<u>aa</u>tikum walaudkhilannakum jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru faman kafara ba\'da <u>dzaa</u>lika minkum faqad <u>dh</u>alla saw<u>aa</u>-a <b>al</b>ssabiil<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (682, '5:13', 5, 13, ' fabim<u>aa</u> naq<u>dh</u>ihim miits<u>aa</u>qahum la\'ann<u>aa</u>hum waja\'aln<u>aa</u> quluubahum q<u>aa</u>siyatan yu<u>h</u>arrifuuna <b>a</b>lkalima \'an maw<u>aad</u>i\'ihi wanasuu <u>h</u>a<u>zhzh</u>an mimm<u>aa</u> <u>dz</u>ukkiruu bihi wal<u>aa</u> taz<u>aa</u>lu ta<u>ththh</u>ali\'u \'al<u>aa</u> kh<u>aa</u>-inatin minhum ill<u>aa</u> qaliilan minhum fa<b>u</b>\'fu \'anhum wa<b>i</b><u>sh</u>fa<u>h</u> inna <b>al</b>l<u>aa</u>ha yu<u>h</u>ibbu <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (683, '5:14', 5, 14, ' wamina <b>al</b>la<u>dz</u>iina q<u>aa</u>luu inn<u>aa</u> na<u>shaa</u>r<u>aa</u> akha<u>dz</u>n<u>aa</u> miits<u>aa</u>qahum fanasuu <u>h</u>a<u>zhzh</u>an mimm<u>aa</u> <u>dz</u>ukkiruu bihi fa-aghrayn<u>aa</u> baynahumu <b>a</b>l\'ad<u>aa</u>wata wa<b>a</b>lbagh<u>dhaa</u>-a il<u>aa</u> yawmi <b>a</b>lqiy<u>aa</u>mati wasawfa yunabbi-uhumu <b>al</b>l<u>aa</u>hu bim<u>aa</u> k<u>aa</u>nuu ya<u>sh</u>na\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (684, '5:15', 5, 15, ' y<u>aa</u> ahla <b>a</b>lkit<u>aa</u>bi qad j<u>aa</u>-akum rasuulun<u>aa</u> yubayyinu lakum katsiiran mimm<u>aa</u> kuntum tukhfuuna mina <b>a</b>lkit<u>aa</u>bi waya\'fuu \'an katsiirin qad j<u>aa</u>-akum mina <b>al</b>l<u>aa</u>hi nuurun wakit<u>aa</u>bun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (685, '5:16', 5, 16, ' yahdii bihi <b>al</b>l<u>aa</u>hu mani ittaba\'a ri<u>dh</u>w<u>aa</u>nahu subula <b>al</b>ssal<u>aa</u>mi wayukhrijuhum mina <b>al</b><u>zhzh</u>ulum<u>aa</u>ti il<u>aa</u> <b>al</b>nnuuri bi-i<u>dz</u>nihi wayahdiihim il<u>aa</u> <u>sh</u>ir<u>aath</u>in mustaqiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (686, '5:17', 5, 17, ' laqad kafara <b>al</b>la<u>dz</u>iina q<u>aa</u>luu inna <b>al</b>l<u>aa</u>ha huwa <b>a</b>lmasii<u>h</u>u ibnu maryama qul faman yamliku mina <b>al</b>l<u>aa</u>hi syay-an in ar<u>aa</u>da an yuhlika <b>a</b>lmasii<u>h</u>a ibna maryama waummahu waman fii <b>a</b>l-ar<u>dh</u>i jamii\'an walill<u>aa</u>hi mulku <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wam<u>aa</u> baynahum<u>aa</u> yakhluqu m<u>aa</u> yasy<u>aa</u>u wa<b>al</b>l<u>aa</u>hu \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (687, '5:18', 5, 18, ' waq<u>aa</u>lati <b>a</b>lyahuudu wa<b>al</b>nna<u>shaa</u>r<u>aa</u> na<u>h</u>nu abn<u>aa</u>u <b>al</b>l<u>aa</u>hi wa-a<u>h</u>ibb<u>aa</u>uhu qul falima yu\'a<u>dzdz</u>ibukum bi<u>dz</u>unuubikum bal antum basyarun mimman khalaqa yaghfiru liman yasy<u>aa</u>u wayu\'a<u>dzdz</u>ibu man yasy<u>aa</u>u walill<u>aa</u>hi mulku <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wam<u>aa</u> baynahum<u>aa</u> wa-ilayhi <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (688, '5:19', 5, 19, ' y<u>aa</u> ahla <b>a</b>lkit<u>aa</u>bi qad j<u>aa</u>-akum rasuulun<u>aa</u> yubayyinu lakum \'al<u>aa</u> fatratin mina <b>al</b>rrusuli an taquuluu m<u>aa</u> j<u>aa</u>-an<u>aa</u> min basyiirin wal<u>aa</u> na<u>dz</u>iirin faqad j<u>aa</u>-akum basyiirun wana<u>dz</u>iirun wa<b>al</b>l<u>aa</u>hu \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (689, '5:20', 5, 20, ' wa-i<u>dz</u> q<u>aa</u>la muus<u>aa</u> liqawmihi y<u>aa</u> qawmi u<u>dz</u>kuruu ni\'mata <b>al</b>l<u>aa</u>hi \'alaykum i<u>dz</u> ja\'ala fiikum anbiy<u>aa</u>-a waja\'alakum muluukan wa<u>aa</u>t<u>aa</u>kum m<u>aa</u> lam yu/ti a<u>h</u>adan mina <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (690, '5:21', 5, 21, ' y<u>aa</u> qawmi udkhuluu <b>a</b>l-ar<u>dh</u>a <b>a</b>lmuqaddasata <b>al</b>latii kataba <b>al</b>l<u>aa</u>hu lakum wal<u>aa</u> tartadduu \'al<u>aa</u> adb<u>aa</u>rikum fatanqalibuu kh<u>aa</u>siriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (691, '5:22', 5, 22, ' q<u>aa</u>luu y<u>aa</u> muus<u>aa</u> inna fiih<u>aa</u> qawman jabb<u>aa</u>riina wa-inn<u>aa</u> lan nadkhulah<u>aa</u> <u>h</u>att<u>aa</u> yakhrujuu minh<u>aa</u> fa-in yakhrujuu minh<u>aa</u> fa-inn<u>aa</u> d<u>aa</u>khiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (692, '5:23', 5, 23, ' q<u>aa</u>la rajul<u>aa</u>ni mina <b>al</b>la<u>dz</u>iina yakh<u>aa</u>fuuna an\'ama <b>al</b>l<u>aa</u>hu \'alayhim<u>aa</u> udkhuluu \'alayhimu <b>a</b>lb<u>aa</u>ba fa-i<u>dzaa</u> dakhaltumuuhu fa-innakum gh<u>aa</u>libuuna wa\'al<u>aa</u> <b>al</b>l<u>aa</u>hi fatawakkaluu in kuntum mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (693, '5:24', 5, 24, ' q<u>aa</u>luu y<u>aa</u> muus<u>aa</u> inn<u>aa</u> lan nadkhulah<u>aa</u> abadan m<u>aa</u> d<u>aa</u>muu fiih<u>aa</u> fa-i<u>dz</u>hab anta warabbuka faq<u>aa</u>til<u>aa</u> inn<u>aa</u> h<u>aa</u>hun<u>aa</u> q<u>aa</u>\'iduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (694, '5:25', 5, 25, ' q<u>aa</u>la rabbi innii l<u>aa</u> amliku ill<u>aa</u> nafsii wa-akhii fa<b>u</b>fruq baynan<u>aa</u> wabayna <b>a</b>lqawmi <b>a</b>lf<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (695, '5:26', 5, 26, ' q<u>aa</u>la fa-innah<u>aa</u> mu<u>h</u>arramatun \'alayhim arba\'iina sanatan yatiihuuna fii <b>a</b>l-ar<u>dh</u>i fal<u>aa</u> ta/sa \'al<u>aa</u> <b>a</b>lqawmi <b>a</b>lf<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (696, '5:27', 5, 27, ' wa<b>u</b>tlu \'alayhim naba-a ibnay <u>aa</u>dama bi<b>a</b>l<u>h</u>aqqi i<u>dz</u> qarrab<u>aa</u> qurb<u>aa</u>nan fatuqubbila min a<u>h</u>adihim<u>aa</u> walam yutaqabbal mina <b>a</b>l-<u>aa</u>khari q<u>aa</u>la la-aqtulannaka q<u>aa</u>la innam<u>aa</u> yataqabbalu <b>al</b>l<u>aa</u>hu mina <b>a</b>lmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (697, '5:28', 5, 28, ' la-in basa<u>th</u>ta ilayya yadaka litaqtulanii m<u>aa</u> an<u>aa</u> bib<u>aa</u>si<u>th</u>in yadiya ilayka li-aqtulaka innii akh<u>aa</u>fu <b>al</b>l<u>aa</u>ha rabba <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (698, '5:29', 5, 29, ' innii uriidu an tabuu-a bi-itsmii wa-itsmika fatakuuna min a<u>sh</u>-<u>haa</u>bi <b>al</b>nn<u>aa</u>ri wa<u>dzaa</u>lika jaz<u>aa</u>u <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (699, '5:30', 5, 30, ' fa<u>th</u>awwa\'at lahu nafsuhu qatla akhiihi faqatalahu fa-a<u>sh</u>ba<u>h</u>a mina <b>a</b>lkh<u>aa</u>siriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (700, '5:31', 5, 31, ' faba\'atsa <b>al</b>l<u>aa</u>hu ghur<u>aa</u>ban yab<u>h</u>atsu fii <b>a</b>l-ar<u>dh</u>i liyuriyahu kayfa yuw<u>aa</u>rii saw-ata akhiihi q<u>aa</u>la y<u>aa</u> waylat<u>aa</u> a\'ajaztu an akuuna mitsla h<u>aadzaa</u> <b>a</b>lghur<u>aa</u>bi fauw<u>aa</u>riya saw-ata akhii fa-a<u>sh</u>ba<u>h</u>a mina <b>al</b>nn<u>aa</u>dimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (701, '5:32', 5, 32, ' min ajli <u>dzaa</u>lika katabn<u>aa</u> \'al<u>aa</u> banii isr<u>aa</u>-iila annahu man qatala nafsan bighayri nafsin aw fas<u>aa</u>din fii <b>a</b>l-ar<u>dh</u>i faka-annam<u>aa</u> qatala <b>al</b>nn<u>aa</u>sa jamii\'an waman a<u>h</u>y<u>aa</u>h<u>aa</u> faka-annam<u>aa</u> a<u>h</u>y<u>aa</u> <b>al</b>nn<u>aa</u>sa jamii\'an walaqad j<u>aa</u>-at-hum rusulun<u>aa</u> bi<b>a</b>lbayyin<u>aa</u>ti tsumma inna katsiiran minhum ba\'da <u>dzaa</u>lika fii <b>a</b>l-ar<u>dh</u>i lamusrifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (702, '5:33', 5, 33, ' innam<u>aa</u> jaz<u>aa</u>u <b>al</b>la<u>dz</u>iina yu<u>haa</u>ribuuna <b>al</b>l<u>aa</u>ha warasuulahu wayas\'awna fii <b>a</b>l-ar<u>dh</u>i fas<u>aa</u>dan an yuqattaluu aw yu<u>sh</u>allabuu aw tuqa<u>ththh</u>a\'a aydiihim wa-arjuluhum min khil<u>aa</u>fin aw yunfaw mina <b>a</b>l-ar<u>dh</u>i <u>dzaa</u>lika lahum khizyun fii <b>al</b>dduny<u>aa</u> walahum fii <b>a</b>l-<u>aa</u>khirati \'a<u>dzaa</u>bun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (703, '5:34', 5, 34, ' ill<u>aa</u> <b>al</b>la<u>dz</u>iina t<u>aa</u>buu min qabli an taqdiruu \'alayhim fa<b>i</b>\'lamuu anna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (704, '5:35', 5, 35, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu ittaquu <b>al</b>l<u>aa</u>ha wa<b>i</b>btaghuu ilayhi <b>a</b>lwasiilata waj<u>aa</u>hiduu fii sabiilihi la\'allakum tufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (705, '5:36', 5, 36, ' inna <b>al</b>la<u>dz</u>iina kafaruu law anna lahum m<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i jamii\'an wamitslahu ma\'ahu liyaftaduu bihi min \'a<u>dzaa</u>bi yawmi <b>a</b>lqiy<u>aa</u>mati m<u>aa</u> tuqubbila minhum walahum \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (706, '5:37', 5, 37, ' yuriiduuna an yakhrujuu mina <b>al</b>nn<u>aa</u>ri wam<u>aa</u> hum bikh<u>aa</u>rijiina minh<u>aa</u> walahum \'a<u>dzaa</u>bun muqiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (707, '5:38', 5, 38, ' wa<b>al</b>ss<u>aa</u>riqu wa<b>al</b>ss<u>aa</u>riqatu fa<b>i</b>q<u>th</u>a\'uu aydiyahum<u>aa</u> jaz<u>aa</u>-an bim<u>aa</u> kasab<u>aa</u> nak<u>aa</u>lan mina <b>al</b>l<u>aa</u>hi wa<b>al</b>l<u>aa</u>hu \'aziizun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (708, '5:39', 5, 39, ' faman t<u>aa</u>ba min ba\'di <u>zh</u>ulmihi wa-a<u>sh</u>la<u>h</u>a fa-inna <b>al</b>l<u>aa</u>ha yatuubu \'alayhi inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (709, '5:40', 5, 40, ' alam ta\'lam anna <b>al</b>l<u>aa</u>ha lahu mulku <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i yu\'a<u>dzdz</u>ibu man yasy<u>aa</u>u wayaghfiru liman yasy<u>aa</u>u wa<b>al</b>l<u>aa</u>hu \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (710, '5:41', 5, 41, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>rrasuulu l<u>aa</u> ya<u>h</u>zunka <b>al</b>la<u>dz</u>iina yus<u>aa</u>ri\'uuna fii <b>a</b>lkufri mina <b>al</b>la<u>dz</u>iina q<u>aa</u>luu <u>aa</u>mann<u>aa</u> bi-afw<u>aa</u>hihim walam tu/min quluubuhum wamina <b>al</b>la<u>dz</u>iina h<u>aa</u>duu samm<u>aa</u>\'uuna lilka<u>dz</u>ibi samm<u>aa</u>\'uuna liqawmin <u>aa</u>khariina lam ya/tuuka yu<u>h</u>arrifuuna <b>a</b>lkalima min ba\'di maw<u>aad</u>i\'ihi yaquuluuna in uutiitum h<u>aadzaa</u> fakhu<u>dz</u>uuhu wa-in lam tu/tawhu fa<b>i</b><u>hts</u>aruu waman yuridi <b>al</b>l<u>aa</u>hu fitnatahu falan tamlika lahu mina <b>al</b>l<u>aa</u>hi syay-an ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina lam yuridi <b>al</b>l<u>aa</u>hu an yu<u>th</u>ahhira quluubahum lahum fii <b>al</b>dduny<u>aa</u> khizyun walahum fii <b>a</b>l-<u>aa</u>khirati \'a<u>dzaa</u>bun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (711, '5:42', 5, 42, ' samm<u>aa</u>\'uuna lilka<u>dz</u>ibi akk<u>aa</u>luuna li<b>l</b>ssu<u>h</u>ti fa-in j<u>aa</u>uuka fa<b>u</b><u>h</u>kum baynahum aw a\'ri<u>dh</u> \'anhum wa-in tu\'ri<u>dh</u> \'anhum falan ya<u>dh</u>urruuka syay-an wa-in <u>h</u>akamta fa<b>u</b><u>h</u>kum baynahum bi<b>a</b>lqis<u>th</u>i inna <b>al</b>l<u>aa</u>ha yu<u>h</u>ibbu <b>a</b>lmuqsi<u>th</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (712, '5:43', 5, 43, ' wakayfa yu<u>h</u>akkimuunaka wa\'indahumu <b>al</b>ttawr<u>aa</u>tu fiih<u>aa</u> <u>h</u>ukmu <b>al</b>l<u>aa</u>hi tsumma yatawallawna min ba\'di <u>dzaa</u>lika wam<u>aa</u> ul<u>aa</u>-ika bi<b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (713, '5:44', 5, 44, ' inn<u>aa</u> anzaln<u>aa</u> <b>al</b>ttawr<u>aa</u>ta fiih<u>aa</u> hudan wanuurun ya<u>h</u>kumu bih<u>aa</u> <b>al</b>nnabiyyuuna <b>al</b>la<u>dz</u>iina aslamuu lilla<u>dz</u>iina h<u>aa</u>duu wa<b>al</b>rrabb<u>aa</u>niyyuuna wa<b>a</b>l-a<u>h</u>b<u>aa</u>ru bim<u>aa</u> istu<u>h</u>fi<u>zh</u>uu min kit<u>aa</u>bi <b>al</b>l<u>aa</u>hi wak<u>aa</u>nuu \'alayhi syuhad<u>aa</u>-a fal<u>aa</u> takhsyawuu <b>al</b>nn<u>aa</u>sa wa<b>i</b>khsyawni wal<u>aa</u> tasytaruu bi-<u>aa</u>y<u>aa</u>tii tsamanan qaliilan waman lam ya<u>h</u>kum bim<u>aa</u> anzala <b>al</b>l<u>aa</u>hu faul<u>aa</u>-ika humu <b>a</b>lk<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (714, '5:45', 5, 45, ' wakatabn<u>aa</u> \'alayhim fiih<u>aa</u> anna <b>al</b>nnafsa bi<b>al</b>nnafsi wa<b>a</b>l\'ayna bi<b>a</b>l\'ayni wa<b>a</b>l-anfa bi<b>a</b>l-anfi wa<b>a</b>lu<u>dz</u>una bi<b>a</b>lu<u>dz</u>uni wa<b>al</b>ssinna bi<b>al</b>ssinni wa<b>a</b>ljuruu<u>h</u>a qi<u>sas</u>un faman ta<u>sh</u>addaqa bihi fahuwa kaff<u>aa</u>ratun lahu waman lam ya<u>h</u>kum bim<u>aa</u> anzala <b>al</b>l<u>aa</u>hu faul<u>aa</u>-ika humu <b>al</b><u>zhzhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (715, '5:46', 5, 46, ' waqaffayn<u>aa</u> \'al<u>aa</u> <u>aa</u>ts<u>aa</u>rihim bi\'iis<u>aa</u> ibni maryama mu<u>sh</u>addiqan lim<u>aa</u> bayna yadayhi mina <b>al</b>ttawr<u>aa</u>ti wa<u>aa</u>tayn<u>aa</u>hu <b>a</b>l-injiila fiihi hudan wanuurun wamu<u>sh</u>addiqan lim<u>aa</u> bayna yadayhi mina <b>al</b>ttawr<u>aa</u>ti wahudan wamaw\'i<u>zh</u>atan lilmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (716, '5:47', 5, 47, ' walya<u>h</u>kum ahlu <b>a</b>l-injiili bim<u>aa</u> anzala <b>al</b>l<u>aa</u>hu fiihi waman lam ya<u>h</u>kum bim<u>aa</u> anzala <b>al</b>l<u>aa</u>hu faul<u>aa</u>-ika humu <b>a</b>lf<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (717, '5:48', 5, 48, ' wa-anzaln<u>aa</u> ilayka <b>a</b>lkit<u>aa</u>ba bi<b>a</b>l<u>h</u>aqqi mu<u>sh</u>addiqan lim<u>aa</u> bayna yadayhi mina <b>a</b>lkit<u>aa</u>bi wamuhayminan \'alayhi fa<b>u</b><u>h</u>kum baynahum bim<u>aa</u> anzala <b>al</b>l<u>aa</u>hu wal<u>aa</u> tattabi\' ahw<u>aa</u>-ahum \'amm<u>aa</u> j<u>aa</u>-aka mina <b>a</b>l<u>h</u>aqqi likullin ja\'aln<u>aa</u> minkum syir\'atan waminh<u>aa</u>jan walaw sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu laja\'alakum ummatan w<u>aah</u>idatan wal<u>aa</u>kin liyabluwakum fiim<u>aa</u> <u>aa</u>t<u>aa</u>kum fa<b>i</b>stabiquu <b>a</b>lkhayr<u>aa</u>ti il<u>aa</u> <b>al</b>l<u>aa</u>hi marji\'ukum jamii\'an fayunabbi-ukum bim<u>aa</u> kuntum fiihi takhtalifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (718, '5:49', 5, 49, ' wa-ani u<u>h</u>kum baynahum bim<u>aa</u> anzala <b>al</b>l<u>aa</u>hu wal<u>aa</u> tattabi\' ahw<u>aa</u>-ahum wa<b>i</b><u>hts</u>arhum an yaftinuuka \'an ba\'<u>dh</u>i m<u>aa</u> anzala <b>al</b>l<u>aa</u>hu ilayka fa-in tawallaw fa<b>i</b>\'lam annam<u>aa</u> yuriidu <b>al</b>l<u>aa</u>hu an yu<u>sh</u>iibahum biba\'<u>dh</u>i <u>dz</u>unuubihim wa-inna katsiiran mina <b>al</b>nn<u>aa</u>si laf<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (719, '5:50', 5, 50, ' afa<u>h</u>ukma <b>a</b>lj<u>aa</u>hiliyyati yabghuuna waman a<u>h</u>sanu mina <b>al</b>l<u>aa</u>hu <u>h</u>ukman liqawmin yuuqinuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (720, '5:51', 5, 51, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tattakhi<u>dz</u>uu <b>a</b>lyahuuda wa<b>al</b>nna<u>shaa</u>r<u>aa</u> awliy<u>aa</u>-a ba\'<u>dh</u>uhum awliy<u>aa</u>u ba\'<u>dh</u>in waman yatawallahum minkum fa-innahu minhum inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yahdii <b>a</b>lqawma <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (721, '5:52', 5, 52, ' fatar<u>aa</u> <b>al</b>la<u>dz</u>iina fii quluubihim mara<u>dh</u>un yus<u>aa</u>ri\'uuna fiihim yaquuluuna nakhsy<u>aa</u> an tu<u>sh</u>iiban<u>aa</u> d<u>aa</u>-iratun fa\'as<u>aa</u> <b>al</b>l<u>aa</u>hu an ya/tiya bi<b>a</b>lfat<u>h</u>i aw amrin min \'indihi fayu<u>sh</u>bi<u>h</u>uu \'al<u>aa</u> m<u>aa</u> asarruu fii anfusihim n<u>aa</u>dimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (722, '5:53', 5, 53, ' wayaquulu <b>al</b>la<u>dz</u>iina <u>aa</u>manuu ah<u>aa</u>ul<u>aa</u>-i <b>al</b>la<u>dz</u>iina aqsamuu bi<b>al</b>l<u>aa</u>hi jahda aym<u>aa</u>nihim innahum lama\'akum <u>h</u>abi<u>th</u>at a\'m<u>aa</u>luhum fa-a<u>sh</u>ba<u>h</u>uu kh<u>aa</u>siriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (723, '5:54', 5, 54, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu man yartadda minkum \'an diinihi fasawfa ya/tii <b>al</b>l<u>aa</u>hu biqawmin yu<u>h</u>ibbuhum wayu<u>h</u>ibbuunahu a<u>dz</u>illatin \'al<u>aa</u> <b>a</b>lmu/miniina a\'izzatin \'al<u>aa</u> <b>a</b>lk<u>aa</u>firiina yuj<u>aa</u>hiduuna fii sabiili <b>al</b>l<u>aa</u>hi wal<u>aa</u> yakh<u>aa</u>fuuna lawmata l<u>aa</u>-imin <u>dzaa</u>lika fa<u>dh</u>lu <b>al</b>l<u>aa</u>hi yu/tiihi man yasy<u>aa</u>u wa<b>al</b>l<u>aa</u>hu w<u>aa</u>si\'un \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (724, '5:55', 5, 55, ' innam<u>aa</u> waliyyukumu <b>al</b>l<u>aa</u>hu warasuuluhu wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu <b>al</b>la<u>dz</u>iina yuqiimuuna <b>al</b><u>shsh</u>al<u>aa</u>ta wayu/tuuna <b>al</b>zzak<u>aa</u>ta wahum r<u>aa</u>ki\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (725, '5:56', 5, 56, ' waman yatawalla <b>al</b>l<u>aa</u>ha warasuulahu wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu fa-inna <u>h</u>izba <b>al</b>l<u>aa</u>hi humu <b>a</b>lgh<u>aa</u>libuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (726, '5:57', 5, 57, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tattakhi<u>dz</u>uu <b>al</b>la<u>dz</u>iina ittakha<u>dz</u>uu diinakum huzuwan wala\'iban mina <b>al</b>la<u>dz</u>iina uutuu <b>a</b>lkit<u>aa</u>ba min qablikum wa<b>a</b>lkuff<u>aa</u>ra awliy<u>aa</u>-a wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha in kuntum mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (727, '5:58', 5, 58, ' wa-i<u>dzaa</u> n<u>aa</u>daytum il<u>aa</u> <b>al</b><u>shsh</u>al<u>aa</u>ti ittakha<u>dz</u>uuh<u>aa</u> huzuwan wala\'iban <u>dzaa</u>lika bi-annahum qawmun l<u>aa</u> ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (728, '5:59', 5, 59, ' qul y<u>aa</u> ahla <b>a</b>lkit<u>aa</u>bi hal tanqimuuna minn<u>aa</u> ill<u>aa</u> an <u>aa</u>mann<u>aa</u> bi<b>al</b>l<u>aa</u>hi wam<u>aa</u> unzila ilayn<u>aa</u> wam<u>aa</u> unzila min qablu wa-anna aktsarakum f<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (729, '5:60', 5, 60, ' qul hal unabbi-ukum bisyarrin min <u>dzaa</u>lika matsuubatan \'inda <b>al</b>l<u>aa</u>hi man la\'anahu <b>al</b>l<u>aa</u>hu wagha<u>dh</u>iba \'alayhi waja\'ala minhumu <b>a</b>lqiradata wa<b>a</b>lkhan<u>aa</u>ziira wa\'abada <b>al</b><u>ththaa</u>ghuuti ul<u>aa</u>-ika syarrun mak<u>aa</u>nan wa-a<u>dh</u>allu \'an saw<u>aa</u>-i <b>al</b>ssabiil<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (730, '5:61', 5, 61, ' wa-i<u>dzaa</u> j<u>aa</u>uukum q<u>aa</u>luu <u>aa</u>mann<u>aa</u> waqad dakhaluu bi<b>a</b>lkufri wahum qad kharajuu bihi wa<b>al</b>l<u>aa</u>hu a\'lamu bim<u>aa</u> k<u>aa</u>nuu yaktumuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (731, '5:62', 5, 62, ' watar<u>aa</u> katsiiran minhum yus<u>aa</u>ri\'uuna fii <b>a</b>l-itsmi wa<b>a</b>l\'udw<u>aa</u>ni wa-aklihimu <b>al</b>ssu<u>h</u>ta labi/sa m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (732, '5:63', 5, 63, ' lawl<u>aa</u> yanh<u>aa</u>humu <b>al</b>rrabb<u>aa</u>niyyuuna wa<b>a</b>l-a<u>h</u>b<u>aa</u>ru \'an qawlihimu <b>a</b>l-itsma wa-aklihimu <b>al</b>ssu<u>h</u>ta labi/sa m<u>aa</u> k<u>aa</u>nuu ya<u>sh</u>na\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (733, '5:64', 5, 64, ' waq<u>aa</u>lati <b>a</b>lyahuudu yadu <b>al</b>l<u>aa</u>hi maghluulatun ghullat aydiihim walu\'inuu bim<u>aa</u> q<u>aa</u>luu bal yad<u>aa</u>hu mabsuu<u>th</u>at<u>aa</u>ni yunfiqu kayfa yasy<u>aa</u>u walayaziidanna katsiiran minhum m<u>aa</u> unzila ilayka min rabbika <u>th</u>ughy<u>aa</u>nan wakufran wa-alqayn<u>aa</u> baynahumu <b>a</b>l\'ad<u>aa</u>wata wa<b>a</b>lbagh<u>dhaa</u>-a il<u>aa</u> yawmi <b>a</b>lqiy<u>aa</u>mati kullam<u>aa</u> awqaduu n<u>aa</u>ran lil<u>h</u>arbi a<u>th</u>fa-ah<u>aa</u> <b>al</b>l<u>aa</u>hu wayas\'awna fii <b>a</b>l-ar<u>dh</u>i fas<u>aa</u>dan wa<b>al</b>l<u>aa</u>hu l<u>aa</u> yu<u>h</u>ibbu <b>a</b>lmufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (734, '5:65', 5, 65, ' walaw anna ahla <b>a</b>lkit<u>aa</u>bi <u>aa</u>manuu wa<b>i</b>ttaqaw lakaffarn<u>aa</u> \'anhum sayyi-<u>aa</u>tihim wala-adkhaln<u>aa</u>hum jann<u>aa</u>ti <b>al</b>nna\'iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (735, '5:66', 5, 66, ' walaw annahum aq<u>aa</u>muu <b>al</b>ttawr<u>aa</u>ta wa<b>a</b>l-injiila wam<u>aa</u> unzila ilayhim min rabbihim la-akaluu min fawqihim wamin ta<u>h</u>ti arjulihim minhum ummatun muqta<u>sh</u>idatun wakatsiirun minhum s<u>aa</u>-a m<u>aa</u> ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (736, '5:67', 5, 67, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>rrasuulu balligh m<u>aa</u> unzila ilayka min rabbika wa-in lam taf\'al fam<u>aa</u> ballaghta ris<u>aa</u>latahu wa<b>al</b>l<u>aa</u>hu ya\'<u>sh</u>imuka mina <b>al</b>nn<u>aa</u>si inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yahdii <b>a</b>lqawma <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (737, '5:68', 5, 68, ' qul y<u>aa</u> ahla <b>a</b>lkit<u>aa</u>bi lastum \'al<u>aa</u> syay-in <u>h</u>att<u>aa</u> tuqiimuu <b>al</b>ttawr<u>aa</u>ta wa<b>a</b>l-injiila wam<u>aa</u> unzila ilaykum min rabbikum walayaziidanna katsiiran minhum m<u>aa</u> unzila ilayka min rabbika <u>th</u>ughy<u>aa</u>nan wakufran fal<u>aa</u> ta/sa \'al<u>aa</u> <b>a</b>lqawmi <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (738, '5:69', 5, 69, ' inna <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa<b>a</b>lla<u>dz</u>iina h<u>aa</u>duu wa<b>al</b><u>shshaa</u>bi-uuna wa<b>al</b>nna<u>shaa</u>r<u>aa</u> man <u>aa</u>mana bi<b>al</b>l<u>aa</u>hi wa<b>a</b>lyawmi <b>a</b>l-<u>aa</u>khiri wa\'amila <u>shaa</u>li<u>h</u>an fal<u>aa</u> khawfun \'alayhim wal<u>aa</u> hum ya<u>h</u>zanuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (739, '5:70', 5, 70, ' laqad akha<u>dz</u>n<u>aa</u> miits<u>aa</u>qa banii isr<u>aa</u>-iila wa-arsaln<u>aa</u> ilayhim rusulan kullam<u>aa</u> j<u>aa</u>-ahum rasuulun bim<u>aa</u> l<u>aa</u> tahw<u>aa</u> anfusuhum fariiqan ka<u>dzdz</u>abuu wafariiqan yaqtuluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (740, '5:71', 5, 71, ' wa<u>h</u>asibuu <b>al</b>l<u>aa</u> takuuna fitnatun fa\'amuu wa<u>sh</u>ammuu tsumma t<u>aa</u>ba <b>al</b>l<u>aa</u>hu \'alayhim tsumma \'amuu wa<u>sh</u>ammuu katsiirun minhum wa<b>al</b>l<u>aa</u>hu ba<u>sh</u>iirun bim<u>aa</u> ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (741, '5:72', 5, 72, ' laqad kafara <b>al</b>la<u>dz</u>iina q<u>aa</u>luu inna <b>al</b>l<u>aa</u>ha huwa <b>a</b>lmasii<u>h</u>u ibnu maryama waq<u>aa</u>la <b>a</b>lmasii<u>h</u>u y<u>aa</u> banii isr<u>aa</u>-iila u\'buduu <b>al</b>l<u>aa</u>ha rabbii warabbakum innahu man yusyrik bi<b>al</b>l<u>aa</u>hi faqad <u>h</u>arrama <b>al</b>l<u>aa</u>hu \'alayhi <b>a</b>ljannata wama/w<u>aa</u>hu <b>al</b>nn<u>aa</u>ru wam<u>aa</u> li<b>l</b><u>zhzhaa</u>limiina min an<u>shaa</u>r<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (742, '5:73', 5, 73, ' laqad kafara <b>al</b>la<u>dz</u>iina q<u>aa</u>luu inna <b>al</b>l<u>aa</u>ha ts<u>aa</u>litsu tsal<u>aa</u>tsatin wam<u>aa</u> min il<u>aa</u>hin ill<u>aa</u> il<u>aa</u>hun w<u>aah</u>idun wa-in lam yantahuu \'amm<u>aa</u> yaquuluuna layamassanna <b>al</b>la<u>dz</u>iina kafaruu minhum \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (743, '5:74', 5, 74, ' afal<u>aa</u> yatuubuuna il<u>aa</u> <b>al</b>l<u>aa</u>hi wayastaghfiruunahu wa<b>al</b>l<u>aa</u>hu ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (744, '5:75', 5, 75, ' m<u>aa</u> <b>a</b>lmasii<u>h</u>u ibnu maryama ill<u>aa</u> rasuulun qad khalat min qablihi <b>al</b>rrusulu waummuhu <u>sh</u>iddiiqatun k<u>aa</u>n<u>aa</u> ya/kul<u>aa</u>ni <b>al</b><u>ththh</u>a\'<u>aa</u>ma un<u>zh</u>ur kayfa nubayyinu lahumu <b>a</b>l-<u>aa</u>y<u>aa</u>ti tsumma un<u>zh</u>ur ann<u>aa</u> yu/fakuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (745, '5:76', 5, 76, ' qul ata\'buduuna min duuni <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> yamliku lakum <u>dh</u>arran wal<u>aa</u> naf\'an wa<b>al</b>l<u>aa</u>hu huwa <b>al</b>ssamii\'u <b>a</b>l\'aliim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (746, '5:77', 5, 77, ' qul y<u>aa</u> ahla <b>a</b>lkit<u>aa</u>bi l<u>aa</u> taghluu fii diinikum ghayra <b>a</b>l<u>h</u>aqqi wal<u>aa </u>tattabi\'uu ahw<u>aa</u>-a qawmin qad <u>dh</u>alluu min qablu wa-a<u>dh</u>alluu katsiiran wa<u>dh</u>alluu \'an saw<u>aa</u>-i <b>al</b>ssabiil<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (747, '5:78', 5, 78, ' lu\'ina <b>al</b>la<u>dz</u>iina kafaruu min banii isr<u>aa</u>-iila \'al<u>aa</u> lis<u>aa</u>ni d<u>aa</u>wuuda wa\'iis<u>aa</u> ibni maryama <u>dzaa</u>lika bim<u>aa</u> \'a<u>sh</u>aw wak<u>aa</u>nuu ya\'taduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (748, '5:79', 5, 79, ' k<u>aa</u>nuu l<u>aa</u> yatan<u>aa</u>hawna \'an munkarin fa\'aluuhu labi/sa m<u>aa</u> k<u>aa</u>nuu yaf\'aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (749, '5:80', 5, 80, ' tar<u>aa</u> katsiiran minhum yatawallawna <b>al</b>la<u>dz</u>iina kafaruu labi/sa m<u>aa </u>qaddamat lahum anfusuhum an sakhi<u>th</u>a <b>al</b>l<u>aa</u>hu \'alayhim wafii <b>a</b>l\'a<u>dzaa</u>bi hum kh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (750, '5:81', 5, 81, ' walaw k<u>aa</u>nuu yu/minuuna bi<b>al</b>l<u>aa</u>hi wa<b>al</b>nnabiyyi wam<u>aa</u> unzila ilayhi m<u>aa</u> ittakha<u>dz</u>uuhum awliy<u>aa</u>-a wal<u>aa</u>kinna katsiiran minhum f<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (751, '5:82', 5, 82, ' latajidanna asyadda <b>al</b>nn<u>aa</u>si \'ad<u>aa</u>watan lilla<u>dz</u>iina <u>aa</u>manuu <b>a</b>lyahuuda wa<b>a</b>lla<u>dz</u>iina asyrakuu walatajidanna aqrabahum mawaddatan lilla<u>dz</u>iina <u>aa</u>manuu <b>al</b>la<u>dz</u>iina q<u>aa</u>luu inn<u>aa</u> na<u>shaa</u>r<u>aa</u> <u>dzaa</u>lika bi-anna minhum qissiisiina waruhb<u>aa</u>nan wa-annahum l<u>aa</u> yastakbiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (752, '5:83', 5, 83, ' wa-i<u>dzaa</u> sami\'uu m<u>aa</u> unzila il<u>aa</u> <b>al</b>rrasuuli tar<u>aa</u> a\'yunahum tafii<u>dh</u>u mina <b>al</b>ddam\'i mimm<u>aa</u> \'arafuu mina <b>a</b>l<u>h</u>aqqi yaquuluuna rabban<u>aa</u> <u>aa</u>mann<u>aa</u> fa<b>u</b>ktubn<u>aa</u> ma\'a <b>al</b>sysy<u>aa</u>hidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (753, '5:84', 5, 84, ' wam<u>aa</u> lan<u>aa</u> l<u>aa</u> nu/minu bi<b>al</b>l<u>aa</u>hi wam<u>aa</u> j<u>aa</u>-an<u>aa</u> mina <b>a</b>l<u>h</u>aqqi wana<u>th</u>ma\'u an yudkhilan<u>aa</u> rabbun<u>aa</u> ma\'a <b>a</b>lqawmi <b>al</b><u>shshaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (754, '5:85', 5, 85, ' fa-ats<u>aa</u>bahumu <b>al</b>l<u>aa</u>hu bim<u>aa</u> q<u>aa</u>luu jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru kh<u>aa</u>lidiina fiih<u>aa</u> wa<u>dzaa</u>lika jaz<u>aa</u>u <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (755, '5:86', 5, 86, ' wa<b>a</b>lla<u>dz</u>iina kafaruu waka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> ul<u>aa</u>-ika a<u>sh</u>-<u>haa</u>bu <b>a</b>lja<u>h</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (756, '5:87', 5, 87, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tu<u>h</u>arrimuu <u>th</u>ayyib<u>aa</u>ti m<u>aa</u> a<u>h</u>alla <b>al</b>l<u>aa</u>hu lakum wal<u>aa</u> ta\'taduu inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yu<u>h</u>ibbu <b>a</b>lmu\'tadiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (757, '5:88', 5, 88, ' wakuluu mimm<u>aa</u> razaqakumu <b>al</b>l<u>aa</u>hu <u>h</u>al<u>aa</u>lan <u>th</u>ayyiban wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha <b>al</b>la<u>dz</u>ii antum bihi mu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (758, '5:89', 5, 89, ' l<u>aa</u> yu-<u>aa</u>khi<u>dz</u>ukumu <b>al</b>l<u>aa</u>hu bi<b>a</b>llaghwi fii aym<u>aa</u>nikum wal<u>aa</u>kin yu-<u>aa</u>khi<u>dz</u>ukum bim<u>aa</u> \'aqqadtumu <b>a</b>l-aym<u>aa</u>na fakaff<u>aa</u>ratuhu i<u>th</u>\'<u>aa</u>mu \'asyarati mas<u>aa</u>kiina min awsa<u>th</u>i m<u>aa</u> tu<u>th</u>\'imuuna ahliikum aw kiswatuhum aw ta<u>h</u>riiru raqabatin faman lam yajid fa<u>sh</u>iy<u>aa</u>mu tsal<u>aa</u>tsati ayy<u>aa</u>min <u>dzaa</u>lika kaff<u>aa</u>ratu aym<u>aa</u>nikum i<u>dzaa</u> <u>h</u>alaftum wa<b>i</b><u>h</u>fa<u>zh</u>uu aym<u>aa</u>nakum ka<u>dzaa</u>lika yubayyinu <b>al</b>l<u>aa</u>hu lakum <u>aa</u>y<u>aa</u>tihi la\'allakum tasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (759, '5:90', 5, 90, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu innam<u>aa</u> <b>a</b>lkhamru wa<b>a</b>lmaysiru wa<b>a</b>l-an<u>shaa</u>bu wa<b>a</b>l-azl<u>aa</u>mu rijsun min \'amali <b>al</b>sysyay<u>th</u><u>aa</u>ni fa<b>i</b>jtanibuuhu la\'allakum tufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (760, '5:91', 5, 91, ' innam<u>aa</u> yuriidu <b>al</b>sysyay<u>th</u><u>aa</u>nu an yuuqi\'a baynakumu <b>a</b>l\'ad<u>aa</u>wata wa<b>a</b>lbagh<u>dhaa</u>-a fii <b>a</b>lkhamri wa<b>a</b>lmaysiri waya<u>sh</u>uddakum \'an <u>dz</u>ikri <b>al</b>l<u>aa</u>hi wa\'ani <b>al</b><u>shsh</u>al<u>aa</u>ti fahal antum muntahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (761, '5:92', 5, 92, ' wa-a<u>th</u>ii\'uu <b>al</b>l<u>aa</u>ha wa-a<u>th</u>ii\'uu <b>al</b>rrasuula wa<b>i</b><u>hts</u>aruu fa-in tawallaytum fa<b>i</b>\'lamuu annam<u>aa</u> \'al<u>aa</u> rasuulin<u>aa</u> <b>a</b>lbal<u>aa</u>ghu <b>a</b>lmubiin<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (762, '5:93', 5, 93, ' laysa \'al<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti jun<u>aah</u>un fiim<u>aa</u> <u>th</u>a\'imuu i<u>dzaa</u> m<u>aa</u> ittaqaw wa<u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti tsumma ittaqaw wa<u>aa</u>manuu tsumma ittaqaw wa-a<u>h</u>sanuu wa<b>al</b>l<u>aa</u>hu yu<u>h</u>ibbu <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (763, '5:94', 5, 94, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu layabluwannakumu <b>al</b>l<u>aa</u>hu bisyay-in mina <b>al</b><u>shsh</u>aydi tan<u>aa</u>luhu aydiikum warim<u>aah</u>ukum liya\'lama <b>al</b>l<u>aa</u>hu man yakh<u>aa</u>fuhu bi<b>a</b>lghaybi famani i\'tad<u>aa</u> ba\'da <u>dzaa</u>lika falahu \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (764, '5:95', 5, 95, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> taqtuluu <b>al</b><u>shsh</u>ayda wa-antum <u>h</u>urumun waman qatalahu minkum muta\'ammidan fajaz<u>aa</u>un mitslu m<u>aa</u> qatala mina <b>al</b>nna\'ami ya<u>h</u>kumu bihi <u>dz</u>aw<u>aa</u> \'adlin minkum hadyan b<u>aa</u>ligha <b>a</b>lka\'bati aw kaff<u>aa</u>ratun <u>th</u>a\'<u>aa</u>mu mas<u>aa</u>kiina aw \'adlu <u>dzaa</u>lika <u>sh</u>iy<u>aa</u>man liya<u>dz</u>uuqa wab<u>aa</u>la amrihi \'af<u>aa</u> <b>al</b>l<u>aa</u>hu \'amm<u>aa</u> salafa waman \'<u>aa</u>da fayantaqimu <b>al</b>l<u>aa</u>hu minhu wa<b>al</b>l<u>aa</u>hu \'aziizun <u>dz</u>uu intiq<u>aa</u>m<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (765, '5:96', 5, 96, ' u<u>h</u>illa lakum <u>sh</u>aydu <b>a</b>lba<u>h</u>ri wa<u>th</u>a\'<u>aa</u>muhu mat<u>aa</u>\'an lakum wali<b>l</b>ssayy<u>aa</u>rati wa<u>h</u>urrima \'alaykum <u>sh</u>aydu <b>a</b>lbarri m<u>aa</u> dumtum <u>h</u>uruman wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha <b>al</b>la<u>dz</u>ii ilayhi tu<u>h</u>syaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (766, '5:97', 5, 97, ' ja\'ala <b>al</b>l<u>aa</u>hu <b>a</b>lka\'bata <b>a</b>lbayta <b>a</b>l<u>h</u>ar<u>aa</u>ma qiy<u>aa</u>man li<b>l</b>nn<u>aa</u>si wa<b>al</b>sysyahra <b>a</b>l<u>h</u>ar<u>aa</u>ma wa<b>a</b>lhadya wa<b>a</b>lqal<u>aa</u>-ida <u>dzaa</u>lika lita\'lamuu anna <b>al</b>l<u>aa</u>ha ya\'lamu m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i wa-anna <b>al</b>l<u>aa</u>ha bikulli syay-in \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (767, '5:98', 5, 98, ' i\'lamuu anna <b>al</b>l<u>aa</u>ha syadiidu <b>a</b>l\'iq<u>aa</u>bi wa-anna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (768, '5:99', 5, 99, ' m<u>aa</u> \'al<u>aa</u> <b>al</b>rrasuuli ill<u>aa</u> <b>a</b>lbal<u>aa</u>ghu wa<b>al</b>l<u>aa</u>hu ya\'lamu m<u>aa</u> tubduuna wam<u>aa</u> taktumuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (769, '5:100', 5, 100, ' qul l<u>aa</u> yastawii <b>a</b>lkhabiitsu wa<b>al</b><u>ththh</u>ayyibu walaw a\'jabaka katsratu <b>a</b>lkhabiitsi fa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha y<u>aa</u> ulii <b>a</b>l-alb<u>aa</u>bi la\'allakum tufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (770, '5:101', 5, 101, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tas-aluu \'an asyy<u>aa</u>-a in tubda lakum tasu/kum wa-in tas-aluu \'anh<u>aa</u> <u>h</u>iina yunazzalu <b>a</b>lqur-<u>aa</u>nu tubda lakum \'af<u>aa</u> <b>al</b>l<u>aa</u>hu \'anh<u>aa</u> wa<b>al</b>l<u>aa</u>hu ghafuurun <u>h</u>aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (771, '5:102', 5, 102, ' qad sa-alah<u>aa</u> qawmun min qablikum tsumma a<u>sh</u>ba<u>h</u>uu bih<u>aa</u> k<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (772, '5:103', 5, 103, ' m<u>aa</u> ja\'ala <b>al</b>l<u>aa</u>hu min ba<u>h</u>iiratin wal<u>aa</u> s<u>aa</u>-ibatin wal<u>aa</u> wa<u>sh</u>iilatin wal<u>aa</u> <u>haa</u>min wal<u>aa</u>kinna <b>al</b>la<u>dz</u>iina kafaruu yaftaruuna \'al<u>aa</u> <b>al</b>l<u>aa</u>hi <b>a</b>lka<u>dz</u>iba wa-aktsaruhum l<u>aa</u> ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (773, '5:104', 5, 104, ' wa-i<u>dzaa</u> qiila lahum ta\'<u>aa</u>law il<u>aa</u> m<u>aa</u> anzala <b>al</b>l<u>aa</u>hu wa-il<u>aa</u> <b>al</b>rrasuuli q<u>aa</u>luu <u>h</u>asbun<u>aa</u> m<u>aa</u> wajadn<u>aa</u> \'alayhi <u>aa</u>b<u>aa</u>-an<u>aa</u> awa law k<u>aa</u>na <u>aa</u>b<u>aa</u>uhum l<u>aa</u> ya\'lamuuna syay-an wal<u>aa</u> yahtaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (774, '5:105', 5, 105, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu \'alaykum anfusakum l<u>aa</u> ya<u>dh</u>urrukum man <u>dh</u>alla i<u>dz</u>a ihtadaytum il<u>aa</u> <b>al</b>l<u>aa</u>hi marji\'ukum jamii\'an fayunabbi-ukum bim<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (775, '5:106', 5, 106, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu syah<u>aa</u>datu baynikum i<u>dzaa</u> <u>h</u>a<u>dh</u>ara a<u>h</u>adakumu <b>a</b>lmawtu <u>h</u>iina <b>a</b>lwa<u>sh</u>iyyati itsn<u>aa</u>ni <u>dz</u>aw<u>aa</u> \'adlin minkum aw <u>aa</u>khar<u>aa</u>ni min ghayrikum in antum <u>dh</u>arabtum fii <b>a</b>l-ar<u>dh</u>i fa-a<u>shaa</u>batkum mu<u>sh</u>iibatu <b>a</b>lmawti ta<u>h</u>bisuunahum<u>aa</u> min ba\'di <b>al</b><u>shsh</u>al<u>aa</u>ti fayuqsim<u>aa</u>ni bi<b>al</b>l<u>aa</u>hi ini irtabtum l<u>aa</u> nasytarii bihi tsamanan walaw k<u>aa</u>na <u>dzaa</u> qurb<u>aa</u> wal<u>aa</u> naktumu syah<u>aa</u>data <b>al</b>l<u>aa</u>hi inn<u>aa</u> i<u>dz</u>an lamina <b>a</b>l-<u>aa</u>tsimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (776, '5:107', 5, 107, ' fa-in \'utsira \'al<u>aa</u> annahum<u>aa</u> ista<u>h</u>aqq<u>aa</u> itsman fa<u>aa</u>khara<u>n</u>i yaquum<u>aa</u>ni maq<u>aa</u>mahum<u>aa</u> mina <b>al</b>la<u>dz</u>iina ista<u>h</u>aqqa \'alayhimu <b>a</b>l-awlay<u>aa</u>ni fayuqsim<u>aa</u>ni bi<b>al</b>l<u>aa</u>hi lasyah<u>aa</u>datun<u>aa</u> a<u>h</u>aqqu min syah<u>aa</u>datihim<u>aa</u> wam<u>aa</u> i\'tadayn<u>aa</u> inn<u>aa</u> i<u>dz</u>an lamina <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (777, '5:108', 5, 108, ' <u>dzaa</u>lika adn<u>aa</u> an ya/tuu bi<b>al</b>sysyah<u>aa</u>dati \'al<u>aa</u> wajhih<u>aa</u> aw yakh<u>aa</u>fuu an turadda aym<u>aa</u>nun ba\'da aym<u>aa</u>nihim wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha wa<b>i</b>sma\'uu wa<b>al</b>l<u>aa</u>hu l<u>aa</u> yahdii <b>a</b>lqawma <b>a</b>lf<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (778, '5:109', 5, 109, ' yawma yajma\'u <b>al</b>l<u>aa</u>hu <b>al</b>rrusula fayaquulu m<u>aatsaa</u> ujibtum q<u>aa</u>luu l<u>aa</u> \'ilma lan<u>aa</u> innaka anta \'all<u>aa</u>mu <b>a</b>lghuyuub<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (779, '5:110', 5, 110, ' i<u>dz</u> q<u>aa</u>la <b>al</b>l<u>aa</u>hu y<u>aa</u> \'iis<u>aa</u> ibna maryama u<u>dz</u>kur ni\'matii \'alayka wa\'al<u>aa</u> w<u>aa</u>lidatika i<u>dz</u> ayyadtuka biruu<u>h</u>i <b>a</b>lqudusi tukallimu <b>al</b>nn<u>aa</u>sa fii <b>a</b>lmahdi wakahlan wa-i<u>dz</u> \'allamtuka <b>a</b>lkit<u>aa</u>ba wa<b>a</b>l<u>h</u>ikmata wa<b>al</b>ttawr<u>aa</u>ta wa<b>a</b>l-injiila wa-i<u>dz</u> takhluqu mina <b>al</b><u>ththh</u>iini kahay-ati <b>al</b><u>ththh</u>ayri bi-i<u>dz</u>nii fatanfukhu fiih<u>aa</u> fatakuunu <u>th</u>ayran bi-i<u>dz</u>nii watubri-u <b>a</b>l-akmaha wa<b>a</b>l-abra<u>sh</u>a bi-i<u>dz</u>nii wa-i<u>dz</u> tukhriju <b>a</b>lmawt<u>aa</u> bi-i<u>dz</u>nii wa-i<u>dz</u> kafaftu banii isr<u>aa</u>-iila \'anka i<u>dz</u> ji/tahum bi<b>a</b>lbayyin<u>aa</u>ti faq<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu minhum in h<u>aadzaa</u> ill<u>aa</u> si<u>h</u>run mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (780, '5:111', 5, 111, ' wa-i<u>dz</u> aw<u>h</u>aytu il<u>aa</u> <b>a</b>l<u>h</u>aw<u>aa</u>riyyiina an <u>aa</u>minuu bii wabirasuulii q<u>aa</u>luu <u>aa</u>mann<u>aa</u> wa<b>i</b>syhad bi-annan<u>aa</u> muslimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (781, '5:112', 5, 112, ' i<u>dz</u> q<u>aa</u>la <b>a</b>l<u>h</u>aw<u>aa</u>riyyuuna y<u>aa</u> \'iis<u>aa</u> ibna maryama hal yasta<u>th</u>ii\'u rabbuka an yunazzila \'alayn<u>aa</u> m<u>aa</u>-idatan mina <b>al</b>ssam<u>aa</u>-i q<u>aa</u>la ittaquu <b>al</b>l<u>aa</u>ha in kuntum mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (782, '5:113', 5, 113, ' q<u>aa</u>luu nuriidu an na/kula minh<u>aa</u> wata<u>th</u>ma-inna quluubun<u>aa</u> wana\'lama an qad <u>sh</u>adaqtan<u>aa</u> wanakuuna \'alayh<u>aa</u> mina <b>al</b>sysy<u>aa</u>hidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (783, '5:114', 5, 114, ' q<u>aa</u>la \'iis<u>aa</u> ibnu maryama <b>al</b>l<u>aa</u>humma rabban<u>aa</u> anzil \'alayn<u>aa</u> m<u>aa</u>-idatan mina <b>al</b>ssam<u>aa</u>-i takuunu lan<u>aa</u> \'iidan li-awwalin<u>aa</u> wa<u>aa</u>khirin<u>aa</u> wa<u>aa</u>yatan minka wa<b>u</b>rzuqn<u>aa</u> wa-anta khayru <b>al</b>rr<u>aa</u>ziqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (784, '5:115', 5, 115, ' q<u>aa</u>la <b>al</b>l<u>aa</u>hu innii munazziluh<u>aa</u> \'alaykum faman yakfur ba\'du minkum fa-innii u\'a<u>dzdz</u>ibuhu \'a<u>dzaa</u>ban l<u>aa</u> u\'a<u>dzdz</u>ibuhu a<u>h</u>adan mina <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (785, '5:116', 5, 116, ' wa-i<u>dz</u> q<u>aa</u>la <b>al</b>l<u>aa</u>hu y<u>aa</u> \'iis<u>aa</u> ibna maryama a-anta qulta li<b>l</b>nn<u>aa</u>si ittakhi<u>dz</u>uunii waummiya il<u>aa</u>hayni min duuni <b>al</b>l<u>aa</u>hi q<u>aa</u>la sub<u>haa</u>naka m<u>aa</u> yakuunu lii an aquula m<u>aa</u> laysa lii bi<u>h</u>aqqin in kuntu qultuhu faqad \'alimtahu ta\'lamu m<u>aa</u> fii nafsii wal<u>aa</u> a\'lamu m<u>aa</u> fii nafsika innaka anta \'all<u>aa</u>mu <b>a</b>lghuyuub<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (786, '5:117', 5, 117, ' m<u>aa</u> qultu lahum ill<u>aa</u> m<u>aa</u> amartanii bihi ani u\'buduu <b>al</b>l<u>aa</u>ha rabbii warabbakum wakuntu \'alayhim syahiidan m<u>aa</u> dumtu fiihim falamm<u>aa</u> tawaffaytanii kunta anta <b>al</b>rraqiiba \'alayhim wa-anta \'al<u>aa</u> kulli syay-in syahiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (787, '5:118', 5, 118, ' in tu\'a<u>dzdz</u>ibhum fa-innahum \'ib<u>aa</u>duka wa-in taghfir lahum fa-innaka anta <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (788, '5:119', 5, 119, ' q<u>aa</u>la <b>al</b>l<u>aa</u>hu h<u>aadzaa</u> yawmu yanfa\'u <b>al</b><u>shshaa</u>diqiina <u>sh</u>idquhum lahum jann<u>aa</u>tun tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru kh<u>aa</u>lidiina fiih<u>aa</u> abadan ra<u>dh</u>iya <b>al</b>l<u>aa</u>hu \'anhum wara<u>dh</u>uu \'anhu <u>dzaa</u>lika <b>a</b>lfawzu <b>a</b>l\'a<u>zh</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (789, '5:120', 5, 120, ' lill<u>aa</u>hi mulku <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wam<u>aa</u> fiihinna wahuwa \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (790, '6:1', 6, 1, ' al<u>h</u>amdu lill<u>aa</u>hi <b>al</b>la<u>dz</u>ii khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a waja\'ala <b>al</b><u>zhzh</u>ulum<u>aa</u>ti wa<b>al</b>nnuura tsumma <b>al</b>la<u>dz</u>iina kafaruu birabbihim ya\'diluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (791, '6:2', 6, 2, ' huwa <b>al</b>la<u>dz</u>ii khalaqakum min <u>th</u>iinin tsumma qa<u>daa</u> ajalan wa-ajalun musamman \'indahu tsumma antum tamtaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (792, '6:3', 6, 3, ' wahuwa <b>al</b>l<u>aa</u>hu fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wafii <b>a</b>l-ar<u>dh</u>i ya\'lamu sirrakum wajahrakum waya\'lamu m<u>aa</u> taksibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (793, '6:4', 6, 4, ' wam<u>aa</u> ta/tiihim min <u>aa</u>yatin min <u>aa</u>y<u>aa</u>ti rabbihim ill<u>aa</u> k<u>aa</u>nuu \'anh<u>aa</u> mu\'ri<u>dh</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (794, '6:5', 6, 5, ' faqad ka<u>dzdz</u>abuu bi<b>a</b>l<u>h</u>aqqi lamm<u>aa</u> j<u>aa</u>-ahum fasawfa ya/tiihim anb<u>aa</u>u m<u>aa</u> k<u>aa</u>nuu bihi yastahzi-uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (795, '6:6', 6, 6, ' alam yaraw kam ahlakn<u>aa</u> min qablihim min qarnin makkann<u>aa</u>hum fii <b>a</b>l-ar<u>dh</u>i m<u>aa</u> lam numakkin lakum wa-arsaln<u>aa</u> <b>al</b>ssam<u>aa</u>-a \'alayhim midr<u>aa</u>ran waja\'aln<u>aa</u> <b>a</b>l-anh<u>aa</u>ra tajrii min ta<u>h</u>tihim fa-ahlakn<u>aa</u>hum bi<u>dz</u>unuubihim wa-ansya/n<u>aa</u> min ba\'dihim qarnan <u>aa</u>khariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (796, '6:7', 6, 7, ' walaw nazzaln<u>aa</u> \'alayka kit<u>aa</u>ban fii qir<u>th</u><u>aa</u>sin falamasuuhu bi-aydiihim laq<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu in h<u>aadzaa</u> ill<u>aa</u> si<u>h</u>run mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (797, '6:8', 6, 8, ' waq<u>aa</u>luu lawl<u>aa</u> unzila \'alayhi malakun walaw anzaln<u>aa</u> malakan laqu<u>dh</u>iya <b>a</b>l-amru tsumma l<u>aa</u> yun<u>zh</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (798, '6:9', 6, 9, ' walaw ja\'aln<u>aa</u>hu malakan laja\'aln<u>aa</u>hu rajulan walalabasn<u>aa</u> \'alayhim m<u>aa</u> yalbisuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (799, '6:10', 6, 10, ' walaqadi istuhzi-a birusulin min qablika fa<u>haa</u>qa bi<b>a</b>lla<u>dz</u>iina sakhiruu minhum m<u>aa</u> k<u>aa</u>nuu bihi yastahzi-uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (800, '6:11', 6, 11, ' qul siiruu fii <b>a</b>l-ar<u>dh</u>i tsumma un<u>zh</u>uruu kayfa k<u>aa</u>na \'<u>aa</u>qibatu <b>a</b>lmuka<u>dzdz</u>ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (801, '6:12', 6, 12, ' qul liman m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i qul lill<u>aa</u>hi kataba \'al<u>aa</u> nafsihi <b>al</b>rra<u>h</u>mata layajma\'annakum il<u>aa</u> yawmi <b>a</b>lqiy<u>aa</u>mati l<u>aa</u> rayba fiihi <b>al</b>la<u>dz</u>iina khasiruu anfusahum fahum l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (802, '6:13', 6, 13, ' walahu m<u>aa</u> sakana fii <b>al</b>layli wa<b>al</b>nnah<u>aa</u>ri wahuwa <b>al</b>ssamii\'u <b>a</b>l\'aliim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (803, '6:14', 6, 14, ' qul aghayra <b>al</b>l<u>aa</u>hi attakhi<u>dz</u>u waliyyan f<u>aath</u>iri <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wahuwa yu<u>th</u>\'imu wal<u>aa</u> yu<u>th</u>\'amu qul innii umirtu an akuuna awwala man aslama wal<u>aa</u> takuunanna mina <b>a</b>lmusyrikiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (804, '6:15', 6, 15, ' qul innii akh<u>aa</u>fu in \'a<u>sh</u>aytu rabbii \'a<u>dzaa</u>ba yawmin \'a<u>zh</u>iim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (805, '6:16', 6, 16, ' man yu<u>sh</u>raf \'anhu yawma-i<u>dz</u>in faqad ra<u>h</u>imahu wa<u>dzaa</u>lika <b>a</b>lfawzu <b>a</b>lmubiin<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (806, '6:17', 6, 17, ' wa-in yamsaska <b>al</b>l<u>aa</u>hu bi<u>dh</u>urrin fal<u>aa</u> k<u>aa</u>syifa lahu ill<u>aa</u> huwa wa-in yamsaska bikhayrin fahuwa \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (807, '6:18', 6, 18, ' wahuwa <b>a</b>lq<u>aa</u>hiru fawqa \'ib<u>aa</u>dihi wahuwa <b>a</b>l<u>h</u>akiimu <b>a</b>lkhabiir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (808, '6:19', 6, 19, ' qul ayyu syay-in akbaru syah<u>aa</u>datan quli <b>al</b>l<u>aa</u>hu syahiidun baynii wabaynakum wauu<u>h</u>iya ilayya h<u>aadzaa</u> <b>a</b>lqur-<u>aa</u>nu li-un<u>dz</u>irakum bihi waman balagha a-innakum latasyhaduuna anna ma\'a <b>al</b>l<u>aa</u>hi <u>aa</u>lihatan ukhr<u>aa</u> qul l<u>aa</u> asyhadu qul innam<u>aa</u> huwa il<u>aa</u>hun w<u>aah</u>idun wa-innanii barii-un mimm<u>aa</u> tusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (809, '6:20', 6, 20, ' <b>al</b>la<u>dz</u>iina <u>aa</u>tayn<u>aa</u>humu <b>a</b>lkit<u>aa</u>ba ya\'rifuunahu kam<u>aa</u> ya\'rifuuna abn<u>aa</u>-ahum <b>al</b>la<u>dz</u>iina khasiruu anfusahum fahum l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (810, '6:21', 6, 21, ' waman a<u>zh</u>lamu mimmani iftar<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ka<u>dz</u>iban aw ka<u>dzdz</u>aba bi-<u>aa</u>y<u>aa</u>tihi innahu l<u>aa</u> yufli<u>h</u>u <b>al</b><u>zhzhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (811, '6:22', 6, 22, ' wayawma na<u>h</u>syuruhum jamii\'an tsumma naquulu lilla<u>dz</u>iina asyrakuu ayna syurak<u>aa</u>ukumu <b>al</b>la<u>dz</u>iina kuntum taz\'umuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (812, '6:23', 6, 23, ' tsumma lam takun fitnatuhum ill<u>aa</u> an q<u>aa</u>luu wa<b>al</b>l<u>aa</u>hi rabbin<u>aa</u> m<u>aa</u> kunn<u>aa</u> musyrikiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (813, '6:24', 6, 24, ' un<u>zh</u>ur kayfa ka<u>dz</u>abuu \'al<u>aa</u> anfusihim wa<u>dh</u>alla \'anhum m<u>aa</u> k<u>aa</u>nuu yaftaruun<b>a </b>\r\r\n');
INSERT INTO quranLatin VALUES (814, '6:25', 6, 25, ' waminhum man yastami\'u ilayka waja\'aln<u>aa</u> \'al<u>aa</u> quluubihim akinnatan an yafqahuuhu wafii <u>aatsaa</u>nihim waqran wa-in yaraw kulla <u>aa</u>yatin l<u>aa</u> yu/minuu bih<u>aa</u> <u>h</u>att<u>aa</u> i<u>dzaa</u> j<u>aa</u>uuka yuj<u>aa</u>diluunaka yaquulu <b>al</b>la<u>dz</u>iina kafaruu in h<u>aadzaa</u> ill<u>aa</u> as<u>aath</u>iiru <b>a</b>l-awwaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (815, '6:26', 6, 26, ' wahum yanhawna \'anhu wayan-awna \'anhu wa-in yuhlikuuna ill<u>aa</u> anfusahum wam<u>aa</u> yasy\'uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (816, '6:27', 6, 27, ' walaw tar<u>aa</u> i<u>dz</u> wuqifuu \'al<u>aa</u> <b>al</b>nn<u>aa</u>ri faq<u>aa</u>luu y<u>aa</u> laytan<u>aa</u> nuraddu wal<u>aa</u> nuka<u>dzdz</u>iba bi-<u>aa</u>y<u>aa</u>ti rabbin<u>aa</u> wanakuuna mina <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (817, '6:28', 6, 28, ' bal bad<u>aa</u> lahum m<u>aa</u> k<u>aa</u>nuu yukhfuuna min qablu walaw rudduu la\'<u>aa</u>duu lim<u>aa</u> nuhuu \'anhu wa-innahum lak<u>aadz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (818, '6:29', 6, 29, ' waq<u>aa</u>luu in hiya ill<u>aa</u> <u>h</u>ay<u>aa</u>tun<u>aa</u> <b>al</b>dduny<u>aa</u> wam<u>aa</u> na<u>h</u>nu bimab\'uutsiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (819, '6:30', 6, 30, ' walaw tar<u>aa</u> i<u>dz</u> wuqifuu \'al<u>aa</u> rabbihim q<u>aa</u>la alaysa h<u>aadzaa</u> bi<b>a</b>l<u>h</u>aqqi q<u>aa</u>luu bal<u>aa</u> warabbin<u>aa</u> q<u>aa</u>la fa<u>dz</u>uuquu al\'a<u>dzaa</u>ba bim<u>aa</u> kuntum takfuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (820, '6:31', 6, 31, ' qad khasira <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu biliq<u>aa</u>-i <b>al</b>l<u>aa</u>hi <u>h</u>att<u>aa</u> i<u>dzaa</u> j<u>aa</u>-at-humu <b>al</b>ss<u>aa</u>\'atu baghtatan q<u>aa</u>luu y<u>aa</u> <u>h</u>asratan<u>aa</u> \'al<u>aa</u> m<u>aa</u> farra<u>th</u>n<u>aa</u> fiih<u>aa</u> wahum ya<u>h</u>miluuna awz<u>aa</u>rahum \'al<u>aa</u> <u>zh</u>uhuurihim <b>a</b>l<u>aa</u> s<u>aa</u>-a m<u>aa</u> yaziruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (821, '6:32', 6, 32, ' wam<u>aa</u> <b>a</b>l<u>h</u>ay<u>aa</u>tu <b>al</b>dduny<u>aa</u> ill<u>aa</u> la\'ibun walahwun wala<b>l</b>dd<u>aa</u>ru <b>a</b>l-<u>aa</u>khirati khayrun lilla<u>dz</u>iina yattaquuna afal<u>aa</u> ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (822, '6:33', 6, 33, ' qad na\'lamu innahu laya<u>h</u>zunuka <b>al</b>la<u>dz</u>ii yaquuluuna fa-innahum l<u>aa</u> yuka<u>dzdz</u>ibuunaka wal<u>aa</u>kinna <b>al</b><u>zhzhaa</u>limiina bi-<u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi yaj<u>h</u>aduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (823, '6:34', 6, 34, ' walaqad ku<u>dzdz</u>ibat rusulun min qablika fa<u>sh</u>abaruu \'al<u>aa</u> m<u>aa</u> ku<u>dzdz</u>ibuu wauu<u>dz</u>uu <u>h</u>att<u>aa</u> at<u>aa</u>hum na<u>sh</u>run<u>aa</u> wal<u>aa</u> mubaddila likalim<u>aa</u>ti <b>al</b>l<u>aa</u>hi walaqad j<u>aa</u>-aka min naba-i <b>a</b>lmursaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (824, '6:35', 6, 35, ' wa-in k<u>aa</u>na kabura \'alayka i\'r<u>aad</u>uhum fa-ini ista<u>th</u>a\'ta an tabtaghiya nafaqan fii <b>a</b>l-ar<u>dh</u>i aw sullaman fii <b>al</b>ssam<u>aa</u>-i fata/tiyahum bi-<u>aa</u>yatin walaw sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu lajama\'ahum \'al<u>aa</u> <b>a</b>lhud<u>aa</u> fal<u>aa</u> takuunanna mina <b>a</b>lj<u>aa</u>hiliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (825, '6:36', 6, 36, ' innam<u>aa</u> yastajiibu <b>al</b>la<u>dz</u>iina yasma\'uuna wa<b>a</b>lmawt<u>aa</u> yab\'atsuhumu <b>al</b>l<u>aa</u>hu tsumma ilayhi yurja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (826, '6:37', 6, 37, ' waq<u>aa</u>luu lawl<u>aa</u> nuzzila \'alayhi <u>aa</u>yatun min rabbihi qul inna <b>al</b>l<u>aa</u>ha q<u>aa</u>dirun \'al<u>aa</u> an yunazzila <u>aa</u>yatan wal<u>aa</u>kinna aktsarahum l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (827, '6:38', 6, 38, ' wam<u>aa</u> min d<u>aa</u>bbatin fii <b>a</b>l-ar<u>dh</u>i wal<u>aa</u> <u>th</u><u>aa</u>-irin ya<u>th</u>iiru bijan<u>aah</u>ayhi ill<u>aa</u> umamun amts<u>aa</u>lukum m<u>aa</u> farra<u>th</u>n<u>aa</u> fii <b>a</b>lkit<u>aa</u>bi min syay-in tsumma il<u>aa</u> rabbihim yu<u>h</u>syaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (828, '6:39', 6, 39, ' wa<b>a</b>lla<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> <u>sh</u>ummun wabukmun fii <b>al</b><u>zhzh</u>ulum<u>aa</u>ti man yasya-i <b>al</b>l<u>aa</u>hu yu<u>dh</u>lilhu waman yasya/ yaj\'alhu \'al<u>aa</u> <u>sh</u>ir<u>aath</u>in mustaqiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (829, '6:40', 6, 40, ' qul ara-aytakum in at<u>aa</u>kum \'a<u>dzaa</u>bu <b>al</b>l<u>aa</u>hi aw atatkumu <b>al</b>ss<u>aa</u>\'atu aghayra <b>al</b>l<u>aa</u>hi tad\'uuna in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (830, '6:41', 6, 41, ' bal iyy<u>aa</u>hu tad\'uuna fayaksyifu m<u>aa</u> tad\'uuna ilayhi in sy<u>aa</u>-a watansawna m<u>aa</u> tusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (831, '6:42', 6, 42, ' walaqad arsaln<u>aa</u> il<u>aa</u> umamin min qablika fa-akha<u>dz</u>n<u>aa</u>hum bi<b>a</b>lba/s<u>aa</u>-i wa<b>al</b><u>dhdh</u>arr<u>aa</u>-i la\'allahum yata<u>dh</u>arra\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (832, '6:43', 6, 43, ' falawl<u>aa</u> i<u>dz</u> j<u>aa</u>-ahum ba/sun<u>aa</u> ta<u>dh</u>arra\'uu wal<u>aa</u>kin qasat quluubuhum wazayyana lahumu <b>al</b>sysyay<u>th</u><u>aa</u>nu m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (833, '6:44', 6, 44, ' falamm<u>aa</u> nasuu m<u>aa</u> <u>dz</u>ukkiruu bihi fata<u>h</u>n<u>aa</u> \'alayhim abw<u>aa</u>ba kulli syay-in <u>h</u>att<u>aa</u> i<u>dzaa</u> fari<u>h</u>uu bim<u>aa</u> uutuu akha<u>dz</u>n<u>aa</u>hum baghtatan fa-i<u>dzaa</u> hum mublisuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (834, '6:45', 6, 45, ' faqu<u>th</u>i\'a d<u>aa</u>biru <b>a</b>lqawmi <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu wa<b>a</b>l<u>h</u>amdu lill<u>aa</u>hi rabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (835, '6:46', 6, 46, ' qul ara-aytum in akha<u>dz</u>a <b>al</b>l<u>aa</u>hu sam\'akum wa-ab<u>shaa</u>rakum wakhatama \'al<u>aa</u> quluubikum man il<u>aa</u>hun ghayru <b>al</b>l<u>aa</u>hi ya/tiikum bihi un<u>zh</u>ur kayfa nu<u>sh</u>arrifu <b>a</b>l-<u>aa</u>y<u>aa</u>ti tsumma hum ya<u>sh</u>difuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (836, '6:47', 6, 47, ' qul ara-aytakum in at<u>aa</u>kum \'a<u>dzaa</u>bu <b>al</b>l<u>aa</u>hi baghtatan aw jahratan hal yuhlaku ill<u>aa</u> <b>a</b>lqawmu <b>al</b><u>zhzhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (837, '6:48', 6, 48, ' wam<u>aa</u> nursilu <b>a</b>lmursaliina ill<u>aa</u> mubasysyiriina wamun<u>dz</u>iriina faman <u>aa</u>mana wa-a<u>sh</u>la<u>h</u>a fal<u>aa</u> khawfun \'alayhim wal<u>aa</u> hum ya<u>h</u>zanuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (838, '6:49', 6, 49, ' wa<b>a</b>lla<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> yamassuhumu <b>a</b>l\'a<u>dzaa</u>bu bim<u>aa</u> k<u>aa</u>nuu yafsuquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (839, '6:50', 6, 50, ' qul l<u>aa</u> aquulu lakum \'indii khaz<u>aa</u>-inu <b>al</b>l<u>aa</u>hi wal<u>aa</u> a\'lamu <b>a</b>lghayba wal<u>aa</u> aquulu lakum innii malakun in attabi\'u ill<u>aa</u> m<u>aa</u> yuu<u>haa</u> ilayya qul hal yastawii <b>a</b>l-a\'m<u>aa</u> wa<b>a</b>lba<u>sh</u>iiru afal<u>aa</u> tatafakkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (840, '6:51', 6, 51, ' wa-an<u>dz</u>ir bihi <b>al</b>la<u>dz</u>iina yakh<u>aa</u>fuuna an yu<u>h</u>syaruu il<u>aa</u> rabbihim laysa lahum min duunihi waliyyun wal<u>aa</u> syafii\'un la\'allahum yattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (841, '6:52', 6, 52, ' wal<u>aa</u> ta<u>th</u>rudi <b>al</b>la<u>dz</u>iina yad\'uuna rabbahum bi<b>a</b>lghad<u>aa</u>ti wa<b>a</b>l\'asyiyyi yuriiduuna wajhahu m<u>aa</u> \'alayka min <u>h</u>is<u>aa</u>bihim min syay-in wam<u>aa</u> min <u>h</u>is<u>aa</u>bika \'alayhim min syay-in fata<u>th</u>rudahum fatakuuna mina <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (842, '6:53', 6, 53, ' waka<u>dzaa</u>lika fatann<u>aa</u> ba\'<u>dh</u>ahum biba\'<u>dh</u>in liyaquuluu ah<u>aa</u>ul<u>aa</u>-i manna <b>al</b>l<u>aa</u>hu \'alayhim min baynin<u>aa</u> alaysa <b>al</b>l<u>aa</u>hu bi-a\'lama bi<b>al</b>sysy<u>aa</u>kiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (843, '6:54', 6, 54, ' wa-i<u>dzaa</u> j<u>aa</u>-aka <b>al</b>la<u>dz</u>iina yu/minuuna bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> faqul sal<u>aa</u>mun \'alaykum kataba rabbukum \'al<u>aa</u> nafsihi <b>al</b>rra<u>h</u>mata annahu man \'amila minkum suu-an bijah<u>aa</u>latin tsumma t<u>aa</u>ba min ba\'dihi wa-a<u>sh</u>la<u>h</u>a fa-annahu ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (844, '6:55', 6, 55, ' waka<u>dzaa</u>lika nufa<u>shsh</u>ilu <b>a</b>l-<u>aa</u>y<u>aa</u>ti walitastabiina sabiilu <b>a</b>lmujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (845, '6:56', 6, 56, ' qul innii nuhiitu an a\'buda <b>al</b>la<u>dz</u>iina tad\'uuna min duuni <b>al</b>l<u>aa</u>hi qul l<u>aa</u> attabi\'u ahw<u>aa</u>-akum qad <u>dh</u>alaltu i<u>dz</u>an wam<u>aa</u> an<u>aa</u> mina <b>a</b>lmuhtadiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (846, '6:57', 6, 57, ' qul innii \'al<u>aa</u> bayyinatin min rabbii waka<u>dzdz</u>abtum bihi m<u>aa</u> \'indii m<u>aa</u> tasta\'jiluuna bihi ini <b>a</b>l<u>h</u>ukmu ill<u>aa</u> lill<u>aa</u>hi yaqu<u>shsh</u>u <b>a</b>l<u>h</u>aqqa wahuwa khayru <b>a</b>lf<u>aas</u>iliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (847, '6:58', 6, 58, ' qul law anna \'indii m<u>aa</u> tasta\'jiluuna bihi laqu<u>dh</u>iya <b>a</b>l-amru baynii wabaynakum wa<b>al</b>l<u>aa</u>hu a\'lamu bi<b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (848, '6:59', 6, 59, ' wa\'indahu maf<u>aa</u>ti<u>h</u>u <b>a</b>lghaybi l<u>aa</u> ya\'lamuh<u>aa</u> ill<u>aa</u> huwa waya\'lamu m<u>aa</u> fii <b>a</b>lbarri wa<b>a</b>lba<u>h</u>ri wam<u>aa</u> tasqu<u>th</u>u min waraqatin ill<u>aa</u> ya\'lamuh<u>aa</u> wal<u>aa</u> <u>h</u>abbatin fii <u>zh</u>ulum<u>aa</u>ti <b>a</b>l-ar<u>dh</u>i wal<u>aa</u> ra<u>th</u>bin wal<u>aa</u> y<u>aa</u>bisin ill<u>aa</u> fii kit<u>aa</u>bin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (849, '6:60', 6, 60, ' wahuwa <b>al</b>la<u>dz</u>ii yatawaff<u>aa</u>kum bi<b>a</b>llayli waya\'lamu m<u>aa</u> jara<u>h</u>tum bi<b>al</b>nnah<u>aa</u>ri tsumma yab\'atsukum fiihi liyuq<u>daa</u> ajalun musamman tsumma ilayhi marji\'ukum tsumma yunabbi-ukum bim<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (850, '6:61', 6, 61, ' wahuwa <b>a</b>lq<u>aa</u>hiru fawqa \'ib<u>aa</u>dihi wayursilu \'alaykum <u>h</u>afa<u>zh</u>atan <u>h</u>att<u>aa</u> i<u>dzaa</u> j<u>aa</u>-a a<u>h</u>adakumu <b>a</b>lmawtu tawaffat-hu rusulun<u>aa</u> wahum l<u>aa</u> yufarri<u>th</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (851, '6:62', 6, 62, ' tsumma rudduu il<u>aa</u> <b>al</b>l<u>aa</u>hi mawl<u>aa</u>humu <b>a</b>l<u>h</u>aqqi <b>a</b>l<u>aa</u> lahu <b>a</b>l<u>h</u>ukmu wahuwa asra\'u <b>a</b>l<u>haa</u>sibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (852, '6:63', 6, 63, ' qul man yunajjiikum min <u>zh</u>ulum<u>aa</u>ti <b>a</b>lbarri wa<b>a</b>lba<u>h</u>ri tad\'uunahu ta<u>dh</u>arru\'an wakhufyatan la-in anj<u>aa</u>n<u>aa</u> min h<u>aadz</u>ihi lanakuunanna mina <b>al</b>sysy<u>aa</u>kiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (853, '6:64', 6, 64, ' quli <b>al</b>l<u>aa</u>hu yunajjiikum minh<u>aa</u> wamin kulli karbin tsumma antum tusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (854, '6:65', 6, 65, ' qul huwa <b>a</b>lq<u>aa</u>diru \'al<u>aa</u> an yab\'atsa \'alaykum \'a<u>dzaa</u>ban min fawqikum aw min ta<u>h</u>ti arjulikum aw yalbisakum syiya\'an wayu<u>dz</u>iiqa ba\'<u>dh</u>akum ba/sa ba\'<u>dh</u>in un<u>zh</u>ur kayfa nu<u>sh</u>arrifu <b>a</b>l-<u>aa</u>y<u>aa</u>ti la\'allahum yafqahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (855, '6:66', 6, 66, ' waka<u>dzdz</u>aba bihi qawmuka wahuwa <b>a</b>l<u>h</u>aqqu qul lastu \'alaykum biwakiil<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (856, '6:67', 6, 67, ' likulli naba-in mustaqarrun wasawfa ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (857, '6:68', 6, 68, ' wa-i<u>dzaa</u> ra-ayta <b>al</b>la<u>dz</u>iina yakhuu<u>dh</u>uuna fii <u>aa</u>y<u>aa</u>tin<u>aa</u> fa-a\'ri<u>dh</u> \'anhum <u>h</u>att<u>aa</u> yakhuu<u>dh</u>uu fii <u>h</u>adiitsin ghayrihi wa-imm<u>aa</u> yunsiyannaka <b>al</b>sysyay<u>th</u><u>aa</u>nu fal<u>aa</u> taq\'ud ba\'da <b>al</b><u>dzdz</u>ikr<u>aa</u> ma\'a <b>a</b>lqawmi <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (858, '6:69', 6, 69, ' wam<u>aa</u> \'al<u>aa</u> <b>al</b>la<u>dz</u>iina yattaquuna min <u>h</u>is<u>aa</u>bihim min syay-in wal<u>aa</u>kin <u>dz</u>ikr<u>aa</u> la\'allahum yattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (859, '6:70', 6, 70, ' wa<u>dz</u>ari <b>al</b>la<u>dz</u>iina ittakha<u>dz</u>uu diinahum la\'iban walahwan wagharrat-humu <b>a</b>l<u>h</u>ay<u>aa</u>tu <b>al</b>dduny<u>aa</u> wa<u>dz</u>akkir bihi an tubsala nafsun bim<u>aa</u> kasabat laysa lah<u>aa</u> min duuni <b>al</b>l<u>aa</u>hi waliyyun wal<u>aa</u> syafii\'un wa-in ta\'dil kulla \'adlin l<u>aa</u> yu/kha<u>dz</u> minh<u>aa</u> ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina ubsiluu bim<u>aa</u> kasabuu lahum syar<u>aa</u>bun min <u>h</u>amiimin wa\'a<u>dzaa</u>bun <b>a</b>liimun bim<u>aa</u> k<u>aa</u>nuu yakfuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (860, '6:71', 6, 71, ' qul anad\'uu min duuni <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> yanfa\'un<u>aa</u> wal<u>aa</u> ya<u>dh</u>urrun<u>aa</u> wanuraddu \'al<u>aa</u> a\'q<u>aa</u>bin<u>aa</u> ba\'da i<u>dz</u> had<u>aa</u>n<u>aa</u> <b>al</b>l<u>aa</u>hu ka<b>a</b>lla<u>dz</u>ii<u> </u>istahwat-hu <b>al</b>sysyay<u>aath</u>iinu fii <b>a</b>l-ar<u>dh</u>i <u>h</u>ayr<u>aa</u>na lahu a<u>sh</u>-<u>haa</u>bun yad\'uunahu il<u>aa</u> <b>a</b>lhud<u>aa</u> i/tin<u>aa</u> qul inna hud<u>aa</u> <b>al</b>l<u>aa</u>hi huwa <b>a</b>lhud<u>aa</u> waumirn<u>aa</u> linuslima lirabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (861, '6:72', 6, 72, ' wa-an aqiimuu <b>al</b><u>shsh</u>al<u>aa</u>ta wa<b>i</b>ttaquuhu wahuwa <b>al</b>la<u>dz</u>ii ilayhi tu<u>h</u>syaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (862, '6:73', 6, 73, ' wahuwa <b>al</b>la<u>dz</u>ii khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a bi<b>a</b>l<u>h</u>aqqi wayawma yaquulu kun fayakuunu qawluhu <b>a</b>l<u>h</u>aqqu walahu <b>a</b>lmulku yawma yunfakhu fii <b>al</b><u>shsh</u>uuri \'<u>aa</u>limu <b>a</b>lghaybi wa<b>al</b>sysyah<u>aa</u>dati wahuwa <b>a</b>l<u>h</u>akiimu <b>a</b>lkhabiir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (863, '6:74', 6, 74, ' wa-i<u>dz</u> q<u>aa</u>la ibr<u>aa</u>hiimu li-abiihi <u>aa</u>zara atattakhi<u>dz</u>u a<u>sh</u>n<u>aa</u>man <u>aa</u>lihatan innii ar<u>aa</u>ka waqawmaka fii <u>dh</u>al<u>aa</u>lin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (864, '6:75', 6, 75, ' waka<u>dzaa</u>lika nurii ibr<u>aa</u>hiima malakuuta <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i waliyakuuna mina <b>a</b>lmuuqiniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (865, '6:76', 6, 76, ' falamm<u>aa</u> janna \'alayhi <b>al</b>laylu ra<u>aa</u> kawkaban q<u>aa</u>la h<u>aadzaa</u> rabbii falamm<u>aa</u> afala q<u>aa</u>la l<u>aa</u> u<u>h</u>ibbu <b>a</b>l-<u>aa</u>filiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (866, '6:77', 6, 77, ' falamm<u>aa</u> ra<u>aa</u> <b>a</b>lqamara b<u>aa</u>zighan q<u>aa</u>la h<u>aadzaa</u> rabbii falamm<u>aa</u> afala q<u>aa</u>la la-in lam yahdinii rabbii la-akuunanna mina <b>a</b>lqawmi <b>al</b><u>dhdhaa</u>lliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (867, '6:78', 6, 78, ' falamm<u>aa</u> ra<u>aa</u> <b>al</b>sysyamsa b<u>aa</u>zighatan q<u>aa</u>la h<u>aadzaa</u> rabbii h<u>aadzaa</u> akbaru falamm<u>aa</u> afalat q<u>aa</u>la y<u>aa</u> qawmi innii barii-un mimm<u>aa</u> tusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (868, '6:79', 6, 79, ' innii wajjahtu wajhiya lilla<u>dz</u>ii fa<u>th</u>ara <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a <u>h</u>aniifan wam<u>aa</u> an<u>aa</u> mina <b>a</b>lmusyrikiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (869, '6:80', 6, 80, ' wa<u>haa</u>jjahu qawmuhu q<u>aa</u>la atu<u>haa</u>jjuunnii fii <b>al</b>l<u>aa</u>hi waqad had<u>aa</u>ni wal<u>aa</u> akh<u>aa</u>fu m<u>aa</u> tusyrikuuna bihi ill<u>aa</u> an yasy<u>aa</u>-a rabbii syay-an wasi\'a rabbii kulla syay-in \'ilman afal<u>aa</u> tata<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (870, '6:81', 6, 81, ' wakayfa akh<u>aa</u>fu m<u>aa</u> asyraktum wal<u>aa</u> takh<u>aa</u>fuuna annakum asyraktum bi<b>al</b>l<u>aa</u>hi m<u>aa</u> lam yunazzil bihi \'alaykum sul<u>th</u><u>aa</u>nan fa-ayyu <b>a</b>lfariiqayni a<u>h</u>aqqu bi<b>a</b>l-amni in kuntum ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (871, '6:82', 6, 82, ' <b>al</b>la<u>dz</u>iina <u>aa</u>manuu walam yalbisuu iim<u>aa</u>nahum bi<u>zh</u>ulmin ul<u>aa</u>-ika lahumu <b>a</b>l-amnu wahum muhtaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (872, '6:83', 6, 83, ' watilka <u>h</u>ujjatun<u>aa</u> <u>aa</u>tayn<u>aa</u>h<u>aa</u> ibr<u>aa</u>hiima \'al<u>aa</u> qawmihi narfa\'u daraj<u>aa</u>tin man nasy<u>aa</u>u inna rabbaka <u>h</u>akiimun \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (873, '6:84', 6, 84, ' wawahabn<u>aa</u> lahu is<u>haa</u>qa waya\'quuba kullan hadayn<u>aa</u> wanuu<u>h</u>an hadayn<u>aa</u> min qablu wamin <u>dz</u>urriyyatihi d<u>aa</u>wuuda wasulaym<u>aa</u>na wa-ayyuuba wayuusufa wamuus<u>aa</u> wah<u>aa</u>ruuna waka<u>dzaa</u>lika najzii <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (874, '6:85', 6, 85, ' wazakariyy<u>aa</u> waya<u>h</u>y<u>aa</u> wa\'iis<u>aa</u> wa<b>i</b>ly<u>aa</u>sa kullun mina <b>al</b><u>shshaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (875, '6:86', 6, 86, ' wa-ism<u>aa</u>\'iila wa<b>i</b>lyasa\'a wayuunusa waluu<u>th</u>an wakullan fa<u>dhdh</u>aln<u>aa</u> \'al<u>aa</u> <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (876, '6:87', 6, 87, ' wamin <u>aa</u>b<u>aa</u>-ihim wa<u>dz</u>urriyy<u>aa</u>tihim wa-ikhw<u>aa</u>nihim wa<b>i</b>jtabayn<u>aa</u>hum wahadayn<u>aa</u>hum il<u>aa</u> <u>sh</u>ir<u>aath</u>in mustaqiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (877, '6:88', 6, 88, ' <u>dzaa</u>lika hud<u>aa</u> <b>al</b>l<u>aa</u>hi yahdii bihi man yasy<u>aa</u>u min \'ib<u>aa</u>dihi walaw asyrakuu la<u>h</u>abi<u>th</u>a \'anhum m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (878, '6:89', 6, 89, ' ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina <u>aa</u>tayn<u>aa</u>humu <b>a</b>lkit<u>aa</u>ba wa<b>a</b>l<u>h</u>ukma wa<b>al</b>nnubuwwata fa-in yakfur bih<u>aa</u> h<u>aa</u>ul<u>aa</u>-i faqad wakkaln<u>aa</u> bih<u>aa</u> qawman laysuu bih<u>aa</u> bik<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (879, '6:90', 6, 90, ' ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina had<u>aa</u> <b>al</b>l<u>aa</u>hu fabihud<u>aa</u>humu iqtadih qul l<u>aa</u> as-alukum \'alayhi ajran in huwa ill<u>aa</u> <u>dz</u>ikr<u>aa</u> lil\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (880, '6:91', 6, 91, ' wam<u>aa</u> qadaruu <b>al</b>l<u>aa</u>ha <u>h</u>aqqa qadrihi i<u>dz</u> q<u>aa</u>luu m<u>aa</u> anzala <b>al</b>l<u>aa</u>hu \'al<u>aa</u> basyarin min syay-in qul man anzala <b>a</b>lkit<u>aa</u>ba <b>al</b>la<u>dz</u>ii j<u>aa</u>-a bihi muus<u>aa</u> nuuran wahudan li<b>l</b>nn<u>aa</u>si taj\'aluunahu qar<u>aath</u>iisa tubduunah<u>aa</u> watukhfuuna katsiiran wa\'ullimtum m<u>aa</u> lam ta\'lamuu antum wal<u>aa</u> <u>aa</u>b<u>aa</u>ukum quli <b>al</b>l<u>aa</u>hu tsumma <u>dz</u>arhum fii khaw<u>dh</u>ihim yal\'abuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (881, '6:92', 6, 92, ' wah<u>aadzaa</u> kit<u>aa</u>bun anzaln<u>aa</u>hu mub<u>aa</u>rakun mu<u>sh</u>addiqu <b>al</b>la<u>dz</u>ii bayna yadayhi walitun<u>dz</u>ira umma <b>a</b>lqur<u>aa</u> waman <u>h</u>awlah<u>aa</u> wa<b>a</b>lla<u>dz</u>iina yu/minuuna bi<b>a</b>l-<u>aa</u>khirati yu/minuuna bihi wahum \'al<u>aa</u> <u>sh</u>al<u>aa</u>tihim yu<u>haa</u>fi<u>zh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (882, '6:93', 6, 93, ' waman a<u>zh</u>lamu mimmani iftar<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ka<u>dz</u>iban aw q<u>aa</u>la uu<u>h</u>iya ilayya walam yuu<u>h</u>a ilayhi syay-un waman q<u>aa</u>la saunzilu mitsla m<u>aa</u> anzala <b>al</b>l<u>aa</u>hu walaw tar<u>aa</u> i<u>dz</u>i <b>al</b><u>zhzhaa</u>limuuna fii ghamar<u>aa</u>ti <b>a</b>lmawti wa<b>a</b>lmal<u>aa</u>-ikatu b<u>aa</u>si<u>th</u>uu aydiihim akhrijuu anfusakumu <b>a</b>lyawma tujzawna \'a<u>dzaa</u>ba <b>a</b>lhuuni bim<u>aa</u> kuntum taquuluuna \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ghayra <b>a</b>l<u>h</u>aqqi wakuntum \'an <u>aa</u>y<u>aa</u>tihi tastakbiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (883, '6:94', 6, 94, ' walaqad ji/tumuun<u>aa</u> fur<u>aa</u>d<u>aa</u> kam<u>aa</u> khalaqn<u>aa</u>kum awwala marratin wataraktum m<u>aa</u> khawwaln<u>aa</u>kum war<u>aa</u>-a <u>zh</u>uhuurikum wam<u>aa</u> nar<u>aa</u> ma\'akum syufa\'<u>aa</u>-akumu <b>al</b>la<u>dz</u>iina za\'amtum annahum fiikum syurak<u>aa</u>u laqad taqa<u>ththh</u>a\'a baynakum wa<u>dh</u>alla \'ankum m<u>aa</u> kuntum taz\'umuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (884, '6:95', 6, 95, ' inna <b>al</b>l<u>aa</u>ha f<u>aa</u>liqu <b>a</b>l<u>h</u>abbi wa<b>al</b>nnaw<u>aa</u> yukhriju <b>a</b>l<u>h</u>ayya mina <b>a</b>lmayyiti wamukhriju <b>a</b>lmayyiti mina <b>a</b>l<u>h</u>ayyi <u>dzaa</u>likumu <b>al</b>l<u>aa</u>hu fa-ann<u>aa</u> tu/fakuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (885, '6:96', 6, 96, ' f<u>aa</u>liqu <b>a</b>l-i<u>sh</u>b<u>aah</u>i waja\'ala <b>al</b>layla sakanan wa<b>al</b>sysyamsa wa<b>a</b>lqamara <u>h</u>usb<u>aa</u>nan <u>dzaa</u>lika taqdiiru <b>a</b>l\'aziizi <b>a</b>l\'aliim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (886, '6:97', 6, 97, ' wahuwa <b>al</b>la<u>dz</u>ii ja\'ala lakumu <b>al</b>nnujuuma litahtaduu bih<u>aa</u> fii <u>zh</u>ulum<u>aa</u>ti <b>a</b>lbarri wa<b>a</b>lba<u>h</u>ri qad fa<u>shsh</u>aln<u>aa</u> <b>a</b>l-<u>aa</u>y<u>aa</u>ti liqawmin ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (887, '6:98', 6, 98, ' wahuwa <b>al</b>la<u>dz</u>ii ansya-akum min nafsin w<u>aah</u>idatin famustaqarrun wamustawda\'un qad fa<u>shsh</u>aln<u>aa</u> <b>a</b>l-<u>aa</u>y<u>aa</u>ti liqawmin yafqahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (888, '6:99', 6, 99, ' wahuwa <b>al</b>la<u>dz</u>ii anzala mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an fa-akhrajn<u>aa</u> bihi nab<u>aa</u>ta kulli syay-in fa-akhrajn<u>aa</u> minhu kha<u>dh</u>iran nukhriju minhu <u>h</u>abban mutar<u>aa</u>kiban wamina <b>al</b>nnakhli min <u>th</u>al\'ih<u>aa</u> qinw<u>aa</u>nun d<u>aa</u>niyatun wajann<u>aa</u>tin min a\'n<u>aa</u>bin wa<b>al</b>zzaytuuna wa<b>al</b>rrumm<u>aa</u>na musytabihan waghayra mutasy<u>aa</u>bihin un<u>zh</u>uruu il<u>aa</u> tsamarihi i<u>dzaa</u> atsmara wayan\'ihi inna fii <u>dzaa</u>likum la<u>aa</u>y<u>aa</u>tin liqawmin yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (889, '6:100', 6, 100, ' waja\'aluu lill<u>aa</u>hi syurak<u>aa</u>-a <b>a</b>ljinna wakhalaqahum wakharaquu lahu baniina waban<u>aa</u>tin bighayri \'ilmin sub<u>haa</u>nahu wata\'<u>aa</u>l<u>aa</u> \'amm<u>aa</u> ya<u>sh</u>ifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (890, '6:101', 6, 101, ' badii\'u <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i ann<u>aa</u> yakuunu lahu waladun walam takun lahu <u>shaah</u>ibatun wakhalaqa kulla syay-in wahuwa bikulli syay-in \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (891, '6:102', 6, 102, ' <u>dzaa</u>likumu <b>al</b>l<u>aa</u>hu rabbukum l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa kh<u>aa</u>liqu kulli syay-in fa<b>u</b>\'buduuhu wahuwa \'al<u>aa</u> kulli syay-in wakiil<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (892, '6:103', 6, 103, ' l<u>aa</u> tudrikuhu <b>a</b>l-ab<u>shaa</u>ru wahuwa yudriku <b>a</b>l-ab<u>shaa</u>ra wahuwa <b>al</b>la<u>th</u>iifu <b>a</b>lkhabiir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (893, '6:104', 6, 104, ' qad j<u>aa</u>-akum ba<u>shaa</u>-iru min rabbikum faman ab<u>sh</u>ara falinafsihi waman \'amiya fa\'alayh<u>aa</u> wam<u>aa</u> an<u>aa</u> \'alaykum bi<u>h</u>afii<u>zh</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (894, '6:105', 6, 105, ' waka<u>dzaa</u>lika nu<u>sh</u>arrifu <b>a</b>l-<u>aa</u>y<u>aa</u>ti waliyaquuluu darasta walinubayyinahu liqawmin ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (895, '6:106', 6, 106, ' ittabi\' m<u>aa</u> uu<u>h</u>iya ilayka min rabbika l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa wa-a\'ri<u>dh</u> \'ani <b>a</b>lmusyrikiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (896, '6:107', 6, 107, ' walaw sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu m<u>aa</u> asyrakuu wam<u>aa</u> ja\'aln<u>aa</u>ka \'alayhim <u>h</u>afii<u>zh</u>an wam<u>aa</u> anta \'alayhim biwakiil<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (897, '6:108', 6, 108, ' wal<u>aa</u> tasubbuu <b>al</b>la<u>dz</u>iina yad\'uuna min duuni <b>al</b>l<u>aa</u>hi fayasubbuu <b>al</b>l<u>aa</u>ha \'adwan bighayri \'ilmin ka<u>dzaa</u>lika zayyann<u>aa</u> likulli ummatin \'amalahum tsumma il<u>aa</u> rabbihim marji\'uhum fayunabbi-uhum bim<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (898, '6:109', 6, 109, ' wa-aqsamuu bi<b>al</b>l<u>aa</u>hi jahda aym<u>aa</u>nihim la-in j<u>aa</u>-at-hum <u>aa</u>yatun layu/minunna bih<u>aa</u> qul innam<u>aa</u> <b>a</b>l-<u>aa</u>y<u>aa</u>tu \'inda <b>al</b>l<u>aa</u>hi wam<u>aa</u> yusy\'irukum annah<u>aa</u> i<u>dzaa</u> j<u>aa</u>-at l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (899, '6:110', 6, 110, ' wanuqallibu af-idatahum wa-ab<u>shaa</u>rahum kam<u>aa</u> lam yu/minuu bihi awwala marratin wana<u>dz</u>aruhum fii <u>th</u>ughy<u>aa</u>nihim ya\'mahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (900, '6:111', 6, 111, ' walaw annan<u>aa</u> nazzaln<u>aa</u> ilayhimu <b>a</b>lmal<u>aa</u>-ikata wakallamahumu <b>a</b>lmawt<u>aa</u> wa<u>h</u>asyarn<u>aa</u> \'alayhim kulla syay-in qubulan m<u>aa</u> k<u>aa</u>nuu liyu/minuu ill<u>aa</u> an yasy<u>aa</u>-a <b>al</b>l<u>aa</u>hu wal<u>aa</u>kinna aktsarahum yajhaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (901, '6:112', 6, 112, ' waka<u>dzaa</u>lika ja\'aln<u>aa</u> likulli nabiyyin \'aduwwan syay<u>aath</u>iina <b>a</b>l-insi wa<b>a</b>ljinni yuu<u>h</u>ii ba\'<u>dh</u>uhum il<u>aa</u> ba\'<u>dh</u>in zukhrufa <b>a</b>lqawli ghuruuran walaw sy<u>aa</u>-a rabbuka m<u>aa</u> fa\'aluuhu fa<u>dz</u>arhum wam<u>aa</u> yaftaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (902, '6:113', 6, 113, ' walita<u>sh</u>gh<u>aa</u> ilayhi af-idatu <b>al</b>la<u>dz</u>iina l<u>aa</u> yu/minuuna bi<b>a</b>l-<u>aa</u>khirati waliyar<u>dh</u>awhu waliyaqtarifuu m<u>aa</u> hum muqtarifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (903, '6:114', 6, 114, ' afaghayra <b>al</b>l<u>aa</u>hi abtaghii <u>h</u>akaman wahuwa <b>al</b>la<u>dz</u>ii anzala ilaykumu <b>a</b>lkit<u>aa</u>ba mufa<u>shsh</u>alan wa<b>a</b>lla<u>dz</u>iina <u>aa</u>tayn<u>aa</u>humu <b>a</b>lkit<u>aa</u>ba ya\'lamuuna annahu munazzalun min rabbika bi<b>a</b>l<u>h</u>aqqi fal<u>aa</u> takuunanna mina <b>a</b>lmumtariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (904, '6:115', 6, 115, ' watammat kalimatu rabbika <u>sh</u>idqan wa\'adlan l<u>aa</u> mubaddila likalim<u>aa</u>tihi wahuwa <b>al</b>ssamii\'u <b>a</b>l\'aliim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (905, '6:116', 6, 116, ' wa-in tu<u>th</u>i\' aktsara man fii <b>a</b>l-ar<u>dh</u>i yu<u>dh</u>illuuka \'an sabiili <b>al</b>l<u>aa</u>hi in yattabi\'uuna ill<u>aa</u> <b>al</b><u>zhzh</u>anna wa-in hum ill<u>aa</u> yakhru<u>sh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (906, '6:117', 6, 117, ' inna rabbaka huwa a\'lamu man ya<u>dh</u>illu \'an sabiilihi wahuwa a\'lamu bi<b>a</b>lmuhtadiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (907, '6:118', 6, 118, ' fakuluu mimm<u>aa</u> <u>dz</u>ukira ismu <b>al</b>l<u>aa</u>hi \'alayhi in kuntum bi-<u>aa</u>y<u>aa</u>tihi mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (908, '6:119', 6, 119, ' wam<u>aa</u> lakum <b>al</b>l<u>aa</u> ta/kuluu mimm<u>aa</u> <u>dz</u>ukira ismu <b>al</b>l<u>aa</u>hi \'alayhi waqad fa<u>shsh</u>ala lakum m<u>aa</u> <u>h</u>arrama \'alaykum ill<u>aa</u> m<u>aa</u> i<u>dth</u>urirtum ilayhi wa-inna katsiiran layu<u>dh</u>illuuna bi-ahw<u>aa</u>-ihim bighayri \'ilmin inna rabbaka huwa a\'lamu bi<b>a</b>lmu\'tadiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (909, '6:120', 6, 120, ' wa<u>dz</u>aruu <u>zhaa</u>hira <b>a</b>l-itsmi wab<u>aath</u>inahu inna <b>al</b>la<u>dz</u>iina yaksibuuna <b>a</b>l-itsma sayujzawna bim<u>aa</u> k<u>aa</u>nuu yaqtarifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (910, '6:121', 6, 121, ' wal<u>aa</u> ta/kuluu mimm<u>aa</u> lam yu<u>dz</u>kari ismu <b>al</b>l<u>aa</u>hi \'alayhi wa-innahu lafisqun wa-inna <b>al</b>sysyay<u>aath</u>iina layuu<u>h</u>uuna il<u>aa</u> awliy<u>aa</u>-ihim liyuj<u>aa</u>diluukum wa-in a<u>th</u>a\'tumuuhum innakum lamusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (911, '6:122', 6, 122, ' awa man k<u>aa</u>na maytan fa-a<u>h</u>yayn<u>aa</u>hu waja\'aln<u>aa</u> lahu nuuran yamsyii bihi fii <b>al</b>nn<u>aa</u>si kaman matsaluhu fii <b>al</b><u>zhzh</u>ulum<u>aa</u>ti laysa bikh<u>aa</u>rijin minh<u>aa</u> ka<u>dzaa</u>lika zuyyina lilk<u>aa</u>firiina m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (912, '6:123', 6, 123, ' waka<u>dzaa</u>lika ja\'aln<u>aa</u> fii kulli qaryatin ak<u>aa</u>bira mujrimiih<u>aa</u> liyamkuruu fiih<u>aa</u> wam<u>aa</u> yamkuruuna ill<u>aa</u> bi-anfusihim wam<u>aa</u> yasy\'uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (913, '6:124', 6, 124, ' wa-i<u>dzaa</u> j<u>aa</u>-at-hum <u>aa</u>yatun q<u>aa</u>luu lan nu/mina <u>h</u>att<u>aa</u> nu/t<u>aa</u> mitsla m<u>aa</u> uutiya rusulu <b>al</b>l<u>aa</u>hi <b>al</b>l<u>aa</u>hu a\'lamu <u>h</u>aytsu yaj\'alu ris<u>aa</u>latahu sayu<u>sh</u>iibu <b>al</b>la<u>dz</u>iina ajramuu <u>sh</u>agh<u>aa</u>run \'inda <b>al</b>l<u>aa</u>hi wa\'a<u>dzaa</u>bun syadiidun bim<u>aa</u> k<u>aa</u>nuu yamkuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (914, '6:125', 6, 125, ' faman yuridi <b>al</b>l<u>aa</u>hu an yahdiyahu yasyra<u>h</u> <u>sh</u>adrahu lil-isl<u>aa</u>mi waman yurid an yu<u>dh</u>illahu yaj\'al <u>sh</u>adrahu <u>dh</u>ayyiqan <u>h</u>arajan ka-annam<u>aa</u> ya<u>shsh</u>a\'\'adu fii <b>al</b>ssam<u>aa</u>-i ka<u>dzaa</u>lika yaj\'alu <b>al</b>l<u>aa</u>hu <b>al</b>rrijsa \'al<u>aa</u> <b>al</b>la<u>dz</u>iina l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (915, '6:126', 6, 126, ' wah<u>aadzaa</u> <u>sh</u>ir<u>aath</u>u rabbika mustaqiiman qad fa<u>shsh</u>aln<u>aa</u> <b>a</b>l-<u>aa</u>y<u>aa</u>ti liqawmin ya<u>dzdz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (916, '6:127', 6, 127, ' lahum d<u>aa</u>ru <b>al</b>ssal<u>aa</u>mi \'inda rabbihim wahuwa waliyyuhum bim<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (917, '6:128', 6, 128, ' wayawma ya<u>h</u>syuruhum jamii\'an y<u>aa</u> ma\'syara <b>a</b>ljinni qadi istaktsartum mina <b>a</b>l-insi waq<u>aa</u>la awliy<u>aa</u>uhum mina <b>a</b>l-insi rabban<u>aa</u> istamta\'a ba\'<u>dh</u>un<u>aa</u> biba\'<u>dh</u>in wabalaghn<u>aa</u> ajalan<u>aa</u> <b>al</b>la<u>dz</u>ii ajjalta lan<u>aa</u> q<u>aa</u>la <b>al</b>nn<u>aa</u>ru matsw<u>aa</u>kum kh<u>aa</u>lidiina fiih<u>aa</u> ill<u>aa</u> m<u>aa</u> sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu inna rabbaka <u>h</u>akiimun \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (918, '6:129', 6, 129, ' waka<u>dzaa</u>lika nuwallii ba\'<u>dh</u>a <b>al</b><u>zhzhaa</u>limiina ba\'<u>dh</u>an bim<u>aa</u> k<u>aa</u>nuu yaksibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (919, '6:130', 6, 130, ' y<u>aa</u> ma\'syara aljinni wa<b>a</b>l-insi alam ya/tikum rusulun minkum yaqu<u>shsh</u>uuna \'alaykum <u>aa</u>y<u>aa</u>tii wayun<u>dz</u>iruunakum liq<u>aa</u>-a yawmikum h<u>aadzaa</u> q<u>aa</u>luu syahidn<u>aa</u> \'al<u>aa</u> anfusin<u>aa</u> wagharrat-humu al<u>h</u>ay<u>aa</u>tu <b>al</b>dduny<u>aa</u> wasyahiduu \'al<u>aa</u> anfusihim annahum k<u>aa</u>nuu k<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (920, '6:131', 6, 131, ' <u>dzaa</u>lika an lam yakun rabbuka muhlika <b>a</b>lqur<u>aa</u> bi<u>zh</u>ulmin wa-ahluh<u>aa</u> gh<u>aa</u>filuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (921, '6:132', 6, 132, ' walikullin daraj<u>aa</u>tun mimm<u>aa</u> \'amiluu wam<u>aa</u> rabbuka bigh<u>aa</u>filin \'amm<u>aa</u> ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (922, '6:133', 6, 133, ' warabbuka <b>a</b>lghaniyyu <u>dz</u>uu <b>al</b>rra<u>h</u>mati in yasya/ yu<u>dz</u>hibkum wayastakhlif min ba\'dikum m<u>aa</u> yasy<u>aa</u>u kam<u>aa</u> ansya-akum min <u>dz</u>urriyyati qawmin <u>aa</u>khariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (923, '6:134', 6, 134, ' inna m<u>aa</u> tuu\'aduuna la<u>aa</u>tin wam<u>aa</u> antum bimu\'jiziin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (924, '6:135', 6, 135, ' qul y<u>aa</u> qawmi i\'maluu \'al<u>aa</u> mak<u>aa</u>natikum innii \'<u>aa</u>milun fasawfa ta\'lamuuna man takuunu lahu \'<u>aa</u>qibatu <b>al</b>dd<u>aa</u>ri innahu l<u>aa</u> yufli<u>h</u>u <b>al</b><u>zhzhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (925, '6:136', 6, 136, ' waja\'aluu lill<u>aa</u>hi mimm<u>aa</u> <u>dz</u>ara-a mina <b>a</b>l<u>h</u>artsi wa<b>a</b>l-an\'<u>aa</u>mi na<u>sh</u>iiban faq<u>aa</u>luu h<u>aadzaa</u> lill<u>aa</u>hi biza\'mihim wah<u>aadzaa</u> lisyurak<u>aa</u>-in<u>aa</u> fam<u>aa</u> k<u>aa</u>na lisyurak<u>aa</u>-ihim fal<u>aa</u> ya<u>sh</u>ilu il<u>aa</u> <b>al</b>l<u>aa</u>hi wam<u>aa</u> k<u>aa</u>na lill<u>aa</u>hi fahuwa ya<u>sh</u>ilu il<u>aa</u> syurak<u>aa</u>-ihim s<u>aa</u>-a m<u>aa</u> ya<u>h</u>kumuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (926, '6:137', 6, 137, ' waka<u>dzaa</u>lika zayyana likatsiirin mina <b>a</b>lmusyrikiina qatla awl<u>aa</u>dihim syurak<u>aa</u>uhum liyurduuhum waliyalbisuu \'alayhim diinahum walaw sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu m<u>aa</u> fa\'aluuhu fa<u>dz</u>arhum wam<u>aa</u> yaftaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (927, '6:138', 6, 138, ' waq<u>aa</u>luu h<u>aadz</u>ihi an\'<u>aa</u>mun wa<u>h</u>artsun <u>h</u>ijrun l<u>aa</u> ya<u>th</u>\'amuh<u>aa</u> ill<u>aa</u> man nasy<u>aa</u>u biza\'mihim wa-an\'<u>aa</u>mun <u>h</u>urrimat <u>zh</u>uhuuruh<u>aa</u> wa-an\'<u>aa</u>mun l<u>aa</u> ya<u>dz</u>kuruuna isma <b>al</b>l<u>aa</u>hi \'alayh<u>aa</u> iftir<u>aa</u>-an \'>\r\n');
INSERT INTO quranLatin VALUES (928, '6:139', 6, 139, ' waq<u>aa</u>luu m<u>aa</u> fii bu<u>th</u>uuni h<u>aadz</u>ihi <b>a</b>l-an\'<u>aa</u>mi kh<u>aa</u>li<u>sh</u>atun li<u>dz</u>ukuurin<u>aa</u> wamu<u>h</u>arramun \'al<u>aa</u> azw<u>aa</u>jin<u>aa</u> wa-in yakun maytatan fahum fiihi syurak<u>aa</u>u sayajziihim wa<u>sh</u>fahum innahu <u>h</u>akiimun \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (929, '6:140', 6, 140, ' qad khasira <b>al</b>la<u>dz</u>iina qataluu awl<u>aa</u>dahum safahan bighayri \'ilmin wa<u>h</u>arramuu m<u>aa</u> razaqahumu <b>al</b>l<u>aa</u>hu iftir<u>aa</u>-an \'al<u>aa</u> <b>al</b>l<u>aa</u>hi qad <u>dh</u>alluu wam<u>aa</u> k<u>aa</u>nuu muhtadiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (930, '6:141', 6, 141, ' wahuwa <b>al</b>la<u>dz</u>ii ansya-a jann<u>aa</u>tin ma\'ruusy<u>aa</u>tin waghayra ma\'ruusy<u>aa</u>tin wa<b>al</b>nnakhla wa<b>al</b>zzar\'a mukhtalifan ukuluhu wa<b>al</b>zzaytuuna wa<b>al</b>rrumm<u>aa</u>na mutasy<u>aa</u>bihan waghayra mutasy<u>aa</u>bihin kuluu min tsamarihi i<u>dzaa</u> atsmara wa<u>aa</u>tuu <u>h</u>aqqahu yawma <u>h</u>a<u>shaa</u>dihi wal<u>aa</u> tusrifuu innahu l<u>aa</u> yu<u>h</u>ibbu <b>a</b>lmusrifiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (931, '6:142', 6, 142, ' wamina <b>a</b>l-an\'<u>aa</u>mi <u>h</u>amuulatan wafarsyan kuluu mimm<u>aa</u> razaqakumu <b>al</b>l<u>aa</u>hu wal<u>aa</u> tattabi\'uu khu<u>th</u>uw<u>aa</u>ti <b>al</b>sysyay<u>th</u><u>aa</u>ni innahu lakum \'aduwwun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (932, '6:143', 6, 143, ' tsam<u>aa</u>niyata azw<u>aa</u>jin mina <b>al</b><u>dhdh</u>a/ni itsnayni wamina <b>a</b>lma\'zi itsnayni qul <u>aa</u><b>l</b><u>dzdz</u>akarayni <u>h</u>arrama ami <b>a</b>luntsayayni amm<u>aa</u> isytamalat \'alayhi ar<u>haa</u>mu <b>a</b>luntsayayni nabbi-uunii bi\'ilmin in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (933, '6:144', 6, 144, ' wamina <b>a</b>l-ibili itsnayni wamina <b>a</b>lbaqari itsnayni qul <u>aa</u><b>l</b><u>dzdz</u>akarayni <u>h</u>arrama ami <b>a</b>luntsayayni amm<u>aa</u> isytamalat \'alayhi ar<u>haa</u>mu <b>a</b>luntsayayni am kuntum syuhad<u>aa</u>-a i<u>dz</u> wa<u>shshaa</u>kumu <b>al</b>l<u>aa</u>hu bih<u>aadzaa</u> faman a<u>zh</u>lamu mimmani iftar<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ka<u>dz</u>iban liyu<u>dh</u>illa <b>al</b>nn<u>aa</u>sa bighayri \'ilmin inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yahdii <b>a</b>lqawma <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (934, '6:145', 6, 145, ' qul l<u>aa</u> ajidu fiim<u>aa</u> uu<u>h</u>iya ilayya mu<u>h</u>arraman \'al<u>aa</u> <u>th</u><u>aa</u>\'imin ya<u>th</u>\'amuhu ill<u>aa</u> an yakuuna maytatan aw daman masfuu<u>h</u>an aw la<u>h</u>ma khinziirin fa-innahu rijsun aw fisqan uhilla lighayri <b>al</b>l<u>aa</u>hi bihi famani i<u>dth</u>urra ghayra b<u>aa</u>ghin wal<u>aa</u> \'<u>aa</u>din fa-inna rabbaka ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (935, '6:146', 6, 146, ' wa\'al<u>aa</u> <b>al</b>la<u>dz</u>iina h<u>aa</u>duu <u>h</u>arramn<u>aa</u> kulla <u>dz</u>ii <u>zh</u>ufurin wamina <b>a</b>lbaqari wa<b>a</b>lghanami <u>h</u>arramn<u>aa</u> \'alayhim syu<u>h</u>uumahum<u>aa</u> ill<u>aa</u> m<u>aa</u> <u>h</u>amalat <u>zh</u>uhuuruhum<u>aa</u> awi <b>a</b>l<u>h</u>aw<u>aa</u>y<u>aa</u> aw m<u>aa</u> ikhtala<u>th</u>a bi\'a<u>zh</u>min <u>dzaa</u>lika jazayn<u>aa</u>hum bibaghyihim wa-inn<u>aa</u> la<u>shaa</u>diquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (936, '6:147', 6, 147, ' fa-in ka<u>dzdz</u>abuuka faqul rabbukum <u>dz</u>uu ra<u>h</u>matin w<u>aa</u>si\'atin wal<u>aa</u> yuraddu ba/suhu \'ani <b>a</b>lqawmi <b>a</b>lmujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (937, '6:148', 6, 148, ' sayaquulu <b>al</b>la<u>dz</u>iina asyrakuu law sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu m<u>aa</u> asyrakn<u>aa</u> wal<u>aa</u> <u>aa</u>b<u>aa</u>un<u>aa</u> wal<u>aa</u> <u>h</u>arramn<u>aa</u> min syay-in ka<u>dzaa</u>lika ka<u>dzdz</u>aba <b>al</b>la<u>dz</u>iina min qablihim <u>h</u>att<u>aa</u> <u>dzaa</u>quu ba/san<u>aa</u> qul hal \'indakum min \'ilmin fatukhrijuuhu lan<u>aa</u> in tattabi\'uuna ill<u>aa</u> <b>al</b><u>zhzh</u>anna wa-in antum ill<u>aa</u> takhru<u>sh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (938, '6:149', 6, 149, ' qul falill<u>aa</u>hi <b>a</b>l<u>h</u>ujjatu <b>a</b>lb<u>aa</u>lighatu falaw sy<u>aa</u>-a lahad<u>aa</u>kum ajma\'iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (939, '6:150', 6, 150, ' qul halumma syuhad<u>aa</u>-akumu <b>al</b>la<u>dz</u>iina yasyhaduuna anna <b>al</b>l<u>aa</u>ha <u>h</u>arrama h<u>aadzaa</u> fa-in syahiduu fal<u>aa</u> tasyhad ma\'ahum wal<u>aa</u> tattabi\' ahw<u>aa</u>-a <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> wa<b>a</b>lla<u>dz</u>iina l<u>aa</u> yu/minuuna bi<b>a</b>l-<u>aa</u>khirati wahum birabbihim ya\'diluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (940, '6:151', 6, 151, ' qul ta\'<u>aa</u>law atlu m<u>aa</u> <u>h</u>arrama rabbukum \'alaykum <b>al</b>l<u>aa</u> tusyrikuu bihi syay-an wabi<b>a</b>lw<u>aa</u>lidayni i<u>h</u>s<u>aa</u>nan wal<u>aa</u> taqtuluu awl<u>aa</u>dakum min iml<u>aa</u>qin na<u>h</u>nu narzuqukum wa-iyy<u>aa</u>hum wal<u>aa</u> taqrabuu <b>a</b>lfaw<u>aah</u>isya m<u>aa</u> <u>zh</u>ahara minh<u>aa</u> wam<u>aa</u> ba<u>th</u>ana wal<u>aa</u> taqtuluu <b>al</b>nnafsa <b>al</b>latii <u>h</u>arrama <b>al</b>l<u>aa</u>hu ill<u>aa</u> bi<b>a</b>l<u>h</u>aqqi <u>dzaa</u>likum wa<u>shshaa</u>kum bihi la\'allakum ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (941, '6:152', 6, 152, ' wal<u>aa</u> taqrabuu m<u>aa</u>la <b>a</b>lyatiimi ill<u>aa</u> bi<b>a</b>llatii hiya a<u>h</u>sanu <u>h</u>att<u>aa</u> yablugha asyuddahu wa-awfuu <b>a</b>lkayla wa<b>a</b>lmiiz<u>aa</u>na bi<b>a</b>lqis<u>th</u>i l<u>aa</u> nukallifu nafsan ill<u>aa</u> wus\'ah<u>aa</u> wa-i<u>dzaa</u> qultum fa<b>i</b>\'diluu walaw k<u>aa</u>na <u>dzaa</u> qurb<u>aa</u> wabi\'ahdi <b>al</b>l<u>aa</u>hi awfuu <u>dzaa</u>likum wa<u>shshaa</u>kum bihi la\'allakum ta<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (942, '6:153', 6, 153, ' wa-anna h<u>aadzaa</u> <u>sh</u>ir<u>aath</u>ii mustaqiiman fa<b>i</b>ttabi\'uuhu wal<u>aa</u> tattabi\'uu <b>al</b>ssubula fatafarraqa bikum \'an sabiilihi <u>dzaa</u>likum wa<u>shshaa</u>kum bihi la\'allakum tattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (943, '6:154', 6, 154, ' tsumma <u>aa</u>tayn<u>aa</u> muus<u>aa</u> <b>a</b>lkit<u>aa</u>ba tam<u>aa</u>man \'al<u>aa</u> <b>al</b>la<u>dz</u>ii a<u>h</u>sana wataf<u>sh</u>iilan likulli syay-in wahudan wara<u>h</u>matan la\'allahum biliq<u>aa</u>-i rabbihim yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (944, '6:155', 6, 155, ' wah<u>aadzaa</u> kit<u>aa</u>bun anzaln<u>aa</u>hu mub<u>aa</u>rakun fa<b>i</b>ttabi\'uuhu wa<b>i</b>ttaquu la\'allakum tur<u>h</u>amuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (945, '6:156', 6, 156, ' an taquuluu innam<u>aa</u> unzila <b>a</b>lkit<u>aa</u>bu \'al<u>aa</u> <u>th</u><u>aa</u>-ifatayni min qablin<u>aa</u> wa-in kunn<u>aa</u> \'an dir<u>aa</u>satihim lagh<u>aa</u>filiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (946, '6:157', 6, 157, ' aw taquuluu law ann<u>aa</u> unzila \'alayn<u>aa</u> <b>a</b>lkit<u>aa</u>bu lakunn<u>aa</u> ahd<u>aa</u> minhum faqad j<u>aa</u>-akum bayyinatun min rabbikum wahudan wara<u>h</u>matun faman a<u>zh</u>lamu mimman ka<u>dzdz</u>aba bi-<u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi wa<u>sh</u>adafa \'anh<u>aa</u> sanajzii <b>al</b>la<u>dz</u>iina ya<u>sh</u>difuuna \'an <u>aa</u>y<u>aa</u>tin<u>aa</u> suu-a <b>a</b>l\'a<u>dzaa</u>bi bim<u>aa</u> k<u>aa</u>nuu ya<u>sh</u>difuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (947, '6:158', 6, 158, ' hal yan<u>zh</u>uruuna ill<u>aa</u> an ta/tiyahumu <b>a</b>lmal<u>aa</u>-ikatu aw ya/tiya rabbuka aw ya/tiya ba\'<u>dh</u>u <u>aa</u>y<u>aa</u>ti rabbika yawma ya/tii ba\'<u>dh</u>u <u>aa</u>y<u>aa</u>ti rabbika l<u>aa</u> yanfa\'u nafsan iim<u>aa</u>nuh<u>aa</u> lam takun <u>aa</u>manat min qablu aw kasabat fii iim<u>aa</u>nih<u>aa</u> khayran quli inta<u>zh</u>iruu inn<u>aa</u> munta<u>zh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (948, '6:159', 6, 159, ' inna <b>al</b>la<u>dz</u>iina farraquu diinahum wak<u>aa</u>nuu syiya\'an lasta minhum fii syay-in innam<u>aa</u> amruhum il<u>aa</u> <b>al</b>l<u>aa</u>hi tsumma yunabbi-uhum bim<u>aa</u> k<u>aa</u>nuu yaf\'aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (949, '6:160', 6, 160, ' man j<u>aa</u>-a bi<b>a</b>l<u>h</u>asanati falahu \'asyru amts<u>aa</u>lih<u>aa</u> waman j<u>aa</u>-a bi<b>al</b>ssayyi-<u>aa</u>ti fal<u>aa</u> yujz<u>aa</u> ill<u>aa</u> mitslah<u>aa</u> wahum l<u>aa</u> yu<u>zh</u>lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (950, '6:161', 6, 161, ' qul innanii had<u>aa</u>nii rabbii il<u>aa</u> <u>sh</u>ir<u>aath</u>in mustaqiimin diinan qiyaman millata ibr<u>aa</u>hiima <u>h</u>aniifan wam<u>aa</u> k<u>aa</u>na mina <b>a</b>lmusyrikiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (951, '6:162', 6, 162, ' qul inna <u>sh</u>al<u>aa</u>tii wanusukii wama<u>h</u>y<u>aa</u>ya wamam<u>aa</u>tii lill<u>aa</u>hi rabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (952, '6:163', 6, 163, ' l<u>aa</u> syariika lahu wabi<u>dzaa</u>lika umirtu wa-an<u>aa</u> awwalu <b>a</b>lmuslimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (953, '6:164', 6, 164, ' qul aghayra <b>al</b>l<u>aa</u>hi abghii rabban wahuwa rabbu kulli syay-in wal<u>aa</u> taksibu kullu nafsin ill<u>aa</u> \'alayh<u>aa</u> wal<u>aa</u> taziru w<u>aa</u>ziratun wizra ukhr<u>aa</u> tsumma il<u>aa</u> rabbikum marji\'ukum fayunabbi-ukum bim<u>aa</u> kuntum fiihi takhtalifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (954, '6:165', 6, 165, ' wahuwa <b>al</b>la<u>dz</u>ii ja\'alakum khal<u>aa</u>-ifa <b>a</b>l-ar<u>dh</u>i warafa\'a ba\'<u>dh</u>akum fawqa ba\'<u>dh</u>in daraj<u>aa</u>tin liyabluwakum fii m<u>aa</u> <u>aa</u>t<u>aa</u>kum inna rabbaka sarii\'u <b>a</b>l\'iq<u>aa</u>bi wa-innahu laghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (955, '7:1', 7, 1, ' alif-l<u>aa</u>m-miim-<u>shaa</u>d</u>\r\r\n');
INSERT INTO quranLatin VALUES (956, '7:2', 7, 2, ' kit<u>aa</u>bun unzila ilayka fal<u>aa</u> yakun fii <u>sh</u>adrika <u>h</u>arajun minhu litun<u>dz</u>ira bihi wa<u>dz</u>ikr<u>aa</u> lilmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (957, '7:3', 7, 3, ' ittabi\'uu m<u>aa</u> unzila ilaykum min rabbikum wal<u>aa</u> tattabi\'uu min duunihi awliy<u>aa</u>-a qaliilan m<u>aa</u> ta<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (958, '7:4', 7, 4, ' wakam min qaryatin ahlakn<u>aa</u>h<u>aa</u> faj<u>aa</u>-ah<u>aa</u> ba/sun<u>aa</u> bay<u>aa</u>tan aw hum q<u>aa</u>-iluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (959, '7:5', 7, 5, ' fam<u>aa</u> k<u>aa</u>na da\'w<u>aa</u>hum i<u>dz</u> j<u>aa</u>-ahum ba/sun<u>aa</u> ill<u>aa</u> an q<u>aa</u>luu inn<u>aa</u> kunn<u>aa</u> <u>zhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (960, '7:6', 7, 6, ' falanas-alanna <b>al</b>la<u>dz</u>iina ursila ilayhim walanas-alanna <b>a</b>lmursaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (961, '7:7', 7, 7, ' falanaqu<u>shsh</u>anna \'alayhim bi\'ilmin wam<u>aa</u> kunn<u>aa</u> gh<u>aa</u>-ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (962, '7:8', 7, 8, ' wa<b>a</b>lwaznu yawma-i<u>dz</u>ini <b>a</b>l<u>h</u>aqqu faman tsaqulat maw<u>aa</u>ziinuhu faul<u>aa</u>-ika humu <b>a</b>lmufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (963, '7:9', 7, 9, ' waman khaffat maw<u>aa</u>ziinuhu faul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina khasiruu anfusahum bim<u>aa</u> k<u>aa</u>nuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> ya<u>zh</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (964, '7:10', 7, 10, ' walaqad makkann<u>aa</u>kum fii <b>a</b>l-ar<u>dh</u>i waja\'aln<u>aa</u> lakum fiih<u>aa</u> ma\'<u>aa</u>yisya qaliilan m<u>aa</u> tasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (965, '7:11', 7, 11, ' walaqad khalaqn<u>aa</u>kum tsumma <u>sh</u>awwarn<u>aa</u>kum tsumma quln<u>aa</u> lilmal<u>aa</u>-ikati usjuduu li-<u>aa</u>dama fasajaduu ill<u>aa</u> ibliisa lam yakun mina <b>al</b>ss<u>aa</u>jidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (966, '7:12', 7, 12, ' q<u>aa</u>la m<u>aa</u> mana\'aka <b>al</b>l<u>aa</u> tasjuda i<u>dz</u> amartuka q<u>aa</u>la an<u>aa</u> khayrun minhu khalaqtanii min n<u>aa</u>rin wakhalaqtahu min <u>th</u>iin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (967, '7:13', 7, 13, ' q<u>aa</u>la fa<b>i</b>hbi<u>th</u> minh<u>aa</u> fam<u>aa</u> yakuunu laka an tatakabbara fiih<u>aa</u> fa<b>u</b>khruj innaka mina <b>al</b><u>shshaa</u>ghiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (968, '7:14', 7, 14, ' q<u>aa</u>la an<u>zh</u>irnii il<u>aa</u> yawmi yub\'atsuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (969, '7:15', 7, 15, ' q<u>aa</u>la innaka mina <b>a</b>lmun<u>zh</u>ariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (970, '7:16', 7, 16, ' q<u>aa</u>la fabim<u>aa</u> aghwaytanii la-aq\'udanna lahum <u>sh</u>ir<u>aath</u>aka <b>a</b>lmustaqiim<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (971, '7:17', 7, 17, ' tsumma la<u>aa</u>tiyannahum min bayni aydiihim wamin khalfihim wa\'an aym<u>aa</u>nihim wa\'an syam<u>aa</u>-ilihim wal<u>aa</u> tajidu aktsarahum sy<u>aa</u>kiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (972, '7:18', 7, 18, ' q<u>aa</u>la ukhruj minh<u>aa</u> ma<u>dz</u>uuman mad<u>h</u>uuran laman tabi\'aka minhum la-amla-anna jahannama minkum ajma\'iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (973, '7:19', 7, 19, ' way<u>aa</u> <u>aa</u>damu uskun anta wazawjuka <b>a</b>ljannata fakul<u>aa</u> min <u>h</u>aytsu syi/tum<u>aa</u> wal<u>aa</u> taqrab<u>aa</u> h<u>aadz</u>ihi <b>al</b>sysyajarata fatakuun<u>aa</u> mina <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (974, '7:20', 7, 20, ' fawaswasa lahum<u>aa</u> <b>al</b>sysyay<u>th</u><u>aa</u>nu liyubdiya lahum<u>aa</u> m<u>aa</u> wuuriya \'anhum<u>aa</u> min saw-<u>aa</u>tihim<u>aa</u> waq<u>aa</u>la m<u>aa</u> nah<u>aa</u>kum<u>aa</u> rabbukum<u>aa</u> \'an h<u>aadz</u>ihi <b>al</b>sysyajarati ill<u>aa</u> an takuun<u>aa</u> malakayni aw takuun<u>aa</u> mina <b>a</b>lkh<u>aa</u>lidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (975, '7:21', 7, 21, ' waq<u>aa</u>samahum<u>aa</u> innii lakum<u>aa</u> lamina <b>al</b>nn<u>aas</u>i<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (976, '7:22', 7, 22, ' fadall<u>aa</u>hum<u>aa</u> bighuruurin falamm<u>aa</u> <u>dzaa</u>q<u>aa</u> <b>al</b>sysyajarata badat lahum<u>aa</u> saw-<u>aa</u>tuhum<u>aa</u> wa<u>th</u>afiq<u>aa</u> yakh<u>sh</u>if<u>aa</u>ni \'alayhim<u>aa</u> min waraqi aljannati wan<u>aa</u>d<u>aa</u>hum<u>aa</u> rabbuhum<u>aa</u> alam anhakum<u>aa</u> \'an tilkum<u>aa</u> <b>al</b>sysyajarati wa-aqul lakum<u>aa</u> inna <b>al</b>sysyay<u>th</u><u>aa</u>na lakum<u>aa</u> \'aduwwun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (977, '7:23', 7, 23, ' q<u>aa</u>l<u>aa</u> rabban<u>aa</u> <u>zh</u>alamn<u>aa</u> anfusan<u>aa</u> wa-in lam taghfir lan<u>aa</u> watar<u>h</u>amn<u>aa </u>lanakuunanna mina <b>a</b>lkh<u>aa</u>siriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (978, '7:24', 7, 24, ' q<u>aa</u>la ihbi<u>th</u>uu ba\'<u>dh</u>ukum liba\'<u>dh</u>in \'aduwwun walakum fii <b>a</b>l-ar<u>dh</u>i mustaqarrun wamat<u>aa</u>\'un il<u>aa</u> <u>h</u>iin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (979, '7:25', 7, 25, ' q<u>aa</u>la fiih<u>aa</u> ta<u>h</u>yawna wafiih<u>aa</u> tamuutuuna waminh<u>aa</u> tukhrajuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (980, '7:26', 7, 26, ' y<u>aa</u> banii <u>aa</u>dama qad anzaln<u>aa</u> \'alaykum lib<u>aa</u>san yuw<u>aa</u>rii saw-<u>aa</u>tikum wariisyan walib<u>aa</u>su <b>al</b>ttaqw<u>aa</u> <u>dzaa</u>lika khayrun <u>dzaa</u>lika min <u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi la\'allahum ya<u>dzdz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (981, '7:27', 7, 27, ' y<u>aa</u> banii <u>aa</u>dama l<u>aa</u> yaftinannakumu <b>al</b>sysyay<u>th</u><u>aa</u>nu kam<u>aa</u> akhraja abawaykum mina <b>a</b>ljannati yanzi\'u \'anhum<u>aa</u> lib<u>aa</u>sahum<u>aa</u> liyuriyahum<u>aa</u> saw-<u>aa</u>tihim<u>aa</u> innahu yar<u>aa</u>kum huwa waqabiiluhu min <u>h</u>aytsu l<u>aa</u> tarawnahum inn<u>aa</u> ja\'aln<u>aa</u> <b>al</b>sysyay<u>aath</u>iina awliy<u>aa</u>-a lilla<u>dz</u>iina l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (982, '7:28', 7, 28, ' wa-i<u>dzaa</u> fa\'aluu f<u>aah</u>isyatan q<u>aa</u>luu wajadn<u>aa</u> \'alayh<u>aa</u> <u>aa</u>b<u>aa</u>-an<u>aa</u> wa<b>al</b>l<u>aa</u>hu amaran<u>aa</u> bih<u>aa</u> qul inna <b>al</b>l<u>aa</u>ha l<u>aa</u> ya/muru bi<b>a</b>lfa<u>h</u>sy<u>aa</u>-i ataquuluuna \'al<u>aa</u> <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (983, '7:29', 7, 29, ' qul amara rabbii bi<b>a</b>lqis<u>th</u>i wa-aqiimuu wujuuhakum \'inda kulli masjidin wa<b>u</b>d\'uuhu mukhli<u>sh</u>iina lahu <b>al</b>ddiina kam<u>aa</u> bada-akum ta\'uuduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (984, '7:30', 7, 30, ' fariiqan had<u>aa</u> wafariiqan <u>h</u>aqqa \'alayhimu <b>al</b><u>dhdh</u>al<u>aa</u>latu innahumu ittakha<u>dz</u>uu <b>al</b>sysyay<u>aath</u>iina awliy<u>aa</u>-a min duuni <b>al</b>l<u>aa</u>hi waya<u>h</u>sabuuna annahum muhtaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (985, '7:31', 7, 31, ' y<u>aa</u> banii <u>aa</u>dama khu<u>dz</u>uu ziinatakum \'inda kulli masjidin wakuluu wa<b>i</b>syrabuu wal<u>aa</u> tusrifuu innahu l<u>aa</u> yu<u>h</u>ibbu <b>a</b>lmusrifiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (986, '7:32', 7, 32, ' qul man <u>h</u>arrama ziinata <b>al</b>l<u>aa</u>hi <b>al</b>latii akhraja li\'ib<u>aa</u>dihi wa<b>al</b><u>ththh</u>ayyib<u>aa</u>ti mina <b>al</b>rrizqi qul hiya lilla<u>dz</u>iina <u>aa</u>manuu fii <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> kh<u>aa</u>li<u>sh</u>atan yawma <b>a</b>lqiy<u>aa</u>mati ka<u>dzaa</u>lika nufa<u>shsh</u>ilu <b>a</b>l-<u>aa</u>y<u>aa</u>ti liqawmin ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (987, '7:33', 7, 33, ' qul innam<u>aa</u> <u>h</u>arrama rabbiya <b>a</b>lfaw<u>aah</u>isya m<u>aa</u> <u>zh</u>ahara minh<u>aa</u> wam<u>aa</u> ba<u>th</u>ana wa<b>a</b>l-itsma wa<b>a</b>lbaghya bighayri <b>a</b>l<u>h</u>aqqi wa-an tusyrikuu bi<b>al</b>l<u>aa</u>hi m<u>aa</u> lam yunazzil bihi sul<u>th</u><u>aa</u>nan wa-an taquuluu \'al<u>aa</u> <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (988, '7:34', 7, 34, ' walikulli ummatin ajalun fa-i<u>dzaa</u> j<u>aa</u>-a ajaluhum l<u>aa</u> yasta/khiruuna s<u>aa</u>\'atan wal<u>aa</u> yastaqdimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (989, '7:35', 7, 35, ' y<u>aa</u> banii <u>aa</u>dama imm<u>aa</u> ya/tiyannakum rusulun minkum yaqu<u>shsh</u>uuna \'alaykum <u>aa</u>y<u>aa</u>tii famani ittaq<u>aa</u> wa-a<u>sh</u>la<u>h</u>a fal<u>aa</u> khawfun \'alayhim wal<u>aa</u> hum ya<u>h</u>zanuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (990, '7:36', 7, 36, ' wa<b>a</b>lla<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> wa<b>i</b>stakbaruu \'anh<u>aa</u> ul<u>aa</u>-ika a<u>sh</u>-<u>haa</u>bu <b>al</b>nn<u>aa</u>ri hum fiih<u>aa</u> kh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (991, '7:37', 7, 37, ' faman a<u>zh</u>lamu mimmani iftar<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ka<u>dz</u>iban aw ka<u>dzdz</u>aba bi-<u>aa</u>y<u>aa</u>tihi ul<u>aa</u>-ika yan<u>aa</u>luhum na<u>sh</u>iibuhum mina <b>a</b>lkit<u>aa</u>bi <u>h</u>att<u>aa</u> i<u>dzaa</u> j<u>aa</u>-at-hum rusulun<u>aa</u> yatawaffawnahum q<u>aa</u>luu ayna m<u>aa</u> kuntum tad\'uuna min duuni <b>al</b>l<u>aa</u>hi q<u>aa</u>luu <u>dh</u>alluu \'ann<u>aa</u> wasyahiduu \'al<u>aa</u> anfusihim annahum k<u>aa</u>nuu k<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (992, '7:38', 7, 38, ' q<u>aa</u>la udkhuluu fii umamin qad khalat min qablikum mina <b>a</b>ljinni wa<b>a</b>l-insi fii <b>al</b>nn<u>aa</u>ri kullam<u>aa</u> dakhalat ummatun la\'anat ukhtah<u>aa</u> <u>h</u>att<u>aa</u> i<u>dzaa</u> idd<u>aa</u>rakuu fiih<u>aa</u> jamii\'an q<u>aa</u>lat ukhr<u>aa</u>hum li-uul<u>aa</u>hum rabban<u>aa</u> h<u>aa</u>ul<u>aa</u>-i a<u>dh</u>alluun<u>aa</u> fa<u>aa</u>tihim \'a<u>dzaa</u>ban <u>dh</u>i\'fan mina <b>al</b>nn<u>aa</u>ri q<u>aa</u>la likullin <u>dh</u>i\'fun wal<u>aa</u>kin l<u>aa</u> ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (993, '7:39', 7, 39, ' waq<u>aa</u>lat uul<u>aa</u>hum li-ukhr<u>aa</u>hum fam<u>aa</u> k<u>aa</u>na lakum \'alayn<u>aa</u> min fa<u>dh</u>lin fa<u>dz</u>uuquu <b>a</b>l\'a<u>dzaa</u>ba bim<u>aa</u> kuntum taksibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (994, '7:40', 7, 40, ' inna <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> wa<b>i</b>stakbaruu \'anh<u>aa</u> l<u>aa</u> tufatta<u>h</u>u lahum abw<u>aa</u>bu <b>al</b>ssam<u>aa</u>-i wal<u>aa</u> yadkhuluuna <b>a</b>ljannata <u>h</u>att<u>aa</u> yalija <b>a</b>ljamalu fii sammi <b>a</b>lkhiy<u>aath</u>i waka<u>dzaa</u>lika najzii <b>a</b>lmujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (995, '7:41', 7, 41, ' lahum min jahannama mih<u>aa</u>dun wamin fawqihim ghaw<u>aa</u>syin waka<u>dzaa</u>lika najzii <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (996, '7:42', 7, 42, ' wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti l<u>aa</u> nukallifu nafsan ill<u>aa</u> wus\'ah<u>aa</u> ul<u>aa</u>-ika a<u>sh</u>-<u>haa</u>bu <b>a</b>ljannati hum fiih<u>aa</u> kh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (997, '7:43', 7, 43, ' wanaza\'n<u>aa</u> m<u>aa</u> fii <u>sh</u>uduurihim min ghillin tajrii min ta<u>h</u>tihimu <b>a</b>l-anh<u>aa</u>ru waq<u>aa</u>luu <b>a</b>l<u>h</u>amdu lill<u>aa</u>hi <b>al</b>la<u>dz</u>ii had<u>aa</u>n<u>aa</u> lih<u>aadzaa</u> wam<u>aa</u> kunn<u>aa</u> linahtadiya lawl<u>aa</u> an had<u>aa</u>n<u>aa</u> <b>al</b>l<u>aa</u>hu laqad j<u>aa</u>-at rusulu rabbin<u>aa</u> bi<b>a</b>l<u>h</u>aqqi wanuuduu an tilkumu <b>a</b>ljannatu uuritstumuuh<u>aa</u> bim<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (998, '7:44', 7, 44, ' wan<u>aa</u>d<u>aa</u> a<u>sh</u>-<u>haa</u>bu <b>a</b>ljannati a<u>sh</u>-<u>haa</u>ba <b>al</b>nn<u>aa</u>ri an qad wajadn<u>aa</u> m<u>aa</u> wa\'adan<u>aa</u> rabbun<u>aa</u> <u>h</u>aqqan fahal wajadtum m<u>aa</u> wa\'ada rabbukum <u>h</u>aqqan q<u>aa</u>luu na\'am fa-a<u>dzdz</u>ana mu-a<u>dzdz</u>inun baynahum an la\'natu <b>al</b>l<u>aa</u>hi \'al<u>aa</u> <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (999, '7:45', 7, 45, ' <b>al</b>la<u>dz</u>iina ya<u>sh</u>udduuna \'an sabiili <b>al</b>l<u>aa</u>hi wayabghuunah<u>aa</u> \'iwajan wahum bi<b>a</b>l-<u>aa</u>khirati k<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1000, '7:46', 7, 46, ' wabaynahum<u>aa</u> <u>h</u>ij<u>aa</u>bun wa\'al<u>aa</u> <b>a</b>l-a\'r<u>aa</u>fi rij<u>aa</u>lun ya\'rifuuna kullan bisiim<u>aa</u>hum wan<u>aa</u>daw a<u>sh</u>-<u>haa</u>ba <b>a</b>ljannati an sal<u>aa</u>mun \'alaykum lam yadkhuluuh<u>aa</u> wahum ya<u>th</u>ma\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1001, '7:47', 7, 47, ' wa-i<u>dzaa</u> <u>sh</u>urifat ab<u>shaa</u>ruhum tilq<u>aa</u>-a a<u>sh</u>-<u>haa</u>bi <b>al</b>nn<u>aa</u>ri q<u>aa</u>luu rabban<u>aa</u> l<u>aa</u> taj\'aln<u>aa</u> ma\'a <b>a</b>lqawmi <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1002, '7:48', 7, 48, ' wan<u>aa</u>d<u>aa</u> a<u>sh</u>-<u>haa</u>bu <b>a</b>l-a\'r<u>aa</u>fi rij<u>aa</u>lan ya\'rifuunahum bisiim<u>aa</u>hum q<u>aa</u>luu m<u>aa</u> aghn<u>aa</u> \'ankum jam\'ukum wam<u>aa</u> kuntum tastakbiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1003, '7:49', 7, 49, ' ah<u>aa</u>ul<u>aa</u>-i <b>al</b>la<u>dz</u>iina aqsamtum l<u>aa</u> yan<u>aa</u>luhumu <b>al</b>l<u>aa</u>hu bira<u>h</u>matin udkhuluu <b>a</b>ljannata l<u>aa</u> khawfun \'alaykum wal<u>aa</u> antum ta<u>h</u>zanuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1004, '7:50', 7, 50, ' wan<u>aa</u>d<u>aa</u> a<u>sh</u>-<u>haa</u>bu <b>al</b>n<u>aa</u>ri a<u>sh</u>-<u>haa</u>ba <b>a</b>ljannati an afii<u>dh</u>uu \'alayn<u>aa</u> mina <b>a</b>lm<u>aa</u>-i aw mimm<u>aa</u> razaqakumu <b>al</b>l<u>aa</u>hu q<u>aa</u>luu inna <b>al</b>l<u>aa</u>ha <u>h</u>arramahum<u>aa</u> \'al<u>aa</u> <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1005, '7:51', 7, 51, ' <b>al</b>la<u>dz</u>iina ittakha<u>dz</u>uu diinahum lahwan wala\'iban wagharrat-humu <b>a</b>l<u>h</u>ay<u>aa</u>tu <b>al</b>dduny<u>aa</u> fa<b>a</b>lyawma nans<u>aa</u>hum kam<u>aa</u> nasuu liq<u>aa</u>-a yawmihim h<u>aadzaa</u> wam<u>aa</u> k<u>aa</u>nuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> yaj<u>h</u>aduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1006, '7:52', 7, 52, ' walaqad ji/n<u>aa</u>hum bikit<u>aa</u>bin fa<u>shsh</u>aln<u>aa</u>hu \'al<u>aa</u> \'ilmin hudan wara<u>h</u>matan liqawmin yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1007, '7:53', 7, 53, ' hal yan<u>zh</u>uruuna ill<u>aa</u> ta/wiilahu yawma ya/tii ta/wiiluhu yaquulu <b>al</b>la<u>dz</u>iina nasuuhu min qablu qad j<u>aa</u>-at rusulu rabbin<u>aa</u> bi<b>a</b>l<u>h</u>aqqi fahal lan<u>aa</u> min syufa\'<u>aa</u>-a fayasyfa\'uu lan<u>aa</u> aw nuraddu fana\'mala ghayra <b>al</b>la<u>dz</u>ii kunn<u>aa</u> na\'malu qad khasiruu anfusahum wa<u>dh</u>alla \'anhum m<u>aa</u> k<u>aa</u>nuu yaftaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1008, '7:54', 7, 54, ' inna rabbakumu <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a fii sittati ayy<u>aa</u>min tsumma istaw<u>aa</u> \'al<u>aa</u> <b>a</b>l\'arsyi yughsyii <b>al</b>layla <b>al</b>nnah<u>aa</u>ra ya<u>th</u>lubuhu <u>h</u>atsiitsan wa<b>al</b>sysyamsa wa<b>a</b>lqamara wa<b>al</b>nnujuuma musakhkhar<u>aa</u>tin bi-amrihi <b>a</b>l<u>aa</u> lahu <b>a</b>lkhalqu wa<b>a</b>l-amru tab<u>aa</u>raka <b>al</b>l<u>aa</u>hu rabbu <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1009, '7:55', 7, 55, ' ud\'uu rabbakum ta<u>dh</u>arru\'an wakhufyatan innahu l<u>aa</u> yu<u>h</u>ibbu <b>a</b>lmu\'tadiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1010, '7:56', 7, 56, ' wal<u>aa</u> tufsiduu fii <b>a</b>l-ar<u>dh</u>i ba\'da i<u>sh</u>l<u>aah</u>ih<u>aa</u> wa<b>u</b>d\'uuhu khawfan wa<u>th</u>ama\'an inna ra<u>h</u>mata <b>al</b>l<u>aa</u>hi qariibun mina <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1011, '7:57', 7, 57, ' wahuwa <b>al</b>la<u>dz</u>ii yursilu <b>al</b>rriy<u>aah</u>a busyran bayna yaday ra<u>h</u>matihi <u>h</u>att<u>aa</u> i<u>dzaa</u> aqallat sa<u>haa</u>ban tsiq<u>aa</u>lan suqn<u>aa</u>hu libaladin mayyitin fa-anzaln<u>aa</u> bihi <b>a</b>lm<u>aa</u>-a fa-akhrajn<u>aa</u> bihi min kulli <b>al</b>tstsamar<u>aa</u>ti ka<u>dzaa</u>lika nukhriju <b>a</b>lmawt<u>aa</u> la\'allakum ta<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1012, '7:58', 7, 58, ' wa<b>a</b>lbaladu <b>al</b><u>ththh</u>ayyibu yakhruju nab<u>aa</u>tuhu bi-i<u>dz</u>ni rabbihi wa<b>a</b>lla<u>dz</u>ii khabutsa l<u>aa</u> yakhruju ill<u>aa</u> nakidan ka<u>dzaa</u>lika nu<u>sh</u>arrifu <b>a</b>l-<u>aa</u>y<u>aa</u>ti liqawmin yasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1013, '7:59', 7, 59, ' laqad arsaln<u>aa</u> nuu<u>h</u>an il<u>aa</u> qawmihi faq<u>aa</u>la y<u>aa</u> qawmi u\'buduu <b>al</b>l<u>aa</u>ha m<u>aa</u> lakum min il<u>aa</u>hin ghayruhu innii akh<u>aa</u>fu \'alaykum \'a<u>dzaa</u>ba yawmin \'a<u>zh</u>iim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1014, '7:60', 7, 60, ' q<u>aa</u>la <b>a</b>lmalau min qawmihi inn<u>aa</u> lanar<u>aa</u>ka fii <u>dh</u>al<u>aa</u>lin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1015, '7:61', 7, 61, ' q<u>aa</u>la y<u>aa</u> qawmi laysa bii <u>dh</u>al<u>aa</u>latun wal<u>aa</u>kinnii rasuulun min rabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1016, '7:62', 7, 62, ' uballighukum ris<u>aa</u>l<u>aa</u>ti rabbii wa-an<u>sh</u>a<u>h</u>u lakum wa-a\'lamu mina <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1017, '7:63', 7, 63, ' awa \'ajibtum an j<u>aa</u>-akum <u>dz</u>ikrun min rabbikum \'al<u>aa</u> rajulin minkum liyun<u>dz</u>irakum walitattaquu wala\'allakum tur<u>h</u>amuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1018, '7:64', 7, 64, ' faka<u>dzdz</u>abuuhu fa-anjayn<u>aa</u>hu wa<b>a</b>lla<u>dz</u>iina ma\'ahu fii <b>a</b>lfulki wa-aghraqn<u>aa</u> <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> innahum k<u>aa</u>nuu qawman \'amiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1019, '7:65', 7, 65, ' wa-il<u>aa</u> \'<u>aa</u>din akh<u>aa</u>hum huudan q<u>aa</u>la y<u>aa</u> qawmi u\'buduu <b>al</b>l<u>aa</u>ha m<u>aa</u> lakum min il<u>aa</u>hin ghayruhu afal<u>aa</u> tattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1020, '7:66', 7, 66, ' q<u>aa</u>la <b>a</b>lmalau <b>al</b>la<u>dz</u>iina kafaruu min qawmihi inn<u>aa</u> lanar<u>aa</u>ka fii saf<u>aa</u>hatin wa-inn<u>aa</u> lana<u>zh</u>unnuka mina <b>a</b>lk<u>aadz</u>ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1021, '7:67', 7, 67, ' q<u>aa</u>la y<u>aa</u> qawmi laysa bii saf<u>aa</u>hatun wal<u>aa</u>kinnii rasuulun min rabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1022, '7:68', 7, 68, ' uballighukum ris<u>aa</u>l<u>aa</u>ti rabbii wa-an<u>aa</u> lakum n<u>aas</u>i<u>h</u>un amiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1023, '7:69', 7, 69, ' awa \'ajibtum an j<u>aa</u>-akum <u>dz</u>ikrun min rabbikum \'al<u>aa</u> rajulin minkum liyun<u>dz</u>irakum wa<b>u</b><u>dz</u>kuruu i<u>dz</u> ja\'alakum khulaf<u>aa</u>-a min ba\'di qawmi nuu<u>h</u>in waz<u>aa</u>dakum fii <b>a</b>lkhalqi bas<u>th</u>atan fa<b>u</b><u>dz</u>kuruu <u>aa</u>l<u>aa</u>-a <b>al</b>l<u>aa</u>hi la\'allakum tufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1024, '7:70', 7, 70, ' q<u>aa</u>luu aji/tan<u>aa</u> lina\'buda <b>al</b>l<u>aa</u>ha wa<u>h</u>dahu wana<u>dz</u>ara m<u>aa</u> k<u>aa</u>na ya\'budu <u>aa</u>b<u>aa</u>un<u>aa</u> fa/tin<u>aa</u> bim<u>aa</u> ta\'idun<u>aa</u> in kunta mina <b>al</b><u>shshaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1025, '7:71', 7, 71, ' q<u>aa</u>la qad waqa\'a \'alaykum min rabbikum rijsun wagha<u>dh</u>abun atuj<u>aa</u>diluunanii fii asm<u>aa</u>-in sammaytumuuh<u>aa</u> antum wa<u>aa</u>b<u>aa</u>ukum m<u>aa</u> nazzala <b>al</b>l<u>aa</u>hu bih<u>aa</u> min sul<u>th</u><u>aa</u>nin fa<b>i</b>nta<u>zh</u>iruu innii ma\'akum mina <b>a</b>lmunta<u>zh</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1026, '7:72', 7, 72, ' fa-anjayn<u>aa</u>hu wa<b>a</b>lla<u>dz</u>iina ma\'ahu bira<u>h</u>matin minn<u>aa</u> waqa<u>th</u>a\'n<u>aa</u> d<u>aa</u>bira <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> wam<u>aa</u> k<u>aa</u>nuu mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1027, '7:73', 7, 73, ' wa-il<u>aa</u> tsamuuda akh<u>aa</u>hum <u>shaa</u>li<u>h</u>an q<u>aa</u>la y<u>aa</u> qawmi u\'buduu <b>al</b>l<u>aa</u>ha m<u>aa</u> lakum min il<u>aa</u>hin ghayruhu qad j<u>aa</u>-atkum bayyinatun min rabbikum h<u>aadz</u>ihi n<u>aa</u>qatu <b>al</b>l<u>aa</u>hi lakum <u>aa</u>yatan fa<u>dz</u>aruuh<u>aa</u> ta/kul fii ar<u>dh</u>i <b>al</b>l<u>aa</u>hi wal<u>aa</u> tamassuuh<u>aa</u> bisuu-in faya/khu<u>dz</u>akum \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1028, '7:74', 7, 74, ' wa<b>u</b><u>dz</u>kuruu i<u>dz</u> ja\'alakum khulaf<u>aa</u>-a min ba\'di \'<u>aa</u>din wabawwa-akum fii <b>a</b>l-ar<u>dh</u>i tattakhi<u>dz</u>uuna min suhuulih<u>aa</u> qu<u>sh</u>uuran watan<u>h</u>ituuna <b>a</b>ljib<u>aa</u>la buyuutan fa<b>u</b><u>dz</u>kuruu <u>aa</u>l<u>aa</u>-a <b>al</b>l<u>aa</u>hi wal<u>aa</u> ta\'tsaw fii <b>a</b>l-ar<u>dh</u>i mufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1029, '7:75', 7, 75, ' q<u>aa</u>la <b>a</b>lmalau <b>al</b>la<u>dz</u>iina istakbaruu min qawmihi lilla<u>dz</u>iina istu<u>dh</u>\'ifuu liman <u>aa</u>mana minhum ata\'lamuuna anna <u>shaa</u>li<u>h</u>an mursalun min rabbihi q<u>aa</u>luu inn<u>aa</u> bim<u>aa</u> ursila bihi mu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1030, '7:76', 7, 76, ' q<u>aa</u>la <b>al</b>la<u>dz</u>iina istakbaruu inn<u>aa</u> bi<b>a</b>lla<u>dz</u>ii <u>aa</u>mantum bihi k<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1031, '7:77', 7, 77, ' fa\'aqaruu <b>al</b>nn<u>aa</u>qata wa\'ataw \'an amri rabbihim waq<u>aa</u>luu y<u>aa</u> <u>shaa</u>li<u>h</u>u i/tin<u>aa</u> bim<u>aa</u> ta\'idun<u>aa</u> in kunta mina <b>a</b>lmursaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1032, '7:78', 7, 78, ' fa-akha<u>dz</u>at-humu <b>al</b>rrajfatu fa-a<u>sh</u>ba<u>h</u>uu fii d<u>aa</u>rihim j<u>aa</u>tsimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1033, '7:79', 7, 79, ' fatawall<u>aa</u> \'anhum waq<u>aa</u>la y<u>aa</u> qawmi laqad ablaghtukum ris<u>aa</u>lata rabbii wana<u>sh</u>a<u>h</u>tu lakum wal<u>aa</u>kin l<u>aa</u> tu<u>h</u>ibbuuna <b>al</b>nn<u>aas</u>i<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1034, '7:80', 7, 80, ' waluu<u>th</u>an i<u>dz</u> q<u>aa</u>la liqawmihi ata/tuuna <b>a</b>lf<u>aah</u>isyata m<u>aa</u> sabaqakum bih<u>aa</u> min a<u>h</u>adin mina <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1035, '7:81', 7, 81, ' innakum lata/tuuna <b>al</b>rrij<u>aa</u>la syahwatan min duuni <b>al</b>nnis<u>aa</u>-i bal antum qawmun musrifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1036, '7:82', 7, 82, ' wam<u>aa</u> k<u>aa</u>na jaw<u>aa</u>ba qawmihi ill<u>aa</u> an q<u>aa</u>luu akhrijuuhum min qaryatikum innahum un<u>aa</u>sun yata<u>th</u>ahharuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1037, '7:83', 7, 83, ' fa-anjayn<u>aa</u>hu wa-ahlahu ill<u>aa</u> imra-atahu k<u>aa</u>nat mina <b>a</b>lgh<u>aa</u>biriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1038, '7:84', 7, 84, ' wa-am<u>th</u>arn<u>aa</u> \'alayhim ma<u>th</u>aran fa<b>u</b>n<u>zh</u>ur kayfa k<u>aa</u>na \'<u>aa</u>qibatu <b>a</b>lmujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1039, '7:85', 7, 85, ' wa-il<u>aa</u> madyana akh<u>aa</u>hum syu\'ayban q<u>aa</u>la y<u>aa</u> qawmi u\'buduu <b>al</b>l<u>aa</u>ha m<u>aa</u> lakum min il<u>aa</u>hin ghayruhu qad j<u>aa</u>-atkum bayyinatun min rabbikum fa-awfuu <b>a</b>lkayla wa<b>a</b>lmiiz<u>aa</u>na wal<u>aa</u> tabkhasuu <b>al</b>nn<u>aa</u>sa asyy<u>aa</u>-ahum wal<u>aa</u> tufsiduu fii <b>a</b>l-ar<u>dh</u>i ba\'da i<u>sh</u>l<u>aah</u>ih<u>aa</u> <u>dzaa</u>likum khayrun lakum in kuntum mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1040, '7:86', 7, 86, ' wal<u>aa</u> taq\'uduu bikulli <u>sh</u>ir<u>aath</u>in tuu\'iduuna wata<u>sh</u>udduuna \'an sabiili <b>al</b>l<u>aa</u>hi man <u>aa</u>mana bihi watabghuunah<u>aa</u> \'iwajan wa<b>u</b><u>dz</u>kuruu i<u>dz</u> kuntum qaliilan fakatstsarakum wa<b>u</b>n<u>zh</u>uruu kayfa k<u>aa</u>na \'<u>aa</u>qibatu <b>a</b>lmufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1041, '7:87', 7, 87, ' wa-in k<u>aa</u>na <u>th</u><u>aa</u>-ifatun minkum <u>aa</u>manuu bi<b>a</b>lla<u>dz</u>ii ursiltu bihi wa<u>th</u><u>aa</u>-ifatun lam yu/minuu fa<b>i</b><u>sh</u>biruu <u>h</u>att<u>aa</u> ya<u>h</u>kuma <b>al</b>l<u>aa</u>hu baynan<u>aa</u> wahuwa khayru <b>a</b>l<u>haa</u>kimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1042, '7:88', 7, 88, ' q<u>aa</u>la <b>a</b>lmalau <b>al</b>la<u>dz</u>iina istakbaruu min qawmihi lanukhrijannaka y<u>aa</u> syu\'aybu wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu ma\'aka min qaryatin<u>aa</u> aw lata\'uudunna fii millatin<u>aa</u> q<u>aa</u>la awa law kunn<u>aa</u> k<u>aa</u>rihiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1043, '7:89', 7, 89, ' qadi iftarayn<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ka<u>dz</u>iban in \'udn<u>aa</u> fii millatikum ba\'da i<u>dz</u> najj<u>aa</u>n<u>aa</u> <b>al</b>l<u>aa</u>hu minh<u>aa</u> wam<u>aa</u> yakuunu lan<u>aa</u> an na\'uuda fiih<u>aa</u> ill<u>aa</u> an yasy<u>aa</u>-a <b>al</b>l<u>aa</u>hu rabbun<u>aa</u> wasi\'a rabbun<u>aa</u> kulla syay-in \'ilman \'al<u>aa</u> <b>al</b>l<u>aa</u>hi tawakkaln<u>aa</u> rabban<u>aa</u> ifta<u>h</u> baynan<u>aa</u> wabayna qawmin<u>aa</u> bi<b>a</b>l<u>h</u>aqqi wa-anta khayru <b>a</b>lf<u>aa</u>ti<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1044, '7:90', 7, 90, ' waq<u>aa</u>la <b>a</b>lmalau <b>al</b>la<u>dz</u>iina kafaruu min qawmihi la-ini ittaba\'tum syu\'ayban innakum i<u>dz</u>an lakh<u>aa</u>siruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1045, '7:91', 7, 91, ' fa-akha<u>dz</u>at-humu <b>al</b>rrajfatu fa-a<u>sh</u>ba<u>h</u>uu fii d<u>aa</u>rihim j<u>aa</u>tsimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1046, '7:92', 7, 92, ' <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu syu\'ayban ka-an lam yaghnaw fiih<u>aa</u> <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu syu\'ayban k<u>aa</u>nuu humu <b>a</b>lkh<u>aa</u>siriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1047, '7:93', 7, 93, ' fatawall<u>aa</u> \'anhum waq<u>aa</u>la y<u>aa</u> qawmi laqad ablaghtukum ris<u>aa</u>l<u>aa</u>ti rabbii wana<u>sh</u>a<u>h</u>tu lakum fakayfa <u>aa</u>s<u>aa</u> \'al<u>aa</u> qawmin k<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1048, '7:94', 7, 94, ' wam<u>aa</u> arsaln<u>aa</u> fii qaryatin min nabiyyin ill<u>aa</u> akha<u>dz</u>n<u>aa</u> ahlah<u>aa</u> bi<b>a</b>lba/s<u>aa</u>-i wa<b>al</b><u>dhdh</u>arr<u>aa</u>-i la\'allahum ya<u>dhdh</u>arra\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1049, '7:95', 7, 95, ' tsumma baddaln<u>aa</u> mak<u>aa</u>na <b>al</b>ssayyi-ati <b>a</b>l<u>h</u>asanata <u>h</u>att<u>aa</u> \'afaw waq<u>aa</u>luu qad massa <u>aa</u>b<u>aa</u>-an<u>aa</u> <b>al</b><u>dhdh</u>arr<u>aa</u>u wa<b>al</b>ssarr<u>aa</u>u fa-akha<u>dz</u>n<u>aa</u>hum baghtatan wahum l<u>aa</u> yasy\'uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1050, '7:96', 7, 96, ' walaw anna ahla <b>a</b>lqur<u>aa</u> <u>aa</u>manuu wa<b>i</b>ttaqaw lafata<u>h</u>n<u>aa</u> \'alayhim barak<u>aa</u>tin mina <b>al</b>ssam<u>aa</u>-i wa<b>a</b>l-ar<u>dh</u>i wal<u>aa</u>kin ka<u>dzdz</u>abuu fa-akha<u>dz</u>n<u>aa</u>hum bim<u>aa</u> k<u>aa</u>nuu yaksibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1051, '7:97', 7, 97, ' afa-amina ahlu <b>a</b>lqur<u>aa</u> an ya/tiyahum ba/sun<u>aa</u> bay<u>aa</u>tan wahum n<u>aa</u>-imuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1052, '7:98', 7, 98, ' awa amina ahlu <b>a</b>lqur<u>aa</u> an ya/tiyahum ba/sun<u>aa</u> <u>dh</u>u<u>h</u>an wahum yal\'abuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1053, '7:99', 7, 99, ' afa-aminuu makra <b>al</b>l<u>aa</u>hi fal<u>aa</u> ya/manu makra <b>al</b>l<u>aa</u>hi ill<u>aa</u> <b>a</b>lqawmu <b>a</b>lkh<u>aa</u>siruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1054, '7:100', 7, 100, ' awa lam yahdi lilla<u>dz</u>iina yaritsuuna <b>a</b>l-ar<u>dh</u>a min ba\'di ahlih<u>aa</u> an law nasy<u>aa</u>u a<u>sh</u>abn<u>aa</u>hum bi<u>dz</u>unuubihim wana<u>th</u>ba\'u \'al<u>aa</u> quluubihim fahum l<u>aa</u> yasma\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1055, '7:101', 7, 101, ' tilka <b>a</b>lqur<u>aa</u> naqu<u>shsh</u>u \'alayka min anb<u>aa</u>-ih<u>aa</u> walaqad j<u>aa</u>-at-hum rusuluhum bi<b>a</b>lbayyin<u>aa</u>ti fam<u>aa</u> k<u>aa</u>nuu liyu/minuu bim<u>aa</u> ka<u>dzdz</u>abuu min qablu ka<u>dzaa</u>lika ya<u>th</u>ba\'u <b>al</b>l<u>aa</u>hu \'al<u>aa</u> quluubi <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1056, '7:102', 7, 102, ' wam<u>aa</u> wajadn<u>aa</u> li-aktsarihim min \'ahdin wa-in wajadn<u>aa</u> aktsarahum laf<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1057, '7:103', 7, 103, ' tsumma ba\'atsn<u>aa</u> min ba\'dihim muus<u>aa</u> bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> il<u>aa</u> fir\'awna wamala-ihi fa<u>zh</u>alamuu bih<u>aa</u> fa<b>u</b>nu<u>zh</u>ur kayfa k<u>aa</u>na \'<u>aa</u>qibatu <b>a</b>lmufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1058, '7:104', 7, 104, ' waq<u>aa</u>la muus<u>aa</u> y<u>aa</u> fir\'awnu innii rasuulun min rabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1059, '7:105', 7, 105, ' <u>h</u>aqiiqun \'al<u>aa</u> an l<u>aa</u> aquula \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ill<u>aa</u> <b>a</b>l<u>h</u>aqqa qad ji/tukum bibayyinatin min rabbikum fa-arsil ma\'iya banii isr<u>aa</u>-iil<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1060, '7:106', 7, 106, ' q<u>aa</u>la in kunta ji/ta bi-<u>aa</u>yatin fa/ti bih<u>aa</u> in kunta mina <b>al</b><u>shshaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1061, '7:107', 7, 107, ' fa-alq<u>aa</u> \'a<u>shaa</u>hu fa-i<u>dzaa</u> hiya tsu\'b<u>aa</u>nun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1062, '7:108', 7, 108, ' wanaza\'a yadahu fa-i<u>dzaa</u> hiya bay<u>daa</u>u li<b>l</b>nn<u>aats</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1063, '7:109', 7, 109, ' q<u>aa</u>la <b>a</b>lmalau min qawmi fir\'awna inna h<u>aadzaa</u> las<u>aah</u>irun \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1064, '7:110', 7, 110, ' yuriidu an yukhrijakum min ar<u>dh</u>ikum fam<u>aatsaa</u> ta/muruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1065, '7:111', 7, 111, ' q<u>aa</u>luu arjih wa-akh<u>aa</u>hu wa-arsil fii <b>a</b>lmad<u>aa</u>-ini <u>haa</u>syiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1066, '7:112', 7, 112, ' ya/tuuka bikulli s<u>aah</u>irin \'aliim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1067, '7:113', 7, 113, ' waj<u>aa</u>-a <b>al</b>ssa<u>h</u>aratu fir\'awna q<u>aa</u>luu inna lan<u>aa</u> la-ajran in kunn<u>aa</u> na<u>h</u>nu <b>a</b>lgh<u>aa</u>libiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1068, '7:114', 7, 114, ' q<u>aa</u>la na\'am wa-innakum lamina <b>a</b>lmuqarrabiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1069, '7:115', 7, 115, ' q<u>aa</u>luu y<u>aa</u> muus<u>aa</u> imm<u>aa</u> an tulqiya wa-imm<u>aa</u> an nakuuna na<u>h</u>nu <b>a</b>lmulqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1070, '7:116', 7, 116, ' q<u>aa</u>la <b>a</b>lquu falamm<u>aa</u> <b>a</b>lqaw sa<u>h</u>aruu a\'yuna <b>al</b>nn<u>aa</u>si wa<b>i</b>starhabuuhum waj<u>aa</u>uu bisi<u>h</u>rin \'a<u>zh</u>iim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1071, '7:117', 7, 117, ' wa-aw<u>h</u>ayn<u>aa</u> il<u>aa</u> muus<u>aa</u> an <b>a</b>lqi \'a<u>shaa</u>ka fa-i<u>dzaa</u> hiya talqafu m<u>aa</u> ya/fikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1072, '7:118', 7, 118, ' fawaqa\'a <b>a</b>l<u>h</u>aqqu waba<u>th</u>ala m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1073, '7:119', 7, 119, ' faghulibuu hun<u>aa</u>lika wa<b>i</b>nqalabuu <u>shaa</u>ghiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1074, '7:120', 7, 120, ' waulqiya <b>al</b>ssa<u>h</u>aratu s<u>aa</u>jidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1075, '7:121', 7, 121, ' q<u>aa</u>luu <u>aa</u>mann<u>aa</u> birabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1076, '7:122', 7, 122, ' rabbi muus<u>aa</u> wah<u>aa</u>ruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1077, '7:123', 7, 123, ' q<u>aa</u>la fir\'awnu <u>aa</u>mantum bihi qabla an <u>aadz</u>ana lakum inna h<u>aadzaa</u> lamakrun makartumuuhu fii <b>a</b>lmadiinati litukhrijuu minh<u>aa</u> ahlah<u>aa</u> fasawfa ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1078, '7:124', 7, 124, ' lauqa<u>th</u>i\'anna aydiyakum wa-arjulakum min khil<u>aa</u>fin tsumma lau<u>sh</u>allibannakum ajma\'iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1079, '7:125', 7, 125, ' q<u>aa</u>luu inn<u>aa</u> il<u>aa</u> rabbin<u>aa</u> munqalibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1080, '7:126', 7, 126, ' wam<u>aa</u> tanqimu minn<u>aa</u> ill<u>aa</u> an <u>aa</u>mann<u>aa</u> bi-<u>aa</u>y<u>aa</u>ti rabbin<u>aa</u> lamm<u>aa</u> j<u>aa</u>-atn<u>aa</u> rabban<u>aa</u> afrigh \'alayn<u>aa</u> <u>sh</u>abran watawaffan<u>aa</u> muslimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1081, '7:127', 7, 127, ' waq<u>aa</u>la <b>a</b>lmalau min qawmi fir\'awna ata<u>dz</u>aru muus<u>aa</u> waqawmahu liyufsiduu fii <b>a</b>l-ar<u>dh</u>i waya<u>dz</u>araka wa<u>aa</u>lihataka q<u>aa</u>la sanuqattilu abn<u>aa</u>-ahum wanasta<u>h</u>yii nis<u>aa</u>-ahum wa-inn<u>aa</u> fawqahum q<u>aa</u>hiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1082, '7:128', 7, 128, ' q<u>aa</u>la muus<u>aa</u> liqawmihi ista\'iinuu bi<b>al</b>l<u>aa</u>hi wa<b>i</b><u>sh</u>biruu inna <b>a</b>l-ar<u>dh</u>a lill<u>aa</u>hi yuuritsuh<u>aa</u> man yasy<u>aa</u>u min \'ib<u>aa</u>dihi wa<b>a</b>l\'<u>aa</u>qibatu lilmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1083, '7:129', 7, 129, ' q<u>aa</u>luu uu<u>dz</u>iin<u>aa</u> min qabli an ta/tiyan<u>aa</u> wamin ba\'di m<u>aa</u> ji/tan<u>aa</u> q<u>aa</u>la \'as<u>aa</u> rabbukum an yuhlika \'aduwwakum wayastakhlifakum fii <b>a</b>l-ar<u>dh</u>i fayan<u>zh</u>ura kayfa ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1084, '7:130', 7, 130, ' walaqad akha<u>dz</u>n<u>aa</u> <u>aa</u>la fir\'awna bi<b>al</b>ssiniina wanaq<u>sh</u>in mina <b>al</b>tstsamar<u>aa</u>ti la\'allahum ya<u>dzdz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1085, '7:131', 7, 131, ' fa-i<u>dzaa</u> j<u>aa</u>-at-humu <b>a</b>l<u>h</u>asanatu q<u>aa</u>luu lan<u>aa</u> h<u>aadz</u>ihi wa-in tu<u>sh</u>ibhum sayyi-atun ya<u>ththh</u>ayyaruu bimuus<u>aa</u> waman ma\'ahu <b>a</b>l<u>aa</u> innam<u>aa</u> <u>th</u><u>aa</u>-iruhum \'inda <b>al</b>l<u>aa</u>hi wal<u>aa</u>kinna aktsarahum l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1086, '7:132', 7, 132, ' waq<u>aa</u>luu mahm<u>aa</u> ta/tin<u>aa</u> bihi min <u>aa</u>yatin litas<u>h</u>aran<u>aa</u> bih<u>aa</u> fam<u>aa</u> na<u>h</u>nu laka bimu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1087, '7:133', 7, 133, ' fa-arsaln<u>aa</u> \'alayhimu <b>al</b><u>ththh</u>uuf<u>aa</u>na wa<b>a</b>ljar<u>aa</u>da wa<b>a</b>lqummala wa<b>al</b><u>dhdh</u>af<u>aa</u>di\'a wa<b>al</b>ddama <u>aa</u>y<u>aa</u>tin mufa<u>shsh</u>al<u>aa</u>tin fa<b>i</b>stakbaruu wak<u>aa</u>nuu qawman mujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1088, '7:134', 7, 134, ' walamm<u>aa</u> waqa\'a \'alayhimu <b>al</b>rrijzu q<u>aa</u>luu y<u>aa</u> muus<u>aa</u> ud\'u lan<u>aa</u> rabbaka bim<u>aa</u> \'ahida \'indaka la-in kasyafta \'ann<u>aa</u> <b>al</b>rrijza lanu/minanna laka walanursilanna ma\'aka banii isr<u>aa</u>-iil<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1089, '7:135', 7, 135, ' falamm<u>aa</u> kasyafn<u>aa</u> \'anhumu <b>al</b>rrijza il<u>aa</u> ajalin hum b<u>aa</u>lighuuhu i<u>dzaa</u> hum yankutsuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1090, '7:136', 7, 136, ' fa<b>i</b>ntaqamn<u>aa</u> minhum fa-aghraqn<u>aa</u>hum fii <b>a</b>lyammi bi-annahum ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> wak<u>aa</u>nuu \'anh<u>aa</u> gh<u>aa</u>filiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1091, '7:137', 7, 137, ' wa-awratsn<u>aa</u> <b>a</b>lqawma <b>al</b>la<u>dz</u>iina k<u>aa</u>nuu yusta<u>dh</u>\'afuuna masy<u>aa</u>riqa <b>a</b>l-ar<u>dh</u>i wamagh<u>aa</u>ribah<u>aa</u> <b>al</b>latii b<u>aa</u>rakn<u>aa</u> fiih<u>aa</u> watammat kalimatu rabbika <b>a</b>l<u>h</u>usn<u>aa</u> \'al<u>aa</u> banii isr<u>aa</u>-iila bim<u>aa</u> <u>sh</u>abaruu wadammarn<u>aa</u> m<u>aa</u> k<u>aa</u>na ya<u>sh</u>na\'u fir\'awnu waqawmuhu wam<u>aa</u> k<u>aa</u>nuu ya\'risyuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1092, '7:138', 7, 138, ' waj<u>aa</u>wazn<u>aa</u> bibanii isr<u>aa</u>-iila <b>a</b>lba<u>h</u>ra fa-ataw \'al<u>aa</u> qawmin ya\'kufuuna \'al<u>aa</u> a<u>sh</u>n<u>aa</u>min lahum q<u>aa</u>luu y<u>aa</u> muus<u>aa</u> ij\'al lan<u>aa</u> il<u>aa</u>han kam<u>aa</u> lahum <u>aa</u>lihatun q<u>aa</u>la innakum qawmun tajhaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1093, '7:139', 7, 139, ' inna h<u>aa</u>ul<u>aa</u>-i mutabbarun m<u>aa</u> hum fiihi wab<u>aath</u>ilun m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1094, '7:140', 7, 140, ' q<u>aa</u>la aghayra <b>al</b>l<u>aa</u>hi abghiikum il<u>aa</u>han wahuwa fa<u>dhdh</u>alakum \'al<u>aa</u> <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1095, '7:141', 7, 141, ' wa-i<u>dz</u> anjayn<u>aa</u>kum min <u>aa</u>li fir\'awna yasuumuunakum suu-a <b>a</b>l\'a<u>dzaa</u>bi yuqattiluuna abn<u>aa</u>-akum wayasta<u>h</u>yuuna nis<u>aa</u>-akum wafii <u>dzaa</u>likum bal<u>aa</u>un min rabbikum \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1096, '7:142', 7, 142, ' waw<u>aa</u>\'adn<u>aa</u> muus<u>aa</u> tsal<u>aa</u>tsiina laylatan wa-atmamn<u>aa</u>h<u>aa</u> bi\'asyrin fatamma miiq<u>aa</u>tu rabbihi arba\'iina laylatan waq<u>aa</u>la muus<u>aa</u> li-akhiihi h<u>aa</u>ruuna ukhlufnii fii qawmii wa-a<u>sh</u>li<u>h</u> wal<u>aa</u> tattabi\' sabiila <b>a</b>lmufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1097, '7:143', 7, 143, ' walamm<u>aa</u> j<u>aa</u>-a muus<u>aa</u> limiiq<u>aa</u>tin<u>aa</u> wakallamahu rabbuhu q<u>aa</u>la rabbi arinii an<u>zh</u>ur ilayka q<u>aa</u>la lan tar<u>aa</u>nii wal<u>aa</u>kini un<u>zh</u>ur il<u>aa</u> <b>a</b>ljabali fa-ini istaqarra mak<u>aa</u>nahu fasawfa tar<u>aa</u>nii falamm<u>aa</u> tajall<u>aa</u> rabbuhu liljabali ja\'alahu dakkan wakharra muus<u>aa</u> <u>sh</u>a\'iqan falamm<u>aa</u> af<u>aa</u>qa q<u>aa</u>la sub<u>haa</u>naka tubtu ilayka wa-an<u>aa</u> awwalu <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1098, '7:144', 7, 144, ' q<u>aa</u>la y<u>aa</u> muus<u>aa</u> innii i<u>sth</u>afaytuka \'al<u>aa</u> <b>al</b>nn<u>aa</u>si biris<u>aa</u>l<u>aa</u>tii wabikal<u>aa</u>mii fakhu<u>dz</u> m<u>aa</u> <u>aa</u>taytuka wakun mina <b>al</b>sysy<u>aa</u>kiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1099, '7:145', 7, 145, ' wakatabn<u>aa</u> lahu fii <b>a</b>l-alw<u>aah</u>i min kulli syay-in maw\'i<u>zh</u>atan wataf<u>sh</u>iilan likulli syay-in fakhu<u>dz</u>h<u>aa</u> biquwwatin wa/mur qawmaka ya/khu<u>dz</u>uu bi-a<u>h</u>sanih<u>aa</u> sauriikum d<u>aa</u>ra <b>a</b>lf<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1100, '7:146', 7, 146, ' sa-a<u>sh</u>rifu \'an <u>aa</u>y<u>aa</u>tiya <b>al</b>la<u>dz</u>iina yatakabbaruuna fii <b>a</b>l-ar<u>dh</u>i bighayri <b>a</b>l<u>h</u>aqqi wa-in yaraw kulla <u>aa</u>yatin l<u>aa</u> yu/minuu bih<u>aa</u> wa-in yaraw sabiila <b>al</b>rrusydi l<u>aa</u> yattakhi<u>dz</u>uuhu sabiilan wa-in yaraw sabiila <b>a</b>lghayyi yattakhi<u>dz</u>uuhu sabiilan <u>dzaa</u>lika bi-annahum ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> wak<u>aa</u>nuu \'anh<u>aa</u> gh<u>aa</u>filiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1101, '7:147', 7, 147, ' wa<b>a</b>lla<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> waliq<u>aa</u>-i <b>a</b>l-<u>aa</u>khirati <u>h</u>abi<u>th</u>at a\'m<u>aa</u>luhum hal yujzawna ill<u>aa</u> m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1102, '7:148', 7, 148, ' wa<b>i</b>ttakha<u>dz</u>a qawmu muus<u>aa</u> min ba\'dihi min <u>h</u>uliyyihim \'ijlan jasadan lahu khuw<u>aa</u>run alam yaraw annahu l<u>aa</u> yukallimuhum wal<u>aa </u>yahdiihim sabiilan ittakha<u>dz</u>uuhu wak<u>aa</u>nuu <u>zhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1103, '7:149', 7, 149, ' walamm<u>aa</u> suqi<u>th</u>a fii aydiihim wara-aw annahum qad <u>dh</u>alluu q<u>aa</u>luu la-in lam yar<u>h</u>amn<u>aa</u> rabbun<u>aa</u> wayaghfir lan<u>aa</u> lanakuunanna mina <b>a</b>lkh<u>aa</u>siriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1104, '7:150', 7, 150, ' walamm<u>aa</u> raja\'a muus<u>aa</u> il<u>aa</u> qawmihi gha<u>dh</u>b<u>aa</u>na asifan q<u>aa</u>la bi/sam<u>aa</u> khalaftumuunii min ba\'dii a\'ajiltum amra rabbikum wa-alq<u>aa </u>al-alw<u>aah</u>a wa-akha<u>dz</u>a bira/si akhiihi yajurruhu ilayhi q<u>aa</u>la ibna umma inna <b>a</b>lqawma ista<u>dh</u>\'afuunii wak<u>aa</u>duu yaqtuluunanii fal<u>aa</u> tusymit biya <b>a</b>l-a\'d<u>aa</u>-a wal<u>aa</u> taj\'alnii ma\'a <b>a</b>lqawmi <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1105, '7:151', 7, 151, ' q<u>aa</u>la rabbi ighfir lii wali-akhii wa-adkhiln<u>aa</u> fii ra<u>h</u>matika wa-anta ar<u>h</u>amu <b>al</b>rr<u>aah</u>imiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1106, '7:152', 7, 152, ' inna <b>al</b>la<u>dz</u>iina ittakha<u>dz</u>uu <b>a</b>l\'ijla sayan<u>aa</u>luhum gha<u>dh</u>abun min rabbihim wa<u>dz</u>illatun fii <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> waka<u>dzaa</u>lika najzii <b>a</b>lmuftariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1107, '7:153', 7, 153, ' wa<b>a</b>lla<u>dz</u>iina \'amiluu <b>al</b>ssayyi-<u>aa</u>ti tsumma t<u>aa</u>buu min ba\'dih<u>aa</u> wa<u>aa</u>manuu inna rabbaka min ba\'dih<u>aa</u> laghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1108, '7:154', 7, 154, ' walamm<u>aa</u> sakata \'an muus<u>aa</u> <b>a</b>lgha<u>dh</u>abu akha<u>dz</u>a <b>a</b>l-alw<u>aah</u>a wafii nuskhatih<u>aa</u> hudan wara<u>h</u>matun lilla<u>dz</u>iina hum lirabbihim yarhabuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1109, '7:155', 7, 155, ' wa<b>i</b>kht<u>aa</u>ra muus<u>aa</u> qawmahu sab\'iina rajulan limiiq<u>aa</u>tin<u>aa</u> falamm<u>aa</u> akha<u>dz</u>at-humu <b>al</b>rrajfatu q<u>aa</u>la rabbi law syi/ta ahlaktahum min qablu wa-iyy<u>aa</u>ya atuhlikun<u>aa</u> bim<u>aa</u> fa\'ala <b>al</b>ssufah<u>aa</u>u minn<u>aa</u> in hiya ill<u>aa</u> fitnatuka tu<u>dh</u>illu bih<u>aa</u> man tasy<u>aa</u>u watahdii man tasy<u>aa</u>u anta waliyyun<u>aa</u> fa<b>i</b>ghfir lan<u>aa</u> wa<b>i</b>r<u>h</u>amn<u>aa</u> wa-anta khayru <b>a</b>lgh<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1110, '7:156', 7, 156, ' wa<b>u</b>ktub lan<u>aa</u> fii h<u>aadz</u>ihi <b>al</b>dduny<u>aa</u> <u>h</u>asanatan wafii <b>a</b>l-<u>aa</u>khirati inn<u>aa</u> hudn<u>aa</u> ilayka q<u>aa</u>la \'a<u>dzaa</u>bii u<u>sh</u>iibu bihi man asy<u>aa</u>u wara<u>h</u>matii wasi\'at kulla syay-in fasa-aktubuh<u>aa</u> lilla<u>dz</u>iina yattaquuna wayu/tuuna <b>al</b>zzak<u>aa</u>ta wa<b>a</b>lla<u>dz</u>iina hum bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1111, '7:157', 7, 157, ' <b>al</b>la<u>dz</u>iina yattabi\'uuna <b>al</b>rrasuula <b>al</b>nnabiyya <b>a</b>l-ummiyya <b>al</b>la<u>dz</u>ii yajiduunahu maktuuban \'indahum fii <b>al</b>ttawr<u>aa</u>ti wa<b>a</b>l-injiili ya/muruhum bi<b>a</b>lma\'ruufi wayanh<u>aa</u>hum \'ani <b>a</b>lmunkari wayu<u>h</u>illu lahumu <b>al</b><u>ththh</u>ayyib<u>aa</u>ti wayu<u>h</u>arrimu \'alayhimu <b>a</b>lkhab<u>aa</u>-itsa waya<u>dh</u>a\'u \'anhum i<u>sh</u>rahum wa<b>a</b>l-aghl<u>aa</u>la <b>al</b>latii k<u>aa</u>nat \'alayhim fa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu bihi wa\'azzaruuhu wana<u>sh</u>aruuhu wa<b>i</b>ttaba\'uu <b>al</b>nnuura <b>al</b>la<u>dz</u>ii unzila ma\'ahu ul<u>aa</u>-ika humu <b>a</b>lmufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1112, '7:158', 7, 158, ' qul y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su innii rasuulu <b>al</b>l<u>aa</u>hi ilaykum jamii\'an <b>al</b>la<u>dz</u>ii lahu mulku <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa yu<u>h</u>yii wayumiitu fa<u>aa</u>minuu bi<b>al</b>l<u>aa</u>hi warasuulihi <b>al</b>nnabiyyi <b>a</b>l-ummiyyi <b>al</b>la<u>dz</u>ii yu/minu bi<b>al</b>l<u>aa</u>hi wakalim<u>aa</u>tihi wa<b>i</b>ttabi\'uuhu la\'allakum tahtaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1113, '7:159', 7, 159, ' wamin qawmi muus<u>aa</u> ummatun yahduuna bi<b>a</b>l<u>h</u>aqqi wabihi ya\'diluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1114, '7:160', 7, 160, ' waqa<u>ththh</u>a\'n<u>aa</u>humu itsnatay \'asyrata asb<u>aath</u>an umaman wa-aw<u>h</u>ayn<u>aa</u> il<u>aa</u> muus<u>aa</u> i<u>dz</u>i istasq<u>aa</u>hu qawmuhu ani i<u>dh</u>rib bi\'a<u>shaa</u>ka <b>a</b>l<u>h</u>ajara fa<b>i</b>nbajasat minhu itsnat<u>aa</u> \'asyrata \'aynan qad \'alima kullu un<u>aa</u>sin masyrabahum wa<u>zh</u>allaln<u>aa</u> \'alayhimu <b>a</b>lgham<u>aa</u>ma wa-anzaln<u>aa</u> \'alayhimu <b>a</b>lmanna wa<b>al</b>ssalw<u>aa</u> kuluu min <u>th</u>ayyib<u>aa</u>ti m<u>aa</u> razaqn<u>aa</u>kum wam<u>aa</u> <u>zh</u>alamuun<u>aa</u> wal<u>aa</u>kin k<u>aa</u>nuu anfusahum ya<u>zh</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1115, '7:161', 7, 161, ' wa-i<u>dz</u> qiila lahumu uskunuu h<u>aadz</u>ihi <b>a</b>lqaryata wakuluu minh<u>aa</u> <u>h</u>aytsu syi/tum waquuluu <u>h</u>i<u>ththh</u>atun wa<b>u</b>dkhuluu <b>a</b>lb<u>aa</u>ba sujjadan naghfir lakum kha<u>th</u>ii-<u>aa</u>tikum sanaziidu <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1116, '7:162', 7, 162, ' fabaddala <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu minhum qawlan ghayra <b>al</b>la<u>dz</u>ii qiila lahum fa-arsaln<u>aa</u> \'alayhim rijzan mina <b>al</b>ssam<u>aa</u>-i bim<u>aa</u> k<u>aa</u>nuu ya<u>zh</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1117, '7:163', 7, 163, ' wa<b>i</b>s-alhum \'ani <b>a</b>lqaryati <b>al</b>latii k<u>aa</u>nat <u>had</u>irata <b>a</b>lba<u>h</u>ri i<u>dz</u> ya\'duuna fii <b>al</b>ssabti i<u>dz</u> ta/tiihim <u>h</u>iit<u>aa</u>nuhum yawma sabtihim syurra\'an wayawma l<u>aa</u> yasbituuna l<u>aa</u> ta/tiihim ka<u>dzaa</u>lika nabluuhum bim<u>aa</u> k<u>aa</u>nuu yafsuquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1118, '7:164', 7, 164, ' wa-i<u>dz</u> q<u>aa</u>lat ummatun minhum lima ta\'i<u>zh</u>uuna qawman <b>al</b>l<u>aa</u>hu muhlikuhum aw mu\'a<u>dzdz</u>ibuhum \'a<u>dzaa</u>ban syadiidan q<u>aa</u>luu ma\'<u>dz</u>iratan il<u>aa</u> rabbikum wala\'allahum yattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1119, '7:165', 7, 165, ' falamm<u>aa</u> nasuu m<u>aa</u> <u>dz</u>ukkiruu bihi anjayn<u>aa</u> <b>al</b>la<u>dz</u>iina yanhawna \'ani <b>al</b>ssuu-i wa-akha<u>dz</u>n<u>aa</u> <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu bi\'a<u>dzaa</u>bin ba-iisin bim<u>aa</u> k<u>aa</u>nuu yafsuquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1120, '7:166', 7, 166, ' falamm<u>aa</u> \'ataw \'an m<u>aa</u> nuhuu \'anhu quln<u>aa</u> lahum kuunuu qiradatan kh<u>aa</u>si-iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1121, '7:167', 7, 167, ' wa-i<u>dz</u> ta-a<u>dzdz</u>ana rabbuka layab\'atsanna \'alayhim il<u>aa</u> yawmi <b>a</b>lqiy<u>aa</u>mati man yasuumuhum suu-a <b>a</b>l\'a<u>dzaa</u>bi inna rabbaka lasarii\'u <b>a</b>l\'iq<u>aa</u>bi wa-innahu laghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1122, '7:168', 7, 168, ' waqa<u>ththh</u>a\'n<u>aa</u>hum fii <b>a</b>l-ar<u>dh</u>i umaman minhumu <b>al</b><u>shshaa</u>li<u>h</u>uuna waminhum duuna <u>dzaa</u>lika wabalawn<u>aa</u>hum bi<b>a</b>l<u>h</u>asan<u>aa</u>ti wa<b>al</b>ssayyi-<u>aa</u>ti la\'allahum yarji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1123, '7:169', 7, 169, ' fakhalafa min ba\'dihim khalfun waritsuu alkit<u>aa</u>ba ya/khu<u>dz</u>uuna \'ara<u>dh</u>a h<u>aadzaa</u> al-adn<u>aa</u> wayaquuluuna sayughfaru lan<u>aa</u> wa-in ya/tihim \'ara<u>dh</u>un mitsluhu ya/khu<u>dz</u>uuhu alam yu/kha<u>dz</u> \'alayhim miits<u>aa</u>qu alkit<u>aa</u>bi an l<u>aa</u> yaquuluu \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ill<u>aa</u> al<u>h</u>aqqa wadarasuu m<u>aa</u> fiihi wa<b>al</b>dd<u>aa</u>ru al-<u>aa</u>khiratu khayrun lilla<u>dz</u>iina yattaquuna afal<u>aa</u> ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1124, '7:170', 7, 170, ' wa<b>a</b>lla<u>dz</u>iina yumassikuuna bi<b>a</b>lkit<u>aa</u>bi wa-aq<u>aa</u>muu <b>al</b><u>shsh</u>al<u>aa</u>ta inn<u>aa</u> l<u>aa</u> nu<u>dh</u>ii\'u ajra <b>a</b>lmu<u>sh</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1125, '7:171', 7, 171, ' wa-i<u>dz</u> nataqn<u>aa</u> <b>a</b>ljabala fawqahum ka-annahu <u>zh</u>ullatun wa<u>zh</u>annuu annahu w<u>aa</u>qi\'un bihim khu<u>dz</u>uu m<u>aa</u> <u>aa</u>tayn<u>aa</u>kum biquwwatin wa<b>u</b><u>dz</u>kuruu m<u>aa</u> fiihi la\'allakum tattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1126, '7:172', 7, 172, ' wa-i<u>dz</u> akha<u>dz</u>a rabbuka min banii <u>aa</u>dama min <u>zh</u>uhuurihim <u>dz</u>urriyyatahum wa-asyhadahum \'al<u>aa</u> anfusihim alastu birabbikum q<u>aa</u>luu bal<u>aa</u> syahidn<u>aa</u> an taquuluu yawma alqiy<u>aa</u>mati inn<u>aa</u> kunn<u>aa</u> \'an h<u>aadzaa</u> gh<u>aa</u>filiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1127, '7:173', 7, 173, ' aw taquuluu innam<u>aa</u> asyraka <u>aa</u>b<u>aa</u>un<u>aa</u> min qablu wakunn<u>aa</u> <u>dz</u>urriyyatan min ba\'dihim afatuhlikun<u>aa</u> bim<u>aa</u> fa\'ala <b>a</b>lmub<u>th</u>iluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1128, '7:174', 7, 174, ' waka<u>dzaa</u>lika nufa<u>shsh</u>ilu <b>a</b>l-<u>aa</u>y<u>aa</u>ti wala\'allahum yarji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1129, '7:175', 7, 175, ' wa<b>u</b>tlu \'alayhim naba-a <b>al</b>la<u>dz</u>ii <u>aa</u>tayn<u>aa</u>hu <u>aa</u>y<u>aa</u>tin<u>aa</u> fa<b>i</b>nsalakha minh<u>aa</u> fa-atba\'ahu <b>al</b>sysyay<u>th</u><u>aa</u>nu fak<u>aa</u>na mina <b>a</b>lgh<u>aa</u>wiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1130, '7:176', 7, 176, ' walaw syi/n<u>aa</u> larafa\'n<u>aa</u>hu bih<u>aa</u> wal<u>aa</u>kinnahu akhlada il<u>aa</u> <b>a</b>l-ar<u>dh</u>i wa<b>i</b>ttaba\'a haw<u>aa</u>hu famatsaluhu kamatsali <b>a</b>lkalbi in ta<u>h</u>mil \'alayhi yalhats aw tatruk-hu yalhats <u>dzaa</u>lika matsalu <b>a</b>lqawmi <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> fa<b>u</b>q<u>sh</u>u<u>sh</u>i <b>a</b>lqa<u>sh</u>a<u>sh</u>a la\'allahum yatafakkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1131, '7:177', 7, 177, ' s<u>aa</u>-a matsalan <b>a</b>lqawmu <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> wa-anfusahum k<u>aa</u>nuu ya<u>zh</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1132, '7:178', 7, 178, ' man yahdi <b>al</b>l<u>aa</u>hu fahuwa <b>a</b>lmuhtadii waman yu<u>dh</u>lil faul<u>aa</u>-ika humu <b>a</b>lkh<u>aa</u>siruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1133, '7:179', 7, 179, ' walaqad <u>dz</u>ara/n<u>aa</u> lijahannama katsiiran mina <b>a</b>ljinni wa<b>a</b>l-insi lahum quluubun l<u>aa</u> yafqahuuna bih<u>aa</u> walahum a\'yunun l<u>aa</u> yub<u>sh</u>iruuna bih<u>aa</u> walahum <u>aatsaa</u>nun l<u>aa</u> yasma\'uuna bih<u>aa</u> ul<u>aa</u>-ika ka<b>a</b>l-an\'<u>aa</u>mi bal hum a<u>dh</u>allu ul<u>aa</u>-ika humu <b>a</b>lgh<u>aa</u>filuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1134, '7:180', 7, 180, ' walill<u>aa</u>hi <b>a</b>l-asm<u>aa</u>u <b>a</b>l<u>h</u>usn<u>aa</u> fa<b>u</b>d\'uuhu bih<u>aa</u> wa<u>dz</u>aruu <b>al</b>la<u>dz</u>iina yul<u>h</u>iduuna fii asm<u>aa</u>-ihi sayujzawna m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1135, '7:181', 7, 181, ' wamimman khalaqn<u>aa</u> ummatun yahduuna bi<b>a</b>l<u>h</u>aqqi wabihi ya\'diluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1136, '7:182', 7, 182, ' wa<b>a</b>lla<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> sanastadrijuhum min <u>h</u>aytsu l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1137, '7:183', 7, 183, ' waumlii lahum inna kaydii matiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1138, '7:184', 7, 184, ' awa lam yatafakkaruu m<u>aa</u> bi<u>shaah</u>ibihim min jinnatin in huwa ill<u>aa</u> na<u>dz</u>iirun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1139, '7:185', 7, 185, ' awalam yan<u>zh</u>uruu fii malakuuti <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wam<u>aa</u> khalaqa <b>al</b>l<u>aa</u>hu min syay-in wa-an \'as<u>aa</u> an yakuuna qadi iqtaraba ajaluhum fabi-ayyi <u>h</u>adiitsin ba\'dahu yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1140, '7:186', 7, 186, ' man yu<u>dh</u>lili <b>al</b>l<u>aa</u>hu fal<u>aa</u> h<u>aa</u>diya lahu waya<u>dz</u>aruhum fii <u>th</u>ughy<u>aa</u>nihim ya\'mahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1141, '7:187', 7, 187, ' yas-aluunaka \'ani <b>al</b>ss<u>aa</u>\'ati ayy<u>aa</u>na murs<u>aa</u>h<u>aa</u> qul innam<u>aa</u> \'ilmuh<u>aa</u> \'inda rabbii l<u>aa</u> yujalliih<u>aa</u> liwaqtih<u>aa</u> ill<u>aa</u> huwa tsaqulat fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i l<u>aa</u> ta/tiikum ill<u>aa</u> baghtatan yas-aluunaka ka-annaka <u>h</u>afiyyun \'anh<u>aa</u> qul innam<u>aa</u> \'ilmuh<u>aa</u> \'inda <b>al</b>l<u>aa</u>hi wal<u>aa</u>kinna aktsara <b>al</b>nn<u>aa</u>si l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1142, '7:188', 7, 188, ' qul l<u>aa</u> amliku linafsii naf\'an wal<u>aa</u> <u>dh</u>arran ill<u>aa</u> m<u>aa</u> sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu walaw kuntu a\'lamu <b>a</b>lghayba la<b>i</b>staktsartu mina <b>a</b>lkhayri wam<u>aa</u> massaniya <b>al</b>ssuu-u in an<u>aa</u> ill<u>aa</u> na<u>dz</u>iirun wabasyiirun liqawmin yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1143, '7:189', 7, 189, ' huwa <b>al</b>la<u>dz</u>ii khalaqakum min nafsin w<u>aah</u>idatin waja\'ala minh<u>aa</u> zawjah<u>aa</u> liyaskuna ilayh<u>aa</u> falamm<u>aa</u> taghasysy<u>aa</u>h<u>aa</u> <u>h</u>amalat <u>h</u>amlan khafiifan famarrat bihi falamm<u>aa</u> atsqalat da\'aw<u>aa</u> <b>al</b>l<u>aa</u>ha rabbahum<u>aa</u> la-in <u>aa</u>taytan<u>aa</u> <u>shaa</u>li<u>h</u>an lanakuunanna mina <b>al</b>sysy<u>aa</u>kiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1144, '7:190', 7, 190, ' falamm<u>aa</u> <u>aa</u>t<u>aa</u>hum<u>aa</u> <u>shaa</u>li<u>h</u>an ja\'al<u>aa</u> lahu syurak<u>aa</u>-a fiim<u>aa</u> <u>aa</u>t<u>aa</u>hum<u>aa</u> fata\'<u>aa</u>l<u>aa</u> <b>al</b>l<u>aa</u>hu \'amm<u>aa</u> yusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1145, '7:191', 7, 191, ' ayusyrikuuna m<u>aa</u> l<u>aa</u> yakhluqu syay-an wahum yukhlaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1146, '7:192', 7, 192, ' wal<u>aa</u> yasta<u>th</u>ii\'uuna lahum na<u>sh</u>ran wal<u>aa</u> anfusahum yan<u>sh</u>uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1147, '7:193', 7, 193, ' wa-in tad\'uuhum il<u>aa</u> <b>a</b>lhud<u>aa</u> l<u>aa</u> yattabi\'uukum saw<u>aa</u>un \'alaykum ada\'awtumuuhum am antum <u>shaa</u>mituun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1148, '7:194', 7, 194, ' inna <b>al</b>la<u>dz</u>iina tad\'uuna min duuni <b>al</b>l<u>aa</u>hi \'ib<u>aa</u>dun amts<u>aa</u>lukum fa<b>u</b>d\'uuhum falyastajiibuu lakum in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1149, '7:195', 7, 195, ' alahum arjulun yamsyuuna bih<u>aa</u> am lahum aydin yab<u>th</u>isyuuna bih<u>aa</u> am lahum a\'yunun yub<u>sh</u>iruuna bih<u>aa</u> am lahum <u>aatsaa</u>nun yasma\'uuna bih<u>aa</u> quli ud\'uu syurak<u>aa</u>-akum tsumma kiiduuni fal<u>aa</u> tun<u>zh</u>iruun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1150, '7:196', 7, 196, ' inna waliyyiya <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii nazzala <b>a</b>lkit<u>aa</u>ba wahuwa yatawall<u>aa</u> <b>al</b><u>shshaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1151, '7:197', 7, 197, ' wa<b>a</b>lla<u>dz</u>iina tad\'uuna min duunihi l<u>aa</u> yasta<u>th</u>ii\'uuna na<u>sh</u>rakum wal<u>aa</u> anfusahum yan<u>sh</u>uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1152, '7:198', 7, 198, ' wa-in tad\'uuhum il<u>aa</u> <b>a</b>lhud<u>aa</u> l<u>aa</u> yasma\'uu watar<u>aa</u>hum yan<u>zh</u>uruuna ilayka wahum l<u>aa</u> yub<u>sh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1153, '7:199', 7, 199, ' khu<u>dz</u>i <b>a</b>l\'afwa wa/mur bi<b>a</b>l\'urfi wa-a\'ri<u>dh</u> \'ani <b>a</b>lj<u>aa</u>hiliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1154, '7:200', 7, 200, ' wa-imm<u>aa</u> yanzaghannaka mina <b>al</b>sysyay<u>th</u><u>aa</u>ni nazghun fa<b>i</b>sta\'i<u>dz</u> bi<b>al</b>l<u>aa</u>hi innahu samii\'un \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1155, '7:201', 7, 201, ' inna <b>al</b>la<u>dz</u>iina ittaqaw i<u>dzaa</u> massahum <u>th</u><u>aa</u>-ifun mina <b>al</b>sysyay<u>th</u><u>aa</u>ni ta<u>dz</u>akkaruu fa-i<u>dzaa</u> hum mub<u>sh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1156, '7:202', 7, 202, ' wa-ikhw<u>aa</u>nuhum yamudduunahum fii <b>a</b>lghayyi tsumma l<u>aa</u> yuq<u>sh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1157, '7:203', 7, 203, ' wa-i<u>dzaa</u> lam ta/tihim bi-<u>aa</u>yatin q<u>aa</u>luu lawl<u>aa</u> ijtabaytah<u>aa</u> qul innam<u>aa</u> attabi\'u m<u>aa</u> yuu<u>haa</u> ilayya min rabbii h<u>aadzaa</u> ba<u>shaa</u>-iru min rabbikum wahudan wara<u>h</u>matun liqawmin yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1158, '7:204', 7, 204, ' wa-i<u>dzaa</u> quri-a <b>a</b>lqur-<u>aa</u>nu fa<b>i</b>stami\'uu lahu wa-an<u>sh</u>ituu la\'allakum tur<u>h</u>amuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1159, '7:205', 7, 205, ' wa<b>u</b><u>dz</u>kur rabbaka fii nafsika ta<u>dh</u>arru\'an wakhiifatan waduuna <b>a</b>ljahri mina <b>a</b>lqawli bi<b>a</b>lghuduwwi wa<b>a</b>l-<u>aasaa</u>li wal<u>aa</u> takun mina <b>a</b>lgh<u>aa</u>filiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1160, '7:206', 7, 206, ' inna <b>al</b>la<u>dz</u>iina \'inda rabbika l<u>aa</u> yastakbiruuna \'an \'ib<u>aa</u>datihi wayusabbi<u>h</u>uunahu walahu yasjuduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1161, '8:1', 8, 1, ' yas-aluunaka \'ani <b>a</b>l-anf<u>aa</u>li quli <b>a</b>l-anf<u>aa</u>lu lill<u>aa</u>hi wa<b>al</b>rrasuuli fa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha wa-a<u>sh</u>li<u>h</u>uu <u>dzaa</u>ta baynikum wa-a<u>th</u>ii\'uu <b>al</b>l<u>aa</u>ha warasuulahu in kuntum mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1162, '8:2', 8, 2, ' innam<u>aa</u> <b>a</b>lmu/minuuna <b>al</b>la<u>dz</u>iina i<u>dzaa</u> <u>dz</u>ukira <b>al</b>l<u>aa</u>hu wajilat quluubuhum wa-i<u>dzaa</u> tuliyat \'alayhim <u>aa</u>y<u>aa</u>tuhu z<u>aa</u>dat-hum iim<u>aa</u>nan wa\'al<u>aa</u> rabbihim yatawakkaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1163, '8:3', 8, 3, ' <b>al</b>la<u>dz</u>iina yuqiimuuna <b>al</b><u>shsh</u>al<u>aa</u>ta wamimm<u>aa</u> razaqn<u>aa</u>hum yunfiquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1164, '8:4', 8, 4, ' ul<u>aa</u>-ika humu <b>a</b>lmu/minuuna <u>h</u>aqqan lahum daraj<u>aa</u>tun \'inda rabbihim wamaghfiratun warizqun kariim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1165, '8:5', 8, 5, ' kam<u>aa</u> akhrajaka rabbuka min baytika bi<b>a</b>l<u>h</u>aqqi wa-inna fariiqan mina <b>a</b>lmu/miniina lak<u>aa</u>rihuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1166, '8:6', 8, 6, ' yuj<u>aa</u>diluunaka fii <b>a</b>l<u>h</u>aqqi ba\'da m<u>aa</u> tabayyana ka-annam<u>aa</u> yus<u>aa</u>quuna il<u>aa</u> <b>a</b>lmawti wahum yan<u>zh</u>uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1167, '8:7', 8, 7, ' wa-i<u>dz</u> ya\'idukumu <b>al</b>l<u>aa</u>hu i<u>h</u>d<u>aa</u> <b>al</b><u>ththaa</u>-ifatayni annah<u>aa</u> lakum watawadduuna anna ghayra <u>dzaa</u>ti <b>al</b>sysyawkati takuunu lakum wayuriidu <b>al</b>l<u>aa</u>hu an yu<u>h</u>iqqa <b>a</b>l<u>h</u>aqqa bikalim<u>aa</u>tihi wayaq<u>th</u>a\'a d<u>aa</u>bira <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1168, '8:8', 8, 8, '  liyu<u>h</u>iqqa <b>a</b>l<u>h</u>aqqa wayub<u>th</u>ila <b>a</b>lb<u>aath</u>ila walaw kariha <b>a</b>lmujrimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1169, '8:9', 8, 9, ' i<u>dz</u> tastaghiitsuuna rabbakum fa<b>i</b>staj<u>aa</u>ba lakum annii mumiddukum bi-alfin mina <b>a</b>lmal<u>aa</u>-ikati murdifiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1170, '8:10', 8, 10, ' wam<u>aa</u> ja\'alahu <b>al</b>l<u>aa</u>hu ill<u>aa</u> busyr<u>aa</u> walita<u>th</u>ma-inna bihi quluubukum wam<u>aa</u> <b>al</b>nna<u>sh</u>ru ill<u>aa</u> min \'indi <b>al</b>l<u>aa</u>hi inna <b>al</b>l<u>aa</u>ha \'aziizun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1171, '8:11', 8, 11, ' i<u>dz</u> yughasysyiikumu <b>al</b>nnu\'<u>aa</u>sa amanatan minhu wayunazzilu \'alaykum mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an liyu<u>th</u>ahhirakum bihi wayu<u>dz</u>hiba \'ankum rijza <b>al</b>sysyay<u>th</u><u>aa</u>ni waliyarbi<u>th</u>a \'al<u>aa</u> quluubikum wayutsabbita bihi <b>a</b>l-aqd<u>aa</u>m<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1172, '8:12', 8, 12, ' i<u>dz</u> yuu<u>h</u>ii rabbuka il<u>aa</u> <b>a</b>lmal<u>aa</u>-ikati annii ma\'akum fatsabbituu <b>al</b>la<u>dz</u>iina <u>aa</u>manuu saulqii fii quluubi <b>al</b>la<u>dz</u>iina kafaruu <b>al</b>rru\'ba fa<b>i</b><u>dh</u>ribuu fawqa <b>a</b>l-a\'n<u>aa</u>qi wa<b>i</b><u>dh</u>ribuu minhum kulla ban<u>aa</u>n<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1173, '8:13', 8, 13, ' <u>dzaa</u>lika bi-annahum sy<u>aa</u>qquu <b>al</b>l<u>aa</u>ha warasuulahu waman yusy<u>aa</u>qiqi <b>al</b>l<u>aa</u>ha warasuulahu fa-inna <b>al</b>l<u>aa</u>ha syadiidu <b>a</b>l\'iq<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1174, '8:14', 8, 14, ' <u>dzaa</u>likum fa<u>dz</u>uuquuhu wa-anna lilk<u>aa</u>firiina \'a<u>dzaa</u>ba <b>al</b>nn<u>aa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1175, '8:15', 8, 15, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu i<u>dzaa</u> laqiitumu <b>al</b>la<u>dz</u>iina kafaruu za<u>h</u>fan fal<u>aa</u> tuwalluuhumu <b>a</b>l-adb<u>aa</u>r<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1176, '8:16', 8, 16, ' waman yuwallihim yawma-i<u>dz</u>in duburahu ill<u>aa</u> muta<u>h</u>arrifan liqit<u>aa</u>lin aw muta<u>h</u>ayyizan il<u>aa</u> fi-atin faqad b<u>aa</u>-a bigha<u>dh</u>abin mina <b>al</b>l<u>aa</u>hi wama/w<u>aa</u>hu jahannamu wabi/sa <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1177, '8:17', 8, 17, ' falam taqtuluuhum wal<u>aa</u>kinna <b>al</b>l<u>aa</u>ha qatalahum wam<u>aa</u> ramayta i<u>dz</u> ramayta wal<u>aa</u>kinna <b>al</b>l<u>aa</u>ha ram<u>aa</u> waliyubliya <b>a</b>lmu/miniina minhu bal<u>aa</u>-an <u>h</u>asanan inna <b>al</b>l<u>aa</u>ha samii\'un \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1178, '8:18', 8, 18, ' <u>dzaa</u>likum wa-anna <b>al</b>l<u>aa</u>ha muuhinu kaydi <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1179, '8:19', 8, 19, ' in tastafti<u>h</u>uu faqad j<u>aa</u>-akumu <b>a</b>lfat<u>h</u>u wa-in tantahuu fahuwa khayrun lakum wa-in ta\'uuduu na\'ud walan tughniya \'ankum fi-atukum syay-an walaw katsurat wa-anna <b>al</b>l<u>aa</u>ha ma\'a <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1180, '8:20', 8, 20, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu a<u>th</u>ii\'uu <b>al</b>l<u>aa</u>ha warasuulahu wal<u>aa</u> tawallaw \'anhu wa-antum tasma\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1181, '8:21', 8, 21, ' wal<u>aa</u> takuunuu ka<b>a</b>lla<u>dz</u>iina q<u>aa</u>luu sami\'n<u>aa</u> wahum l<u>aa</u> yasma\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1182, '8:22', 8, 22, ' inna syarra <b>al</b>ddaw<u>aa</u>bbi \'inda <b>al</b>l<u>aa</u>hi <b>al</b><u>shsh</u>ummu <b>a</b>lbukmu <b>al</b>la<u>dz</u>iina l<u>aa</u> ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1183, '8:23', 8, 23, ' walaw \'alima <b>al</b>l<u>aa</u>hu fiihim khayran la-asma\'ahum walaw asma\'ahum latawallaw wahum mu\'ri<u>dh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1184, '8:24', 8, 24, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu istajiibuu lill<u>aa</u>hi wali<b>l</b>rrasuuli i<u>dzaa</u> da\'<u>aa</u>kum lim<u>aa</u> yu<u>h</u>yiikum wa<b>i</b>\'lamuu anna <b>al</b>l<u>aa</u>ha ya<u>h</u>uulu bayna <b>a</b>lmar-i waqalbihi wa-annahu ilayhi tu<u>h</u>syaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1185, '8:25', 8, 25, ' wa<b>i</b>ttaquu fitnatan l<u>aa</u> tu<u>sh</u>iibanna <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu minkum kh<u>aass</u>atan wa<b>i</b>\'lamuu anna <b>al</b>l<u>aa</u>ha syadiidu <b>a</b>l\'iq<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1186, '8:26', 8, 26, ' wa<b>u</b><u>dz</u>kuruu i<u>dz</u> antum qaliilun musta<u>dh</u>\'afuuna fii <b>a</b>l-ar<u>dh</u>i takh<u>aa</u>fuuna an yatakha<u>ththh</u>afakumu <b>al</b>nn<u>aa</u>su fa<u>aa</u>w<u>aa</u>kum wa-ayyadakum bina<u>sh</u>rihi warazaqakum mina <b>al</b><u>ththh</u>ayyib<u>aa</u>ti la\'allakum tasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1187, '8:27', 8, 27, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> takhuunuu <b>al</b>l<u>aa</u>ha wa<b>al</b>rrasuula watakhuunuu am<u>aa</u>n<u>aa</u>tikum wa-antum ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1188, '8:28', 8, 28, ' wa<b>i</b>\'lamuu annam<u>aa</u> amw<u>aa</u>lukum wa-awl<u>aa</u>dukum fitnatun wa-anna <b>al</b>l<u>aa</u>ha \'indahu ajrun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1189, '8:29', 8, 29, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>m<u>aa</u>nuu in tattaquu <b>al</b>l<u>aa</u>ha yaj\'al lakum furq<u>aa</u>nan wayukaffir \'ankum sayyi-<u>aa</u>tikum wayaghfir lakum wa<b>al</b>l<u>aa</u>hu <u>dz</u>uu <b>a</b>lfa<u>dh</u>li <b>a</b>l\'a<u>zh</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1190, '8:30', 8, 30, ' wa-i<u>dz</u> yamkuru bika <b>al</b>la<u>dz</u>iina kafaruu liyutsbituuka aw yaqtuluuka aw yukhrijuuka wayamkuruuna wayamkuru <b>al</b>l<u>aa</u>hu wa<b>al</b>l<u>aa</u>hu khayru <b>a</b>lm<u>aa</u>kiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1191, '8:31', 8, 31, ' wa-i<u>dzaa</u> tutl<u>aa</u> \'alayhim <u>aa</u>y<u>aa</u>tun<u>aa</u> q<u>aa</u>luu qad sami\'n<u>aa</u> law nasy<u>aa</u>u laquln<u>aa</u> mitsla h<u>aadzaa</u> in h<u>aadzaa</u> ill<u>aa</u> as<u>aath</u>iiru <b>a</b>l-awwaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1192, '8:32', 8, 32, ' wa-i<u>dz</u> q<u>aa</u>luu <b>al</b>l<u>aa</u>humma in k<u>aa</u>na h<u>aadzaa</u> huwa <b>a</b>l<u>h</u>aqqa min \'indika fa-am<u>th</u>ir \'alayn<u>aa</u> <u>h</u>ij<u>aa</u>ratan mina <b>al</b>ssam<u>aa</u>-i awi i/tin<u>aa</u> bi\'a<u>dzaa</u>bin <b>a</b>liim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1193, '8:33', 8, 33, ' wam<u>aa</u> k<u>aa</u>na <b>al</b>l<u>aa</u>hu liyu\'a<u>dzdz</u>ibahum wa-anta fiihim wam<u>aa</u> k<u>aa</u>na <b>al</b>l<u>aa</u>hu mu\'a<u>dzdz</u>ibahum wahum yastaghfiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1194, '8:34', 8, 34, ' wam<u>aa</u> lahum <b>al</b>l<u>aa</u> yu\'a<u>dzdz</u>ibahumu <b>al</b>l<u>aa</u>hu wahum ya<u>sh</u>udduuna \'ani <b>a</b>lmasjidi <b>a</b>l<u>h</u>ar<u>aa</u>mi wam<u>aa</u> k<u>aa</u>nuu awliy<u>aa</u>-ahu in awliy<u>aa</u>uhu ill<u>aa</u> <b>a</b>lmuttaquuna wal<u>aa</u>kinna aktsarahum l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1195, '8:35', 8, 35, ' wam<u>aa</u> k<u>aa</u>na <u>sh</u>al<u>aa</u>tuhum \'inda <b>a</b>lbayti ill<u>aa</u> muk<u>aa</u>-an wata<u>sh</u>diyatan fa<u>dz</u>uuquu <b>a</b>l\'a<u>dzaa</u>ba bim<u>aa</u> kuntum takfuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1196, '8:36', 8, 36, ' inna <b>al</b>la<u>dz</u>iina kafaruu yunfiquuna amw<u>aa</u>lahum liya<u>sh</u>udduu \'an sabiili <b>al</b>l<u>aa</u>hi fasayunfiquunah<u>aa</u> tsumma takuunu \'alayhim <u>h</u>asratan tsumma yughlabuuna wa<b>a</b>lla<u>dz</u>iina kafaruu il<u>aa</u> jahannama yu<u>h</u>syaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1197, '8:37', 8, 37, ' liyamiiza <b>al</b>l<u>aa</u>hu <b>a</b>lkhabiitsa mina <b>al</b><u>ththh</u>ayyibi wayaj\'ala <b>a</b>lkhabiitsa ba\'<u>dh</u>ahu \'al<u>aa</u> ba\'<u>dh</u>in fayarkumahu jamii\'an fayaj\'alahu fii jahannama ul<u>aa</u>-ika humu <b>a</b>lkh<u>aa</u>siruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1198, '8:38', 8, 38, ' qul lilla<u>dz</u>iina kafaruu in yantahuu yughfar lahum m<u>aa</u> qad salafa wa-in ya\'uuduu faqad ma<u>dh</u>at sunnatu <b>a</b>l-awwaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1199, '8:39', 8, 39, ' waq<u>aa</u>tiluuhum <u>h</u>att<u>aa</u> l<u>aa</u> takuuna fitnatun wayakuuna <b>al</b>ddiinu kulluhu lill<u>aa</u>hi fa-ini intahaw fa-inna <b>al</b>l<u>aa</u>ha bim<u>aa</u> ya\'maluuna ba<u>sh</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1200, '8:40', 8, 40, ' wa-in tawallaw fa<b>i</b>\'lamuu anna <b>al</b>l<u>aa</u>ha mawl<u>aa</u>kum ni\'ma <b>a</b>lmawl<u>aa</u> wani\'ma <b>al</b>nna<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1201, '8:41', 8, 41, ' wa<b>i</b>\'lamuu annam<u>aa</u> ghanimtum min syay-in fa-anna lill<u>aa</u>hi khumusahu wali<b>l</b>rrasuuli wali<u>dz</u>ii <b>a</b>lqurb<u>aa</u> wa<b>a</b>lyat<u>aa</u>m<u>aa</u> wa<b>a</b>lmas<u>aa</u>kiini wa<b>i</b>bni <b>al</b>ssabiili in kuntum <u>aa</u>mantum bi<b>al</b>l<u>aa</u>hi wam<u>aa</u> anzaln<u>aa</u> \'al<u>aa</u> \'abdin<u>aa</u> yawma <b>a</b>lfurq<u>aa</u>ni yawma iltaq<u>aa</u> <b>a</b>ljam\'<u>aa</u>ni wa<b>al</b>l<u>aa</u>hu \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1202, '8:42', 8, 42, ' i<u>dz</u> antum bi<b>a</b>l\'udwati <b>al</b>dduny<u>aa</u> wahum bi<b>a</b>l\'udwati <b>a</b>lqu<u>sh</u>w<u>aa</u> wa<b>al</b>rrakbu asfala minkum walaw taw<u>aa</u>\'adtum la<b>i</b>khtalaftum fii <b>a</b>lmii\'<u>aa</u>di wal<u>aa</u>kin liyaq<u>dh</u>iya <b>al</b>l<u>aa</u>hu amran k<u>aa</u>na maf\'uulan liyahlika man halaka \'an bayyinatin waya<u>h</u>y<u>aa</u> man <u>h</u>ayya \'an bayyinatin wa-inna <b>al</b>l<u>aa</u>ha lasamii\'un \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1203, '8:43', 8, 43, ' i<u>dz</u> yuriikahumu <b>al</b>l<u>aa</u>hu fii man<u>aa</u>mika qaliilan walaw ar<u>aa</u>kahum katsiiran lafasyiltum walatan<u>aa</u>za\'tum fii <b>a</b>l-amri wal<u>aa</u>kinna <b>al</b>l<u>aa</u>ha sallama innahu \'aliimun bi<u>dzaa</u>ti <b>al</b><u>shsh</u>uduur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1204, '8:44', 8, 44, ' wa-i<u>dz</u> yuriikumuuhum i<u>dz</u>i iltaqaytum fii a\'yunikum qaliilan wayuqallilukum fii a\'yunihim liyaq<u>dh</u>iya <b>al</b>l<u>aa</u>hu amran k<u>aa</u>na maf\'uulan wa-il<u>aa</u> <b>al</b>l<u>aa</u>hi turja\'u <b>a</b>l-umuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1205, '8:45', 8, 45, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu i<u>dzaa</u> laqiitum fi-atan fa<b>u</b>tsbutuu wa<b>u</b><u>dz</u>kuruu <b>al</b>l<u>aa</u>ha katsiiran la\'allakum tufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1206, '8:46', 8, 46, ' wa-a<u>th</u>ii\'uu <b>al</b>l<u>aa</u>ha warasuulahu wal<u>aa</u> tan<u>aa</u>za\'uu fatafsyaluu wata<u>dz</u>haba rii<u>h</u>ukum wa<b>i</b><u>sh</u>biruu inna <b>al</b>l<u>aa</u>ha ma\'a <b>al</b><u>shshaa</u>biriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1207, '8:47', 8, 47, ' wal<u>aa</u> takuunuu ka<b>a</b>lla<u>dz</u>iina kharajuu min diy<u>aa</u>rihim ba<u>th</u>aran wari-<u>aa</u>-a <b>al</b>nn<u>aa</u>si waya<u>sh</u>udduuna \'an sabiili <b>al</b>l<u>aa</u>hi wa<b>al</b>l<u>aa</u>hu bim<u>aa</u> ya\'maluuna mu<u>h</u>ii<u>th</u><b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1208, '8:48', 8, 48, ' wa-i<u>dz</u> zayyana lahumu <b>al</b>sysyay<u>th</u><u>aa</u>nu a\'m<u>aa</u>lahum waq<u>aa</u>la l<u>aa</u> gh<u>aa</u>liba lakumu <b>a</b>lyawma mina <b>al</b>nn<u>aa</u>si wa-innii j<u>aa</u>run lakum falamm<u>aa</u> tar<u>aa</u>-ati <b>a</b>lfi-at<u>aa</u>ni naka<u>sh</u>a \'al<u>aa</u> \'aqibayhi waq<u>aa</u>la innii barii-un minkum innii ar<u>aa</u> m<u>aa</u> l<u>aa</u> tarawna innii akh<u>aa</u>fu <b>al</b>l<u>aa</u>ha wa<b>al</b>l<u>aa</u>hu syadiidu <b>a</b>l\'iq<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1209, '8:49', 8, 49, ' i<u>dz</u> yaquulu <b>a</b>lmun<u>aa</u>fiquuna wa<b>a</b>lla<u>dz</u>iina fii quluubihim mara<u>dh</u>un gharra h<u>aa</u>ul<u>aa</u>-i diinuhum waman yatawakkal \'al<u>aa</u> <b>al</b>l<u>aa</u>hi fa-inna <b>al</b>l<u>aa</u>ha \'aziizun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1210, '8:50', 8, 50, ' walaw tar<u>aa</u> i<u>dz</u> yatawaff<u>aa</u> <b>al</b>la<u>dz</u>iina kafaruu <b>a</b>lmal<u>aa</u>-ikatu ya<u>dh</u>ribuuna wujuuhahum wa-adb<u>aa</u>rahum wa<u>dz</u>uuquu \'a<u>dzaa</u>ba <b>a</b>l<u>h</u>ariiq<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1211, '8:51', 8, 51, ' <u>dzaa</u>lika bim<u>aa</u> qaddamat aydiikum wa-anna <b>al</b>l<u>aa</u>ha laysa bi<u>zh</u>all<u>aa</u>min lil\'abiid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1212, '8:52', 8, 52, ' kada/bi <u>aa</u>li fir\'awna wa<b>a</b>lla<u>dz</u>iina min qablihim kafaruu bi-<u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi fa-akha<u>dz</u>ahumu <b>al</b>l<u>aa</u>hu bi<u>dz</u>unuubihim inna <b>al</b>l<u>aa</u>ha qawiyyun syadiidu <b>a</b>l\'iq<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1213, '8:53', 8, 53, ' <u>dzaa</u>lika bi-anna <b>al</b>l<u>aa</u>ha lam yaku mughayyiran ni\'matan an\'amah<u>aa</u> \'al<u>aa</u> qawmin <u>h</u>att<u>aa</u> yughayyiruu m<u>aa</u> bi-anfusihim wa-anna <b>al</b>l<u>aa</u>ha samii\'un \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1214, '8:54', 8, 54, ' kada/bi <u>aa</u>li fir\'awna wa<b>a</b>lla<u>dz</u>iina min qablihim ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>ti rabbihim fa-ahlakn<u>aa</u>hum bi<u>dz</u>unuubihim wa-aghraqn<u>aa</u> <u>aa</u>la fir\'awna wakullun k<u>aa</u>nuu <u>zhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1215, '8:55', 8, 55, ' inna syarra <b>al</b>ddaw<u>aa</u>bbi \'inda <b>al</b>l<u>aa</u>hi <b>al</b>la<u>dz</u>iina kafaruu fahum l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1216, '8:56', 8, 56, ' <b>al</b>la<u>dz</u>iina \'<u>aa</u>hadta minhum tsumma yanqu<u>dh</u>uuna \'ahdahum fii kulli marratin wahum l<u>aa</u> yattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1217, '8:57', 8, 57, ' fa-imm<u>aa</u> tatsqafannahum fii <b>a</b>l<u>h</u>arbi fasyarrid bihim man khalfahum la\'allahum ya<u>dzdz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1218, '8:58', 8, 58, ' wa-imm<u>aa</u> takh<u>aa</u>fanna min qawmin khiy<u>aa</u>natan fa<b>i</b>nbi<u>dz</u> ilayhim \'al<u>aa</u> sawa-in inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yu<u>h</u>ibbu <b>a</b>lkh<u>aa</u>-iniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1219, '8:59', 8, 59, ' wal<u>aa</u> ya<u>h</u>sabanna <b>al</b>la<u>dz</u>iina kafaruu sabaquu innahum l<u>aa</u> yu\'jizuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1220, '8:60', 8, 60, ' wa-a\'idduu lahum m<u>aa</u> ista<u>th</u>a\'tum min quwwatin wamin rib<u>aath</u>i <b>a</b>lkhayli turhibuuna bihi \'aduwwa <b>al</b>l<u>aa</u>hi wa\'aduwwakum wa<u>aa</u>khariina min duunihim l<u>aa</u> ta\'lamuunahumu <b>al</b>l<u>aa</u>hu ya\'lamuhum wam<u>aa</u> tunfiquu min syay-in fii sabiili <b>al</b>l<u>aa</u>hi yuwaffa ilaykum wa-antum l<u>aa</u> tu<u>zh</u>lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1221, '8:61', 8, 61, ' wa-in jana<u>h</u>uu li<b>l</b>ssalmi fa<b>i</b>jna<u>h</u> lah<u>aa</u> watawakkal \'al<u>aa</u> <b>al</b>l<u>aa</u>hi innahu huwa <b>al</b>ssamii\'u <b>a</b>l\'aliim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1222, '8:62', 8, 62, ' wa-in yuriiduu an yakhda\'uuka fa-inna <u>h</u>asbaka <b>al</b>l<u>aa</u>hu huwa <b>al</b>la<u>dz</u>ii ayyadaka bina<u>sh</u>rihi wabi<b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1223, '8:63', 8, 63, ' wa-allafa bayna quluubihim law anfaqta m<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i jamii\'an m<u>aa</u> <b>al</b>lafta bayna quluubihim wal<u>aa</u>kinna <b>al</b>l<u>aa</u>ha <b>al</b>lafa baynahum innahu \'aziizun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1224, '8:64', 8, 64, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nnabiyyu <u>h</u>asbuka <b>al</b>l<u>aa</u>hu wamani ittaba\'aka mina <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1225, '8:65', 8, 65, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nnabiyyu <u>h</u>arri<u>dh</u>i <b>a</b>lmu/miniina \'al<u>aa</u> <b>a</b>lqit<u>aa</u>li in yakun minkum \'isyruuna <u>shaa</u>biruuna yaghlibuu mi-atayni wa-in yakun minkum mi-atun yaghlibuu <b>a</b>lfan mina <b>al</b>la<u>dz</u>iina kafaruu bi-annahum qawmun l<u>aa</u> yafqahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1226, '8:66', 8, 66, ' al-<u>aa</u>na khaffafa <b>al</b>l<u>aa</u>hu \'ankum wa\'alima anna fiikum <u>dh</u>a\'fan fa-in yakun minkum mi-atun <u>shaa</u>biratun yaghlibuu mi-atayni wa-in yakun minkum <b>a</b>lfun yaghlibuu <b>a</b>lfayni bi-i<u>dz</u>ni <b>al</b>l<u>aa</u>hi wa<b>al</b>l<u>aa</u>hu ma\'a <b>al</b><u>shshaa</u>biriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1227, '8:67', 8, 67, ' m<u>aa</u> k<u>aa</u>na linabiyyin an yakuuna lahu asr<u>aa</u> <u>h</u>att<u>aa</u> yutskhina fii <b>a</b>l-ar<u>dh</u>i turiiduuna \'ara<u>dh</u>a <b>al</b>dduny<u>aa</u> wa<b>al</b>l<u>aa</u>hu yuriidu <b>a</b>l-<u>aa</u>khirata wa<b>al</b>l<u>aa</u>hu \'aziizun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1228, '8:68', 8, 68, ' lawl<u>aa</u> kit<u>aa</u>bun mina <b>al</b>l<u>aa</u>hi sabaqa lamassakum fiim<u>aa</u> akha<u>dz</u>tum \'a<u>dzaa</u>bun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1229, '8:69', 8, 69, ' fakuluu mimm<u>aa</u> ghanimtum <u>h</u>al<u>aa</u>lan <u>th</u>ayyiban wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1230, '8:70', 8, 70, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nnabiyyu qul liman fii aydiikum mina <b>a</b>l-asr<u>aa</u> in ya\'lami <b>al</b>l<u>aa</u>hu fii quluubikum khayran yu/tikum khayran mimm<u>aa</u> ukhi<u>dz</u>a minkum wayaghfir lakum wa<b>al</b>l<u>aa</u>hu ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1231, '8:71', 8, 71, ' wa-in yuriiduu khiy<u>aa</u>nataka faqad kh<u>aa</u>nuu <b>al</b>l<u>aa</u>ha min qablu fa-amkana minhum wa<b>al</b>l<u>aa</u>hu \'aliimun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1232, '8:72', 8, 72, ' inna <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wah<u>aa</u>jaruu waj<u>aa</u>haduu bi-amw<u>aa</u>lihim wa-anfusihim fii sabiili <b>al</b>l<u>aa</u>hi wa<b>a</b>lla<u>dz</u>iina <u>aa</u>waw wana<u>sh</u>aruu ul<u>aa</u>-ika ba\'<u>dh</u>uhum awliy<u>aa</u>u ba\'<u>dh</u>in wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu walam yuh<u>aa</u>jiruu m<u>aa</u> lakum min wal<u>aa</u>yatihim min syay-in <u>h</u>att<u>aa</u> yuh<u>aa</u>jiruu wa-ini istan<u>sh</u>aruukum fii <b>al</b>ddiini fa\'alaykumu <b>al</b>nna<u>sh</u>ru ill<u>aa</u> \'al<u>aa</u> qawmin baynakum wabaynahum miits<u>aa</u>qun wa<b>al</b>l<u>aa</u>hu bim<u>aa</u> ta\'maluuna ba<u>sh</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1233, '8:73', 8, 73, ' wa<b>a</b>lla<u>dz</u>iina kafaruu ba\'<u>dh</u>uhum awliy<u>aa</u>u ba\'<u>dh</u>in ill<u>aa</u> taf\'aluuhu takun fitnatun fii <b>a</b>l-ar<u>dh</u>i wafas<u>aa</u>dun kabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1234, '8:74', 8, 74, ' wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu wah<u>aa</u>jaruu waj<u>aa</u>haduu fii sabiili <b>al</b>l<u>aa</u>hi wa<b>a</b>lla<u>dz</u>iina <u>aa</u>waw wana<u>sh</u>aruu ul<u>aa</u>-ika humu <b>a</b>lmu/minuuna <u>h</u>aqqan lahum maghfiratun warizqun kariim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1235, '8:75', 8, 75, ' wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu min ba\'du wah<u>aa</u>jaruu waj<u>aa</u>haduu ma\'akum faul<u>aa</u>-ika minkum wauluu <b>a</b>l-ar<u>haa</u>mi ba\'<u>dh</u>uhum awl<u>aa</u> biba\'<u>dh</u>in fii kit<u>aa</u>bi <b>al</b>l<u>aa</u>hi inna <b>al</b>l<u>aa</u>ha bikulli syay-in \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1236, '9:1', 9, 1, ' bar<u>aa</u>-atun mina <b>al</b>l<u>aa</u>hi warasuulihi il<u>aa</u> <b>al</b>la<u>dz</u>iina \'<u>aa</u>hadtum mina <b>a</b>lmusyrikiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1237, '9:2', 9, 2, ' fasii<u>h</u>uu fii <b>a</b>l-ar<u>dh</u>i arba\'ata asyhurin wa<b>i</b>\'lamuu annakum ghayru mu\'jizii <b>al</b>l<u>aa</u>hi wa-anna <b>al</b>l<u>aa</u>ha mukhzii <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1238, '9:3', 9, 3, ' wa-a<u>dzaa</u>nun mina <b>al</b>l<u>aa</u>hi warasuulihi il<u>aa</u> <b>al</b>nn<u>aa</u>si yawma <b>a</b>l<u>h</u>ajji <b>a</b>l-akbari anna <b>al</b>l<u>aa</u>ha barii-un mina <b>a</b>lmusyrikiina warasuuluhu fa-in tubtum fahuwa khayrun lakum wa-in tawallaytum fa<b>i</b>\'lamuu annakum ghayru mu\'jizii <b>al</b>l<u>aa</u>hi wabasysyiri <b>al</b>la<u>dz</u>iina kafaruu bi\'a<u>dzaa</u>bin <b>a</b>liim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1239, '9:4', 9, 4, ' ill<u>aa</u> <b>al</b>la<u>dz</u>iina \'<u>aa</u>hadtum mina <b>a</b>lmusyrikiina tsumma lam yanqu<u>sh</u>uukum syay-an walam yu<u>zhaa</u>hiruu \'alaykum a<u>h</u>adan fa-atimmuu ilayhim \'ahdahum il<u>aa</u> muddatihim inna <b>al</b>l<u>aa</u>ha yu<u>h</u>ibbu <b>a</b>lmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1240, '9:5', 9, 5, ' fa-i<u>dzaa</u> insalakha <b>a</b>l-asyhuru <b>a</b>l<u>h</u>urumu fa<b>u</b>qtuluu <b>a</b>lmusyrikiina <u>h</u>aytsu wajadtumuuhum wakhu<u>dz</u>uuhum wa<b>u</b><u>hs</u>uruuhum wa<b>u</b>q\'uduu lahum kulla mar<u>sh</u>adin fa-in t<u>aa</u>buu wa-aq<u>aa</u>muu <b>al</b><u>shsh</u>al<u>aa</u>ta wa<u>aa</u>tawuu <b>al</b>zzak<u>aa</u>ta fakhalluu sabiilahum inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1241, '9:6', 9, 6, ' wa-in a<u>h</u>adun mina <b>a</b>lmusyrikiina istaj<u>aa</u>raka fa-ajirhu <u>h</u>att<u>aa</u> yasma\'a kal<u>aa</u>ma <b>al</b>l<u>aa</u>hi tsumma ablighhu ma/manahu <u>dzaa</u>lika bi-annahum qawmun l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1242, '9:7', 9, 7, ' kayfa yakuunu lilmusyrikiina \'ahdun \'inda <b>al</b>l<u>aa</u>hi wa\'inda rasuulihi ill<u>aa</u> <b>al</b>la<u>dz</u>iina \'<u>aa</u>hadtum \'inda <b>a</b>lmasjidi <b>a</b>l<u>h</u>ar<u>aa</u>mi fam<u>aa</u> istaq<u>aa</u>muu lakum fa<b>i</b>staqiimuu lahum inna <b>al</b>l<u>aa</u>ha yu<u>h</u>ibbu <b>a</b>lmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1243, '9:8', 9, 8, ' kayfa wa-in ya<u>zh</u>haruu \'alaykum l<u>aa</u> yarqubuu fiikum illan wal<u>aa</u> <u>dz</u>immatan yur<u>dh</u>uunakum bi-afw<u>aa</u>hihim wata/b<u>aa</u> quluubuhum wa-aktsaruhum f<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1244, '9:9', 9, 9, ' isytaraw bi-<u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi tsamanan qaliilan fa<u>sh</u>adduu \'an sabiilihi innahum s<u>aa</u>-a m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1245, '9:10', 9, 10, ' l<u>aa</u> yarqubuuna fii mu/minin illan wal<u>aa</u> <u>dz</u>immatan waul<u>aa</u>-ika humu <b>a</b>lmu\'taduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1246, '9:11', 9, 11, ' fa-in t<u>aa</u>buu wa-aq<u>aa</u>muu <b>al</b><u>shsh</u>al<u>aa</u>ta wa<u>aa</u>tawuu <b>al</b>zzak<u>aa</u>ta fa-ikhw<u>aa</u>nukum fii <b>al</b>ddiini wanufa<u>shsh</u>ilu <b>a</b>l-<u>aa</u>y<u>aa</u>ti liqawmin ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1247, '9:12', 9, 12, ' wa-in nakatsuu aym<u>aa</u>nahum min ba\'di \'ahdihim wa<u>th</u>a\'anuu fii diinikum faq<u>aa</u>tiluu a-immata <b>a</b>lkufri innahum l<u>aa</u> aym<u>aa</u>na lahum la\'allahum yantahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1248, '9:13', 9, 13, ' al<u>aa</u> tuq<u>aa</u>tiluuna qawman nakatsuu aym<u>aa</u>nahum wahammuu bi-ikhr<u>aa</u>ji <b>al</b>rrasuuli wahum badauukum awwala marratin atakhsyawnahum fa<b>al</b>l<u>aa</u>hu a<u>h</u>aqqu an takhsyawhu in kuntum mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1249, '9:14', 9, 14, ' q<u>aa</u>tiluuhum yu\'a<u>dzdz</u>ibhumu <b>al</b>l<u>aa</u>hu bi-aydiikum wayukhzihim wayan<u>sh</u>urkum \'alayhim wayasyfi <u>sh</u>uduura qawmin mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1250, '9:15', 9, 15, ' wayu<u>dz</u>hib ghay<u>zh</u>a quluubihim wayatuubu <b>al</b>l<u>aa</u>hu \'al<u>aa</u> man yasy<u>aa</u>u wa<b>al</b>l<u>aa</u>hu \'aliimun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1251, '9:16', 9, 16, ' am <u>h</u>asibtum an tutrakuu walamm<u>aa</u> ya\'lami <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>iina j<u>aa</u>haduu minkum walam yattakhi<u>dz</u>uu min duuni <b>al</b>l<u>aa</u>hi wal<u>aa</u> rasuulihi wal<u>aa</u> <b>a</b>lmu/miniina waliijatan wa<b>al</b>l<u>aa</u>hu khabiirun bim<u>aa</u> ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1252, '9:17', 9, 17, ' m<u>aa</u> k<u>aa</u>na lilmusyrikiina an ya\'muruu mas<u>aa</u>jida <b>al</b>l<u>aa</u>hi sy<u>aa</u>hidiina \'al<u>aa</u> anfusihim bi<b>a</b>lkufri ul<u>aa</u>-ika <u>h</u>abi<u>th</u>at a\'m<u>aa</u>luhum wafii <b>al</b>nn<u>aa</u>ri hum kh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1253, '9:18', 9, 18, ' innam<u>aa</u> ya\'muru mas<u>aa</u>jida <b>al</b>l<u>aa</u>hi man <u>aa</u>mana bi<b>al</b>l<u>aa</u>hi wa<b>a</b>lyawmi <b>a</b>l-<u>aa</u>khiri wa-aq<u>aa</u>ma <b>al</b><u>shsh</u>al<u>aa</u>ta wa<u>aa</u>t<u>aa</u> <b>al</b>zzak<u>aa</u>ta walam yakhsya ill<u>aa</u> <b>al</b>l<u>aa</u>ha fa\'as<u>aa</u> ul<u>aa</u>-ika an yakuunuu mina <b>a</b>lmuhtadiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1254, '9:19', 9, 19, ' aja\'altum siq<u>aa</u>yata <b>a</b>l<u>haa</u>jji wa\'im<u>aa</u>rata <b>a</b>lmasjidi <b>a</b>l<u>h</u>ar<u>aa</u>mi kaman <u>aa</u>mana bi<b>al</b>l<u>aa</u>hi wa<b>a</b>lyawmi <b>a</b>l-<u>aa</u>khiri waj<u>aa</u>hada fii sabiili <b>al</b>l<u>aa</u>hi l<u>aa</u> yastawuuna \'inda <b>al</b>l<u>aa</u>hi wa<b>al</b>l<u>aa</u>hu l<u>aa</u> yahdii <b>a</b>lqawma <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1255, '9:20', 9, 20, ' <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wah<u>aa</u>jaruu waj<u>aa</u>haduu fii sabiili <b>al</b>l<u>aa</u>hi bi-amw<u>aa</u>lihim wa-anfusihim a\'<u>zh</u>amu darajatan \'inda <b>al</b>l<u>aa</u>hi waul<u>aa</u>-ika humu <b>a</b>lf<u>aa</u>-izuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1256, '9:21', 9, 21, ' yubasysyiruhum rabbuhum bira<u>h</u>matin minhu wari<u>dh</u>w<u>aa</u>nin wajann<u>aa</u>tin lahum fiih<u>aa</u> na\'iimun muqiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1257, '9:22', 9, 22, ' kh<u>aa</u>lidiina fiih<u>aa</u> abadan inna <b>al</b>l<u>aa</u>ha \'indahu ajrun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1258, '9:23', 9, 23, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tattakhi<u>dz</u>uu <u>aa</u>b<u>aa</u>-akum wa-ikhw<u>aa</u>nakum awliy<u>aa</u>-a ini ista<u>h</u>abbuu <b>a</b>lkufra \'al<u>aa</u> <b>a</b>l-iim<u>aa</u>ni waman yatawallahum minkum faul<u>aa</u>-ika humu <b>al</b><u>zhzhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1259, '9:24', 9, 24, ' qul in k<u>aa</u>na <u>aa</u>b<u>aa</u>ukum wa-abn<u>aa</u>ukum wa-ikhw<u>aa</u>nukum wa-azw<u>aa</u>jukum wa\'asyiiratukum wa-amw<u>aa</u>lun iqtaraftumuuh<u>aa</u> watij<u>aa</u>ratun takhsyawna kas<u>aa</u>dah<u>aa</u> wamas<u>aa</u>kinu tar<u>dh</u>awnah<u>aa</u> a<u>h</u>abba ilaykum mina <b>al</b>l<u>aa</u>hi warasuulihi wajih<u>aa</u>din fii sabiilihi fatarabba<u>sh</u>uu <u>h</u>att<u>aa</u> ya/tiya <b>al</b>l<u>aa</u>hu bi-amrihi wa<b>al</b>l<u>aa</u>hu l<u>aa</u> yahdii <b>a</b>lqawma <b>a</b>lf<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1260, '9:25', 9, 25, ' laqad na<u>sh</u>arakumu <b>al</b>l<u>aa</u>hu fii maw<u>aath</u>ina katsiiratin wayawma <u>h</u>unaynin i<u>dz</u> a\'jabatkum katsratukum falam tughni \'ankum syay-an wa<u>daa</u>qat \'alaykumu <b>a</b>l-ar<u>dh</u>u bim<u>aa</u> ra<u>h</u>ubat tsumma wallaytum mudbiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1261, '9:26', 9, 26, ' tsumma anzala <b>al</b>l<u>aa</u>hu sakiinatahu \'al<u>aa</u> rasuulihi wa\'al<u>aa</u> <b>a</b>lmu/miniina wa-anzala junuudan lam tarawh<u>aa</u> wa\'a<u>dzdz</u>aba <b>al</b>la<u>dz</u>iina kafaruu wa<u>dzaa</u>lika jaz<u>aa</u>u <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1262, '9:27', 9, 27, ' tsumma yatuubu <b>al</b>l<u>aa</u>hu min ba\'di <u>dzaa</u>lika \'al<u>aa</u> man yasy<u>aa</u>u wa<b>al</b>l<u>aa</u>hu ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1263, '9:28', 9, 28, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu innam<u>aa</u> <b>a</b>lmusyrikuuna najasun fal<u>aa</u> yaqrabuu <b>a</b>lmasjida <b>a</b>l<u>h</u>ar<u>aa</u>ma ba\'da \'<u>aa</u>mihim h<u>aadzaa</u> wa-in khiftum \'aylatan fasawfa yughniikumu <b>al</b>l<u>aa</u>hu min fa<u>dh</u>lihi in sy<u>aa</u>-a inna <b>al</b>l<u>aa</u>ha \'aliimun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1264, '9:29', 9, 29, ' q<u>aa</u>tiluu <b>al</b>la<u>dz</u>iina l<u>aa</u> yu/minuuna bi<b>al</b>l<u>aa</u>hi wal<u>aa</u> bi<b>a</b>lyawmi <b>a</b>l-<u>aa</u>khiri wal<u>aa</u> yu<u>h</u>arrimuuna m<u>aa</u> <u>h</u>arrama <b>al</b>l<u>aa</u>hu warasuuluhu wal<u>aa</u> yadiinuuna diina <b>a</b>l<u>h</u>aqqi mina <b>al</b>la<u>dz</u>iina uutuu <b>a</b>lkit<u>aa</u>ba <u>h</u>att<u>aa</u> yu\'<u>th</u>uu <b>a</b>ljizyata \'an yadin wahum <u>shaa</u>ghiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1265, '9:30', 9, 30, ' waq<u>aa</u>lati <b>a</b>lyahuudu \'uzayrun ibnu <b>al</b>l<u>aa</u>hi waq<u>aa</u>lati <b>al</b>nna<u>shaa</u>r<u>aa</u> <b>a</b>lmasii<u>h</u>u ibnu <b>al</b>l<u>aa</u>hi <u>dzaa</u>lika qawluhum bi-afw<u>aa</u>hihim yu<u>daa</u>hi-uuna qawla <b>al</b>la<u>dz</u>iina kafaruu min qablu q<u>aa</u>talahumu <b>al</b>l<u>aa</u>hu ann<u>aa</u> yu/fakuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1266, '9:31', 9, 31, ' ittakha<u>dz</u>uu a<u>h</u>b<u>aa</u>rahum waruhb<u>aa</u>nahum arb<u>aa</u>ban min duuni <b>al</b>l<u>aa</u>hi wa<b>a</b>lmasii<u>h</u>a ibna maryama wam<u>aa</u> umiruu ill<u>aa</u> liya\'buduu il<u>aa</u>han w<u>aah</u>idan l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa sub<u>haa</u>nahu \'amm<u>aa</u> yusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1267, '9:32', 9, 32, ' yuriiduuna an yu<u>th</u>fi-uu nuura <b>al</b>l<u>aa</u>hi bi-afw<u>aa</u>hihim waya/b<u>aa</u> <b>al</b>l<u>aa</u>hu ill<u>aa</u> an yutimma nuurahu walaw kariha <b>a</b>lk<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1268, '9:33', 9, 33, ' huwa <b>al</b>la<u>dz</u>ii arsala rasuulahu bi<b>a</b>lhud<u>aa</u> wadiini <b>a</b>l<u>h</u>aqqi liyu<u>zh</u>hirahu \'al<u>aa</u> <b>al</b>ddiini kullihi walaw kariha <b>a</b>lmusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1269, '9:34', 9, 34, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu inna katsiiran mina <b>a</b>l-a<u>h</u>b<u>aa</u>ri wa<b>al</b>rruhb<u>aa</u>ni laya/kuluuna amw<u>aa</u>la <b>al</b>nn<u>aa</u>si bi<b>a</b>lb<u>aath</u>ili waya<u>sh</u>udduuna \'an sabiili <b>al</b>l<u>aa</u>hi wa<b>a</b>lla<u>dz</u>iina yaknizuuna <b>al</b><u>dzdz</u>ahaba wa<b>a</b>lfi<u>dhdh</u>ata wal<u>aa</u> yunfiquunah<u>aa</u> fii sabiili <b>al</b>l<u>aa</u>hi fabasysyirhum bi\'a<u>dzaa</u>bin <b>a</b>liim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1270, '9:35', 9, 35, ' yawma yu<u>h</u>m<u>aa</u> \'alayh<u>aa</u> fii n<u>aa</u>ri jahannama fatukw<u>aa</u> bih<u>aa</u> jib<u>aa</u>huhum wajunuubuhum wa<u>zh</u>uhuuruhum h<u>aadzaa</u> m<u>aa</u> kanaztum li-anfusikum fa<u>dz</u>uuquu m<u>aa</u> kuntum taknizuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1271, '9:36', 9, 36, ' inna \'iddata <b>al</b>sysyuhuuri \'inda <b>al</b>l<u>aa</u>hi itsn<u>aa</u> \'asyara syahran fii kit<u>aa</u>bi <b>al</b>l<u>aa</u>hi yawma khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a minh<u>aa</u> arba\'atun <u>h</u>urumun <u>dzaa</u>lika <b>al</b>ddiinu <b>a</b>lqayyimu fal<u>aa</u> ta<u>zh</u>limuu fiihinna anfusakum waq<u>aa</u>tiluu <b>a</b>lmusyrikiina k<u>aa</u>ffatan kam<u>aa</u> yuq<u>aa</u>tiluunakum k<u>aa</u>ffatan wa<b>i</b>\'lamuu anna <b>al</b>l<u>aa</u>ha ma\'a <b>a</b>lmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1272, '9:37', 9, 37, ' innam<u>aa</u> <b>al</b>nnasii-u ziy<u>aa</u>datun fii <b>a</b>lkufri yu<u>dh</u>allu bihi <b>al</b>la<u>dz</u>iina kafaruu yu<u>h</u>illuunahu \'<u>aa</u>man wayu<u>h</u>arrimuunahu \'<u>aa</u>man liyuw<u>aath</u>i-uu \'iddata m<u>aa</u> <u>h</u>arrama <b>al</b>l<u>aa</u>hu fayu<u>h</u>illuu m<u>aa</u> <u>h</u>arrama <b>al</b>l<u>aa</u>hu zuyyina lahum suu-u a\'m<u>aa</u>lihim wa<b>al</b>l<u>aa</u>hu l<u>aa</u> yahdii <b>a</b>lqawma <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1273, '9:38', 9, 38, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu m<u>aa</u> lakum i<u>dzaa</u> qiila lakumu infiruu fii sabiili <b>al</b>l<u>aa</u>hi itsts<u>aa</u>qaltum il<u>aa</u> <b>a</b>l-ar<u>dh</u>i ara<u>dh</u>iitum bi<b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> mina <b>a</b>l-<u>aa</u>khirati fam<u>aa</u> mat<u>aa</u>\'u <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> fii <b>a</b>l-<u>aa</u>khirati ill<u>aa</u> qaliil<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1274, '9:39', 9, 39, ' ill<u>aa</u> tanfiruu yu\'a<u>dzdz</u>ibkum \'a<u>dzaa</u>ban <b>a</b>liiman wayastabdil qawman ghayrakum wal<u>aa</u> ta<u>dh</u>urruuhu syay-an wa<b>al</b>l<u>aa</u>hu \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1275, '9:40', 9, 40, ' ill<u>aa</u> tan<u>sh</u>uruuhu faqad na<u>sh</u>arahu <b>al</b>l<u>aa</u>hu i<u>dz</u> akhrajahu <b>al</b>la<u>dz</u>iina kafaruu ts<u>aa</u>niya itsnayni i<u>dz</u> hum<u>aa</u> fii <b>a</b>lgh<u>aa</u>ri i<u>dz</u> yaquulu li<u>shaah</u>ibihi l<u>aa</u> ta<u>h</u>zan inna <b>al</b>l<u>aa</u>ha ma\'an<u>aa</u> fa-anzala <b>al</b>l<u>aa</u>hu sakiinatahu \'alayhi wa-ayyadahu bijunuudin lam tarawh<u>aa</u> waja\'ala kalimata <b>al</b>la<u>dz</u>iina kafaruu <b>al</b>ssufl<u>aa</u> wakalimatu <b>al</b>l<u>aa</u>hi hiya <b>a</b>l\'uly<u>aa</u> wa<b>al</b>l<u>aa</u>hu \'aziizun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1276, '9:41', 9, 41, ' infiruu khif<u>aa</u>fan watsiq<u>aa</u>lan waj<u>aa</u>hiduu bi-amw<u>aa</u>likum wa-anfusikum fii sabiili <b>al</b>l<u>aa</u>hi <u>dzaa</u>likum khayrun lakum in kuntum ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1277, '9:42', 9, 42, ' law k<u>aa</u>na \'ara<u>dh</u>an qariiban wasafaran q<u>aas</u>idan la<b>i</b>ttaba\'uuka wal<u>aa</u>kin ba\'udat \'alayhimu <b>al</b>sysyuqqatu wasaya<u>h</u>lifuuna bi<b>al</b>l<u>aa</u>hi lawi ista<u>th</u>a\'n<u>aa</u> lakharajn<u>aa</u> ma\'akum yuhlikuuna anfusahum wa<b>al</b>l<u>aa</u>hu ya\'lamu innahum lak<u>aadz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1278, '9:43', 9, 43, ' \'af<u>aa</u> <b>al</b>l<u>aa</u>hu \'anka lima a<u>dz</u>inta lahum <u>h</u>att<u>aa</u> yatabayyana laka <b>al</b>la<u>dz</u>iina <u>sh</u>adaquu wata\'lama <b>a</b>lk<u>aadz</u>ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1279, '9:44', 9, 44, ' l<u>aa</u> yasta/<u>dz</u>inuka <b>al</b>la<u>dz</u>iina yu/minuuna bi<b>al</b>l<u>aa</u>hi wa<b>a</b>lyawmi <b>a</b>l-<u>aa</u>khiri an yuj<u>aa</u>hiduu bi-amw<u>aa</u>lihim wa-anfusihim wa<b>al</b>l<u>aa</u>hu \'aliimun bi<b>a</b>lmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1280, '9:45', 9, 45, ' innam<u>aa</u> yasta/<u>dz</u>inuka <b>al</b>la<u>dz</u>iina l<u>aa</u> yu/minuuna bi<b>al</b>l<u>aa</u>hi wa<b>a</b>lyawmi <b>a</b>l-<u>aa</u>khiri wa<b>i</b>rt<u>aa</u>bat quluubuhum fahum fii raybihim yataraddaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1281, '9:46', 9, 46, ' walaw ar<u>aa</u>duu <b>a</b>lkhuruuja la-a\'adduu lahu \'uddatan wal<u>aa</u>kin kariha <b>al</b>l<u>aa</u>hu inbi\'<u>aa</u>tsahum fatsabba<u>th</u>ahum waqiila uq\'uduu ma\'a <b>a</b>lq<u>aa</u>\'idiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1282, '9:47', 9, 47, ' law kharajuu fiikum m<u>aa</u> z<u>aa</u>duukum ill<u>aa</u> khab<u>aa</u>lan wala-aw<u>dh</u>a\'uu khil<u>aa</u>lakum yabghuunakumu <b>a</b>lfitnata wafiikum samm<u>aa</u>\'uuna lahum wa<b>al</b>l<u>aa</u>hu \'aliimun bi<b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1283, '9:48', 9, 48, ' laqadi ibtaghawuu <b>a</b>lfitnata min qablu waqallabuu laka <b>a</b>l-umuura <u>h</u>att<u>aa</u> j<u>aa</u>-a <b>a</b>l<u>h</u>aqqu wa<u>zh</u>ahara amru <b>al</b>l<u>aa</u>hi wahum k<u>aa</u>rihuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1284, '9:49', 9, 49, ' waminhum man yaquulu i/<u>dz</u>an lii wal<u>aa</u> taftinnii <b>a</b>l<u>aa</u> fii <b>a</b>lfitnati saqa<u>th</u>uu wa-inna jahannama lamu<u>h</u>ii<u>th</u>atun bi<b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1285, '9:50', 9, 50, ' in tu<u>sh</u>ibka <u>h</u>asanatun tasu/hum wa-in tu<u>sh</u>ibka mu<u>sh</u>iibatun yaquuluu qad akha<u>dz</u>n<u>aa</u> amran<u>aa</u> min qablu wayatawallaw wahum fari<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1286, '9:51', 9, 51, ' qul lan yu<u>sh</u>iiban<u>aa</u> ill<u>aa</u> m<u>aa</u> kataba <b>al</b>l<u>aa</u>hu lan<u>aa</u> huwa mawl<u>aa</u>n<u>aa</u> wa\'al<u>aa</u> <b>al</b>l<u>aa</u>hi falyatawakkali <b>a</b>lmu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1287, '9:52', 9, 52, ' qul hal tarabba<u>sh</u>uuna bin<u>aa</u> ill<u>aa</u> i<u>h</u>d<u>aa</u> <b>a</b>l<u>h</u>usnayayni wana<u>h</u>nu natarabba<u>sh</u>u bikum an yu<u>sh</u>iibakumu <b>al</b>l<u>aa</u>hu bi\'a<u>dzaa</u>bin min \'indihi aw bi-aydiin<u>aa</u> fatarabba<u>sh</u>uu inn<u>aa</u> ma\'akum mutarabbi<u>sh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1288, '9:53', 9, 53, ' qul anfiquu <u>th</u>aw\'an aw karhan lan yutaqabbala minkum innakum kuntum qawman f<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1289, '9:54', 9, 54, ' wam<u>aa</u> mana\'ahum an tuqbala minhum nafaq<u>aa</u>tuhum ill<u>aa</u> annahum kafaruu bi<b>al</b>l<u>aa</u>hi wabirasuulihi wal<u>aa</u> ya/tuuna <b>al</b><u>shsh</u>al<u>aa</u>ta ill<u>aa</u> wahum kus<u>aa</u>l<u>aa</u> wal<u>aa</u> yunfiquuna ill<u>aa</u> wahum k<u>aa</u>rihuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1290, '9:55', 9, 55, ' fal<u>aa</u> tu\'jibka amw<u>aa</u>luhum wal<u>aa</u> awl<u>aa</u>duhum innam<u>aa</u> yuriidu <b>al</b>l<u>aa</u>hu liyu\'a<u>dzdz</u>ibahum bih<u>aa</u> fii <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> watazhaqa anfusuhum wahum k<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1291, '9:56', 9, 56, ' waya<u>h</u>lifuuna bi<b>al</b>l<u>aa</u>hi innahum laminkum wam<u>aa</u> hum minkum wal<u>aa</u>kinnahum qawmun yafraquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1292, '9:57', 9, 57, ' law yajiduuna malja-an aw magh<u>aa</u>r<u>aa</u>tin aw muddakhalan lawallaw ilayhi wahum yajma<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1293, '9:58', 9, 58, ' waminhum man yalmizuka fii <b>al</b><u>shsh</u>adaq<u>aa</u>ti fa-in u\'<u>th</u>uu minh<u>aa</u> ra<u>dh</u>uu wa-in lam yu\'<u>th</u>aw minh<u>aa</u> i<u>dzaa</u> hum yaskha<u>th</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1294, '9:59', 9, 59, ' walaw annahum ra<u>dh</u>uu m<u>aa</u> <u>aa</u>t<u>aa</u>humu <b>al</b>l<u>aa</u>hu warasuuluhu waq<u>aa</u>luu <u>h</u>asbun<u>aa</u> <b>al</b>l<u>aa</u>hu sayu/tiin<u>aa</u> <b>al</b>l<u>aa</u>hu min fa<u>dh</u>lihi warasuuluhu inn<u>aa</u> il<u>aa</u> <b>al</b>l<u>aa</u>hi r<u>aa</u>ghibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1295, '9:60', 9, 60, ' innam<u>aa</u> <b>al</b><u>shsh</u>adaq<u>aa</u>tu lilfuqar<u>aa</u>-i wa<b>a</b>lmas<u>aa</u>kiini wa<b>a</b>l\'<u>aa</u>miliina \'alayh<u>aa</u> wa<b>a</b>lmu-allafati quluubuhum wafii <b>al</b>rriq<u>aa</u>bi wa<b>a</b>lgh<u>aa</u>rimiina wafii sabiili <b>al</b>l<u>aa</u>hi wa<b>i</b>bni <b>al</b>ssabiili farii<u>dh</u>atan mina <b>al</b>l<u>aa</u>hi wa<b>al</b>l<u>aa</u>hu \'aliimun <u>h</u>akiimun</u>\r\r\n');
INSERT INTO quranLatin VALUES (1296, '9:61', 9, 61, ' waminhumu <b>al</b>la<u>dz</u>iina yu/<u>dz</u>uuna <b>al</b>nnabiyya wayaquuluuna huwa u<u>dz</u>unun qul u<u>dz</u>unu khayrin lakum yu/minu bi<b>al</b>l<u>aa</u>hi wayu/minu lilmu/miniina wara<u>h</u>matun lilla<u>dz</u>iina <u>aa</u>manuu minkum wa<b>a</b>lla<u>dz</u>iina yu/<u>dz</u>uuna rasuula <b>al</b>l<u>aa</u>hi lahum \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1297, '9:62', 9, 62, ' ya<u>h</u>lifuuna bi<b>al</b>l<u>aa</u>hi lakum liyur<u>dh</u>uukum wa<b>al</b>l<u>aa</u>hu warasuuluhu a<u>h</u>aqqu an yur<u>dh</u>uuhu in k<u>aa</u>nuu mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1298, '9:63', 9, 63, ' alam ya\'lamuu annahu man yu<u>haa</u>didi <b>al</b>l<u>aa</u>ha warasuulahu fa-anna lahu n<u>aa</u>ra jahannama kh<u>aa</u>lidan fiih<u>aa</u> <u>dzaa</u>lika <b>a</b>lkhizyu <b>a</b>l\'a<u>zh</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1299, '9:64', 9, 64, ' ya<u>hts</u>aru <b>a</b>lmun<u>aa</u>fiquuna an tunazzala \'alayhim suuratun tunabbi-uhum bim<u>aa</u> fii quluubihim quli istahzi-uu inna <b>al</b>l<u>aa</u>ha mukhrijun m<u>aa</u> ta<u>hts</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1300, '9:65', 9, 65, ' wala-in sa-altahum layaquulunna innam<u>aa</u> kunn<u>aa</u> nakhuu<u>dh</u>u wanal\'abu qul abi<b>al</b>l<u>aa</u>hi wa<u>aa</u>y<u>aa</u>tihi warasuulihi kuntum tastahzi-uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1301, '9:66', 9, 66, ' l<u>aa</u> ta\'ta<u>dz</u>iruu qad kafartum ba\'da iim<u>aa</u>nikum in na\'fu \'an <u>th</u><u>aa</u>-ifatin minkum nu\'a<u>dzdz</u>ib <u>th</u><u>aa</u>-ifatan bi-annahum k<u>aa</u>nuu mujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1302, '9:67', 9, 67, ' almun<u>aa</u>fiquuna wa<b>a</b>lmun<u>aa</u>fiq<u>aa</u>tu ba\'<u>dh</u>uhum min ba\'<u>dh</u>in ya/muruuna bi<b>a</b>lmunkari wayanhawna \'ani <b>a</b>lma\'ruufi wayaqbi<u>dh</u>uuna aydiyahum nasuu <b>al</b>l<u>aa</u>ha fanasiyahum inna <b>a</b>lmun<u>aa</u>fiqiina humu <b>a</b>lf<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1303, '9:68', 9, 68, ' wa\'ada <b>al</b>l<u>aa</u>hu <b>a</b>lmun<u>aa</u>fiqiina wa<b>a</b>lmun<u>aa</u>fiq<u>aa</u>ti wa<b>a</b>lkuff<u>aa</u>ra n<u>aa</u>ra jahannama kh<u>aa</u>lidiina fiih<u>aa</u> hiya <u>h</u>asbuhum wala\'anahumu <b>al</b>l<u>aa</u>hu walahum \'a<u>dzaa</u>bun muqiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1304, '9:69', 9, 69, ' ka<b>a</b>lla<u>dz</u>iina min qablikum k<u>aa</u>nuu asyadda minkum quwwatan wa-aktsara amw<u>aa</u>lan wa-awl<u>aa</u>dan fa<b>i</b>stamta\'uu bikhal<u>aa</u>qihim fa<b>i</b>stamta\'tum bikhal<u>aa</u>qikum kam<u>aa</u> istamta\'a <b>al</b>la<u>dz</u>iina min qablikum bikhal<u>aa</u>qihim wakhu<u>dh</u>tum ka<b>a</b>lla<u>dz</u>ii kh<u>aad</u>uu ul<u>aa</u>-ika <u>h</u>abi<u>th</u>at a\'m<u>aa</u>luhum fii <b>al</b>dduny<u>aa</u> wa<b>a</b>l-<u>aa</u>khirati waul<u>aa</u>-ika humu <b>a</b>lkh<u>aa</u>siruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1305, '9:70', 9, 70, ' alam ya/tihim nabau <b>al</b>la<u>dz</u>iina min qablihim qawmi nuu<u>h</u>in wa\'<u>aa</u>din watsamuuda waqawmi ibr<u>aa</u>hiima wa-a<u>sh</u>-<u>haa</u>bi madyana wa<b>a</b>lmu/tafik<u>aa</u>ti atat-hum rusuluhum bi<b>a</b>lbayyin<u>aa</u>ti fam<u>aa</u> k<u>aa</u>na <b>al</b>l<u>aa</u>hu liya<u>zh</u>limahum wal<u>aa</u>kin k<u>aa</u>nuu anfusahum ya<u>zh</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1306, '9:71', 9, 71, ' wa<b>a</b>lmu/minuuna wa<b>a</b>lmu/min<u>aa</u>tu ba\'<u>dh</u>uhum awliy<u>aa</u>u ba\'<u>dh</u>in ya/muruuna bi<b>a</b>lma\'ruufi wayanhawna \'ani <b>a</b>lmunkari wayuqiimuuna <b>al</b><u>shsh</u>al<u>aa</u>ta wayu/tuuna <b>al</b>zzak<u>aa</u>ta wayu<u>th</u>ii\'uuna <b>al</b>l<u>aa</u>ha warasuulahu ul<u>aa</u>-ika sayar<u>h</u>amuhumu <b>al</b>l<u>aa</u>hu inna <b>al</b>l<u>aa</u>ha \'aziizun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1307, '9:72', 9, 72, ' wa\'ada <b>al</b>l<u>aa</u>hu <b>a</b>lmu/miniina wa<b>a</b>lmu/min<u>aa</u>ti jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru kh<u>aa</u>lidiina fiih<u>aa</u> wamas<u>aa</u>kina <u>th</u>ayyibatan fii jann<u>aa</u>ti \'adnin wari<u>dh</u>w<u>aa</u>nun mina <b>al</b>l<u>aa</u>hi akbaru <u>dzaa</u>lika huwa <b>a</b>lfawzu <b>a</b>l\'a<u>zh</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1308, '9:73', 9, 73, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nnabiyyu j<u>aa</u>hidi <b>a</b>lkuff<u>aa</u>ra wa<b>a</b>lmun<u>aa</u>fiqiina wa<b>u</b>ghlu<u>zh</u> \'alayhim wama/w<u>aa</u>hum jahannamu wabi/sa <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1309, '9:74', 9, 74, ' ya<u>h</u>lifuuna bi<b>al</b>l<u>aa</u>hi m<u>aa</u> q<u>aa</u>luu walaqad q<u>aa</u>luu kalimata <b>a</b>lkufri wakafaruu ba\'da isl<u>aa</u>mihim wahammuu bim<u>aa</u> lam yan<u>aa</u>luu wam<u>aa</u> naqamuu ill<u>aa</u> an aghn<u>aa</u>humu <b>al</b>l<u>aa</u>hu warasuuluhu min fa<u>dh</u>lihi fa-in yatuubuu yaku khayran lahum wa-in yatawallaw yu\'a<u>dzdz</u>ibhumu <b>al</b>l<u>aa</u>hu \'a<u>dzaa</u>ban <b>a</b>liiman fii <b>al</b>dduny<u>aa</u> wa<b>a</b>l-<u>aa</u>khirati wam<u>aa</u> lahum fii <b>a</b>l-ar<u>dh</u>i min waliyyin wal<u>aa</u> na<u>sh</u>iir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1310, '9:75', 9, 75, ' waminhum man \'<u>aa</u>hada <b>al</b>l<u>aa</u>ha la-in <u>aa</u>t<u>aa</u>n<u>aa</u> min fa<u>dh</u>lihi lana<u>shsh</u>addaqanna walanakuunanna mina <b>al</b><u>shshaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1311, '9:76', 9, 76, ' falamm<u>aa</u> <u>aa</u>t<u>aa</u>hum min fa<u>dh</u>lihi bakhiluu bihi watawallaw wahum mu\'ri<u>dh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1312, '9:77', 9, 77, ' fa-a\'qabahum nif<u>aa</u>qan fii quluubihim il<u>aa</u> yawmi yalqawnahu bim<u>aa</u> akhlafuu <b>al</b>l<u>aa</u>ha m<u>aa</u> wa\'aduuhu wabim<u>aa</u> k<u>aa</u>nuu yak<u>dz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1313, '9:78', 9, 78, ' alam ya\'lamuu anna <b>al</b>l<u>aa</u>ha ya\'lamu sirrahum wanajw<u>aa</u>hum wa-anna <b>al</b>l<u>aa</u>ha \'all<u>aa</u>mu <b>a</b>lghuyuub<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1314, '9:79', 9, 79, ' <b>al</b>la<u>dz</u>iina yalmizuuna <b>a</b>lmu<u>ththh</u>awwi\'iina mina <b>a</b>lmu/miniina fii <b>al</b><u>shsh</u>adaq<u>aa</u>ti wa<b>a</b>lla<u>dz</u>iina l<u>aa</u> yajiduuna ill<u>aa</u> juhdahum fayaskharuuna minhum sakhira <b>al</b>l<u>aa</u>hu minhum walahum \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1315, '9:80', 9, 80, ' istaghfir lahum aw l<u>aa</u> tastaghfir lahum in tastaghfir lahum sab\'iina marratan falan yaghfira <b>al</b>l<u>aa</u>hu lahum <u>dzaa</u>lika bi-annahum kafaruu bi<b>al</b>l<u>aa</u>hi warasuulihi wa<b>al</b>l<u>aa</u>hu l<u>aa</u> yahdii <b>a</b>lqawma <b>a</b>lf<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1316, '9:81', 9, 81, ' fari<u>h</u>a <b>a</b>lmukhallafuuna bimaq\'adihim khil<u>aa</u>fa rasuuli <b>al</b>l<u>aa</u>hi wakarihuu an yuj<u>aa</u>hiduu bi-amw<u>aa</u>lihim wa-anfusihim fii sabiili <b>al</b>l<u>aa</u>hi waq<u>aa</u>luu l<u>aa</u> tanfiruu fii <b>a</b>l<u>h</u>arri qul n<u>aa</u>ru jahannama asyaddu <u>h</u>arran law k<u>aa</u>nuu yafqahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1317, '9:82', 9, 82, ' falya<u>dh</u>akuu qaliilan walyabkuu katsiiran jaz<u>aa</u>-an bim<u>aa</u> k<u>aa</u>nuu yaksibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1318, '9:83', 9, 83, ' fa-in raja\'aka <b>al</b>l<u>aa</u>hu il<u>aa</u> <u>th</u><u>aa</u>-ifatin minhum fa<b>i</b>sta/<u>dz</u>anuuka lilkhuruuji faqul lan takhrujuu ma\'iya abadan walan tuq<u>aa</u>tiluu ma\'iya \'aduwwan innakum ra<u>dh</u>iitum bi<b>a</b>lqu\'uudi awwala marratin fa<b>u</b>q\'uduu ma\'a <b>a</b>lkh<u>aa</u>lifiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1319, '9:84', 9, 84, ' wal<u>aa</u> tu<u>sh</u>alli \'al<u>aa</u> a<u>h</u>adin minhum m<u>aa</u>ta abadan wal<u>aa</u> taqum \'al<u>aa</u> qabrihi innahum kafaruu bi<b>al</b>l<u>aa</u>hi warasuulihi wam<u>aa</u>tuu wahum f<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1320, '9:85', 9, 85, ' wal<u>aa</u> tu\'jibka amw<u>aa</u>luhum wa-awl<u>aa</u>duhum innam<u>aa</u> yuriidu <b>al</b>l<u>aa</u>hu an yu\'a<u>dzdz</u>ibahum bih<u>aa</u> fii <b>al</b>dduny<u>aa</u> watazhaqa anfusuhum wahum k<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1321, '9:86', 9, 86, ' wa-i<u>dzaa</u> unzilat suuratun an <u>aa</u>minuu bi<b>al</b>l<u>aa</u>hi waj<u>aa</u>hiduu ma\'a rasuulihi ista/<u>dz</u>anaka uluu <b>al</b><u>ththh</u>awli minhum waq<u>aa</u>luu <u>dz</u>arn<u>aa</u> nakun ma\'a <b>a</b>lq<u>aa</u>\'idiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1322, '9:87', 9, 87, ' ra<u>dh</u>uu bi-an yakuunuu ma\'a <b>a</b>lkhaw<u>aa</u>lifi wa<u>th</u>ubi\'a \'al<u>aa</u> quluubihim fahum l<u>aa</u> yafqahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1323, '9:88', 9, 88, ' l<u>aa</u>kini <b>al</b>rrasuulu wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu ma\'ahu j<u>aa</u>haduu bi-amw<u>aa</u>lihim wa-anfusihim waul<u>aa</u>-ika lahumu <b>a</b>lkhayr<u>aa</u>tu waul<u>aa</u>-ika humu <b>a</b>lmufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1324, '9:89', 9, 89, ' a\'adda <b>al</b>l<u>aa</u>hu lahum jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru kh<u>aa</u>lidiina fiih<u>aa</u> <u>dzaa</u>lika <b>a</b>lfawzu <b>a</b>l\'a<u>zh</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1325, '9:90', 9, 90, ' waj<u>aa</u>-a <b>a</b>lmu\'a<u>dzdz</u>iruuna mina <b>a</b>l-a\'r<u>aa</u>bi liyu/<u>dz</u>ana lahum waqa\'ada <b>al</b>la<u>dz</u>iina ka<u>dz</u>abuu <b>al</b>l<u>aa</u>ha warasuulahu sayu<u>sh</u>iibu <b>al</b>la<u>dz</u>iina kafaruu minhum \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1326, '9:91', 9, 91, ' laysa \'al<u>aa</u> <b>al</b><u>dhdh</u>u\'af<u>aa</u>-i wal<u>aa</u> \'al<u>aa</u> <b>a</b>lmar<u>daa</u> wal<u>aa</u> \'al<u>aa</u> <b>al</b>la<u>dz</u>iina l<u>aa</u> yajiduuna m<u>aa</u> yunfiquuna <u>h</u>arajun i<u>dzaa</u> na<u>sh</u>a<u>h</u>uu lill<u>aa</u>hi warasuulihi m<u>aa</u> \'al<u>aa</u> <b>a</b>lmu<u>h</u>siniina min sabiilin wa<b>al</b>l<u>aa</u>hu ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1327, '9:92', 9, 92, ' wal<u>aa</u> \'al<u>aa</u> <b>al</b>la<u>dz</u>iina i<u>dzaa</u> m<u>aa</u> atawka lita<u>h</u>milahum qulta l<u>aa</u> ajidu m<u>aa</u> a<u>h</u>milukum \'alayhi tawallaw wa-a\'yunuhum tafii<u>dh</u>u mina <b>al</b>ddam\'i <u>h</u>azanan <b>al</b>l<u>aa</u> yajiduu m<u>aa</u> yunfiquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1328, '9:93', 9, 93, ' innam<u>aa</u> <b>al</b>ssabiilu \'al<u>aa</u> <b>al</b>la<u>dz</u>iina yasta/<u>dz</u>inuunaka wahum aghniy<u>aa</u>u ra<u>dh</u>uu bi-an yakuunuu ma\'a <b>a</b>lkhaw<u>aa</u>lifi wa<u>th</u>aba\'a <b>al</b>l<u>aa</u>hu \'al<u>aa</u> quluubihim fahum l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1329, '9:94', 9, 94, ' ya\'ta<u>dz</u>iruuna ilaykum i<u>dzaa</u> raja\'tum ilayhim qul l<u>aa</u> ta\'ta<u>dz</u>iruu lan nu/mina lakum qad nabba-an<u>aa</u> <b>al</b>l<u>aa</u>hu min akhb<u>aa</u>rikum wasayar<u>aa</u> <b>al</b>l<u>aa</u>hu \'amalakum warasuuluhu tsumma turadduuna il<u>aa</u> \'<u>aa</u>limi <b>a</b>lghaybi wa<b>al</b>sysyah<u>aa</u>dati fayunabbi-ukum bim<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1330, '9:95', 9, 95, ' saya<u>h</u>lifuuna bi<b>al</b>l<u>aa</u>hi lakum i<u>dzaa</u> inqalabtum ilayhim litu\'ri<u>dh</u>uu \'anhum fa-a\'ri<u>dh</u>uu \'anhum innahum rijsun wama/w<u>aa</u>hum jahannamu jaz<u>aa</u>-an bim<u>aa</u> k<u>aa</u>nuu yaksibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1331, '9:96', 9, 96, ' ya<u>h</u>lifuuna lakum litar<u>dh</u>aw \'anhum fa-in tar<u>dh</u>aw \'anhum fa-inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yar<u>daa</u> \'ani <b>a</b>lqawmi <b>a</b>lf<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1332, '9:97', 9, 97, ' al-a\'r<u>aa</u>bu asyaddu kufran wanif<u>aa</u>qan wa-ajdaru <b>al</b>l<u>aa</u> ya\'lamuu <u>h</u>uduuda m<u>aa</u> anzala <b>al</b>l<u>aa</u>hu \'al<u>aa</u> rasuulihi wa<b>al</b>l<u>aa</u>hu \'aliimun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1333, '9:98', 9, 98, ' wamina <b>a</b>l-a\'r<u>aa</u>bi man yattakhi<u>dz</u>u m<u>aa</u> yunfiqu maghraman wayatarabba<u>sh</u>u bikumu <b>al</b>ddaw<u>aa</u>-ira \'alayhim d<u>aa</u>-iratu <b>al</b>ssaw-i wa<b>al</b>l<u>aa</u>hu samii\'un \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1334, '9:99', 9, 99, ' wamina <b>a</b>l-a\'r<u>aa</u>bi man yu/minu bi<b>al</b>l<u>aa</u>hi wa<b>a</b>lyawmi <b>a</b>l-<u>aa</u>khiri wayattakhi<u>dz</u>u m<u>aa</u> yunfiqu qurub<u>aa</u>tin \'inda <b>al</b>l<u>aa</u>hi wa<u>sh</u>alaw<u>aa</u>ti <b>al</b>rrasuuli <b>a</b>l<u>aa</u> innah<u>aa</u> qurbatun lahum sayudkhiluhumu <b>al</b>l<u>aa</u>hu fii ra<u>h</u>matihi inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1335, '9:100', 9, 100, ' wa<b>al</b>ss<u>aa</u>biquuna <b>a</b>l-awwaluuna mina <b>a</b>lmuh<u>aa</u>jiriina wa<b>a</b>l-an<u>shaa</u>ri wa<b>a</b>lla<u>dz</u>iina ittaba\'uuhum bi-i<u>h</u>s<u>aa</u>nin ra<u>dh</u>iya <b>al</b>l<u>aa</u>hu \'anhum wara<u>dh</u>uu \'anhu wa-a\'adda lahum jann<u>aa</u>tin tajrii ta<u>h</u>tah<u>aa</u> <b>a</b>l-anh<u>aa</u>ru kh<u>aa</u>lidiina fiih<u>aa</u> abadan <u>dzaa</u>lika <b>a</b>lfawzu <b>a</b>l\'a<u>zh</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1336, '9:101', 9, 101, ' wamimman <u>h</u>awlakum mina <b>a</b>l-a\'r<u>aa</u>bi mun<u>aa</u>fiquuna wamin ahli <b>a</b>lmadiinati maraduu \'al<u>aa</u> <b>al</b>nnif<u>aa</u>qi l<u>aa</u> ta\'lamuhum na<u>h</u>nu na\'lamuhum sanu\'a<u>dzdz</u>ibuhum marratayni tsumma yuradduuna il<u>aa</u> \'a<u>dzaa</u>bin \'a<u>zh</u>iim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1337, '9:102', 9, 102, ' wa<u>aa</u>kharuuna i\'tarafuu bi<u>dz</u>unuubihim khala<u>th</u>uu \'amalan <u>shaa</u>li<u>h</u>an wa<u>aa</u>khara sayyi-an \'as<u>aa</u> <b>al</b>l<u>aa</u>hu an yatuuba \'alayhim inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1338, '9:103', 9, 103, ' khu<u>dz</u> min amw<u>aa</u>lihim <u>sh</u>adaqatan tu<u>th</u>ahhiruhum watuzakkiihim bih<u>aa</u> wa<u>sh</u>alli \'alayhim inna <u>sh</u>al<u>aa</u>taka sakanun lahum wa<b>al</b>l<u>aa</u>hu samii\'un \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1339, '9:104', 9, 104, ' alam ya\'lamuu anna <b>al</b>l<u>aa</u>ha huwa yaqbalu <b>al</b>ttawbata \'an \'ib<u>aa</u>dihi waya/khu<u>dz</u>u <b>al</b><u>shsh</u>adaq<u>aa</u>ti wa-anna <b>al</b>l<u>aa</u>ha huwa <b>al</b>ttaww<u>aa</u>bu <b>al</b>rra<u>h</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1340, '9:105', 9, 105, ' waquli i\'maluu fasayar<u>aa</u> <b>al</b>l<u>aa</u>hu \'amalakum warasuuluhu wa<b>a</b>lmu/minuuna wasaturadduuna il<u>aa</u> \'<u>aa</u>limi <b>a</b>lghaybi wa<b>al</b>sysyah<u>aa</u>dati fayunabbi-ukum bim<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1341, '9:106', 9, 106, ' wa<u>aa</u>kharuuna murjawna li-amri <b>al</b>l<u>aa</u>hi imm<u>aa</u> yu\'a<u>dzdz</u>ibuhum wa-imm<u>aa</u> yatuubu \'alayhim wa<b>al</b>l<u>aa</u>hu \'aliimun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1342, '9:107', 9, 107, ' wa<b>a</b>lla<u>dz</u>iina ittakha<u>dz</u>uu masjidan <u>dh</u>ir<u>aa</u>ran wakufran watafriiqan bayna <b>a</b>lmu/miniina wa-ir<u>shaa</u>dan liman <u>haa</u>raba <b>al</b>l<u>aa</u>ha warasuulahu min qablu walaya<u>h</u>lifunna in aradn<u>aa</u> ill<u>aa</u> <b>a</b>l<u>h</u>usn<u>aa</u> wa<b>al</b>l<u>aa</u>hu yasyhadu innahum lak<u>aadz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1343, '9:108', 9, 108, ' l<u>aa</u> taqum fiihi abadan lamasjidun ussisa \'al<u>aa</u> <b>al</b>ttaqw<u>aa</u> min awwali yawmin a<u>h</u>aqqu an taquuma fiihi fiihi rij<u>aa</u>lun yu<u>h</u>ibbuuna an yata<u>th</u>ahharuu wa<b>al</b>l<u>aa</u>hu yu<u>h</u>ibbu <b>a</b>lmu<u>ththh</u>ahhiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1344, '9:109', 9, 109, ' afaman assasa buny<u>aa</u>nahu \'al<u>aa</u> taqw<u>aa</u> mina <b>al</b>l<u>aa</u>hi wari<u>dh</u>w<u>aa</u>nin khayrun am man assasa buny<u>aa</u>nahu \'al<u>aa</u> syaf<u>aa</u> jurufin h<u>aa</u>rin fa<b>i</b>nh<u>aa</u>ra bihi fii n<u>aa</u>ri jahannama wa<b>al</b>l<u>aa</u>hu l<u>aa</u> yahdii <b>a</b>lqawma <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1345, '9:110', 9, 110, ' l<u>aa</u> yaz<u>aa</u>lu buny<u>aa</u>nuhumu <b>al</b>la<u>dz</u>ii banaw riibatan fii quluubihim ill<u>aa</u> an taqa<u>ththh</u>a\'a quluubuhum wa<b>al</b>l<u>aa</u>hu \'aliimun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1346, '9:111', 9, 111, ' inna <b>al</b>l<u>aa</u>ha isytar<u>aa</u> mina <b>a</b>lmu/miniina anfusahum wa-amw<u>aa</u>lahum bi-anna lahumu <b>a</b>ljannata yuq<u>aa</u>tiluuna fii sabiili <b>al</b>l<u>aa</u>hi fayaqtuluuna wayuqtaluuna wa\'dan \'alayhi <u>h</u>aqqan fii <b>al</b>ttawr<u>aa</u>ti wa<b>a</b>l-injiili wa<b>a</b>lqur-<u>aa</u>ni waman awf<u>aa</u> bi\'ahdihi mina <b>al</b>l<u>aa</u>hi fa<b>i</b>stabsyiruu bibay\'ikumu <b>al</b>la<u>dz</u>ii b<u>aa</u>ya\'tum bihi wa<u>dzaa</u>lika huwa <b>a</b>lfawzu <b>a</b>l\'a<u>zh</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1347, '9:112', 9, 112, ' a<b>l</b>tt<u>aa</u>-ibuuna <b>a</b>l\'<u>aa</u>biduuna <b>a</b>l<u>haa</u>miduuna <b>al</b>ss<u>aa</u>-i<u>h</u>uuna <b>al</b>rr<u>aa</u>ki\'uuna <b>al</b>ss<u>aa</u>jiduuna <b>a</b>l-<u>aa</u>miruuna bi<b>a</b>lma\'ruufi wa<b>al</b>nn<u>aa</u>huuna \'ani <b>a</b>lmunkari wa<b>a</b>l<u>haa</u>fi<u>zh</u>uuna li<u>h</u>uduudi <b>al</b>l<u>aa</u>hi wabasysyiri <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1348, '9:113', 9, 113, ' m<u>aa</u> k<u>aa</u>na li<b>l</b>nnabiyyi wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu an yastaghfiruu lilmusyrikiina walaw k<u>aa</u>nuu ulii qurb<u>aa</u> min ba\'di m<u>aa</u> tabayyana lahum annahum a<u>sh</u>-<u>haa</u>bu <b>a</b>lja<u>h</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1349, '9:114', 9, 114, ' wam<u>aa</u> k<u>aa</u>na istighf<u>aa</u>ru ibr<u>aa</u>hiima li-abiihi ill<u>aa</u> \'an maw\'idatin wa\'adah<u>aa</u> iyy<u>aa</u>hu falamm<u>aa</u> tabayyana lahu annahu \'aduwwun lill<u>aa</u>hi tabarra-a minhu inna ibr<u>aa</u>hiima la-aww<u>aa</u>hun <u>h</u>aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1350, '9:115', 9, 115, ' wam<u>aa</u> k<u>aa</u>na <b>al</b>l<u>aa</u>hu liyu<u>dh</u>illa qawman ba\'da i<u>dz</u> had<u>aa</u>hum <u>h</u>att<u>aa</u> yubayyina lahum m<u>aa</u> yattaquuna inna <b>al</b>l<u>aa</u>ha bikulli syay-in \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1351, '9:116', 9, 116, ' inna <b>al</b>l<u>aa</u>ha lahu mulku <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i yu<u>h</u>yii wayumiitu wam<u>aa</u> lakum min duuni <b>al</b>l<u>aa</u>hi min waliyyin wal<u>aa</u> na<u>sh</u>iir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1352, '9:117', 9, 117, ' laqad t<u>aa</u>ba <b>al</b>l<u>aa</u>hu \'al<u>aa</u> <b>al</b>nnabiyyi wa<b>a</b>lmuh<u>aa</u>jiriina wa<b>a</b>l-an<u>shaa</u>ri <b>al</b>la<u>dz</u>iina ittaba\'uuhu fii s<u>aa</u>\'ati <b>a</b>l\'usrati min ba\'di m<u>aa</u> k<u>aa</u>da yaziighu quluubu fariiqin minhum tsumma t<u>aa</u>ba \'alayhim innahu bihim rauufun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1353, '9:118', 9, 118, ' wa\'al<u>aa</u> <b>al</b>tstsal<u>aa</u>tsati <b>al</b>la<u>dz</u>iina khullifuu <u>h</u>att<u>aa</u> i<u>dzaa</u> <u>daa</u>qat \'alayhimu <b>a</b>l-ar<u>dh</u>u bim<u>aa</u> ra<u>h</u>ubat wa<u>daa</u>qat \'alayhim anfusuhum wa<u>zh</u>annuu an l<u>aa</u> malja-a mina <b>al</b>l<u>aa</u>hi ill<u>aa</u> ilayhi tsumma t<u>aa</u>ba \'alayhim liyatuubuu inna <b>al</b>l<u>aa</u>ha huwa <b>al</b>ttaww<u>aa</u>bu <b>al</b>rra<u>h</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1354, '9:119', 9, 119, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu ittaquu <b>al</b>l<u>aa</u>ha wakuunuu ma\'a <b>al</b><u>shshaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1355, '9:120', 9, 120, ' m<u>aa</u> k<u>aa</u>na li-ahli <b>a</b>lmadiinati waman <u>h</u>awlahum mina <b>a</b>l-a\'r<u>aa</u>bi an yatakhallafuu \'an rasuuli <b>al</b>l<u>aa</u>hi wal<u>aa</u> yarghabuu bi-anfusihim \'an nafsihi <u>dzaa</u>lika bi-annahum l<u>aa</u> yu<u>sh</u>iibuhum <u>zh</u>amaun wal<u>aa</u> na<u>sh</u>abun wal<u>aa</u> makhma<u>sh</u>atun fii sabiili <b>al</b>l<u>aa</u>hi wal<u>aa</u> ya<u>th</u>auuna maw<u>th</u>i-an yaghii<u>zh</u>u <b>a</b>lkuff<u>aa</u>ra wal<u>aa</u> yan<u>aa</u>luuna min \'aduwwin naylan ill<u>aa</u> kutiba lahum bihi \'amalun <u>shaa</u>li<u>h</u>un inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yu<u>dh</u>ii\'u ajra <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1356, '9:121', 9, 121, ' wal<u>aa</u> yunfiquuna nafaqatan <u>sh</u>aghiiratan wal<u>aa</u> kabiiratan wal<u>aa</u> yaq<u>th</u>a\'uuna w<u>aa</u>diyan ill<u>aa</u> kutiba lahum liyajziyahumu <b>al</b>l<u>aa</u>hu a<u>h</u>sana m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1357, '9:122', 9, 122, ' wam<u>aa</u> k<u>aa</u>na <b>a</b>lmu/minuuna liyanfiruu k<u>aa</u>ffatan falawl<u>aa</u> nafara min kulli firqatin minhum <u>th</u><u>aa</u>-ifatun liyatafaqqahuu fii <b>al</b>ddiini waliyun<u>dz</u>iruu qawmahum i<u>dzaa</u> raja\'uu ilayhim la\'allahum ya<u>hts</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1358, '9:123', 9, 123, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu q<u>aa</u>tiluu <b>al</b>la<u>dz</u>iina yaluunakum mina <b>a</b>lkuff<u>aa</u>ri walyajiduu fiikum ghil<u>zh</u>atan wa<b>i</b>\'lamuu anna <b>al</b>l<u>aa</u>ha ma\'a <b>a</b>lmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1359, '9:124', 9, 124, ' wa-i<u>dzaa</u> m<u>aa</u> unzilat suuratun faminhum man yaquulu ayyukum z<u>aa</u>dat-hu h<u>aadz</u>ihi iim<u>aa</u>nan fa-amm<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu faz<u>aa</u>dat-hum iim<u>aa</u>nan wahum yastabsyiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1360, '9:125', 9, 125, ' wa-amm<u>aa</u> <b>al</b>la<u>dz</u>iina fii quluubihim mara<u>dh</u>un faz<u>aa</u>dat-hum rijsan il<u>aa</u> rijsihim wam<u>aa</u>tuu wahum k<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1361, '9:126', 9, 126, ' awa l<u>aa</u> yarawna annahum yuftanuuna fii kulli \'<u>aa</u>min marratan aw marratayni tsumma l<u>aa</u> yatuubuuna wal<u>aa</u> hum ya<u>dzdz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1362, '9:127', 9, 127, ' wa-i<u>dzaa</u> m<u>aa</u> unzilat suuratun na<u>zh</u>ara ba\'<u>dh</u>uhum il<u>aa</u> ba\'<u>dh</u>in hal yar<u>aa</u>kum min a<u>h</u>adin tsumma in<u>sh</u>arafuu <u>sh</u>arafa <b>al</b>l<u>aa</u>hu quluubahum bi-annahum qawmun l<u>aa</u> yafqahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1363, '9:128', 9, 128, ' laqad j<u>aa</u>-akum rasuulun min anfusikum \'aziizun \'alayhi m<u>aa </u>\'anittum <u>h</u>arii<u>sh</u>un \'alaykum bi<b>a</b>lmu/miniina rauufun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1364, '9:129', 9, 129, ' fa-in tawallaw faqul <u>h</u>asbiya <b>al</b>l<u>aa</u>hu l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa \'alayhi tawakkaltu wahuwa rabbu <b>a</b>l\'arsyi <b>a</b>l\'a<u>zh</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1365, '10:1', 10, 1, ' alif-l<u>aa</u>m-r<u>aa</u> tilka <u>aa</u>y<u>aa</u>tu <b>a</b>lkit<u>aa</u>bi <b>a</b>l<u>h</u>akiim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1366, '10:2', 10, 2, ' ak<u>aa</u>na li<b>l</b>nn<u>aa</u>si \'ajaban an aw<u>h</u>ayn<u>aa</u> il<u>aa</u> rajulin minhum an an<u>dz</u>iri <b>al</b>nn<u>aa</u>sa wabasysyiri <b>al</b>la<u>dz</u>iina <u>aa</u>manuu anna lahum qadama <u>sh</u>idqin \'inda rabbihim q<u>aa</u>la <b>a</b>lk<u>aa</u>firuuna inna h<u>aadzaa</u> las<u>aah</u>irun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1367, '10:3', 10, 3, ' inna rabbakumu <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a fii sittati ayy<u>aa</u>min tsumma istaw<u>aa</u> \'al<u>aa</u> <b>a</b>l\'arsyi yudabbiru <b>a</b>l-amra m<u>aa</u> min syafii\'in ill<u>aa</u> min ba\'di i<u>dz</u>nihi <u>dzaa</u>likumu <b>al</b>l<u>aa</u>hu rabbukum fa<b>u</b>\'buduuhu afal<u>aa</u> ta<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1368, '10:4', 10, 4, ' ilayhi marji\'ukum jamii\'an wa\'da <b>al</b>l<u>aa</u>hi <u>h</u>aqqan innahu yabdau <b>a</b>lkhalqa tsumma yu\'iiduhu liyajziya <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti bi<b>a</b>lqis<u>th</u>i wa<b>a</b>lla<u>dz</u>iina kafaruu lahum syar<u>aa</u>bun min <u>h</u>amiimin wa\'a<u>dzaa</u>bun <b>a</b>liimun bim<u>aa</u> k<u>aa</u>nuu yakfuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1369, '10:5', 10, 5, ' huwa <b>al</b>la<u>dz</u>ii ja\'ala <b>al</b>sysyamsa <u>dh</u>iy<u>aa</u>-an wa<b>a</b>lqamara nuuran waqaddarahu man<u>aa</u>zila lita\'lamuu \'adada <b>al</b>ssiniina wa<b>a</b>l<u>h</u>is<u>aa</u>ba m<u>aa</u> khalaqa <b>al</b>l<u>aa</u>hu <u>dzaa</u>lika ill<u>aa</u> bi<b>a</b>l<u>h</u>aqqi yufa<u>shsh</u>ilu <b>a</b>l-<u>aa</u>y<u>aa</u>ti liqawmin ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1370, '10:6', 10, 6, ' inna fii ikhtil<u>aa</u>fi <b>al</b>layli wa<b>al</b>nnah<u>aa</u>ri wam<u>aa</u> khalaqa <b>al</b>l<u>aa</u>hu fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i la<u>aa</u>y<u>aa</u>tin liqawmin yattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1371, '10:7', 10, 7, ' inna <b>al</b>la<u>dz</u>iina l<u>aa</u> yarjuuna liq<u>aa</u>-an<u>aa</u> wara<u>dh</u>uu bi<b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> wa<b>i</b><u>th</u>ma-annuu bih<u>aa</u> wa<b>a</b>lla<u>dz</u>iina hum \'an <u>aa</u>y<u>aa</u>tin<u>aa</u> gh<u>aa</u>filuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1372, '10:8', 10, 8, ' ul<u>aa</u>-ika ma/w<u>aa</u>humu <b>al</b>nn<u>aa</u>ru bim<u>aa</u> k<u>aa</u>nuu yaksibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1373, '10:9', 10, 9, ' inna <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti yahdiihim rabbuhum bi-iim<u>aa</u>nihim tajrii min ta<u>h</u>tihimu <b>a</b>l-anh<u>aa</u>ru fii jann<u>aa</u>ti <b>al</b>nna\'iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1374, '10:10', 10, 10, ' da\'w<u>aa</u>hum fiih<u>aa</u> sub<u>haa</u>naka <b>al</b>l<u>aa</u>humma wata<u>h</u>iyyatuhum fiih<u>aa</u> sal<u>aa</u>mun wa<u>aa</u>khiru da\'w<u>aa</u>hum ani <b>a</b>l<u>h</u>amdu lill<u>aa</u>hi rabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1375, '10:11', 10, 11, ' walaw yu\'ajjilu <b>al</b>l<u>aa</u>hu li<b>l</b>nn<u>aa</u>si <b>al</b>sysyarra isti\'j<u>aa</u>lahum bi<b>a</b>lkhayri laqu<u>dh</u>iya ilayhim ajaluhum fana<u>dz</u>aru <b>al</b>la<u>dz</u>iina l<u>aa</u> yarjuuna liq<u>aa</u>-an<u>aa</u> fii <u>th</u>ughy<u>aa</u>nihim ya\'mahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1376, '10:12', 10, 12, ' wa-i<u>dzaa</u> massa <b>a</b>l-ins<u>aa</u>na <b>al</b><u>dhdh</u>urru da\'<u>aa</u>n<u>aa</u> lijanbihi aw q<u>aa</u>\'idan aw q<u>aa</u>-iman falamm<u>aa</u> kasyafn<u>aa</u> \'anhu <u>dh</u>urrahu marra ka-an lam yad\'un<u>aa</u> il<u>aa</u> <u>dh</u>urrin massahu ka<u>dzaa</u>lika zuyyina lilmusrifiina m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1377, '10:13', 10, 13, ' walaqad ahlakn<u>aa</u> <b>a</b>lquruuna min qablikum lamm<u>aa</u> <u>zh</u>alamuu waj<u>aa</u>-at-hum rusuluhum bi<b>a</b>lbayyin<u>aa</u>ti wam<u>aa</u> k<u>aa</u>nuu liyu/minuu ka<u>dzaa</u>lika najzii <b>a</b>lqawma <b>a</b>lmujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1378, '10:14', 10, 14, ' tsumma ja\'aln<u>aa</u>kum khal<u>aa</u>-ifa fii <b>a</b>l-ar<u>dh</u>i min ba\'dihim linan<u>zh</u>ura kayfa ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1379, '10:15', 10, 15, ' wa-i<u>dzaa</u> tutl<u>aa</u> \'alayhim <u>aa</u>y<u>aa</u>tun<u>aa</u> bayyin<u>aa</u>tin q<u>aa</u>la <b>al</b>la<u>dz</u>iina l<u>aa</u> yarjuuna liq<u>aa</u>-an<u>aa</u> i/ti biqur-<u>aa</u>nin ghayri h<u>aadzaa</u> aw baddilhu qul m<u>aa</u> yakuunu lii an ubaddilahu min tilq<u>aa</u>-i nafsii in attabi\'u ill<u>aa</u> m<u>aa</u> yuu<u>haa</u> ilayya innii akh<u>aa</u>fu in \'a<u>sh</u>aytu rabbii \'a<u>dzaa</u>ba yawmin \'a<u>zh</u>iim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1380, '10:16', 10, 16, ' qul law sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu m<u>aa</u> talawtuhu \'alaykum wal<u>aa</u> adr<u>aa</u>kum bihi faqad labitstu fiikum \'umuran min qablihi afal<u>aa</u> ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1381, '10:17', 10, 17, ' faman a<u>zh</u>lamu mimmani iftar<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ka<u>dz</u>iban aw ka<u>dzdz</u>aba bi-<u>aa</u>y<u>aa</u>tihi innahu l<u>aa</u> yufli<u>h</u>u <b>a</b>lmujrimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1382, '10:18', 10, 18, ' waya\'buduuna min duuni <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> ya<u>dh</u>urruhum wal<u>aa</u> yanfa\'uhum wayaquuluuna h<u>aa</u>ul<u>aa</u>-i syufa\'<u>aa</u>un<u>aa</u> \'inda <b>al</b>l<u>aa</u>hi qul atunabbi-uuna <b>al</b>l<u>aa</u>ha bim<u>aa</u> l<u>aa</u> ya\'lamu fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wal<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i sub<u>haa</u>nahu wata\'<u>aa</u>l<u>aa</u> \'amm<u>aa</u> yusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1383, '10:19', 10, 19, ' wam<u>aa</u> k<u>aa</u>na <b>al</b>nn<u>aa</u>su ill<u>aa</u> ummatan w<u>aah</u>idatan fa<b>i</b>khtalafuu walawl<u>aa</u> kalimatun sabaqat min rabbika laqu<u>dh</u>iya baynahum fiim<u>aa</u> fiihi yakhtalifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1384, '10:20', 10, 20, ' wayaquuluuna lawl<u>aa</u> unzila \'alayhi <u>aa</u>yatun min rabbihi faqul innam<u>aa</u> <b>a</b>lghaybu lill<u>aa</u>hi fa<b>i</b>nta<u>zh</u>iruu innii ma\'akum mina <b>a</b>lmunta<u>zh</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1385, '10:21', 10, 21, ' wa-i<u>dzaa</u> a<u>dz</u>aqn<u>aa</u> <b>al</b>nn<u>aa</u>sa ra<u>h</u>matan min ba\'di <u>dh</u>arr<u>aa</u>-a massat-hum i<u>dzaa</u> lahum makrun fii <u>aa</u>y<u>aa</u>tin<u>aa</u> quli <b>al</b>l<u>aa</u>hu asra\'u makran inna rusulan<u>aa</u> yaktubuuna m<u>aa</u> tamkuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1386, '10:22', 10, 22, ' huwa <b>al</b>la<u>dz</u>ii yusayyirukum fii <b>a</b>lbarri wa<b>a</b>lba<u>h</u>ri <u>h</u>att<u>aa</u> i<u>dzaa</u> kuntum fii <b>a</b>lfulki wajarayna bihim birii<u>h</u>in <u>th</u>ayyibatin wafari<u>h</u>uu bih<u>aa</u> j<u>aa</u>-at-h<u>aa</u> rii<u>h</u>un \'<u>aas</u>ifun waj<u>aa</u>-ahumu <b>a</b>lmawju min kulli mak<u>aa</u>nin wa<u>zh</u>annuu annahum u<u>h</u>ii<u>th</u>a bihim da\'awuu <b>al</b>l<u>aa</u>ha mukhli<u>sh</u>iina lahu <b>al</b>ddiina la-in anjaytan<u>aa</u> min h<u>aadz</u>ihi lanakuunanna mina <b>al</b>sysy<u>aa</u>kiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1387, '10:23', 10, 23, ' falamm<u>aa</u> anj<u>aa</u>hum i<u>dzaa</u> hum yabghuuna fii <b>a</b>l-ar<u>dh</u>i bighayri <b>a</b>l<u>h</u>aqqi y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su innam<u>aa</u> baghyukum \'al<u>aa</u> anfusikum mat<u>aa</u>\'a <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> tsumma ilayn<u>aa</u> marji\'ukum fanunabbi-ukum bim<u>aa</u> kuntum ta\'maluun<b>a </b>\r\r\n');
INSERT INTO quranLatin VALUES (1388, '10:24', 10, 24, ' innam<u>aa</u> matsalu <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> kam<u>aa</u>-in anzaln<u>aa</u>hu mina <b>al</b>ssam<u>aa</u>-i fa<b>i</b>khtala<u>th</u>a bihi nab<u>aa</u>tu <b>a</b>l-ar<u>dh</u>i mimm<u>aa</u> ya/kulu <b>al</b>nn<u>aa</u>su wa<b>a</b>l-an\'<u>aa</u>mu <u>h</u>att<u>aa</u> i<u>dzaa</u> akha<u>dz</u>ati <b>a</b>l-ar<u>dh</u>u zukhrufah<u>aa</u> wa<b>i</b>zzayyanat wa<u>zh</u>anna ahluh<u>aa</u> annahum q<u>aa</u>diruuna \'alayh<u>aa</u> at<u>aa</u>h<u>aa</u> amrun<u>aa</u> laylan aw nah<u>aa</u>ran faja\'aln<u>aa</u>h<u>aa</u> <u>h</u>a<u>sh</u>iidan ka-an lam taghna bi<b>a</b>l-amsi ka<u>dzaa</u>lika nufa<u>shsh</u>ilu <b>a</b>l-<u>aa</u>y<u>aa</u>ti liqawmin yatafakkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1389, '10:25', 10, 25, ' wa<b>al</b>l<u>aa</u>hu yad\'uu il<u>aa</u> d<u>aa</u>ri <b>al</b>ssal<u>aa</u>mi wayahdii man yasy<u>aa</u>u il<u>aa</u> <u>sh</u>ir<u>aath</u>in mustaqiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1390, '10:26', 10, 26, ' lilla<u>dz</u>iina a<u>h</u>sanuu <b>a</b>l<u>h</u>usn<u>aa</u> waziy<u>aa</u>datun wal<u>aa</u> yarhaqu wujuuhahum qatarun wal<u>aa</u> <u>dz</u>illatun ul<u>aa</u>-ika a<u>sh</u>-<u>haa</u>bu <b>a</b>ljannati hum fiih<u>aa</u> kh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1391, '10:27', 10, 27, ' wa<b>a</b>lla<u>dz</u>iina kasabuu <b>al</b>ssayyi-<u>aa</u>ti jaz<u>aa</u>u sayyi-atin bimitslih<u>aa</u> watarhaquhum <u>dz</u>illatun m<u>aa</u> lahum mina <b>al</b>l<u>aa</u>hi min \'<u>aas</u>imin ka-annam<u>aa</u> ughsyiyat wujuuhuhum qi<u>th</u>a\'an mina <b>al</b>layli mu<u>zh</u>liman ul<u>aa</u>-ika a<u>sh</u>-<u>haa</u>bu <b>al</b>nn<u>aa</u>ri hum fiih<u>aa</u> kh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1392, '10:28', 10, 28, ' wayawma na<u>h</u>syuruhum jamii\'an tsumma naquulu lilla<u>dz</u>iina asyrakuu mak<u>aa</u>nakum antum wasyurak<u>aa</u>ukum fazayyaln<u>aa</u> baynahum waq<u>aa</u>la syurak<u>aa</u>uhum m<u>aa</u> kuntum iyy<u>aa</u>n<u>aa</u> ta\'buduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1393, '10:29', 10, 29, ' fakaf<u>aa</u> bi<b>al</b>l<u>aa</u>hi syahiidan baynan<u>aa</u> wabaynakum in kunn<u>aa</u> \'an \'ib<u>aa</u>datikum lagh<u>aa</u>filiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1394, '10:30', 10, 30, ' hun<u>aa</u>lika tabluu kullu nafsin m<u>aa</u> aslafat warudduu il<u>aa</u> <b>al</b>l<u>aa</u>hi mawl<u>aa</u>humu <b>a</b>l<u>h</u>aqqi wa<u>dh</u>alla \'anhum m<u>aa</u> k<u>aa</u>nuu yaftaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1395, '10:31', 10, 31, ' qul man yarzuqukum mina <b>al</b>ssam<u>aa</u>-i wa<b>a</b>l-ar<u>dh</u>i amman yamliku <b>al</b>ssam\'a wa<b>a</b>l-ab<u>shaa</u>ra waman yukhriju <b>a</b>l<u>h</u>ayya mina <b>a</b>lmayyiti wayukhriju <b>a</b>lmayyita mina <b>a</b>l<u>h</u>ayyi waman yudabbiru <b>a</b>l-amra fasayaquuluuna <b>al</b>l<u>aa</u>hu faqul afal<u>aa</u> tattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1396, '10:32', 10, 32, ' fa<u>dzaa</u>likumu <b>al</b>l<u>aa</u>hu rabbukumu <b>a</b>l<u>h</u>aqqu fam<u>aatsaa</u> ba\'da <b>a</b>l<u>h</u>aqqi ill<u>aa</u> <b>al</b><u>dhdh</u>al<u>aa</u>lu fa-ann<u>aa</u> tu<u>sh</u>rafuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1397, '10:33', 10, 33, ' ka<u>dzaa</u>lika <u>h</u>aqqat kalimatu rabbika \'al<u>aa</u> <b>al</b>la<u>dz</u>iina fasaquu annahum l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1398, '10:34', 10, 34, ' qul hal min syurak<u>aa</u>-ikum man yabdau <b>a</b>lkhalqa tsumma yu\'iiduhu quli <b>al</b>l<u>aa</u>hu yabdau <b>a</b>lkhalqa tsumma yu\'iiduhu fa-ann<u>aa</u> tu/fakuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1399, '10:35', 10, 35, ' qul hal min syurak<u>aa</u>-ikum man yahdii il<u>aa</u> <b>a</b>l<u>h</u>aqqi quli <b>al</b>l<u>aa</u>hu yahdii lil<u>h</u>aqqi afaman yahdii il<u>aa</u> <b>a</b>l<u>h</u>aqqi a<u>h</u>aqqu an yuttaba\'a amman l<u>aa</u> yahiddii ill<u>aa</u> an yuhd<u>aa</u> fam<u>aa</u> lakum kayfa ta<u>h</u>kumuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1400, '10:36', 10, 36, ' wam<u>aa</u> yattabi\'u aktsaruhum ill<u>aa</u> <u>zh</u>annan inna <b>al</b><u>zhzh</u>anna l<u>aa</u> yughnii mina <b>a</b>l<u>h</u>aqqi syay-an inna <b>al</b>l<u>aa</u>ha \'aliimun bim<u>aa</u> yaf\'aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1401, '10:37', 10, 37, ' wam<u>aa</u> k<u>aa</u>na h<u>aadzaa</u> <b>a</b>lqur-<u>aa</u>nu an yuftar<u>aa</u> min duuni <b>al</b>l<u>aa</u>hi wal<u>aa</u>kin ta<u>sh</u>diiqa <b>al</b>la<u>dz</u>ii bayna yadayhi wataf<u>sh</u>iila <b>a</b>lkit<u>aa</u>bi l<u>aa</u> rayba fiihi min rabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1402, '10:38', 10, 38, ' am yaquuluuna iftar<u>aa</u>hu qul fa/tuu bisuuratin mitslihi wa<b>u</b>d\'uu mani ista<u>th</u>a\'tum min duuni <b>al</b>l<u>aa</u>hi in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1403, '10:39', 10, 39, ' bal ka<u>dzdz</u>abuu bim<u>aa</u> lam yu<u>h</u>ii<u>th</u>uu bi\'ilmihi walamm<u>aa</u> ya/tihim ta/wiiluhu ka<u>dzaa</u>lika ka<u>dzdz</u>aba <b>al</b>la<u>dz</u>iina min qablihim fa<b>u</b>n<u>zh</u>ur kayfa k<u>aa</u>na \'<u>aa</u>qibatu <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1404, '10:40', 10, 40, ' waminhum man yu/minu bihi waminhum man l<u>aa</u> yu/minu bihi warabbuka a\'lamu bi<b>a</b>lmufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1405, '10:41', 10, 41, ' wa-in ka<u>dzdz</u>abuuka faqul lii \'amalii walakum \'amalukum antum barii-uuna mimm<u>aa</u> a\'malu wa-an<u>aa</u> barii-un mimm<u>aa</u> ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1406, '10:42', 10, 42, ' waminhum man yastami\'uuna ilayka afa-anta tusmi\'u <b>al</b><u>shsh</u>umma walaw k<u>aa</u>nuu l<u>aa</u> ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1407, '10:43', 10, 43, ' waminhum man yan<u>zh</u>uru ilayka afa-anta tahdii <b>a</b>l\'umya walaw k<u>aa</u>nuu l<u>aa</u> yub<u>sh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1408, '10:44', 10, 44, ' inna <b>al</b>l<u>aa</u>ha l<u>aa</u> ya<u>zh</u>limu <b>al</b>nn<u>aa</u>sa syay-an wal<u>aa</u>kinna <b>al</b>nn<u>aa</u>sa anfusahum ya<u>zh</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1409, '10:45', 10, 45, ' wayawma ya<u>h</u>syuruhum ka-an lam yalbatsuu ill<u>aa</u> s<u>aa</u>\'atan mina <b>al</b>nnah<u>aa</u>ri yata\'<u>aa</u>rafuuna baynahum qad khasira <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu biliq<u>aa</u>-i <b>al</b>l<u>aa</u>hi wam<u>aa</u> k<u>aa</u>nuu muhtadiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1410, '10:46', 10, 46, ' wa-imm<u>aa</u> nuriyannaka ba\'<u>dh</u>a <b>al</b>la<u>dz</u>ii na\'iduhum aw natawaffayannaka fa-ilayn<u>aa</u> marji\'uhum tsumma <b>al</b>l<u>aa</u>hu syahiidun \'al<u>aa</u> m<u>aa</u> yaf\'aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1411, '10:47', 10, 47, ' walikulli ummatin rasuulun fa-i<u>dzaa</u> j<u>aa</u>-a rasuuluhum qu<u>dh</u>iya baynahum bi<b>a</b>lqis<u>th</u>i wahum l<u>aa</u> yu<u>zh</u>lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1412, '10:48', 10, 48, ' wayaquuluuna mat<u>aa</u> h<u>aadzaa</u> <b>a</b>lwa\'du in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1413, '10:49', 10, 49, ' qul l<u>aa</u> amliku linafsii <u>dh</u>arran wal<u>aa</u> naf\'an ill<u>aa</u> m<u>aa</u> sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu likulli ummatin ajalun i<u>dzaa</u> j<u>aa</u>-a ajaluhum fal<u>aa</u> yasta/khiruuna s<u>aa</u>\'atan wal<u>aa</u> yastaqdimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1414, '10:50', 10, 50, ' qul ara-aytum in at<u>aa</u>kum \'a<u>dzaa</u>buhu bay<u>aa</u>tan aw nah<u>aa</u>ran m<u>aatsaa</u> yasta\'jilu minhu <b>a</b>lmujrimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1415, '10:51', 10, 51, ' atsumma i<u>dzaa</u> m<u>aa</u> waqa\'a <u>aa</u>mantum bihi <u>aa</u>l-<u>aa</u>na waqad kuntum bihi tasta\'jiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1416, '10:52', 10, 52, ' tsumma qiila lilla<u>dz</u>iina <u>zh</u>alamuu <u>dz</u>uuquu \'a<u>dzaa</u>ba <b>a</b>lkhuldi hal tujzawna ill<u>aa</u> bim<u>aa</u> kuntum taksibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1417, '10:53', 10, 53, ' wayastanbi-uunaka a<u>h</u>aqqun huwa qul ii warabbii innahu la<u>h</u>aqqun wam<u>aa</u> antum bimu\'jiziin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1418, '10:54', 10, 54, ' walaw anna likulli nafsin <u>zh</u>alamat m<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i la<b>i</b>ftadat bihi wa-asarruu <b>al</b>nnad<u>aa</u>mata lamm<u>aa</u> ra-awuu <b>a</b>l\'a<u>dzaa</u>ba waqu<u>dh</u>iya baynahum bi<b>a</b>lqis<u>th</u>i wahum l<u>aa</u> yu<u>zh</u>lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1419, '10:55', 10, 55, ' al<u>aa</u> inna lill<u>aa</u>hi m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i <b>a</b>l<u>aa</u> inna wa\'da <b>al</b>l<u>aa</u>hi <u>h</u>aqqun wal<u>aa</u>kinna aktsarahum l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1420, '10:56', 10, 56, ' huwa yu<u>h</u>yii wayumiitu wa-ilayhi turja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1421, '10:57', 10, 57, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su qad j<u>aa</u>-atkum maw\'i<u>zh</u>atun min rabbikum wasyif<u>aa</u>un lim<u>aa</u> fii <b>al</b><u>shsh</u>uduuri wahudan wara<u>h</u>matun lilmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1422, '10:58', 10, 58, ' qul bifa<u>dh</u>li <b>al</b>l<u>aa</u>hi wabira<u>h</u>matihi fabi<u>dzaa</u>lika falyafra<u>h</u>uu huwa khayrun mimm<u>aa</u> yajma\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1423, '10:59', 10, 59, ' qul ara-aytum m<u>aa</u> anzala <b>al</b>l<u>aa</u>hu lakum min rizqin faja\'altum minhu <u>h</u>ar<u>aa</u>man wa<u>h</u>al<u>aa</u>lan qul <u>aa</u>ll<u>aa</u>hu a<u>dz</u>ina lakum am \'al<u>aa</u> <b>al</b>l<u>aa</u>hi taftaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1424, '10:60', 10, 60, ' wam<u>aa</u> <u>zh</u>annu <b>al</b>la<u>dz</u>iina yaftaruuna \'al<u>aa</u> <b>al</b>l<u>aa</u>hi <b>a</b>lka<u>dz</u>iba yawma <b>a</b>lqiy<u>aa</u>mati inna <b>al</b>l<u>aa</u>ha la<u>dz</u>uu fa<u>dh</u>lin \'al<u>aa</u> <b>al</b>nn<u>aa</u>si wal<u>aa</u>kinna aktsarahum l<u>aa</u> yasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1425, '10:61', 10, 61, ' wam<u>aa</u> takuunu fii sya/nin wam<u>aa</u> tatluu minhu min qur-<u>aa</u>nin wal<u>aa</u> ta\'maluuna min \'amalin ill<u>aa</u> kunn<u>aa</u> \'alaykum syuhuudan i<u>dz</u> tufii<u>dh</u>uuna fiihi wam<u>aa</u> ya\'zubu \'an rabbika min mitsq<u>aa</u>li <u>dz</u>arratin fii <b>a</b>l-ar<u>dh</u>i wal<u>aa</u> fii <b>al</b>ssam<u>aa</u>-i wal<u>aa</u> a<u>sh</u>ghara min <u>dzaa</u>lika wal<u>aa</u> akbara ill<u>aa</u> fii kit<u>aa</u>bin mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1426, '10:62', 10, 62, ' al<u>aa</u> inna awliy<u>aa</u>-a <b>al</b>l<u>aa</u>hi l<u>aa</u> khawfun \'alayhim wal<u>aa</u> hum ya<u>h</u>zanuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1427, '10:63', 10, 63, ' <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wak<u>aa</u>nuu yattaquun<b>a </b>\r\r\n');
INSERT INTO quranLatin VALUES (1428, '10:64', 10, 64, ' lahumu <b>a</b>lbusyr<u>aa</u> fii <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> wafii <b>a</b>l-<u>aa</u>khirati l<u>aa</u> tabdiila likalim<u>aa</u>ti <b>al</b>l<u>aa</u>hi <u>dzaa</u>lika huwa <b>a</b>lfawzu <b>a</b>l\'a<u>zh</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1429, '10:65', 10, 65, ' wal<u>aa</u> ya<u>h</u>zunka qawluhum inna <b>a</b>l\'izzata lill<u>aa</u>hi jamii\'an huwa <b>al</b>ssamii\'u <b>a</b>l\'aliim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1430, '10:66', 10, 66, ' al<u>aa</u> inna lill<u>aa</u>hi man fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti waman fii <b>a</b>l-ar<u>dh</u>i wam<u>aa</u> yattabi\'u <b>al</b>la<u>dz</u>iina yad\'uuna min duuni <b>al</b>l<u>aa</u>hi syurak<u>aa</u>-a in yattabi\'uuna ill<u>aa</u> <b>al</b><u>zhzh</u>anna wa-in hum ill<u>aa</u> yakhru<u>sh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1431, '10:67', 10, 67, ' huwa <b>al</b>la<u>dz</u>ii ja\'ala lakumu <b>al</b>layla litaskunuu fiihi wa<b>al</b>nnah<u>aa</u>ra mub<u>sh</u>iran inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin liqawmin yasma\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1432, '10:68', 10, 68, ' q<u>aa</u>luu ittakha<u>dz</u>a <b>al</b>l<u>aa</u>hu waladan sub<u>haa</u>nahu huwa <b>a</b>lghaniyyu lahu m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i in \'indakum min sul<u>th</u><u>aa</u>nin bih<u>aadzaa</u> ataquuluuna \'al<u>aa</u> <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1433, '10:69', 10, 69, ' qul inna <b>al</b>la<u>dz</u>iina yaftaruuna \'al<u>aa</u> <b>al</b>l<u>aa</u>hi <b>a</b>lka<u>dz</u>iba l<u>aa</u> yufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1434, '10:70', 10, 70, ' mat<u>aa</u>\'un fii <b>al</b>dduny<u>aa</u> tsumma ilayn<u>aa</u> marji\'uhum tsumma nu<u>dz</u>iiquhumu <b>a</b>l\'a<u>dzaa</u>ba <b>al</b>sysyadiida bim<u>aa</u> k<u>aa</u>nuu yakfuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1435, '10:71', 10, 71, ' wa<b>u</b>tlu \'alayhim naba-a nuu<u>h</u>in i<u>dz</u> q<u>aa</u>la liqawmihi y<u>aa</u> qawmi in k<u>aa</u>na kabura \'alaykum maq<u>aa</u>mii wata<u>dz</u>kiirii bi-<u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi fa\'al<u>aa</u> <b>al</b>l<u>aa</u>hi tawakkaltu fa-ajmi\'uu amrakum wasyurak<u>aa</u>-akum tsumma l<u>aa</u> yakun amrukum \'alaykum ghummatan tsumma iq<u>dh</u>uu ilayya wal<u>aa</u> tun<u>zh</u>iruun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1436, '10:72', 10, 72, ' fa-in tawallaytum fam<u>aa</u> sa-altukum min ajrin in ajriya ill<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi waumirtu an akuuna mina <b>a</b>lmuslimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1437, '10:73', 10, 73, ' faka<u>dzdz</u>abuuhu fanajjayn<u>aa</u>hu waman ma\'ahu fii <b>a</b>lfulki waja\'aln<u>aa</u>hum khal<u>aa</u>-ifa wa-aghraqn<u>aa</u> <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> fa<b>u</b>n<u>zh</u>ur kayfa k<u>aa</u>na \'<u>aa</u>qibatu <b>a</b>lmun<u>dz</u>ariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1438, '10:74', 10, 74, ' tsumma ba\'atsn<u>aa</u> min ba\'dihi rusulan il<u>aa</u> qawmihim faj<u>aa</u>uuhum bi<b>a</b>lbayyin<u>aa</u>ti fam<u>aa</u> k<u>aa</u>nuu liyu/minuu bim<u>aa</u> ka<u>dzdz</u>abuu bihi min qablu ka<u>dzaa</u>lika na<u>th</u>ba\'u \'al<u>aa</u> quluubi <b>a</b>lmu\'tadiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1439, '10:75', 10, 75, ' tsumma ba\'atsn<u>aa</u> min ba\'dihim muus<u>aa</u> wah<u>aa</u>ruuna il<u>aa</u> fir\'awna wamala-ihi bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> fa<b>i</b>stakbaruu wak<u>aa</u>nuu qawman mujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1440, '10:76', 10, 76, ' falamm<u>aa</u> j<u>aa</u>-ahumu <b>a</b>l<u>h</u>aqqu min \'indin<u>aa</u> q<u>aa</u>luu inna h<u>aadzaa</u> lasi<u>h</u>run mubiin<b>un </b>\r\r\n');
INSERT INTO quranLatin VALUES (1441, '10:77', 10, 77, ' q<u>aa</u>la muus<u>aa</u> ataquuluuna lil<u>h</u>aqqi lamm<u>aa</u> j<u>aa</u>-akum asi<u>h</u>run h<u>aadzaa</u> wal<u>aa</u> yufli<u>h</u>u <b>al</b>ss<u>aah</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1442, '10:78', 10, 78, ' q<u>aa</u>luu aji/tan<u>aa</u> litalfitan<u>aa</u> \'amm<u>aa</u> wajadn<u>aa</u> \'alayhi <u>aa</u>b<u>aa</u>-an<u>aa</u> watakuuna lakum<u>aa</u> <b>a</b>lkibriy<u>aa</u>u fii <b>a</b>l-ar<u>dh</u>i wam<u>aa</u> na<u>h</u>nu lakum<u>aa</u> bimu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1443, '10:79', 10, 79, ' waq<u>aa</u>la fir\'awnu i/tuunii bikulli s<u>aah</u>irin \'aliim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1444, '10:80', 10, 80, ' falamm<u>aa</u> j<u>aa</u>-a <b>al</b>ssa<u>h</u>aratu q<u>aa</u>la lahum muus<u>aa</u> <b>a</b>lquu m<u>aa</u> antum mulquun<b>a </b>\r\r\n');
INSERT INTO quranLatin VALUES (1445, '10:81', 10, 81, ' falamm<u>aa</u> <b>a</b>lqaw q<u>aa</u>la muus<u>aa</u> m<u>aa</u> ji/tum bihi <b>al</b>ssi<u>h</u>ru inna <b>al</b>l<u>aa</u>ha sayub<u>th</u>iluhu inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yu<u>sh</u>li<u>h</u>u \'amala <b>a</b>lmufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1446, '10:82', 10, 82, ' wayu<u>h</u>iqqu <b>al</b>l<u>aa</u>hu <b>a</b>l<u>h</u>aqqa bikalim<u>aa</u>tihi walaw kariha <b>a</b>lmujrimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1447, '10:83', 10, 83, ' fam<u>aa</u> <u>aa</u>mana limuus<u>aa</u> ill<u>aa</u> <u>dz</u>urriyyatun min qawmihi \'al<u>aa</u> khawfin min fir\'awna wamala-ihim an yaftinahum wa-inna fir\'awna la\'<u>aa</u>lin fii <b>a</b>l-ar<u>dh</u>i wa-innahu lamina <b>a</b>lmusrifiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1448, '10:84', 10, 84, ' waq<u>aa</u>la muus<u>aa</u> y<u>aa</u> qawmi in kuntum <u>aa</u>mantum bi<b>al</b>l<u>aa</u>hi fa\'alayhi tawakkaluu in kuntum muslimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1449, '10:85', 10, 85, ' faq<u>aa</u>luu \'al<u>aa</u> <b>al</b>l<u>aa</u>hi tawakkaln<u>aa</u> rabban<u>aa</u> l<u>aa</u> taj\'aln<u>aa</u> fitnatan lilqawmi <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1450, '10:86', 10, 86, ' wanajjin<u>aa</u> bira<u>h</u>matika mina <b>a</b>lqawmi <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1451, '10:87', 10, 87, ' wa-aw<u>h</u>ayn<u>aa</u> il<u>aa</u> muus<u>aa</u> wa-akhiihi an tabawwa<u>aa</u> liqawmikum<u>aa</u> bimi<u>sh</u>ra buyuutan wa<b>i</b>j\'aluu buyuutakum qiblatan wa-aqiimuu <b>al</b><u>shsh</u>al<u>aa</u>ta wabasysyiri <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1452, '10:88', 10, 88, ' waq<u>aa</u>la muus<u>aa</u> rabban<u>aa</u> innaka <u>aa</u>tayta fir\'awna wamala-ahu ziinatan wa-amw<u>aa</u>lan fii <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> rabban<u>aa</u> liyu<u>dh</u>illuu \'an sabiilika rabban<u>aa</u> i<u>th</u>mis \'al<u>aa</u> amw<u>aa</u>lihim wa<b>u</b>sydud \'al<u>aa</u> quluubihim fal<u>aa</u> yu/minuu <u>h</u>att<u>aa</u> yarawuu <b>a</b>l\'a<u>dzaa</u>ba <b>a</b>l-aliim<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1453, '10:89', 10, 89, ' q<u>aa</u>la qad ujiibat da\'watukum<u>aa</u> fa<b>i</b>staqiim<u>aa</u> wal<u>aa</u> tattabi\'<u>aa</u>nni sabiila <b>al</b>la<u>dz</u>iina l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1454, '10:90', 10, 90, ' waj<u>aa</u>wazn<u>aa</u> bibanii isr<u>aa</u>-iila <b>a</b>lba<u>h</u>ra fa-atba\'ahum fir\'awnu wajunuuduhu baghyan wa\'adwan <u>h</u>att<u>aa</u> i<u>dzaa</u> adrakahu <b>a</b>lgharaqu q<u>aa</u>la <u>aa</u>mantu annahu l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> <b>al</b>la<u>dz</u>ii <u>aa</u>manat bihi banuu isr<u>aa</u>-iila wa-an<u>aa</u> mina <b>a</b>lmuslimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1455, '10:91', 10, 91, ' <u>aa</u>l-<u>aa</u>na waqad \'a<u>sh</u>ayta qablu wakunta mina <b>a</b>lmufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1456, '10:92', 10, 92, ' fa<b>a</b>lyawma nunajjiika bibadanika litakuuna liman khalfaka <u>aa</u>yatan wa-inna katsiiran mina <b>al</b>nn<u>aa</u>si \'an <u>aa</u>y<u>aa</u>tin<u>aa</u> lagh<u>aa</u>filuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1457, '10:93', 10, 93, ' walaqad bawwa/n<u>aa</u> banii isr<u>aa</u>-iila mubawwa-a <u>sh</u>idqin warazaqn<u>aa</u>hum mina <b>al</b><u>ththh</u>ayyib<u>aa</u>ti fam<u>aa</u> ikhtalafuu <u>h</u>att<u>aa</u> j<u>aa</u>-ahumu <b>a</b>l\'ilmu inna rabbaka yaq<u>dh</u>ii baynahum yawma <b>a</b>lqiy<u>aa</u>mati fiim<u>aa</u> k<u>aa</u>nuu fiihi yakhtalifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1458, '10:94', 10, 94, ' fa-in kunta fii syakkin mimm<u>aa</u> anzalnn<u>aa</u> ilayka fa<b>i</b>s-ali <b>al</b>la<u>dz</u>iina yaqrauuna <b>a</b>lkit<u>aa</u>ba min qablika laqad j<u>aa</u>-aka <b>a</b>l<u>h</u>aqqu min rabbika fal<u>aa</u> takuunanna mina <b>a</b>lmumtariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1459, '10:95', 10, 95, ' wal<u>aa</u> takuunanna mina <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi fatakuuna mina <b>a</b>lkh<u>aa</u>siriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1460, '10:96', 10, 96, ' inna <b>al</b>la<u>dz</u>iina <u>h</u>aqqat \'alayhim kalimatu rabbika l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1461, '10:97', 10, 97, ' walaw j<u>aa</u>-at-hum kullu <u>aa</u>yatin <u>h</u>att<u>aa</u> yarawuu <b>a</b>l\'a<u>dzaa</u>ba <b>a</b>l-aliim<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1462, '10:98', 10, 98, ' falawl<u>aa</u> k<u>aa</u>nat qaryatun <u>aa</u>manat fanafa\'ah<u>aa</u> iim<u>aa</u>nuh<u>aa</u> ill<u>aa</u> qawma yuunusa lamm<u>aa</u> <u>aa</u>manuu kasyafn<u>aa</u> \'anhum \'a<u>dzaa</u>ba <b>a</b>lkhizyi fii <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> wamatta\'n<u>aa</u>hum il<u>aa</u> <u>h</u>iin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1463, '10:99', 10, 99, ' walaw sy<u>aa</u>-a rabbuka la<u>aa</u>mana man fii <b>a</b>l-ar<u>dh</u>i kulluhum jamii\'an afa-anta tukrihu <b>al</b>nn<u>aa</u>sa <u>h</u>att<u>aa</u> yakuunuu mu/miniin<b>a </b>\r\r\n');
INSERT INTO quranLatin VALUES (1464, '10:100', 10, 100, ' wam<u>aa</u> k<u>aa</u>na linafsin an tu/mina ill<u>aa</u> bi-i<u>dz</u>ni <b>al</b>l<u>aa</u>hi wayaj\'alu <b>al</b>rrijsa \'al<u>aa</u> <b>al</b>la<u>dz</u>iina l<u>aa</u> ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1465, '10:101', 10, 101, ' quli un<u>zh</u>uruu m<u>aatsaa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wam<u>aa</u> tughnii <b>a</b>l-<u>aa</u>y<u>aa</u>tu wa<b>al</b>nnu<u>dz</u>uru \'an qawmin l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1466, '10:102', 10, 102, ' fahal yanta<u>zh</u>iruuna ill<u>aa</u> mitsla ayy<u>aa</u>mi <b>al</b>la<u>dz</u>iina khalaw min qablihim qul fa<b>i</b>nta<u>zh</u>iruu innii ma\'akum mina <b>a</b>lmunta<u>zh</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1467, '10:103', 10, 103, ' tsumma nunajjii rusulan<u>aa</u> wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu ka<u>dzaa</u>lika <u>h</u>aqqan \'alayn<u>aa</u> nunjii <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1468, '10:104', 10, 104, ' qul y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su in kuntum fii syakkin min diinii fal<u>aa</u> a\'budu <b>al</b>la<u>dz</u>iina ta\'buduuna min duuni <b>al</b>l<u>aa</u>hi wal<u>aa</u>kin a\'budu <b>al</b>l<u>aa</u>ha <b>al</b>la<u>dz</u>ii yatawaff<u>aa</u>kum waumirtu an akuuna mina <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1469, '10:105', 10, 105, ' wa-an aqim wajhaka li<b>l</b>ddiini <u>h</u>aniifan wal<u>aa</u> takuunanna mina <b>a</b>lmusyrikiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1470, '10:106', 10, 106, ' wal<u>aa</u> tad\'u min duuni <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> yanfa\'uka wal<u>aa</u> ya<u>dh</u>urruka fa-in fa\'alta fa-innaka i<u>dz</u>an mina <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1471, '10:107', 10, 107, ' wa-in yamsaska <b>al</b>l<u>aa</u>hu bi<u>dh</u>urrin fal<u>aa</u> k<u>aa</u>syifa lahu ill<u>aa</u> huwa wa-in yuridka bikhayrin fal<u>aa</u> r<u>aa</u>dda lifa<u>dh</u>lihi yu<u>sh</u>iibu bihi man yasy<u>aa</u>u min \'ib<u>aa</u>dihi wahuwa <b>a</b>lghafuuru <b>al</b>rra<u>h</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1472, '10:108', 10, 108, ' qul y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su qad j<u>aa</u>-akumu <b>a</b>l<u>h</u>aqqu min rabbikum famani ihtad<u>aa</u> fa-innam<u>aa</u> yahtadii linafsihi waman <u>dh</u>alla fa-innam<u>aa</u> ya<u>dh</u>illu \'alayh<u>aa</u> wam<u>aa</u> an<u>aa</u> \'alaykum biwakiil<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1473, '10:109', 10, 109, ' wa<b>i</b>ttabi\' m<u>aa</u> yuu<u>haa</u> ilayka wa<b>i</b><u>sh</u>bir <u>h</u>att<u>aa</u> ya<u>h</u>kuma <b>al</b>l<u>aa</u>hu wahuwa khayru <b>a</b>l<u>haa</u>kimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1474, '11:1', 11, 1, ' alif-l<u>aa</u>m-r<u>aa</u> kit<u>aa</u>bun u<u>h</u>kimat <u>aa</u>y<u>aa</u>tuhu tsumma fu<u>shsh</u>ilat min ladun <u>h</u>akiimin khabiir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1475, '11:2', 11, 2, ' <b>al</b>l<u>aa</u> ta\'buduu ill<u>aa</u> <b>al</b>l<u>aa</u>ha innanii lakum minhu na<u>dz</u>iirun wabasyiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1476, '11:3', 11, 3, ' wa-ani istaghfiruu rabbakum tsumma tuubuu ilayhi yumatti\'kum mat<u>aa</u>\'an <u>h</u>asanan il<u>aa</u> ajalin musamman wayu/ti kulla <u>dz</u>ii fa<u>dh</u>lin fa<u>dh</u>lahu wa-in tawallaw fa-inii akh<u>aa</u>fu \'alaykum \'a<u>dzaa</u>ba yawmin kabiir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1477, '11:4', 11, 4, ' il<u>aa</u> <b>al</b>l<u>aa</u>hi marji\'ukum wahuwa \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1478, '11:5', 11, 5, ' al<u>aa</u> innahum yatsnuuna <u>sh</u>uduurahum liyastakhfuu minhu <b>a</b>l<u>aa</u> <u>h</u>iina yastaghsyuuna tsiy<u>aa</u>bahum ya\'lamu m<u>aa</u> yusirruuna wam<u>aa</u> yu\'linuuna innahu \'aliimun bi<u>dzaa</u>ti <b>al</b><u>shsh</u>uduur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1479, '11:6', 11, 6, ' wam<u>aa</u> min d<u>aa</u>bbatin fii <b>a</b>l-ar<u>dh</u>i ill<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi rizquh<u>aa</u> waya\'lamu mustaqarrah<u>aa</u> wamustawda\'ah<u>aa</u> kullun fii kit<u>aa</u>bin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1480, '11:7', 11, 7, ' wahuwa <b>al</b>la<u>dz</u>ii khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a fii sittati ayy<u>aa</u>min wak<u>aa</u>na \'arsyuhu \'al<u>aa</u> <b>a</b>lm<u>aa</u>-i liyabluwakum ayyukum a<u>h</u>sanu \'amalan wala-in qulta innakum mab\'uutsuuna min ba\'di <b>a</b>lmawti layaquulanna <b>al</b>la<u>dz</u>iina kafaruu in h<u>aadzaa</u> ill<u>aa</u> si<u>h</u>run mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1481, '11:8', 11, 8, ' wala-in akhkharn<u>aa</u> \'anhumu <b>a</b>l\'a<u>dzaa</u>ba il<u>aa</u> ummatin ma\'duudatin layaquulunna m<u>aa</u> ya<u>h</u>bisuhu <b>a</b>l<u>aa</u> yawma ya/tiihim laysa ma<u>sh</u>ruufan \'anhum wa<u>haa</u>qa bihim m<u>aa</u> k<u>aa</u>nuu bihi yastahzi-uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1482, '11:9', 11, 9, ' wala-in a<u>dz</u>aqn<u>aa</u> <b>a</b>l-ins<u>aa</u>na minn<u>aa</u> ra<u>h</u>matan tsumma naza\'n<u>aa</u>h<u>aa</u> minhu innahu layauusun kafuur<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1483, '11:10', 11, 10, ' wala-in a<u>dz</u>aqn<u>aa</u>hu na\'m<u>aa</u>-a ba\'da <u>dh</u>arr<u>aa</u>-a massat-hu layaquulanna <u>dz</u>ahaba <b>al</b>ssayyi-<u>aa</u>tu \'annii innahu lafari<u>h</u>un fakhuur<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1484, '11:11', 11, 11, ' ill<u>aa</u> <b>al</b>la<u>dz</u>iina <u>sh</u>abaruu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti ul<u>aa</u>-ika lahum maghfiratun wa-ajrun kabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1485, '11:12', 11, 12, ' fala\'allaka t<u>aa</u>rikun ba\'<u>dh</u>a m<u>aa</u> yuu<u>haa</u> ilayka wa<u>daa</u>-iqun bihi <u>sh</u>adruka an yaquuluu lawl<u>aa</u> unzila \'alayhi kanzun aw j<u>aa</u>-a ma\'ahu malakun innam<u>aa</u> anta na<u>dz</u>iirun wa<b>al</b>l<u>aa</u>hu \'al<u>aa</u> kulli syay-in wakiil<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1486, '11:13', 11, 13, ' am yaquuluuna iftar<u>aa</u>hu qul fa/tuu bi\'asyri suwarin mitslihi muftaray<u>aa</u>tin wa<b>u</b>d\'uu mani ista<u>th</u>a\'tum min duuni <b>al</b>l<u>aa</u>hi in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1487, '11:14', 11, 14, ' fa-illam yastajiibuu lakum fa<b>i</b>\'lamuu annam<u>aa</u> unzila bi\'ilmi <b>al</b>l<u>aa</u>hi wa-an l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa fahal antum muslimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1488, '11:15', 11, 15, ' man k<u>aa</u>na yuriidu <b>a</b>l<u>h</u>ay<u>aa</u>ta <b>al</b>dduny<u>aa</u> waziinatah<u>aa</u> nuwaffi ilayhim a\'m<u>aa</u>lahum fiih<u>aa</u> wahum fiih<u>aa</u> l<u>aa</u> yubkhasuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1489, '11:16', 11, 16, ' ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina laysa lahum fii <b>a</b>l-<u>aa</u>khirati ill<u>aa</u> <b>al</b>nn<u>aa</u>ru wa<u>h</u>abi<u>th</u>a m<u>aa</u> <u>sh</u>ana\'uu fiih<u>aa</u> wab<u>aath</u>ilun m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1490, '11:17', 11, 17, ' afaman k<u>aa</u>na \'al<u>aa</u> bayyinatin min rabbihi wayatluuhu sy<u>aa</u>hidun minhu wamin qablihi kit<u>aa</u>bu muus<u>aa</u> im<u>aa</u>man wara<u>h</u>matan ul<u>aa</u>-ika yu/minuuna bihi waman yakfur bihi mina <b>a</b>l-a<u>h</u>z<u>aa</u>bi fa<b>al</b>nn<u>aa</u>ru maw\'iduhu fal<u>aa</u> taku fii miryatin minhu innahu <b>a</b>l<u>h</u>aqqu min rabbika wal<u>aa</u>kinna aktsara <b>al</b>nn<u>aa</u>si l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1491, '11:18', 11, 18, ' waman a<u>zh</u>lamu mimmani iftar<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ka<u>dz</u>iban ul<u>aa</u>-ika yu\'ra<u>dh</u>uuna \'al<u>aa</u> rabbihim wayaquulu <b>a</b>l-asyh<u>aa</u>du h<u>aa</u>ul<u>aa</u>-i <b>al</b>la<u>dz</u>iina ka<u>dz</u>abuu \'al<u>aa</u> rabbihim <b>a</b>l<u>aa</u> la\'natu <b>al</b>l<u>aa</u>hi \'al<u>aa</u> <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1492, '11:19', 11, 19, ' <b>al</b>la<u>dz</u>iina ya<u>sh</u>udduuna \'an sabiili <b>al</b>l<u>aa</u>hi wayabghuunah<u>aa</u> \'iwajan wahum bi<b>a</b>l-<u>aa</u>khirati hum k<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1493, '11:20', 11, 20, ' ul<u>aa</u>-ika lam yakuunuu mu\'jiziina fii <b>a</b>l-ar<u>dh</u>i wam<u>aa</u> k<u>aa</u>na lahum min duuni <b>al</b>l<u>aa</u>hi min awliy<u>aa</u>-a yu<u>daa</u>\'afu lahumu <b>a</b>l\'a<u>dzaa</u>bu m<u>aa</u> k<u>aa</u>nuu yasta<u>th</u>ii\'uuna <b>al</b>ssam\'a wam<u>aa</u> k<u>aa</u>nuu yub<u>sh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1494, '11:21', 11, 21, ' ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina khasiruu anfusahum wa<u>dh</u>alla \'anhum m<u>aa</u> k<u>aa</u>nuu yaftaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1495, '11:22', 11, 22, ' l<u>aa</u> jarama annahum fii <b>a</b>l-<u>aa</u>khirati humu <b>a</b>l-akhsaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1496, '11:23', 11, 23, ' inna <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shaa</u>li<u>haa</u>ti wa-akhbatuu il<u>aa</u> rabbihim ul<u>aa</u>-ika a<u>sh</u>-<u>haa</u>bu <b>a</b>ljannati hum fiih<u>aa</u> kh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1497, '11:24', 11, 24, ' matsalu <b>a</b>lfariiqayni ka<b>a</b>l<b>-</b>a\'m<u>aa</u> wa<b>a</b>l-a<u>sh</u>ammi wa<b>a</b>lba<u>sh</u>iiri wa<b>al</b>ssamii\'i hal yastawiy<u>aa</u>ni matsalan afal<u>aa</u> ta<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1498, '11:25', 11, 25, ' walaqad arsaln<u>aa</u> nuu<u>h</u>an il<u>aa</u> qawmihi innii lakum na<u>dz</u>iirun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1499, '11:26', 11, 26, ' an l<u>aa</u> ta\'buduu ill<u>aa</u> <b>al</b>l<u>aa</u>ha innii akh<u>aa</u>fu \'alaykum \'a<u>dzaa</u>ba yawmin <b>a</b>liim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1500, '11:27', 11, 27, ' faq<u>aa</u>la <b>a</b>lmalau <b>al</b>la<u>dz</u>iina kafaruu min qawmihi m<u>aa</u> nar<u>aa</u>ka ill<u>aa</u> basyaran mitslan<u>aa</u> wam<u>aa</u> nar<u>aa</u>ka ittaba\'aka ill<u>aa</u> <b>al</b>la<u>dz</u>iina hum ar<u>aadz</u>ilun<u>aa</u> b<u>aa</u>diya <b>al</b>rra/yi wam<u>aa</u> nar<u>aa</u> lakum \'alayn<u>aa</u> min fa<u>dh</u>lin bal na<u>zh</u>unnukum k<u>aadz</u>ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1501, '11:28', 11, 28, ' q<u>aa</u>la y<u>aa</u> qawmi ara-aytum in kuntu \'al<u>aa</u> bayyinatin min rabbii wa<u>aa</u>t<u>aa</u>nii ra<u>h</u>matan min \'indihi fa\'ummiyat \'alaykum anulzimukumuuh<u>aa</u> wa-antum lah<u>aa</u> k<u>aa</u>rihuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1502, '11:29', 11, 29, ' way<u>aa</u> qawmi l<u>aa</u> as-alukum \'alayhi m<u>aa</u>lan in ajriya ill<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi wam<u>aa</u> an<u>aa</u> bi<u>th</u><u>aa</u>ridi <b>al</b>la<u>dz</u>iina <u>aa</u>manuu innahum mul<u>aa</u>quu rabbihim wal<u>aa</u>kinnii ar<u>aa</u>kum qawman tajhaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1503, '11:30', 11, 30, ' way<u>aa</u> qawmi man yan<u>sh</u>urunii mina <b>al</b>l<u>aa</u>hi in <u>th</u>aradtuhum afal<u>aa</u> ta<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1504, '11:31', 11, 31, ' wal<u>aa</u> aquulu lakum \'indii khaz<u>aa</u>-inu <b>al</b>l<u>aa</u>hi wal<u>aa</u> a\'lamu <b>a</b>lghayba wal<u>aa</u> aquulu innii malakun wal<u>aa</u> aquulu lilla<u>dz</u>iina tazdarii a\'yunukum lan yu/tiyahumu <b>al</b>l<u>aa</u>hu khayran <b>al</b>l<u>aa</u>hu a\'lamu bim<u>aa</u> fii anfusihim innii i<u>dz</u>an lamina <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1505, '11:32', 11, 32, ' q<u>aa</u>luu y<u>aa</u> nuu<u>h</u>u qad j<u>aa</u>daltan<u>aa</u> fa-aktsarta jid<u>aa</u>lan<u>aa</u> fa/tin<u>aa</u> bim<u>aa</u> ta\'idun<u>aa</u> in kunta mina <b>al</b><u>shshaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1506, '11:33', 11, 33, ' q<u>aa</u>la innam<u>aa</u> ya/tiikum bihi <b>al</b>l<u>aa</u>hu in sy<u>aa</u>-a wam<u>aa</u> antum bimu\'jiziin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1507, '11:34', 11, 34, ' wal<u>aa</u> yanfa\'ukum nu<u>sy</u>ii in aradtu an an<u>sh</u>a<u>h</u>a lakum in k<u>aa</u>na <b>al</b>l<u>aa</u>hu yuriidu an yughwiyakum huwa rabbukum wa-ilayhi turja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1508, '11:35', 11, 35, ' am yaquuluuna iftar<u>aa</u>hu qul ini iftaraytuhu fa\'alayya ijr<u>aa</u>mii wa-an<u>aa</u> barii-un mimm<u>aa</u> tujrimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1509, '11:36', 11, 36, ' wauu<u>h</u>iya il<u>aa</u> nuu<u>h</u>in annahu lan yu/mina min qawmika ill<u>aa</u> man qad <u>aa</u>mana fal<u>aa</u> tabta-is bim<u>aa</u> k<u>aa</u>nuu yaf\'aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1510, '11:37', 11, 37, ' wa<b>i</b><u>sh</u>na\'i <b>a</b>lfulka bi-a\'yunin<u>aa</u> wawa<u>h</u>yin<u>aa</u> wal<u>aa</u> tukh<u>aath</u>ibnii fii <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu innahum mughraquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1511, '11:38', 11, 38, ' waya<u>sh</u>na\'u <b>a</b>lfulka wakullam<u>aa</u> marra \'alayhi malaun min qawmihi sakhiruu minhu q<u>aa</u>la in taskharuu minn<u>aa</u> fa-inn<u>aa</u> naskharu minkum kam<u>aa</u> taskharuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1512, '11:39', 11, 39, ' fasawfa ta\'lamuuna man ya/tiihi \'a<u>dzaa</u>bun yukhziihi waya<u>h</u>illu \'alayhi \'a<u>dzaa</u>bun muqiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1513, '11:40', 11, 40, ' <u>h</u>att<u>aa</u> i<u>dzaa</u> j<u>aa</u>-a amrun<u>aa</u> waf<u>aa</u>ra <b>al</b>ttannuuru quln<u>aa</u> i<u>h</u>mil fiih<u>aa</u> min kullin zawjayni itsnayni wa-ahlaka ill<u>aa</u> man sabaqa \'alayhi <b>a</b>lqawlu waman <u>aa</u>mana wam<u>aa</u> <u>aa</u>mana ma\'ahu ill<u>aa</u> qaliil<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1514, '11:41', 11, 41, ' waq<u>aa</u>la irkabuu fiih<u>aa</u> bismi <b>al</b>l<u>aa</u>hi majr<u>aa</u>h<u>aa</u> wamurs<u>aa</u>h<u>aa</u> inna rabbii laghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1515, '11:42', 11, 42, ' wahiya tajrii bihim fii mawjin ka<b>a</b>ljib<u>aa</u>li wan<u>aa</u>d<u>aa</u> nuu<u>h</u>unu ibnahu wak<u>aa</u>na fii ma\'zilin y<u>aa</u> bunayya irkab ma\'an<u>aa</u> wal<u>aa</u> takun ma\'a <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1516, '11:43', 11, 43, ' q<u>aa</u>la sa<u>aa</u>wii il<u>aa</u> jabalin ya\'<u>sh</u>imunii mina <b>a</b>lm<u>aa</u>-i q<u>aa</u>la l<u>aa</u> \'<u>aas</u>ima <b>a</b>lyawma min amri <b>al</b>l<u>aa</u>hi ill<u>aa</u> man ra<u>h</u>ima wa<u>haa</u>la baynahum<u>aa</u> <b>a</b>lmawju fak<u>aa</u>na mina <b>a</b>lmughraqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1517, '11:44', 11, 44, ' waqiila y<u>aa</u> ar<u>dh</u>u ibla\'ii m<u>aa</u>-aki way<u>aa</u> sam<u>aa</u>u aqli\'ii waghii<u>dh</u>a <b>a</b>lm<u>aa</u>u waqu<u>dh</u>iya <b>a</b>l-amru wa<b>i</b>stawat \'al<u>aa</u> <b>a</b>ljuudiyyi waqiila bu\'dan lilqawmi <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1518, '11:45', 11, 45, ' wan<u>aa</u>d<u>aa</u> nuu<u>h</u>un rabbahu faq<u>aa</u>la rabbi inna ibnii min ahlii wa-inna wa\'daka <b>a</b>l<u>h</u>aqqu wa-anta a<u>h</u>kamu <b>a</b>l<u>haa</u>kimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1519, '11:46', 11, 46, ' q<u>aa</u>la y<u>aa</u> nuu<u>h</u>u innahu laysa min ahlika innahu \'amalun ghayru <u>shaa</u>li<u>h</u>in fal<u>aa</u> tas-alni m<u>aa</u> laysa laka bihi \'ilmun innii a\'i<u>zh</u>uka an takuuna mina <b>a</b>lj<u>aa</u>hiliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1520, '11:47', 11, 47, ' q<u>aa</u>la rabbi innii a\'uu<u>dz</u>u bika an as-alaka m<u>aa</u> laysa lii bihi \'ilmun wa-ill<u>aa</u> taghfir lii watar<u>h</u>amnii akun mina <b>a</b>lkh<u>aa</u>siriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1521, '11:48', 11, 48, ' qiila y<u>aa</u> nuu<u>h</u>u ihbi<u>th</u> bisal<u>aa</u>min minn<u>aa</u> wabarak<u>aa</u>tin \'alayka wa\'al<u>aa</u> umamin mimman ma\'aka waumamun sanumatti\'uhum tsumma yamassuhum minn<u>aa</u> \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1522, '11:49', 11, 49, ' tilka min anb<u>aa</u>-i <b>a</b>lghaybi nuu<u>h</u>iih<u>aa</u> ilayka m<u>aa</u> kunta ta\'lamuh<u>aa</u> anta wal<u>aa</u> qawmuka min qabli h<u>aadzaa</u> fa<b>i</b><u>sh</u>bir inna <b>a</b>l\'<u>aa</u>qibata lilmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1523, '11:50', 11, 50, ' wa-il<u>aa</u> \'<u>aa</u>din akh<u>aa</u>hum huudan q<u>aa</u>la y<u>aa</u> qawmi u\'buduu <b>al</b>l<u>aa</u>ha m<u>aa</u> lakum min il<u>aa</u>hin ghayruhu in antum ill<u>aa</u> muftaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1524, '11:51', 11, 51, ' y<u>aa</u> qawmi l<u>aa</u> as-alukum \'alayhi ajran in ajriya ill<u>aa</u> \'al<u>aa</u> <b>al</b>la<u>dz</u>ii fa<u>th</u>aranii afal<u>aa</u> ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1525, '11:52', 11, 52, ' way<u>aa</u> qawmi istaghfiruu rabbakum tsumma tuubuu ilayhi yursili <b>al</b>ssam<u>aa</u>-a \'alaykum midr<u>aa</u>ran wayazidkum quwwatan il<u>aa</u> quwwatikum wal<u>aa</u> tatawallaw mujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1526, '11:53', 11, 53, ' q<u>aa</u>luu y<u>aa</u> huudu m<u>aa</u> ji/tan<u>aa</u> bibayyinatin wam<u>aa</u> na<u>h</u>nu bit<u>aa</u>rikii <u>aa</u>lihatin<u>aa</u> \'an qawlika wam<u>aa</u> na<u>h</u>nu laka bimu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1527, '11:54', 11, 54, ' in naquulu ill<u>aa</u> i\'tar<u>aa</u>ka ba\'<u>dh</u>u <u>aa</u>lihatin<u>aa</u> bisuu-in q<u>aa</u>la innii usyhidu <b>al</b>l<u>aa</u>ha wa<b>i</b>syhaduu annii barii-un mimm<u>aa</u> tusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1528, '11:55', 11, 55, ' min duunihi fakiiduunii jamii\'an tsumma l<u>aa</u> tun<u>zh</u>iruun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1529, '11:56', 11, 56, ' innii tawakkaltu \'al<u>aa</u> <b>al</b>l<u>aa</u>hi rabbii warabbikum m<u>aa</u> min d<u>aa</u>bbatin ill<u>aa</u> huwa <u>aa</u>khi<u>dz</u>un bin<u>aas</u>iyatih<u>aa</u> inna rabbii \'al<u>aa</u> <u>sh</u>ir<u>aath</u>in mustaqiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1530, '11:57', 11, 57, ' fa-in tawallaw faqad ablaghtukum m<u>aa</u> ursiltu bihi ilaykum wayastakhlifu rabbii qawman ghayrakum wal<u>aa</u> ta<u>dh</u>urruunahu syay-an inna rabbii \'al<u>aa</u> kulli syay-in <u>h</u>afii<u>zh</u><b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1531, '11:58', 11, 58, ' walamm<u>aa</u> j<u>aa</u>-a amrun<u>aa</u> najjayn<u>aa</u> huudan wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu ma\'ahu bira<u>h</u>matin minn<u>aa</u> wanajjayn<u>aa</u>hum min \'a<u>dzaa</u>bin ghalii<u>zh</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1532, '11:59', 11, 59, ' watilka \'<u>aa</u>dun ja<u>h</u>aduu bi-<u>aa</u>y<u>aa</u>ti rabbihim wa\'a<u>sh</u>aw rusulahu wa<b>i</b>ttaba\'uu amra kulli jabb<u>aa</u>rin \'aniid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1533, '11:60', 11, 60, ' wautbi\'uu fii h<u>aadz</u>ihi <b>al</b>dduny<u>aa</u> la\'natan wayawma <b>a</b>lqiy<u>aa</u>mati <b>a</b>l<u>aa</u> inna \'<u>aa</u>dan kafaruu rabbahum <b>a</b>l<u>aa</u> bu\'dan li\'<u>aa</u>din qawmi huud<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1534, '11:61', 11, 61, ' wa-il<u>aa</u> tsamuuda akh<u>aa</u>hum <u>shaa</u>li<u>h</u>an q<u>aa</u>la y<u>aa</u> qawmi u\'buduu <b>al</b>l<u>aa</u>ha m<u>aa</u> lakum min il<u>aa</u>hin ghayruhu huwa ansya-akum mina <b>a</b>l-ar<u>dh</u>i wa<b>i</b>sta\'marakum fiih<u>aa</u> fa<b>i</b>staghfiruuhu tsumma tuubuu ilayhi inna rabbii qariibun mujiib<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1535, '11:62', 11, 62, ' q<u>aa</u>luu y<u>aa</u> <u>shaa</u>li<u>h</u>u qad kunta fiin<u>aa</u> marjuwwan qabla h<u>aadzaa</u> atanh<u>aa</u>n<u>aa</u> an na\'buda m<u>aa</u> ya\'budu <u>aa</u>b<u>aa</u>un<u>aa</u> wa-innan<u>aa</u> lafii syakkin mimm<u>aa</u> tad\'uun<u>aa</u> ilayhi muriib<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1536, '11:63', 11, 63, ' q<u>aa</u>la y<u>aa</u> qawmi ara-aytum in kuntu \'al<u>aa</u> bayyinatin min rabbii wa<u>aa</u>t<u>aa</u>nii minhu ra<u>h</u>matan faman yan<u>sh</u>urunii mina <b>al</b>l<u>aa</u>hi in \'a<u>sh</u>aytuhu fam<u>aa</u> taziiduunanii ghayra takhsiir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1537, '11:64', 11, 64, ' way<u>aa</u> qawmi h<u>aadz</u>ihi n<u>aa</u>qatu <b>al</b>l<u>aa</u>hi lakum <u>aa</u>yatan fa<u>dz</u>aruuh<u>aa</u> ta/kul fii ar<u>dh</u>i <b>al</b>l<u>aa</u>hi wal<u>aa</u> tamassuuh<u>aa</u> bisuu-in faya/khu<u>dz</u>akum \'a<u>dzaa</u>bun qariib<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1538, '11:65', 11, 65, ' fa\'aqaruuh<u>aa</u> faq<u>aa</u>la tamatta\'uu fii d<u>aa</u>rikum tsal<u>aa</u>tsata ayy<u>aa</u>min <u>dzaa</u>lika wa\'dun ghayru mak<u>dz</u>uub<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1539, '11:66', 11, 66, ' falamm<u>aa</u> j<u>aa</u>-a amrun<u>aa</u> najjayn<u>aa</u> <u>shaa</u>li<u>h</u>an wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu ma\'ahu bira<u>h</u>matin minn<u>aa</u> wamin khizyi yawmi-i<u>dz</u>in inna rabbaka huwa <b>a</b>lqawiyyu <b>a</b>l\'aziiz<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1540, '11:67', 11, 67, ' wa-akha<u>dz</u>a <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu <b>al</b><u>shsh</u>ay<u>h</u>atu fa-a<u>sh</u>ba<u>h</u>uu fii diy<u>aa</u>rihim j<u>aa</u>tsimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1541, '11:68', 11, 68, ' ka-an lam yaghnaw fiih<u>aa</u> <b>a</b>l<u>aa</u> inna tsamuuda kafaruu rabbahum <b>a</b>l<u>aa</u> bu\'dan litsamuud<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1542, '11:69', 11, 69, ' walaqad j<u>aa</u>-at rusulun<u>aa</u> ibr<u>aa</u>hiima bi<b>a</b>lbusyr<u>aa</u> q<u>aa</u>luu sal<u>aa</u>man q<u>aa</u>la sal<u>aa</u>mun fam<u>aa</u> labitsa an j<u>aa</u>-a bi\'ijlin <u>h</u>anii<u>dz</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1543, '11:70', 11, 70, ' falamm<u>aa</u> ra<u>aa</u> aydiyahum l<u>aa</u> ta<u>sh</u>ilu ilayhi nakirahum wa-awjasa minhum khiifatan q<u>aa</u>luu l<u>aa</u> takhaf inn<u>aa</u> ursiln<u>aa</u> il<u>aa</u> qawmi luu<u>th</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1544, '11:71', 11, 71, ' wa<b>i</b>mra-atuhu q<u>aa</u>-imatun fa<u>dh</u>a<u>h</u>ikat fabasysyarn<u>aa</u>h<u>aa</u> bi-is<u>haa</u>qa wamin war<u>aa</u>-i is<u>haa</u>qa ya\'quub<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1545, '11:72', 11, 72, ' q<u>aa</u>lat y<u>aa</u> waylat<u>aa</u> a-alidu wa-an<u>aa</u> \'ajuuzun wah<u>aadzaa</u> ba\'lii syaykhan inna h<u>aadzaa</u> lasyay-un \'ajiib<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1546, '11:73', 11, 73, ' q<u>aa</u>luu ata\'jabiina min amri <b>al</b>l<u>aa</u>hi ra<u>h</u>matu <b>al</b>l<u>aa</u>hi wabarak<u>aa</u>tuhu \'alaykum ahla <b>a</b>lbayti innahu <u>h</u>amiidun majiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1547, '11:74', 11, 74, ' falamm<u>aa</u> <u>dz</u>ahaba \'an ibr<u>aa</u>hiima <b>al</b>rraw\'u waj<u>aa</u>-at-hu <b>a</b>lbusyr<u>aa </u>yuj<u>aa</u>dilun<u>aa</u> fii qawmi luu<u>th</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1548, '11:75', 11, 75, ' inna ibr<u>aa</u>hiima la<u>h</u>aliimun aww<u>aa</u>hun muniib<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1549, '11:76', 11, 76, ' y<u>aa</u> ibr<u>aa</u>hiimu a\'ri<u>dh</u> \'an h<u>aadzaa</u> innahu qad j<u>aa</u>-a amru rabbika wa-innahum <u>aa</u>tiihim \'a<u>dzaa</u>bun ghayru marduud<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1550, '11:77', 11, 77, ' walamm<u>aa</u> j<u>aa</u>-at rusulun<u>aa</u> luu<u>th</u>an sii-a bihim wa<u>daa</u>qa bihim <u>dz</u>ar\'an waq<u>aa</u>la h<u>aadzaa</u> yawmun \'a<u>sh</u>iib<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1551, '11:78', 11, 78, ' waj<u>aa</u>-ahu qawmuhu yuhra\'uuna ilayhi wamin qablu k<u>aa</u>nuu ya\'maluuna <b>al</b>ssayyi-<u>aa</u>ti q<u>aa</u>la y<u>aa</u> qawmi h<u>aa</u>ul<u>aa</u>-i ban<u>aa</u>tii hunna a<u>th</u>haru lakum fa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha wal<u>aa</u> tukhzuuni fii <u>dh</u>ayfii alaysa minkum rajulun rasyiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1552, '11:79', 11, 79, ' q<u>aa</u>luu laqad \'alimta m<u>aa</u> lan<u>aa</u> fii ban<u>aa</u>tika min <u>h</u>aqqin wa-innaka lata\'lamu m<u>aa</u> nuriid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1553, '11:80', 11, 80, ' q<u>aa</u>la law anna lii bikum quwwatan aw <u>aa</u>wii il<u>aa</u> ruknin syadiid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1554, '11:81', 11, 81, ' q<u>aa</u>luu y<u>aa</u> luu<u>th</u>u inn<u>aa</u> rusulu rabbika lan ya<u>sh</u>iluu ilayka fa-asri bi-ahlika biqi<u>th</u>\'in mina <b>al</b>layli wal<u>aa</u> yaltafit minkum a<u>h</u>adun ill<u>aa</u> imra-ataka innahu mu<u>sh</u>iibuh<u>aa</u> m<u>aa</u> a<u>shaa</u>bahum inna maw\'idahumu <b>al</b><u>shsh</u>ub<u>h</u>u alaysa <b>al</b><u>shsh</u>ub<u>h</u>u biqariib<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1555, '11:82', 11, 82, ' falamm<u>aa</u> j<u>aa</u>-a amrun<u>aa</u> ja\'aln<u>aa</u> \'<u>aa</u>liyah<u>aa</u> s<u>aa</u>filah<u>aa</u> wa-am<u>th</u>arn<u>aa</u> \'alayh<u>aa</u> <u>h</u>ij<u>aa</u>ratan min sijjiilin man<u>dh</u>uud<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1556, '11:83', 11, 83, ' musawwamatan \'inda rabbika wam<u>aa</u> hiya mina <b>al</b><u>zhzhaa</u>limiina biba\'iid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1557, '11:84', 11, 84, ' wa-il<u>aa</u> madyana akh<u>aa</u>hum syu\'ayban q<u>aa</u>la y<u>aa</u> qawmi u\'buduu <b>al</b>l<u>aa</u>ha m<u>aa</u> lakum min il<u>aa</u>hin ghayruhu wal<u>aa</u> tanqu<u>sh</u>uu <b>a</b>lmiky<u>aa</u>la wa<b>a</b>lmiiz<u>aa</u>na innii ar<u>aa</u>kum bikhayrin wa-innii akh<u>aa</u>fu \'alaykum \'a<u>dzaa</u>ba yawmin mu<u>h</u>ii<u>th</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1558, '11:85', 11, 85, ' way<u>aa</u> qawmi awfuu <b>a</b>lmiky<u>aa</u>la wa<b>a</b>lmiiz<u>aa</u>na bi<b>a</b>lqis<u>th</u>i wal<u>aa</u> tabkhasuu <b>al</b>nn<u>aa</u>sa asyy<u>aa</u>-ahum wal<u>aa</u> ta\'tsaw fii <b>a</b>l-ar<u>dh</u>i mufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1559, '11:86', 11, 86, ' baqiyyatu <b>al</b>l<u>aa</u>hi khayrun lakum in kuntum mu/miniina wam<u>aa</u> an<u>aa</u> \'alaykum bi<u>h</u>afii<u>zh</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1560, '11:87', 11, 87, ' q<u>aa</u>luu y<u>aa</u> syu\'aybu a<u>sh</u>al<u>aa</u>tuka ta/muruka an natruka m<u>aa</u> ya\'budu <u>aa</u>b<u>aa</u>un<u>aa</u> aw an naf\'ala fii amw<u>aa</u>lin<u>aa</u> m<u>aa</u> nasy<u>aa</u>u innaka la-anta <b>a</b>l<u>h</u>aliimu <b>al</b>rrasyiid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1561, '11:88', 11, 88, ' q<u>aa</u>la y<u>aa</u> qawmi ara-aytum in kuntu \'al<u>aa</u> bayyinatin min rabbii warazaqanii minhu rizqan <u>h</u>asanan wam<u>aa</u> uriidu an ukh<u>aa</u>lifakum il<u>aa</u> m<u>aa</u> anh<u>aa</u>kum \'anhu in uriidu ill<u>aa</u> <b>a</b>l-i<u>sh</u>l<u>aah</u>a m<u>aa</u> ista<u>th</u>a\'tu wam<u>aa</u> tawfiiqii ill<u>aa</u> bi<b>al</b>l<u>aa</u>hi \'alayhi tawakkaltu wa-ilayhi uniib<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1562, '11:89', 11, 89, ' way<u>aa</u> qawmi l<u>aa</u> yajrimannakum syiq<u>aa</u>qii an yu<u>sh</u>iibakum mitslu m<u>aa</u> a<u>shaa</u>ba qawma nuu<u>h</u>in aw qawma huudin aw qawma <u>shaa</u>li<u>h</u>in wam<u>aa</u> qawmu luu<u>th</u>in minkum biba\'iid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1563, '11:90', 11, 90, ' wa<b>i</b>staghfiruu rabbakum tsumma tuubuu ilayhi inna rabbii ra<u>h</u>iimun waduud<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1564, '11:91', 11, 91, ' q<u>aa</u>luu y<u>aa</u> syu\'aybu m<u>aa</u> nafqahu katsiiran mimm<u>aa</u> taquulu wa-inn<u>aa</u> lanar<u>aa</u>ka fiin<u>aa</u> <u>dh</u>a\'iifan walawl<u>aa</u> rah<u>th</u>uka larajamn<u>aa</u>ka wam<u>aa</u> anta \'alayn<u>aa</u> bi\'aziiz<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1565, '11:92', 11, 92, ' q<u>aa</u>la y<u>aa</u> qawmi arah<u>th</u>ii a\'azzu \'alaykum mina <b>al</b>l<u>aa</u>hi wa<b>i</b>ttakha<u>dz</u>tumuuhu war<u>aa</u>-akum <u>zh</u>ihriyyan inna rabbii bim<u>aa</u> ta\'maluuna mu<u>h</u>ii<u>th</u><b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1566, '11:93', 11, 93, ' way<u>aa</u> qawmi i\'maluu \'al<u>aa</u> mak<u>aa</u>natikum innii \'<u>aa</u>milun sawfa ta\'lamuuna man ya/tiihi \'a<u>dzaa</u>bun yukhziihi waman huwa k<u>aadz</u>ibun wa<b>i</b>rtaqibuu inne ma\'akum raqiib<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1567, '11:94', 11, 94, ' walamm<u>aa</u> j<u>aa</u>-a amrun<u>aa</u> najjayn<u>aa</u> syu\'ayban wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu ma\'ahu bira<u>h</u>matin minn<u>aa</u> wa-akha<u>dz</u>ati <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu <b>al</b><u>shsh</u>ay<u>h</u>atu fa-a<u>sh</u>ba<u>h</u>uu fii diy<u>aa</u>rihim j<u>aa</u>tsimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1568, '11:95', 11, 95, ' ka-an lam yaghnaw fiih<u>aa</u> <b>a</b>l<u>aa</u> bu\'dan limadyana kam<u>aa</u> ba\'idat tsamuud<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1569, '11:96', 11, 96, ' walaqad arsaln<u>aa</u> muus<u>aa</u> bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> wasul<u>th</u><u>aa</u>nin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1570, '11:97', 11, 97, ' il<u>aa</u> fir\'awna wamala-ihi fa<b>i</b>ttaba\'uu amra fir\'awna wam<u>aa</u> amru fir\'awna birasyiid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1571, '11:98', 11, 98, ' yaqdumu qawmahu yawma <b>a</b>lqiy<u>aa</u>mati fa-awradahumu <b>al</b>nn<u>aa</u>ra wabi/sa <b>a</b>lwirdu <b>a</b>lmawruud<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1572, '11:99', 11, 99, ' wautbi\'uu fii h<u>aadz</u>ihi la\'natan wayawma <b>a</b>lqiy<u>aa</u>mati bi/sa <b>al</b>rrifdu <b>a</b>lmarfuud<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1573, '11:100', 11, 100, ' <u>dzaa</u>lika min anb<u>aa</u>-i <b>a</b>lqur<u>aa</u> naqu<u>shsh</u>uhu \'alayka minh<u>aa</u> q<u>aa</u>-imun wa<u>h</u>a<u>sh</u>iid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1574, '11:101', 11, 101, ' wam<u>aa</u> <u>zh</u>alamn<u>aa</u>hum wal<u>aa</u>kin <u>zh</u>alamuu anfusahum fam<u>aa</u> aghnat \'anhum <u>aa</u>lihatuhumu <b>al</b>latii yad\'uuna min duuni <b>al</b>l<u>aa</u>hi min syay-in lamm<u>aa</u> j<u>aa</u>-a amru rabbika wam<u>aa</u> z<u>aa</u>duuhum ghayra tatbiib<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1575, '11:102', 11, 102, ' waka<u>dzaa</u>lika akh<u>dz</u>u rabbika i<u>dzaa</u> akha<u>dz</u>a <b>a</b>lqur<u>aa</u> wahiya <u>zhaa</u>limatun inna akh<u>dz</u>ahu <b>a</b>liimun syadiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1576, '11:103', 11, 103, ' inna fii <u>dzaa</u>lika la<u>aa</u>yatan liman kh<u>aa</u>fa \'a<u>dzaa</u>ba <b>a</b>l-<u>aa</u>khirati <u>dzaa</u>lika yawmun majmuu\'un lahu <b>al</b>nn<u>aa</u>su wa<u>dzaa</u>lika yawmun masyhuudun</u>\r\r\n');
INSERT INTO quranLatin VALUES (1577, '11:104', 11, 104, ' wam<u>aa</u> nu-akhkhiruhu ill<u>aa</u> li-ajalin ma\'duud<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1578, '11:105', 11, 105, ' yawma ya/ti l<u>aa</u> takallamu nafsun ill<u>aa</u> bi-i<u>dz</u>nihi faminhum syaqiyyun wasa\'iid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1579, '11:106', 11, 106, ' fa-amm<u>aa</u> <b>al</b>la<u>dz</u>iina syaquu fafii <b>al</b>nn<u>aa</u>ri lahum fiih<u>aa</u> zafiirun wasyahiiq<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1580, '11:107', 11, 107, ' kh<u>aa</u>lidiina fiih<u>aa</u> m<u>aa</u> d<u>aa</u>mati <b>al</b>ssam<u>aa</u>w<u>aa</u>tu wa<b>a</b>l-ar<u>dh</u>u ill<u>aa</u> m<u>aa</u> sy<u>aa</u>-a rabbuka inna rabbaka fa\'\'<u>aa</u>lun lim<u>aa</u> yuriid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1581, '11:108', 11, 108, ' wa-amm<u>aa</u> <b>al</b>la<u>dz</u>iina su\'iduu fafii <b>a</b>ljannati kh<u>aa</u>lidiina fiih<u>aa</u> m<u>aa</u> d<u>aa</u>mati <b>al</b>ssam<u>aa</u>w<u>aa</u>tu wa<b>a</b>l-ar<u>dh</u>u ill<u>aa</u> m<u>aa</u> sy<u>aa</u>-a rabbuka \'a<u>th</u><u>aa</u>-an ghayra maj<u>dz</u>uu<u>dz</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1582, '11:109', 11, 109, ' fal<u>aa</u> taku fii miryatin mimm<u>aa</u> ya\'budu h<u>aa</u>ul<u>aa</u>-i m<u>aa</u> ya\'buduuna ill<u>aa</u> kam<u>aa</u> ya\'budu <u>aa</u>b<u>aa</u>uhum min qablu wa-inn<u>aa</u> lamuwaffuuhum na<u>sh</u>iibahum ghayra manquu<u>sh</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1583, '11:110', 11, 110, ' walaqad <u>aa</u>tayn<u>aa</u> muus<u>aa</u> <b>a</b>lkit<u>aa</u>ba fa<b>i</b>khtulifa fiihi walawl<u>aa</u> kalimatun sabaqat min rabbika laqu<u>dh</u>iya baynahum wa-innahum lafii syakkin minhu muriib<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1584, '11:111', 11, 111, ' wa-inna kullan lamm<u>aa</u> layuwaffiyannahum rabbuka a\'m<u>aa</u>lahum innahu bim<u>aa</u> ya\'maluuna khabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1585, '11:112', 11, 112, ' fa<b>i</b>staqim kam<u>aa</u> umirta waman t<u>aa</u>ba ma\'aka wal<u>aa</u> ta<u>th</u>ghaw innahu bim<u>aa</u> ta\'maluuna ba<u>sh</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1586, '11:113', 11, 113, ' wal<u>aa</u> tarkanuu il<u>aa</u> <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu fatamassakumu <b>al</b>nn<u>aa</u>ru wam<u>aa</u> lakum min duuni <b>al</b>l<u>aa</u>hi min awliy<u>aa</u>-a tsumma l<u>aa</u> tun<u>sh</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1587, '11:114', 11, 114, ' wa-aqimi <b>al</b><u>shsh</u>al<u>aa</u>ta <u>th</u>arafayi <b>al</b>nnah<u>aa</u>ri wazulafan mina <b>al</b>layli inna <b>a</b>l<u>h</u>asan<u>aa</u>ti yu<u>dz</u>hibna <b>al</b>ssayyi-<u>aa</u>ti <u>dzaa</u>lika <u>dz</u>ikr<u>aa</u> li<b>l</b><u>dzdzaa</u>kiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1588, '11:115', 11, 115, ' wa<b>i</b><u>sh</u>bir fa-inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yu<u>dh</u>ii\'u ajra <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1589, '11:116', 11, 116, ' falawl<u>aa</u> k<u>aa</u>na mina <b>a</b>lquruuni min qablikum uluu baqiyyatin yanhawna \'ani <b>a</b>lfas<u>aa</u>di fii <b>a</b>l-ar<u>dh</u>i ill<u>aa</u> qaliilan mimman anjayn<u>aa</u> minhum wa<b>i</b>ttaba\'a <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu m<u>aa</u> utrifuu fiihi wak<u>aa</u>nuu mujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1590, '11:117', 11, 117, ' wam<u>aa</u> k<u>aa</u>na rabbuka liyuhlika <b>a</b>lqur<u>aa</u> bi<u>zh</u>ulmin wa-ahluh<u>aa</u> mu<u>sh</u>li<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1591, '11:118', 11, 118, ' walaw sy<u>aa</u>-a rabbuka laja\'ala <b>al</b>nn<u>aa</u>sa ummatan w<u>aah</u>idatan wal<u>aa</u> yaz<u>aa</u>luuna mukhtalifiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1592, '11:119', 11, 119, ' ill<u>aa</u> man ra<u>h</u>ima rabbuka wali<u>dzaa</u>lika khalaqahum watammat kalimatu rabbika la-amla-anna jahannama mina <b>a</b>ljinnati wa<b>al</b>nn<u>aa</u>si ajma\'iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1593, '11:120', 11, 120, ' wakullan naqu<u>shsh</u>u \'alayka min anb<u>aa</u>-i <b>al</b>rrusuli m<u>aa</u> nutsabbitu bihi fu-<u>aa</u>daka waj<u>aa</u>-aka fii h<u>aadz</u>ihi <b>a</b>l<u>h</u>aqqu wamaw\'i<u>zh</u>atun wa<u>dz</u>ikr<u>aa</u> lilmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1594, '11:121', 11, 121, ' waqul lilla<u>dz</u>iina l<u>aa</u> yu/minuuna i\'maluu \'al<u>aa</u> mak<u>aa</u>natikum inn<u>aa</u> \'<u>aa</u>miluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1595, '11:122', 11, 122, ' wa<b>i</b>nta<u>zh</u>iruu inn<u>aa</u> munta<u>zh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1596, '11:123', 11, 123, ' walill<u>aa</u>hi ghaybu <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wa-ilayhi yurja\'u <b>a</b>l-amru kulluhu fa<b>u</b>\'budhu watawakkal \'alayhi wam<u>aa</u> rabbuka bigh<u>aa</u>filin \'amm<u>aa</u> ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1597, '12:1', 12, 1, ' alif-l<u>aa</u>m-r<u>aa</u> tilka <u>aa</u>y<u>aa</u>tu <b>a</b>lkit<u>aa</u>bi <b>a</b>lmubiin<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1598, '12:2', 12, 2, ' inn<u>aa</u> anzaln<u>aa</u>hu qur-<u>aa</u>nan \'arabiyyan la\'allakum ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1599, '12:3', 12, 3, ' na<u>h</u>nu naqu<u>shsh</u>u \'alayka a<u>h</u>sana <b>a</b>lqa<u>sh</u>a<u>sh</u>i bim<u>aa</u> aw<u>h</u>ayn<u>aa</u> ilayka h<u>aadzaa</u> <b>a</b>lqur-<u>aa</u>na wa-in kunta min qablihi lamina <b>a</b>lgh<u>aa</u>filiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1600, '12:4', 12, 4, ' i<u>dz</u> q<u>aa</u>la yuusufu li-abiihi y<u>aa</u> abati innii ra-aytu a<u>h</u>ada \'asyara kawkaban wa<b>al</b>sysyamsa wa<b>a</b>lqamara ra-aytuhum lii s<u>aa</u>jidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1601, '12:5', 12, 5, ' q<u>aa</u>la y<u>aa</u> bunayya l<u>aa</u> taq<u>sh</u>u<u>sh</u> ru/y<u>aa</u>ka \'al<u>aa</u> ikhwatika fayakiiduu laka kaydan inna <b>al</b>sysyay<u>th</u><u>aa</u>na lil-ins<u>aa</u>ni \'aduwwun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1602, '12:6', 12, 6, ' waka<u>dzaa</u>lika yajtabiika rabbuka wayu\'allimuka min ta/wiili <b>a</b>l-a<u>haa</u>diitsi wayutimmu ni\'matahu \'alayka wa\'al<u>aa</u> <u>aa</u>li ya\'quuba kam<u>aa</u> atammah<u>aa</u> \'al<u>aa</u> abawayka min qablu ibr<u>aa</u>hiima wa-is<u>haa</u>qa inna rabbaka \'aliimun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1603, '12:7', 12, 7, ' laqad k<u>aa</u>na fii yuusufa wa-ikhwatihi <u>aa</u>y<u>aa</u>tun li<b>l</b>ss<u>aa</u>-iliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1604, '12:8', 12, 8, ' i<u>dz</u> q<u>aa</u>luu layuusufu wa-akhuuhu a<u>h</u>abbu il<u>aa</u> abiin<u>aa</u> minn<u>aa</u> wana<u>h</u>nu \'u<u>sh</u>batun inna ab<u>aa</u>n<u>aa</u> lafii <u>dh</u>al<u>aa</u>lin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1605, '12:9', 12, 9, ' uqtuluu yuusufa awi i<u>th</u>ra<u>h</u>uuhu ar<u>dh</u>an yakhlu lakum wajhu abiikum watakuunuu min ba\'dihi qawman <u>shaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1606, '12:10', 12, 10, ' q<u>aa</u>la q<u>aa</u>-ilun minhum l<u>aa</u> taqtuluu yuusufa wa-alquuhu fii ghay<u>aa</u>bati <b>a</b>ljubbi yaltaqi<u>th</u>hu ba\'<u>dh</u>u <b>al</b>ssayy<u>aa</u>rati in kuntum f<u>aa</u>\'iliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1607, '12:11', 12, 11, ' q<u>aa</u>luu y<u>aa</u> ab<u>aa</u>n<u>aa</u> m<u>aa</u> laka l<u>aa</u> ta/mann<u>aa</u> \'al<u>aa</u> yuusufa wa-inn<u>aa</u> lahu lan<u>aas</u>i<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1608, '12:12', 12, 12, ' arsilhu ma\'an<u>aa</u> ghadan yarta\' wayal\'ab wa-inn<u>aa</u> lahu la<u>haa</u>fi<u>zh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1609, '12:13', 12, 13, ' q<u>aa</u>la innii laya<u>h</u>zununii an tsa<u>dz</u>habuu bihi wa-akh<u>aa</u>fu an ya/kulahu <b>al</b><u>dzdz</u>i/bu wa-antum \'anhu gh<u>aa</u>filuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1610, '12:14', 12, 14, ' q<u>aa</u>luu la-in akalahu <b>al</b><u>dzdz</u>i/bu wana<u>h</u>nu \'u<u>sh</u>batun inn<u>aa</u> i<u>dz</u>an lakh<u>aa</u>siruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1611, '12:15', 12, 15, ' falamm<u>aa</u> <u>dz</u>ahabuu bihi wa-ajma\'uu an yaj\'aluuhu fii ghay<u>aa</u>bati <b>a</b>ljubbi wa-aw<u>h</u>ayn<u>aa</u> ilayhi latunabi-annahum bi-amrihim h<u>aadzaa</u> wahum l<u>aa</u> yasy\'uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1612, '12:16', 12, 16, ' waj<u>aa</u>uu ab<u>aa</u>hum \'isy<u>aa</u>-an yabkuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1613, '12:17', 12, 17, ' q<u>aa</u>luu y<u>aa</u> ab<u>aa</u>n<u>aa</u> inn<u>aa</u> <u>dz</u>ahabn<u>aa</u> nastabiqu watarakn<u>aa</u> yuusufa \'inda mat<u>aa</u>\'in<u>aa</u> fa-akalahu <b>al</b><u>dzdz</u>i/bu wam<u>aa</u> anta bimu/minin lan<u>aa</u> walaw kunn<u>aa</u> <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1614, '12:18', 12, 18, ' waj<u>aa</u>uu \'al<u>aa</u> qamii<u>sh</u>ihi bidamin ka<u>dz</u>ibin q<u>aa</u>la bal sawwalat lakum anfusukum amran fa<u>sh</u>abrun jamiilun wa<b>al</b>l<u>aa</u>hu <b>a</b>lmusta\'<u>aa</u>nu \'al<u>aa</u> m<u>aa</u> ta<u>sh</u>ifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1615, '12:19', 12, 19, ' waj<u>aa</u>-at sayy<u>aa</u>ratun fa-arsaluu w<u>aa</u>ridahum fa-adl<u>aa</u> dalwahu q<u>aa</u>la y<u>aa</u> busyr<u>aa</u> h<u>aadzaa</u> ghul<u>aa</u>mun wa-asarruuhu bi<u>daa</u>\'atan wa<b>al</b>l<u>aa</u>hu \'aliimun bim<u>aa</u> ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1616, '12:20', 12, 20, ' wasyarawhu bitsamanin bakhsin dar<u>aa</u>hima ma\'duudatin wak<u>aa</u>nuu fiihi mina <b>al</b>zz<u>aa</u>hidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1617, '12:21', 12, 21, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>ii isytar<u>aa</u>hu min mi<u>sh</u>ra li<b>i</b>mra-atihi akrimii matsw<u>aa</u>hu \'as<u>aa</u> an yanfa\'an<u>aa</u> aw nattakhi<u>dz</u>ahu waladan waka<u>dzaa</u>lika makkann<u>aa</u> liyuusufa fii <b>a</b>l-ar<u>dh</u>i walinu\'allimahu min ta/wiili <b>a</b>l-a<u>haa</u>diitsi wa<b>al</b>l<u>aa</u>hu gh<u>aa</u>libun \'al<u>aa</u> amrihi wal<u>aa</u>kinna aktsara <b>al</b>nn<u>aa</u>si l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1618, '12:22', 12, 22, ' walamm<u>aa</u> balagha asyuddahu <u>aa</u>tayn<u>aa</u>hu <u>h</u>ukman wa\'ilman waka<u>dzaa</u>lika najzii <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1619, '12:23', 12, 23, ' war<u>aa</u>wadat-hu <b>al</b>latii huwa fii baytih<u>aa</u> \'an nafsihi waghallaqati <b>a</b>l-abw<u>aa</u>ba waq<u>aa</u>lat hayta laka q<u>aa</u>la ma\'<u>aadz</u>a <b>al</b>l<u>aa</u>hi innahu rabbii a<u>h</u>sana matsw<u>aa</u>ya innahu l<u>aa</u> yufli<u>h</u>u <b>al</b><u>zhzhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1620, '12:24', 12, 24, ' walaqad hammat bihi wahamma bih<u>aa</u> lawl<u>aa</u> an ra<u>aa</u> burh<u>aa</u>na rabbihi ka<u>dzaa</u>lika lina<u>sh</u>rifa \'anhu <b>al</b>ssuu-a wa<b>a</b>lfa<u>h</u>sy<u>aa</u>-a innahu min \'ib<u>aa</u>din<u>aa</u> <b>a</b>lmukhla<u>sh</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1621, '12:25', 12, 25, ' wa<b>i</b>stabaq<u>aa</u> <b>a</b>lb<u>aa</u>ba waqaddat qamii<u>sh</u>ahu min duburin wa-alfay<u>aa</u> sayyidah<u>aa</u> lad<u>aa</u> <b>a</b>lb<u>aa</u>bi q<u>aa</u>lat m<u>aa</u> jaz<u>aa</u>u man ar<u>aa</u>da bi-ahlika suu-an ill<u>aa</u> an yusjana aw \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1622, '12:26', 12, 26, ' q<u>aa</u>la hiya r<u>aa</u>wadatnii \'an nafsii wasyahida sy<u>aa</u>hidun min ahlih<u>aa</u> in k<u>aa</u>na qamii<u>sh</u>uhu qudda min qubulin fa<u>sh</u>adaqat wahuwa mina <b>a</b>lk<u>aadz</u>ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1623, '12:27', 12, 27, ' wa-in k<u>aa</u>na qamii<u>sh</u>uhu qudda min duburin faka<u>dz</u>abat wahuwa mina <b>al</b><u>shshaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1624, '12:28', 12, 28, ' falamm<u>aa</u> ra<u>aa</u> qamii<u>sh</u>ahu qudda min duburin q<u>aa</u>la innahu min kaydikunna inna kaydakunna \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1625, '12:29', 12, 29, ' yuusufu a\'ri<u>dh</u> \'an h<u>aadzaa</u> wa<b>i</b>staghfirii li<u>dz</u>anbiki innaki kunti mina <b>a</b>lkh<u>aath</u>i-iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1626, '12:30', 12, 30, ' waq<u>aa</u>la niswatun fii <b>a</b>lmadiinati imra-atu <b>a</b>l\'aziizi tur<u>aa</u>widu fat<u>aa</u>h<u>aa</u> \'an nafsihi qad syaghafah<u>aa</u> <u>h</u>ubban inn<u>aa</u> lanar<u>aa</u>h<u>aa</u> fii <u>dh</u>al<u>aa</u>lin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1627, '12:31', 12, 31, ' falamm<u>aa</u> sami\'at bimakrihinna arsalat ilayhinna wa-a\'tadat lahunna muttaka-an wa<u>aa</u>tat kulla w<u>aah</u>idatin minhunna sikkiinan waq<u>aa</u>lati ukhruj \'alayhinna falamm<u>aa</u> ra-aynahu akbarnahu waqa<u>ththh</u>a\'na aydiyahunna waqulna <u>haa</u>sya lill<u>aa</u>hi m<u>aa</u> h<u>aadzaa</u> basyaran in h<u>aadzaa</u> ill<u>aa</u> malakun kariim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1628, '12:32', 12, 32, ' q<u>aa</u>lat fa<u>dzaa</u>likunna <b>al</b>la<u>dz</u>ii lumtunnanii fiihi walaqad r<u>aa</u>wadtuhu \'an nafsihi fa<b>i</b>st\'<u>sh</u>ama wala-in lam yaf\'al m<u>aa</u> <u>aa</u>muruhu layusjananna walayakuunan mina <b>al</b><u>shshaa</u>ghiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1629, '12:33', 12, 33, ' q<u>aa</u>la rabbi <b>al</b>ssijnu a<u>h</u>abbu ilayya mimm<u>aa</u> yad\'uunanii ilayhi wa-ill<u>aa</u> ta<u>sh</u>rif \'annii kaydahunna a<u>sh</u>bu ilayhinna wa-akun mina <b>a</b>lj<u>aa</u>hiliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1630, '12:34', 12, 34, ' fa<b>i</b>staj<u>aa</u>ba lahu rabbuhu fa<u>sh</u>arafa \'anhu kaydahunna innahu huwa <b>al</b>ssamii\'u <b>a</b>l\'aliim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1631, '12:35', 12, 35, ' tsumma bad<u>aa</u> lahum min ba\'di m<u>aa</u> ra-awuu <b>a</b>l-<u>aa</u>y<u>aa</u>ti layasjununnahu <u>h</u>att<u>aa</u> <u>h</u>iin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1632, '12:36', 12, 36, ' wadakhala ma\'ahu <b>al</b>ssijna fatay<u>aa</u>ni q<u>aa</u>la a<u>h</u>aduhum<u>aa</u> innii ar<u>aa</u>nii a\'<u>sh</u>iru khamran waq<u>aa</u>la <b>a</b>l-<u>aa</u>kharu innii ar<u>aa</u>nii a<u>h</u>milu fawqa ra/sii khubzan ta/kulu <b>al</b><u>ththh</u>ayru minhu nabbi/n<u>aa</u> bita/wiilihi inn<u>aa</u> nar<u>aa</u>ka mina <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1633, '12:37', 12, 37, ' q<u>aa</u>la l<u>aa</u> ya/tiikum<u>aa</u> <u>th</u>a\'<u>aa</u>mun turzaq<u>aa</u>nihi ill<u>aa</u> nabba/tukum<u>aa</u> bita/wiilihi qabla an ya/tiyakum<u>aa</u> <u>dzaa</u>likum<u>aa</u> mimm<u>aa</u> \'allamanii rabbii innii taraktu millata qawmin l<u>aa</u> yu/minuuna bi<b>al</b>l<u>aa</u>hi wahum bi<b>a</b>l-<u>aa</u>khirati hum k<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1634, '12:38', 12, 38, ' wa<b>i</b>ttaba\'tu millata <u>aa</u>b<u>aa</u>-ii ibr<u>aa</u>hiima wa-is<u>haa</u>qa waya\'quuba m<u>aa</u> k<u>aa</u>na lan<u>aa</u> an nusyrika bi<b>al</b>l<u>aa</u>hi min syay-in <u>dzaa</u>lika min fa<u>dh</u>li <b>al</b>l<u>aa</u>hi \'alayn<u>aa</u> wa\'al<u>aa</u> <b>al</b>nn<u>aa</u>si wal<u>aa</u>kinna aktsara <b>al</b>nn<u>aa</u>si l<u>aa</u> yasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1635, '12:39', 12, 39, ' y<u>aa</u> <u>shaah</u>ibayi <b>al</b>ssijni a-arb<u>aa</u>bun mutafarriquuna khayrun ami <b>al</b>l<u>aa</u>hu <b>a</b>lw<u>aah</u>idu <b>a</b>lqahh<u>aa</u>r<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1636, '12:40', 12, 40, ' m<u>aa</u> ta\'buduuna min duunihi ill<u>aa</u> asm<u>aa</u>-an sammaytumuuh<u>aa</u> antum wa<u>aa</u>b<u>aa</u>ukum m<u>aa</u> anzala <b>al</b>l<u>aa</u>hu bih<u>aa</u> min sul<u>th</u><u>aa</u>nin ini <b>a</b>l<u>h</u>ukmu ill<u>aa</u> lill<u>aa</u>hi amara <b>al</b>l<u>aa</u> ta\'buduu ill<u>aa</u> iyy<u>aa</u>hu <u>dzaa</u>lika <b>al</b>ddiinu <b>a</b>lqayyimu wal<u>aa</u>kinna aktsara <b>al</b>nn<u>aa</u>si l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1637, '12:41', 12, 41, ' y<u>aa</u> <u>shaah</u>ibayi <b>al</b>ssijni amm<u>aa</u> a<u>h</u>adukum<u>aa</u> fayasqii rabbahu khamran wa-amm<u>aa</u> <b>a</b>l-<u>aa</u>kharu fayu<u>sh</u>labu fata/kulu <b>al</b><u>ththh</u>ayru min ra/sihi qu<u>dh</u>iya <b>a</b>l-amru <b>al</b>la<u>dz</u>ii fiihi tastaftiy<u>aa</u>n<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1638, '12:42', 12, 42, ' waq<u>aa</u>la lilla<u>dz</u>ii <u>zh</u>anna annahu n<u>aa</u>jin minhum<u>aa</u> u<u>dz</u>kurnii \'inda rabbika fa-ans<u>aa</u>hu <b>al</b>sysyay<u>th</u><u>aa</u>nu <u>dz</u>ikra rabbihi falabitsa fii <b>al</b>ssijni bi<u>dh</u>\'a siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1639, '12:43', 12, 43, ' waq<u>aa</u>la <b>a</b>lmaliku innii ar<u>aa</u> sab\'a baqar<u>aa</u>tin sim<u>aa</u>nin ya/kuluhunna sab\'un \'ij<u>aa</u>fun wasab\'a sunbul<u>aa</u>tin khu<u>dh</u>rin waukhara y<u>aa</u>bis<u>aa</u>tin y<u>aa</u> ayyuh<u>aa</u> <b>a</b>lmalau aftuunii fii ru/y<u>aa</u>ya in kuntum li<b>l</b>rru/y<u>aa</u> ta\'buruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1640, '12:44', 12, 44, ' q<u>aa</u>luu a<u>dh</u>gh<u>aa</u>tsu a<u>h</u>l<u>aa</u>min wam<u>aa</u> na<u>h</u>nu bita/wiili <b>a</b>l-a<u>h</u>l<u>aa</u>mi bi\'<u>aa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1641, '12:45', 12, 45, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>ii naj<u>aa</u> minhum<u>aa</u> wa<b>i</b>ddakara ba\'da ummatin an<u>aa</u> unabbi-ukum bita/wiilihi fa-arsiluun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1642, '12:46', 12, 46, ' yuusufu ayyuh<u>aa</u> <b>al</b><u>shsh</u>iddiiqu aftin<u>aa</u> fii sab\'i baqar<u>aa</u>tin sim<u>aa</u>nin ya/kuluhunna sab\'un \'ij<u>aa</u>fun wasab\'i sunbul<u>aa</u>tin khu<u>dh</u>rin waukhara y<u>aa</u>bis<u>aa</u>tin la\'allii arji\'u il<u>aa</u> <b>al</b>nn<u>aa</u>si la\'allahum ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1643, '12:47', 12, 47, ' q<u>aa</u>la tazra\'uuna sab\'a siniina da-aban fam<u>aa</u> <u>h</u>a<u>sh</u>adtum fa<u>dz</u>aruuhu fii sunbulihi ill<u>aa</u> qaliilan mimm<u>aa</u> ta/kuluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1644, '12:48', 12, 48, ' tsumma ya/tii min ba\'di <u>dzaa</u>lika sab\'un syid<u>aa</u>dun ya/kulna m<u>aa</u> qaddamtum lahunna ill<u>aa</u> qaliilan mimm<u>aa</u> tu<u>hs</u>inuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1645, '12:49', 12, 49, ' tsumma ya/tii min ba\'di <u>dzaa</u>lika \'<u>aa</u>mun fiihi yugh<u>aa</u>tsu <b>al</b>nn<u>aa</u>su wafiihi ya\'<u>sh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1646, '12:50', 12, 50, ' waq<u>aa</u>la <b>a</b>lmaliku i/tuunii bihi falamm<u>aa</u> j<u>aa</u>-ahu <b>al</b>rrasuulu q<u>aa</u>la irji\' il<u>aa</u> rabbika fa<b>i</b>s-alhu m<u>aa</u> b<u>aa</u>lu <b>al</b>nniswati <b>al</b>l<u>aa</u>tii qa<u>ththh</u>a\'na aydiyahunna inna rabbii bikaydihinna \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1647, '12:51', 12, 51, ' q<u>aa</u>la m<u>aa</u> kha<u>th</u>bukunna i<u>dz</u> r<u>aa</u>wadtunna yuusufa \'an nafsihi qulna <u>haa</u>sya lill<u>aa</u>hi m<u>aa</u> \'alimn<u>aa</u> \'alayhi min suu-in q<u>aa</u>lati imra-atu <b>a</b>l\'aziizi <b>a</b>l-<u>aa</u>na <u>h</u>a<u>sy</u>a<u>sh</u>a <b>a</b>l<u>h</u>aqqu an<u>aa</u> r<u>aa</u>wadtuhu \'an nafsihi wa-innahu lamina <b>al</b><u>shshaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1648, '12:52', 12, 52, ' <u>dzaa</u>lika liya\'lama annii lam akhunhu bi<b>a</b>lghaybi wa-anna <b>al</b>l<u>aa</u>ha l<u>aa</u> yahdii kayda <b>a</b>lkh<u>aa</u>-iniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1649, '12:53', 12, 53, ' wam<u>aa</u> ubarri-u nafsii inna <b>al</b>nnafsa la-amm<u>aa</u>ratun bi<b>al</b>ssuu-i ill<u>aa</u> m<u>aa</u> ra<u>h</u>ima rabbii inna rabbii ghafuurun ra<u>h</u>iim<b>un </b>\r\r\n');
INSERT INTO quranLatin VALUES (1650, '12:54', 12, 54, ' waq<u>aa</u>la <b>a</b>lmaliku i/tuunii bihi astakhli<u>sh</u>hu linafsii falamm<u>aa</u> kallamahu q<u>aa</u>la innaka <b>a</b>lyawma ladayn<u>aa</u> makiinun amiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1651, '12:55', 12, 55, ' q<u>aa</u>la ij\'alnii \'al<u>aa</u> khaz<u>aa</u>-ini <b>a</b>l-ar<u>dh</u>i innii <u>h</u>afii<u>zh</u>un \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1652, '12:56', 12, 56, ' waka<u>dzaa</u>lika makann<u>aa</u> liyuusufa fii <b>a</b>l-ar<u>dh</u>i yatabawwau minh<u>aa</u> <u>h</u>aytsu yasy<u>aa</u>u nu<u>sh</u>iibu bira<u>h</u>matin<u>aa</u> man nasy<u>aa</u>u wal<u>aa</u> nu<u>dh</u>ii\'u ajra <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1653, '12:57', 12, 57, ' wala-ajru <b>a</b>l-<u>aa</u>khirati khayrun lilla<u>dz</u>iina <u>aa</u>manuu wak<u>aa</u>nuu yattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1654, '12:58', 12, 58, ' waj<u>aa</u>-a ikhwatu yuusufa fadakhaluu \'alayhi fa\'arafahum wahum lahu munkiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1655, '12:59', 12, 59, ' walamm<u>aa</u> jahhazahum bijah<u>aa</u>zihim q<u>aa</u>la i/tuunii bi-akhin lakum min abiikum <b>a</b>l<u>aa</u> tarawna annii uufii <b>a</b>lkayla wa-an<u>aa</u> khayru <b>a</b>lmunziliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1656, '12:60', 12, 60, ' fa-in lam ta/tuunii bihi fal<u>aa</u> kayla lakum \'indii wal<u>aa</u> taqrabuun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1657, '12:61', 12, 61, ' q<u>aa</u>luu sanur<u>aa</u>widu \'anhu ab<u>aa</u>hu wa-inn<u>aa</u> laf<u>aa</u>\'iluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1658, '12:62', 12, 62, ' waq<u>aa</u>la lifity<u>aa</u>nihi ij\'aluu bi<u>daa</u>\'atahum fii ri<u>haa</u>lihim la\'allahum ya\'rifuunah<u>aa</u> i<u>dzaa</u> inqalabuu il<u>aa</u> ahlihim la\'allahum yarji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1659, '12:63', 12, 63, ' falamm<u>aa</u> raja\'uu il<u>aa</u> abiihim q<u>aa</u>luu y<u>aa</u> ab<u>aa</u>n<u>aa</u> muni\'a minn<u>aa</u> <b>a</b>lkaylu fa-arsil ma\'an<u>aa</u> akh<u>aa</u>n<u>aa</u> naktal wa-inn<u>aa</u> lahu la<u>haa</u>fi<u>zh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1660, '12:64', 12, 64, ' q<u>aa</u>la hal <u>aa</u>manukum \'alayhi ill<u>aa</u> kam<u>aa</u> amintukum \'al<u>aa</u> akhiihi min qablu fa<b>al</b>l<u>aa</u>hu khayrun <u>haa</u>fi<u>zh</u>an wahuwa ar<u>h</u>amu <b>al</b>rr<u>aah</u>imiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1661, '12:65', 12, 65, ' walamm<u>aa</u> fata<u>h</u>uu mat<u>aa</u>\'ahum wajaduu bi<u>daa</u>\'atahum ruddat ilayhim q<u>aa</u>luu y<u>aa</u> ab<u>aa</u>n<u>aa</u> m<u>aa</u> nabghii h<u>aadz</u>ihi bi<u>daa</u>\'atun<u>aa</u> ruddat ilayn<u>aa</u> wanamiiru ahlan<u>aa</u> wana<u>h</u>fa<u>zh</u>u akh<u>aa</u>n<u>aa</u> wanazd<u>aa</u>du kayla ba\'iirin <u>dzaa</u>lika kaylun yasiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1662, '12:66', 12, 66, ' q<u>aa</u>la lan ursilahu ma\'akum <u>h</u>att<u>aa</u> tu/tuuni mawtsiqan mina <b>al</b>l<u>aa</u>hi lata/tunnanii bihi ill<u>aa</u> an yu<u>hath</u>a bikum falamm<u>aa</u> <u>aa</u>tawhu mawtsiqahum q<u>aa</u>la <b>al</b>l<u>aa</u>hu \'al<u>aa</u> m<u>aa</u> naquulu wakiil<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1663, '12:67', 12, 67, ' waq<u>aa</u>la y<u>aa</u> baniyya l<u>aa</u> tadkhuluu min b<u>aa</u>bin w<u>aah</u>idin wa<b>u</b>dkhuluu min abw<u>aa</u>bin mutafarriqatin wam<u>aa</u> ughnii \'ankum mina <b>al</b>l<u>aa</u>hi min syay-in ini <b>a</b>l<u>h</u>ukmu ill<u>aa</u> lill<u>aa</u>hi \'alayhi tawakkaltu wa\'alayhi falyatawakkali <b>a</b>lmutawakkiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1664, '12:68', 12, 68, ' walamm<u>aa</u> dakhaluu min <u>h</u>aytsu amarahum abuuhum m<u>aa</u> k<u>aa</u>na yughnii \'anhum mina <b>al</b>l<u>aa</u>hi min syay-in ill<u>aa</u> <u>haa</u>jatan fii nafsi ya\'quuba qa<u>daa</u>h<u>aa</u> wa-innahu la<u>dz</u>uu \'ilmin lim<u>aa</u> \'allamn<u>aa</u>hu wal<u>aa</u>kinna aktsara <b>al</b>nn<u>aa</u>si l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1665, '12:69', 12, 69, ' walamm<u>aa</u> dakhaluu \'al<u>aa</u> yuusufa <u>aa</u>w<u>aa</u> ilayhi akh<u>aa</u>hu q<u>aa</u>la innii an<u>aa</u> akhuuka fal<u>aa</u> tabta-is bim<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1666, '12:70', 12, 70, ' falamm<u>aa</u> jahhazahum bijah<u>aa</u>zihim ja\'ala <b>al</b>ssiq<u>aa</u>yata fii ra<u>h</u>li akhiihi tsumma a<u>dzdz</u>ana mu-a<u>dzdz</u>inun ayyatuh<u>aa</u> <b>a</b>l\'iiru innakum las<u>aa</u>riquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1667, '12:71', 12, 71, ' q<u>aa</u>luu wa-aqbaluu \'alayhim m<u>aatsaa</u> tafqiduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1668, '12:72', 12, 72, ' q<u>aa</u>luu nafqidu <u>sh</u>uw<u>aa</u>\'a <b>a</b>lmaliki waliman j<u>aa</u>-a bihi <u>h</u>imlu ba\'iirin wa-an<u>aa</u> bihi za\'iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1669, '12:73', 12, 73, ' q<u>aa</u>luu ta<b>al</b>l<u>aa</u>hi laqad \'alimtum m<u>aa</u> ji/n<u>aa</u> linufsida fii <b>a</b>l-ar<u>dh</u>i wam<u>aa</u> kunn<u>aa</u> s<u>aa</u>riqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1670, '12:74', 12, 74, ' q<u>aa</u>luu fam<u>aa</u> jaz<u>aa</u>uhu in kuntum k<u>aadz</u>ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1671, '12:75', 12, 75, ' q<u>aa</u>luu jaz<u>aa</u>uhu man wujida fii ra<u>h</u>lihi fahuwa jaz<u>aa</u>uhu ka<u>dzaa</u>lika najzii <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1672, '12:76', 12, 76, ' fabada-a bi-aw\'iyatihim qabla wi\'<u>aa</u>-i akhiihi tsumma istakhrajah<u>aa</u> min wi\'<u>aa</u>-i akhiihi ka<u>dzaa</u>lika kidn<u>aa</u> liyuusufa m<u>aa</u> k<u>aa</u>na liya/khu<u>dz</u>a akh<u>aa</u>hu fii diini <b>a</b>lmaliki ill<u>aa</u> an yasy<u>aa</u>-a <b>al</b>l<u>aa</u>hu narfa\'u daraj<u>aa</u>tin man nasy<u>aa</u>u wafawqa kulli <u>dz</u>ii \'ilmin \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1673, '12:77', 12, 77, ' q<u>aa</u>luu in yasriq faqad saraqa akhun lahu min qablu fa-asarrah<u>aa</u> yuusufu fii nafsihi walam yubdih<u>aa</u> lahum q<u>aa</u>la antum syarrun mak<u>aa</u>nan wa<b>al</b>l<u>aa</u>hu a\'lamu bim<u>aa</u> ta<u>sh</u>ifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1674, '12:78', 12, 78, ' q<u>aa</u>luu y<u>aa</u> ayyuh<u>aa</u> <b>a</b>l\'aziizu inna lahu aban syaykhan kabiiran fakhu<u>dz</u> a<u>h</u>adan<u>aa</u> mak<u>aa</u>nahu inn<u>aa</u> nar<u>aa</u>ka mina <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1675, '12:79', 12, 79, ' q<u>aa</u>la ma\'<u>aadz</u>a <b>al</b>l<u>aa</u>hi an na/khu<u>dz</u>a ill<u>aa</u> man wajadn<u>aa</u> mat<u>aa</u>\'an<u>aa</u> \'indahu inn<u>aa</u> i<u>dz</u>an la<u>zhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1676, '12:80', 12, 80, ' falamm<u>aa</u> istay-asuu minhu khala<u>sh</u>uu najiyyan q<u>aa</u>la kabiiruhum alam ta\'lamuu anna ab<u>aa</u>kum qad akha<u>dz</u>a \'alaykum mawtsiqan mina <b>al</b>l<u>aa</u>hi wamin qablu m<u>aa</u> farra<u>th</u>tum fii yuusufa falan abra<u>h</u>a al-ar<u>dh</u>a <u>h</u>att<u>aa</u> ya/<u>dz</u>ana lii abii aw ya<u>h</u>kuma <b>al</b>l<u>aa</u>hu lii wahuwa khayru al<u>haa</u>kimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1677, '12:81', 12, 81, ' irji\'uu il<u>aa</u> abiikum faquuluu y<u>aa</u> ab<u>aa</u>n<u>aa</u> inna ibnaka saraqa wam<u>aa</u> syahidn<u>aa</u> ill<u>aa</u> bim<u>aa</u> \'alimn<u>aa</u> wam<u>aa</u> kunn<u>aa</u> lilghaybi <u>haa</u>fi<u>zh</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1678, '12:82', 12, 82, ' wa<b>i</b>s-ali <b>a</b>lqaryata <b>al</b>latii kunn<u>aa</u> fiih<u>aa</u> wa<b>a</b>l\'iira <b>al</b>latii aqbaln<u>aa</u> fiih<u>aa</u> wa-inn<u>aa</u> la<u>shaa</u>diquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1679, '12:83', 12, 83, ' q<u>aa</u>la bal sawwalat lakum anfusukum amran fa<u>sh</u>abrun jamiilun \'as<u>aa</u> <b>al</b>l<u>aa</u>hu an ya/tiyanii bihim jamii\'an innahu huwa <b>a</b>l\'aliimu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1680, '12:84', 12, 84, ' watawall<u>aa</u> \'anhum waq<u>aa</u>la y<u>aa</u> asaf<u>aa</u> \'al<u>aa</u> yuusufa wa<b>i</b>bya<u>dhdh</u>at \'ayn<u>aa</u>hu mina <b>a</b>l<u>h</u>uzni fahuwa ka<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1681, '12:85', 12, 85, ' q<u>aa</u>luu ta<b>al</b>l<u>aa</u>hi taftau ta<u>dz</u>kuru yuusufa <u>h</u>att<u>aa</u> takuuna <u>h</u>ara<u>dh</u>an aw takuuna mina <b>a</b>lh<u>aa</u>likiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1682, '12:86', 12, 86, ' q<u>aa</u>la innam<u>aa</u> asykuu batstsii wa<u>h</u>uznii il<u>aa</u> <b>al</b>l<u>aa</u>hi wa-a\'lamu mina <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1683, '12:87', 12, 87, ' y<u>aa</u> baniyya i<u>dz</u>habuu fata<u>h</u>assasuu min yuusufa wa-akhiihi wal<u>aa</u> tay-asuu min raw<u>h</u>i <b>al</b>l<u>aa</u>hi innahu l<u>aa</u> yay-asu min raw<u>h</u>i <b>al</b>l<u>aa</u>hi ill<u>aa</u> <b>a</b>lqawmu <b>a</b>lk<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1684, '12:88', 12, 88, ' falamm<u>aa</u> dakhaluu \'alayhi q<u>aa</u>luu y<u>aa</u> ayyuh<u>aa</u> <b>a</b>l\'aziizu massan<u>aa</u> wa-ahlan<u>aa</u> <b>al</b><u>dhdh</u>urru waji/n<u>aa</u> bibi<u>daa</u>\'atin muzj<u>aa</u>tin fa-awfi lan<u>aa</u> <b>a</b>lkayla wata<u>sh</u>addaq \'alayn<u>aa</u> inna <b>al</b>l<u>aa</u>ha yajzii <b>a</b>lmuta<u>sh</u>addiqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1685, '12:89', 12, 89, ' q<u>aa</u>la hal \'alimtum m<u>aa</u> fa\'altum biyuusufa wa-akhiihi i<u>dz</u> antum j<u>aa</u>hiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1686, '12:90', 12, 90, ' q<u>aa</u>luu a-innaka la-anta yuusufa q<u>aa</u>la an<u>aa</u> yuusufu wah<u>aadzaa</u> akhii qad manna <b>al</b>l<u>aa</u>hu \'alayn<u>aa</u> innahu man yattaqi waya<u>sh</u>bir fa-inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yu<u>dh</u>ii\'u ajra <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1687, '12:91', 12, 91, ' q<u>aa</u>luu ta<b>al</b>l<u>aa</u>hi laqad <u>aa</u>tsaraka <b>al</b>l<u>aa</u>hu \'alayn<u>aa</u> wa-in kunn<u>aa</u> lakh<u>aath</u>i-iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1688, '12:92', 12, 92, ' q<u>aa</u>la l<u>aa</u> tatsriiba \'alaykumu <b>a</b>lyawma yaghfiru <b>al</b>l<u>aa</u>hu lakum wahuwa ar<u>h</u>amu <b>al</b>rr<u>aah</u>imiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1689, '12:93', 12, 93, ' i<u>dz</u>habuu biqamii<u>sh</u>ii h<u>aadzaa</u> fa-alquuhu \'al<u>aa</u> wajhi abii ya/ti ba<u>sh</u>iiran wa/tuunii bi-ahlikum ajma\'iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1690, '12:94', 12, 94, ' walamm<u>aa</u> fa<u>sh</u>alati <b>a</b>l\'iiru q<u>aa</u>la abuuhum innii la-ajidu rii<u>h</u>a yuusufa lawl<u>aa</u> an tufanniduun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1691, '12:95', 12, 95, ' q<u>aa</u>luu ta<b>al</b>l<u>aa</u>hi innaka lafii <u>dh</u>al<u>aa</u>lika <b>a</b>lqadiim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1692, '12:96', 12, 96, ' falamm<u>aa</u> an j<u>aa</u>-a albasyiiru alq<u>aa</u>hu \'al<u>aa</u> wajhihi fa<b>i</b>rtadda ba<u>sh</u>iiran q<u>aa</u>la alam aqul lakum innii a\'lamu mina <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1693, '12:97', 12, 97, ' q<u>aa</u>luu y<u>aa</u> ab<u>aa</u>n<u>aa</u> istaghfir lan<u>aa</u> <u>dz</u>unuuban<u>aa</u> inn<u>aa</u> kunn<u>aa</u> kh<u>aath</u>i-iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1694, '12:98', 12, 98, ' q<u>aa</u>la sawfa astaghfiru lakum rabbii innahu huwa <b>a</b>lghafuuru <b>al</b>rra<u>h</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1695, '12:99', 12, 99, ' falamm<u>aa</u> dakhaluu \'al<u>aa</u> yuusufa <u>aa</u>w<u>aa</u> ilayhi abawayhi waq<u>aa</u>la udkhuluu mi<u>sh</u>ra in sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu <u>aa</u>miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1696, '12:100', 12, 100, ' warafa\'a abawayhi \'al<u>aa</u> <b>a</b>l\'arsyi wakharruu lahu sujjadan waq<u>aa</u>la y<u>aa</u> abati h<u>aadzaa</u> ta/wiilu ru/y<u>aa</u>ya min qablu qad ja\'alah<u>aa</u> rabbii <u>h</u>aqqan waqad a<u>h</u>sana bii i<u>dz</u> akhrajanii mina <b>al</b>ssijni waj<u>aa</u>-a bikum mina <b>a</b>lbadwi min ba\'di an nazagha <b>al</b>sysyay<u>th</u><u>aa</u>nu baynii wabayna ikhwatii inna rabbii la<u>th</u>iifun lim<u>aa</u> yasy<u>aa</u>u innahu huwa <b>a</b>l\'aliimu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1697, '12:101', 12, 101, ' rabbi qad <u>aa</u>taytanii mina <b>a</b>lmulki wa\'allamtanii min ta/wiili <b>a</b>l-a<u>haa</u>diitsi f<u>aath</u>ira <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i anta waliyyii fii <b>al</b>dduny<u>aa</u> wa<b>a</b>l-<u>aa</u>khirati tawaffanii musliman wa-al<u>h</u>iqnii bi<b>al</b><u>shshaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1698, '12:102', 12, 102, ' <u>dzaa</u>lika min anb<u>aa</u>-i <b>a</b>lghaybi nuu<u>h</u>iihi ilayka wam<u>aa</u> kunta ladayhim i<u>dz</u> ajma\'uu amrahum wahum yamkuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1699, '12:103', 12, 103, ' wam<u>aa</u> aktsaru <b>al</b>nn<u>aa</u>si walaw <u>h</u>ara<u>sh</u>ta bimu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1700, '12:104', 12, 104, ' wam<u>aa</u> tas-aluhum \'alayhi min ajrin in huwa ill<u>aa</u> <u>dz</u>ikrun lil\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1701, '12:105', 12, 105, ' waka-ayyin min <u>aa</u>yatin fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i yamurruuna \'alayh<u>aa</u> wahum \'anh<u>aa</u> mu\'ri<u>dh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1702, '12:106', 12, 106, ' wam<u>aa</u> yu/minu aktsaruhum bi<b>al</b>l<u>aa</u>hi ill<u>aa</u> wahum musyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1703, '12:107', 12, 107, ' afa-aminuu an ta/tiyahum gh<u>aa</u>syiyatun min \'a<u>dzaa</u>bi <b>al</b>l<u>aa</u>hi aw ta/tiyahumu <b>al</b>ss<u>aa</u>\'atu baghtatan wahum l<u>aa</u> yasy\'uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1704, '12:108', 12, 108, ' qul h<u>aadz</u>ihi sabiilii ad\'uu il<u>aa</u> <b>al</b>l<u>aa</u>hi \'al<u>aa</u> ba<u>sh</u>iiratin an<u>aa</u> wamani ittaba\'anii wasub<u>haa</u>na <b>al</b>l<u>aa</u>hi wam<u>aa</u> an<u>aa</u> mina <b>a</b>lmusyrikiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1705, '12:109', 12, 109, ' wam<u>aa</u> arsaln<u>aa</u> min qablika ill<u>aa</u> rij<u>aa</u>lan nuu<u>h</u>ii ilayhim min ahli <b>a</b>lqur<u>aa</u> afalam yasiiruu fii <b>a</b>l-ar<u>dh</u>i fayan<u>zh</u>uruu kayfa k<u>aa</u>na \'<u>aa</u>qibatu <b>al</b>la<u>dz</u>iina min qablihim walad<u>aa</u>ru <b>a</b>l-<u>aa</u>khirati khayrun lilla<u>dz</u>iina ittaqaw afal<u>aa</u> ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1706, '12:110', 12, 110, ' <u>h</u>att<u>aa</u> i<u>dzaa</u> istay-asa <b>al</b>rrusulu wa<u>zh</u>annuu annahum qad ku<u>dz</u>ibuu j<u>aa</u>-ahum na<u>sh</u>run<u>aa</u> fanujjiya man nasy<u>aa</u>u wal<u>aa</u> yuraddu ba/sun<u>aa</u> \'ani <b>a</b>lqawmi <b>a</b>lmujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1707, '12:111', 12, 111, ' laqad k<u>aa</u>na fii qa<u>sh</u>a<u>sh</u>ihim \'ibratun li-ulii <b>a</b>l-alb<u>aa</u>bi m<u>aa</u> k<u>aa</u>na <u>h</u>adiitsan yuftar<u>aa</u> wal<u>aa</u>kin ta<u>sh</u>diiqa <b>al</b>la<u>dz</u>ii bayna yadayhi wataf<u>sh</u>iila kulli syay-in wahudan wara<u>h</u>matan liqawmin yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1708, '13:1', 13, 1, ' alif-l<u>aa</u>m-miim-r<u>aa</u> tilka <u>aa</u>y<u>aa</u>tu <b>a</b>lkit<u>aa</u>bi wa<b>a</b>lla<u>dz</u>ii unzila ilayka min rabbika <b>a</b>l<u>h</u>aqqu wal<u>aa</u>kinna aktsara <b>al</b>nn<u>aa</u>si l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1709, '13:2', 13, 2, ' <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii rafa\'a <b>al</b>ssam<u>aa</u>w<u>aa</u>ti bighayri \'amadin tarawnah<u>aa</u> tsumma istaw<u>aa</u> \'al<u>aa</u> <b>a</b>l\'arsyi wasakhkhara <b>al</b>sysyamsa wa<b>a</b>lqamara kullun yajrii li-ajalin musamman yudabbiru <b>a</b>l-amra yufa<u>shsh</u>ilu <b>a</b>l-<u>aa</u>y<u>aa</u>ti la\'allakum biliq<u>aa</u>-i rabbikum tuuqinuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1710, '13:3', 13, 3, ' wahuwa <b>al</b>la<u>dz</u>ii madda <b>a</b>l-ar<u>dh</u>a waja\'ala fiih<u>aa</u> raw<u>aa</u>siya wa-anh<u>aa</u>ran wamin kulli <b>al</b>tstsamar<u>aa</u>ti ja\'ala fiih<u>aa</u> zawjayni itsnayni yughsyii <b>al</b>layla <b>al</b>nnah<u>aa</u>ra inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin liqawmin yatafakkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1711, '13:4', 13, 4, ' wafii <b>a</b>l-ar<u>dh</u>i qi<u>th</u>a\'un mutaj<u>aa</u>wir<u>aa</u>tun wajann<u>aa</u>tun min a\'n<u>aa</u>bin wazar\'un wanakhiilun <u>sh</u>inw<u>aa</u>nun waghayru <u>sh</u>inw<u>aa</u>nin yusq<u>aa</u> bim<u>aa</u>-in w<u>aah</u>idin wanufa<u>dhdh</u>ilu ba\'<u>dh</u>ah<u>aa</u> \'al<u>aa</u> ba\'<u>dh</u>in fii <b>a</b>lukuli inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin liqawmin ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1712, '13:5', 13, 5, ' wa-in ta\'jab fa\'ajabun qawluhum a-i<u>dzaa</u> kunn<u>aa</u> tur<u>aa</u>ban a-inn<u>aa</u> lafii khalqin jadiidin ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina kafaruu birabbihim waul<u>aa</u>-ika <b>a</b>l-aghl<u>aa</u>lu fii a\'n<u>aa</u>qihim waul<u>aa</u>-ika a<u>sh</u>-<u>haa</u>bu <b>al</b>nn<u>aa</u>ri hum fiih<u>aa</u> kh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1713, '13:6', 13, 6, ' wayasta\'jiluunaka bi<b>al</b>ssayyi-ati qabla <b>a</b>l<u>h</u>asanati waqad khalat min qablihimu <b>a</b>lmatsul<u>aa</u>tu wa-inna rabbaka la<u>dz</u>uu maghfiratin li<b>l</b>nn<u>aa</u>si \'al<u>aa</u> <u>zh</u>ulmihim wa-inna rabbaka lasyadiidu <b>a</b>l\'iq<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1714, '13:7', 13, 7, ' wayaquulu <b>al</b>la<u>dz</u>iina kafaruu lawl<u>aa</u> unzila \'alayhi <u>aa</u>yatun min rabbihi innam<u>aa</u> anta mun<u>dz</u>irun walikulli qawmin h<u>aa</u>d<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1715, '13:8', 13, 8, ' <b>al</b>l<u>aa</u>hu ya\'lamu m<u>aa</u> ta<u>h</u>milu kullu unts<u>aa</u> wam<u>aa</u> taghii<u>dh</u>u <b>a</b>l-ar<u>haa</u>mu wam<u>aa</u> tazd<u>aa</u>du wakullu syay-in \'indahu bimiqd<u>aa</u>r<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1716, '13:9', 13, 9, '  \'<u>aa</u>limu <b>a</b>lghaybi wa<b>al</b>sysyah<u>aa</u>dati <b>a</b>lkabiiru <b>a</b>lmuta\'<u>aa</u>l<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1717, '13:10', 13, 10, ' saw<u>aa</u>un minkum man asarra <b>a</b>lqawla waman jahara bihi waman huwa mustakhfin bi<b>a</b>llayli was<u>aa</u>ribun bi<b>al</b>nnah<u>aa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1718, '13:11', 13, 11, ' lahu mu\'aqqib<u>aa</u>tun min bayni yadayhi wamin khalfihi ya<u>h</u>fa<u>zh</u>uunahu min amri <b>al</b>l<u>aa</u>hi inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yughayyiru m<u>aa</u> biqawmin <u>h</u>att<u>aa</u> yughayyiruu m<u>aa</u> bi-anfusihim wa-i<u>dzaa</u> ar<u>aa</u>da <b>al</b>l<u>aa</u>hu biqawmin suu-an fal<u>aa</u> maradda lahu wam<u>aa</u> lahum min duunihi min w<u>aa</u>l<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1719, '13:12', 13, 12, ' huwa <b>al</b>la<u>dz</u>ii yuriikumu <b>a</b>lbarqa khawfan wa<u>th</u>ama\'an wayunsyi-u <b>al</b>ssa<u>haa</u>ba <b>al</b>tstsiq<u>aa</u>l<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1720, '13:13', 13, 13, ' wayusabbi<u>h</u>u <b>al</b>rra\'du bi<u>h</u>amdihi wa<b>a</b>lmal<u>aa</u>-ikatu min khiifatihi wayursilu <b>al</b><u>shsh</u>aw<u>aa</u>\'iqa fayu<u>sh</u>iibu bih<u>aa</u> man yasy<u>aa</u>u wahum yuj<u>aa</u>diluuna fii <b>al</b>l<u>aa</u>hi wahuwa syadiidu <b>a</b>lmi<u>haa</u>l<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1721, '13:14', 13, 14, ' lahu da\'watu <b>a</b>l<u>h</u>aqqi wa<b>a</b>lla<u>dz</u>iina yad\'uuna min duunihi l<u>aa</u> yastajiibuuna lahum bisyay-in ill<u>aa</u> kab<u>aa</u>si<u>th</u>i kaffayhi il<u>aa</u> <b>a</b>lm<u>aa</u>-i liyablugha f<u>aa</u>hu wam<u>aa</u> huwa bib<u>aa</u>lighihi wam<u>aa</u> du\'<u>aa</u>u <b>a</b>lk<u>aa</u>firiina ill<u>aa</u> fii <u>dh</u>al<u>aa</u>l<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1722, '13:15', 13, 15, ' walill<u>aa</u>hi yasjudu man fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i <u>th</u>aw\'an wakarhan wa<u>zh</u>il<u>aa</u>luhum bi<b>a</b>lghuduwwi wa<b>a</b>l-<u>aasaa</u>l<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1723, '13:16', 13, 16, ' qul man rabbu <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i quli <b>al</b>l<u>aa</u>hu qul afa<b>i</b>ttakha<u>dz</u>tum min duunihi awliy<u>aa</u>-a l<u>aa</u> yamlikuuna li-anfusihim naf\'an wal<u>aa</u> <u>dh</u>arran qul hal yastawii <b>a</b>l-a\'m<u>aa</u> wa<b>a</b>lba<u>sh</u>iiru am hal tastawii <b>al</b><u>zhzh</u>ulum<u>aa</u>tu wa<b>al</b>nnuuru am ja\'aluu lill<u>aa</u>hi syurak<u>aa</u>-a khalaquu kakhalqihi fatasy<u>aa</u>baha <b>a</b>lkhalqu \'alayhim quli <b>al</b>l<u>aa</u>hu kh<u>aa</u>liqu kulli syay-in wahuwa <b>a</b>lw<u>aah</u>idu <b>a</b>lqahh<u>aa</u>r<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1724, '13:17', 13, 17, ' anzala mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an fas<u>aa</u>lat awdiyatun biqadarih<u>aa</u> fa<b>i</b><u>h</u>tamala <b>al</b>ssaylu zabadan r<u>aa</u>biyan wamimm<u>aa</u> yuuqiduuna \'alayhi fii <b>al</b>nn<u>aa</u>ri ibtigh<u>aa</u>-a <u>h</u>ilyatin aw mat<u>aa</u>\'in zabadun mitsluhu ka<u>dzaa</u>lika ya<u>dh</u>ribu <b>al</b>l<u>aa</u>hu <b>a</b>l<u>h</u>aqqa wa<b>a</b>lb<u>aath</u>ila fa-amm<u>aa</u> <b>al</b>zzabadu faya<u>dz</u>habu juf<u>aa</u>-an wa-amm<u>aa</u> m<u>aa</u> yanfa\'u <b>al</b>nn<u>aa</u>sa fayamkutsu fii <b>a</b>l-ar<u>dh</u>i ka<u>dzaa</u>lika ya<u>dh</u>ribu <b>al</b>l<u>aa</u>hu <b>a</b>l-amts<u>aa</u>l<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1725, '13:18', 13, 18, ' lilla<u>dz</u>iina istaj<u>aa</u>buu lirabbihimu <b>a</b>l<u>h</u>usn<u>aa</u> wa<b>a</b>lla<u>dz</u>iina lam yastajiibuu lahu law anna lahum m<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i jamii\'an wamitslahu ma\'ahu la<b>i</b>ftadaw bihi ul<u>aa</u>-ika lahum suu-u <b>a</b>l<u>h</u>is<u>aa</u>bi wama/w<u>aa</u>hum jahannamu wabi/sa <b>a</b>lmih<u>aa</u>d<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1726, '13:19', 13, 19, ' afaman ya\'lamu annam<u>aa</u> unzila ilayka min rabbika <b>a</b>l<u>h</u>aqqu kaman huwa a\'m<u>aa</u> innam<u>aa</u> yata<u>dz</u>akkaru uluu <b>a</b>l-alb<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1727, '13:20', 13, 20, ' <b>al</b>la<u>dz</u>iina yuufuuna bi\'ahdi <b>al</b>l<u>aa</u>hi wal<u>aa</u> yanqu<u>dh</u>uuna <b>a</b>lmiits<u>aa</u>q<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1728, '13:21', 13, 21, ' wa<b>a</b>lla<u>dz</u>iina ya<u>sh</u>iluuna m<u>aa</u> amara <b>al</b>l<u>aa</u>hu bihi an yuu<u>sh</u>ala wayakhsyawna rabbahum wayakh<u>aa</u>fuuna suu-a <b>a</b>l<u>h</u>is<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1729, '13:22', 13, 22, ' wa<b>a</b>lla<u>dz</u>iina <u>sh</u>abaruu ibtigh<u>aa</u>-a wajhi rabbihim wa-aq<u>aa</u>muu <b>al</b><u>shsh</u>al<u>aa</u>ta wa-anfaquu mimm<u>aa</u> razaqn<u>aa</u>hum sirran wa\'al<u>aa</u>niyatan wayadrauuna bi<b>a</b>l<u>h</u>asanati <b>al</b>ssayyi-ata ul<u>aa</u>-ika lahum \'uqb<u>aa</u> <b>al</b>dd<u>aa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1730, '13:23', 13, 23, ' jann<u>aa</u>tu \'adnin yadkhuluunah<u>aa</u> waman <u>sh</u>ala<u>h</u>a min <u>aa</u>b<u>aa</u>-ihim wa-azw<u>aa</u>jihim wa<u>dz</u>urriyy<u>aa</u>tihim wa<b>a</b>lmal<u>aa</u>-ikatu yadkhuluuna \'alayhim min kulli b<u>aa</u>b<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1731, '13:24', 13, 24, ' sal<u>aa</u>mun \'alaykum bim<u>aa</u> <u>sh</u>abartum fani\'ma \'uqb<u>aa</u> <b>al</b>dd<u>aa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1732, '13:25', 13, 25, ' wa<b>a</b>lla<u>dz</u>iina yanqu<u>dh</u>uuna \'ahda <b>al</b>l<u>aa</u>hi min ba\'di miits<u>aa</u>qihi wayaq<u>th</u>a\'uuna m<u>aa</u> amara <b>al</b>l<u>aa</u>hu bihi an yuu<u>sh</u>ala wayufsiduuna fii <b>a</b>l-ar<u>dh</u>i ul<u>aa</u>-ika lahumu <b>al</b>la\'natu walahum suu-u <b>al</b>dd<u>aa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1733, '13:26', 13, 26, ' <b>al</b>l<u>aa</u>hu yabsu<u>th</u>u <b>al</b>rrizqa liman yasy<u>aa</u>u wayaqdiru wafari<u>h</u>uu bi<b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> wam<u>aa</u> <b>a</b>l<u>h</u>ay<u>aa</u>tu <b>al</b>dduny<u>aa</u> fii <b>a</b>l-<u>aa</u>khirati ill<u>aa</u> mat<u>aa</u>\'<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1734, '13:27', 13, 27, ' wayaquulu <b>al</b>la<u>dz</u>iina kafaruu lawl<u>aa</u> unzila \'alayhi <u>aa</u>yatun min rabbihi qul inna <b>al</b>l<u>aa</u>ha yu<u>dh</u>illu man yasy<u>aa</u>u wayahdii ilayhi man an<u>aa</u>b<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1735, '13:28', 13, 28, ' <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wata<u>th</u>ma-innu quluubuhum bi<u>dz</u>ikri <b>al</b>l<u>aa</u>hi <b>a</b>l<u>aa</u> bi<u>dz</u>ikri <b>al</b>l<u>aa</u>hi ta<u>th</u>ma-innu <b>a</b>lquluub<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1736, '13:29', 13, 29, ' <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti <u>th</u>uub<u>aa</u> lahum wa<u>h</u>usnu ma<u>aa</u>b<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1737, '13:30', 13, 30, ' ka<u>dzaa</u>lika arsaln<u>aa</u>ka fii ummatin qad khalat min qablih<u>aa</u> umamun litatluwa \'alayhimu <b>al</b>la<u>dz</u>ii aw<u>h</u>ayn<u>aa</u> ilayka wahum yakfuruuna bi<b>al</b>rra<u>h</u>m<u>aa</u>ni qul huwa rabbii l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa \'alayhi tawakkaltu wa-ilayhi mat<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1738, '13:31', 13, 31, ' walaw anna qur-<u>aa</u>nan suyyirat bihi <b>a</b>ljib<u>aa</u>lu aw qu<u>ththh</u>i\'at bihi <b>a</b>l-ar<u>dh</u>u aw kullima bihi <b>a</b>lmawt<u>aa</u> bal lill<u>aa</u>hi <b>a</b>l-amru jamii\'an afalam yay-asi <b>al</b>la<u>dz</u>iina <u>aa</u>manuu an law yasy<u>aa</u>u <b>al</b>l<u>aa</u>hu lahad<u>aa</u> <b>al</b>nn<u>aa</u>sa jamii\'an wal<u>aa</u> yaz<u>aa</u>lu <b>al</b>la<u>dz</u>iina kafaruu tu<u>sh</u>iibuhum bim<u>aa</u> <u>sh</u>ana\'uu q<u>aa</u>ri\'atun aw ta<u>h</u>ullu qariiban min d<u>aa</u>rihim <u>h</u>att<u>aa</u> ya/tiya wa\'du <b>al</b>l<u>aa</u>hi inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yukhlifu <b>a</b>lmii\'<u>aa</u>d<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1739, '13:32', 13, 32, ' walaqadi istuhzi-a birusulin min qablika fa-amlaytu lilla<u>dz</u>iina kafaruu tsumma akha<u>dz</u>tuhum fakayfa k<u>aa</u>na \'iq<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1740, '13:33', 13, 33, ' afaman huwa q<u>aa</u>-imun \'al<u>aa</u> kulli nafsin bim<u>aa</u> kasabat waja\'aluu lill<u>aa</u>hi syurak<u>aa</u>-a qul sammuuhum am tunabbi-uunahu bim<u>aa</u> l<u>aa</u> ya\'lamu fii <b>a</b>l-ar<u>dh</u>i am bi<u>zhaa</u>hirin mina <b>a</b>lqawli bal zuyyina lilla<u>dz</u>iina kafaruu makruhum wa<u>sh</u>udduu \'ani <b>al</b>ssabiili waman yu<u>dh</u>lili <b>al</b>l<u>aa</u>hu fam<u>aa</u> lahu min h<u>aa</u>d<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1741, '13:34', 13, 34, ' lahum \'a<u>dzaa</u>bun fii <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> wala\'a<u>dzaa</u>bu <b>a</b>l-<u>aa</u>khirati asyaqqu wam<u>aa</u> lahum mina <b>al</b>l<u>aa</u>hi min w<u>aa</u>q<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1742, '13:35', 13, 35, ' matsalu <b>a</b>ljannati <b>al</b>latii wu\'ida <b>a</b>lmuttaquuna tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru ukuluh<u>aa</u> d<u>aa</u>-imun wa<u>zh</u>illuh<u>aa</u> tilka \'uqb<u>aa</u> <b>al</b>la<u>dz</u>iina ittaqaw wa\'uqb<u>aa</u> <b>a</b>lk<u>aa</u>firiina <b>al</b>nn<u>aa</u>r<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1743, '13:36', 13, 36, ' wa<b>a</b>lla<u>dz</u>iina <u>aa</u>tayn<u>aa</u>humu <b>a</b>lkit<u>aa</u>ba yafra<u>h</u>uuna bim<u>aa</u> unzila ilayka wamina <b>a</b>l-a<u>h</u>z<u>aa</u>bi man yunkiru ba\'<u>dh</u>ahu qul innam<u>aa</u> umirtu an a\'buda <b>al</b>l<u>aa</u>ha wal<u>aa</u> usyrika bihi ilayhi ad\'uu wa-ilayhi ma<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1744, '13:37', 13, 37, ' waka<u>dzaa</u>lika anzaln<u>aa</u>hu <u>h</u>ukman \'arabiyyan wala-ini ittaba\'ta ahw<u>aa</u>-ahum ba\'da m<u>aa</u> j<u>aa</u>-aka mina <b>a</b>l\'ilmi m<u>aa</u> laka mina <b>al</b>l<u>aa</u>hi min waliyyin wal<u>aa</u> w<u>aa</u>q<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1745, '13:38', 13, 38, ' walaqad arsaln<u>aa</u> rusulan min qablika waja\'aln<u>aa</u> lahum azw<u>aa</u>jan wa<u>dz</u>urriyyatan wam<u>aa</u> k<u>aa</u>na lirasuulin an ya/tiya bi-<u>aa</u>yatin ill<u>aa</u> bi-i<u>dz</u>ni <b>al</b>l<u>aa</u>hi likulli ajalin kit<u>aa</u>b<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1746, '13:39', 13, 39, ' yam<u>h</u>uu <b>al</b>l<u>aa</u>hu m<u>aa</u> yasy<u>aa</u>u wayutsbitu wa\'indahu ummu <b>a</b>lkit<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1747, '13:40', 13, 40, ' wa-in m<u>aa</u> nuriyannaka ba\'<u>dh</u>a <b>al</b>la<u>dz</u>ii na\'iduhum aw natawaffayannaka fa-innam<u>aa</u> \'alayka <b>a</b>lbal<u>aa</u>ghu wa\'alayn<u>aa</u> <b>a</b>l<u>h</u>is<u>aa</u>b<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1748, '13:41', 13, 41, ' awa lam yaraw ann<u>aa</u> na/tii <b>a</b>l-ar<u>dh</u>a nanqu<u>sh</u>uh<u>aa</u> min a<u>th</u>r<u>aa</u>fih<u>aa</u> wa<b>al</b>l<u>aa</u>hu ya<u>h</u>kumu l<u>aa</u> mu\'aqqiba li<u>h</u>ukmihi wahuwa sarii\'u <b>a</b>l<u>h</u>is<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1749, '13:42', 13, 42, ' waqad makara <b>al</b>la<u>dz</u>iina min qablihim falill<u>aa</u>hi <b>a</b>lmakru jamii\'an ya\'lamu m<u>aa</u> taksibu kullu nafsin wasaya\'lamu <b>a</b>lkuff<u>aa</u>ru liman \'uqb<u>aa</u> <b>al</b>dd<u>aa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1750, '13:43', 13, 43, ' wayaquulu <b>al</b>la<u>dz</u>iina kafaruu lasta mursalan qul kaf<u>aa</u> bi<b>al</b>l<u>aa</u>hi syahiidan baynii wabaynakum waman \'indahu \'ilmu <b>a</b>lkit<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1751, '14:1', 14, 1, ' alif-l<u>aa</u>m-r<u>aa</u> kit<u>aa</u>bun anzaln<u>aa</u>hu ilayka litukhrija <b>al</b>nn<u>aa</u>sa mina <b>al</b><u>zhzh</u>ulum<u>aa</u>ti il<u>aa</u> <b>al</b>nnuuri bi-i<u>dz</u>ni rabbihim il<u>aa</u> <u>sh</u>ir<u>aath</u>i <b>a</b>l\'aziizi <b>a</b>l<u>h</u>amiid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1752, '14:2', 14, 2, ' <b>al</b>l<u>aa</u>hi <b>al</b>la<u>dz</u>ii lahu m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i wawaylun lilk<u>aa</u>firiina min \'a<u>dzaa</u>bin syadiid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1753, '14:3', 14, 3, ' <b>al</b>la<u>dz</u>iina yasta<u>h</u>ibbuuna <b>a</b>l<u>h</u>ay<u>aa</u>ta <b>al</b>dduny<u>aa</u> \'al<u>aa</u> <b>a</b>l-<u>aa</u>khirati waya<u>sh</u>udduuna \'an sabiili <b>al</b>l<u>aa</u>hi wayabghuunah<u>aa</u> \'iwajan ul<u>aa</u>-ika fii <u>dh</u>al<u>aa</u>lin ba\'iid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1754, '14:4', 14, 4, ' wam<u>aa</u> arsaln<u>aa</u> min rasuulin ill<u>aa</u> bilis<u>aa</u>ni qawmihi liyubayyina lahum fayu<u>dh</u>illu <b>al</b>l<u>aa</u>hu man yasy<u>aa</u>u wayahdii man yasy<u>aa</u>u wahuwa <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1755, '14:5', 14, 5, ' walaqad arsaln<u>aa</u> muus<u>aa</u> bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> an akhrij qawmaka mina <b>al</b><u>zhzh</u>ulum<u>aa</u>ti il<u>aa</u> <b>al</b>nnuuri wa<u>dz</u>akkirhum bi-ayy<u>aa</u>mi <b>al</b>l<u>aa</u>hi inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin likulli <u>sh</u>abb<u>aa</u>rin syakuur<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1756, '14:6', 14, 6, ' wa-i<u>dz</u> q<u>aa</u>la muus<u>aa</u> liqawmihi u<u>dz</u>kuruu ni\'mata <b>al</b>l<u>aa</u>hi \'alaykum i<u>dz</u> anj<u>aa</u>kum min <u>aa</u>li fir\'awna yasuumuunakum suu-a <b>a</b>l\'a<u>dzaa</u>bi wayu<u>dz</u>abbi<u>h</u>uuna abn<u>aa</u>-akum wayasta<u>h</u>yuuna nis<u>aa</u>-akum wafii <u>dzaa</u>likum bal<u>aa</u>un min rabbikum \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1757, '14:7', 14, 7, ' wa-i<u>dz</u> ta-a<u>dzdz</u>ana rabbukum la-in syakartum la-aziidannakum wala-in kafartum inna \'a<u>dzaa</u>bii lasyadiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1758, '14:8', 14, 8, ' waq<u>aa</u>la muus<u>aa</u> in takfuruu antum waman fii <b>a</b>l-ar<u>dh</u>i jamii\'an fa-inna <b>al</b>l<u>aa</u>ha laghaniyyun <u>h</u>amiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1759, '14:9', 14, 9, ' alam ya/tikum nabau <b>al</b>la<u>dz</u>iina min qablikum qawmi nuu<u>h</u>in wa\'<u>aa</u>din watsamuuda wa<b>a</b>lla<u>dz</u>iina min ba\'dihim l<u>aa</u> ya\'lamuhum ill<u>aa</u> <b>al</b>l<u>aa</u>hu j<u>aa</u>-at-hum rusuluhum bi<b>a</b>lbayyin<u>aa</u>ti faradduu aydiyahum fii afw<u>aa</u>hihim waq<u>aa</u>luu inn<u>aa</u> kafarn<u>aa</u> bim<u>aa</u> ursiltum bihi wa-inn<u>aa</u> lafii syakkin mimm<u>aa</u> tad\'uunan<u>aa</u> ilayhi muriib<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1760, '14:10', 14, 10, ' q<u>aa</u>lat rusuluhum afii <b>al</b>l<u>aa</u>hi syakkun f<u>aath</u>iri <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i yad\'uukum liyaghfira lakum min <u>dz</u>unuubikum wayu-akhkhirakum il<u>aa</u> ajalin musamman q<u>aa</u>luu in antum ill<u>aa</u> basyarun mitslun<u>aa</u> turiiduuna an ta<u>sh</u>udduun<u>aa</u> \'amm<u>aa</u> k<u>aa</u>na ya\'budu <u>aa</u>b<u>aa</u>un<u>aa</u> fa/tuun<u>aa</u> bisul<u>th</u><u>aa</u>nin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1761, '14:11', 14, 11, ' q<u>aa</u>lat lahum rusuluhum in na<u>h</u>nu ill<u>aa</u> basyarun mitslukum wal<u>aa</u>kinna <b>al</b>l<u>aa</u>ha yamunnu \'al<u>aa</u> man yasy<u>aa</u>u min \'ib<u>aa</u>dihi wam<u>aa</u> k<u>aa</u>na lan<u>aa</u> an na/tiyakum bisul<u>th</u><u>aa</u>nin ill<u>aa</u> bi-i<u>dz</u>ni <b>al</b>l<u>aa</u>hi wa\'al<u>aa</u> <b>al</b>l<u>aa</u>hi falyatawakkali <b>a</b>lmu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1762, '14:12', 14, 12, ' wam<u>aa</u> lan<u>aa</u> <b>al</b>l<u>aa</u> natawakkala \'al<u>aa</u> <b>al</b>l<u>aa</u>hi waqad had<u>aa</u>n<u>aa</u> subulan<u>aa</u> walana<u>sh</u>biranna \'al<u>aa</u> m<u>aa</u> <u>aadz</u>aytumuun<u>aa</u> wa\'al<u>aa</u> <b>al</b>l<u>aa</u>hi falyatawakkali <b>a</b>lmutawakkiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1763, '14:13', 14, 13, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu lirusulihim lanukhrijannakum min ar<u>dh</u>in<u>aa</u> aw lata\'uudunna fii millatin<u>aa</u> fa-aw<u>haa</u> ilayhim rabbuhum lanuhlikanna <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1764, '14:14', 14, 14, ' walanuskinannakumu <b>a</b>l-ar<u>dh</u>a min ba\'dihim <u>dzaa</u>lika liman kh<u>aa</u>fa maq<u>aa</u>mii wakh<u>aa</u>fa wa\'iid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1765, '14:15', 14, 15, ' wa<b>i</b>stafta<u>h</u>uu wakh<u>aa</u>ba kullu jabb<u>aa</u>rin \'aniid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1766, '14:16', 14, 16, ' min war<u>aa</u>-ihi jahannamu wayusq<u>aa</u> min m<u>aa</u>-in <u>sh</u>adiid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1767, '14:17', 14, 17, ' yatajarra\'uhu wal<u>aa</u> yak<u>aa</u>du yusiighuhu waya/tiihi <b>a</b>lmawtu min kulli mak<u>aa</u>nin wam<u>aa</u> huwa bimayyitin wamin war<u>aa</u>-ihi \'a<u>dzaa</u>bun ghalii<u>zh</u><b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1768, '14:18', 14, 18, ' matsalu <b>al</b>la<u>dz</u>iina kafaruu birabbihim a\'m<u>aa</u>luhum karam<u>aa</u>din isytaddat bihi <b>al</b>rrii<u>h</u>u fii yawmin \'<u>aas</u>ifin l<u>aa</u> yaqdiruuna mimm<u>aa</u> kasabuu \'al<u>aa</u> syay-in <u>dzaa</u>lika huwa <b>al</b><u>dhdh</u>al<u>aa</u>lu <b>a</b>lba\'iid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1769, '14:19', 14, 19, ' alam tara anna <b>al</b>l<u>aa</u>ha khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a bi<b>a</b>l<u>h</u>aqqi in yasya/ yu<u>dz</u>hibkum waya/ti bikhalqin jadiid<b>in </b>\r\r\n');
INSERT INTO quranLatin VALUES (1770, '14:20', 14, 20, ' wam<u>aa</u> <u>dzaa</u>lika \'al<u>aa</u> <b>al</b>l<u>aa</u>hi bi\'aziiz<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1771, '14:21', 14, 21, ' wabarazuu lill<u>aa</u>hi jamii\'an faq<u>aa</u>la <b>al</b><u>dhdh</u>u\'af<u>aa</u>u lilla<u>dz</u>iina istakbaruu inn<u>aa</u> kunn<u>aa</u> lakum taba\'an fahal antum mughnuuna \'ann<u>aa</u> min \'a<u>dzaa</u>bi <b>al</b>l<u>aa</u>hi min syay-in q<u>aa</u>luu law had<u>aa</u>n<u>aa</u> <b>al</b>l<u>aa</u>hu lahadayn<u>aa</u>kum saw<u>aa</u>un \'alayn<u>aa</u> ajazi\'n<u>aa</u> am <u>sh</u>abarn<u>aa</u> m<u>aa</u> lan<u>aa</u> min ma<u>h</u>ii<u>sh</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1772, '14:22', 14, 22, ' waq<u>aa</u>la <b>al</b>sysyay<u>th</u><u>aa</u>nu lamm<u>aa</u> qu<u>dh</u>iya <b>a</b>l-amru inna <b>al</b>l<u>aa</u>ha wa\'adakum wa\'da <b>a</b>l<u>h</u>aqqi wawa\'adtukum fa-akhlaftukum wam<u>aa</u> k<u>aa</u>na liya \'alaykum min sul<u>th</u><u>aa</u>nin ill<u>aa</u> an da\'awtukum fa<b>i</b>stajabtum lii fal<u>aa</u> taluumuunii waluumuu anfusakum m<u>aa</u> an<u>aa</u> bimu<u>sh</u>rikhikum wam<u>aa</u> antum bimu<u>sh</u>rikhiyya innii kafartu bim<u>aa</u> asyraktumuuni min qablu inna <b>al</b><u>zhzhaa</u>limiina lahum \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1773, '14:23', 14, 23, ' waudkhila <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru kh<u>aa</u>lidiina fiih<u>aa</u> bi-i<u>dz</u>ni rabbihim ta<u>h</u>iyyatuhum fiih<u>aa</u> sal<u>aa</u>m<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1774, '14:24', 14, 24, ' alam tara kayfa <u>dh</u>araba <b>al</b>l<u>aa</u>hu matsalan kalimatan <u>th</u>ayyibatan kasyajaratin <u>th</u>ayyibatin a<u>sh</u>luh<u>aa</u> ts<u>aa</u>bitun wafar\'uh<u>aa</u> fii <b>al</b>ssam<u>aa</u>/-<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1775, '14:25', 14, 25, ' tu/tii ukulah<u>aa</u> kulla <u>h</u>iinin bi-i<u>dz</u>ni rabbih<u>aa</u> waya<u>dh</u>ribu <b>al</b>l<u>aa</u>hu <b>a</b>l-amts<u>aa</u>la li<b>l</b>nn<u>aa</u>si la\'allahum yata<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1776, '14:26', 14, 26, ' wamatsalu kalimatin khabiitsatin kasyajaratin khabiitsatin ijtutstsat min fawqi <b>a</b>l-ar<u>dh</u>i m<u>aa</u> lah<u>aa</u> min qar<u>aa</u>r<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1777, '14:27', 14, 27, ' yutsabbitu <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>iina <u>aa</u>manuu bi<b>a</b>lqawli <b>al</b>tsts<u>aa</u>biti fii <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> wafii <b>a</b>l-<u>aa</u>khirati wayu<u>dh</u>illu <b>al</b>l<u>aa</u>hu <b>al</b><u>zhzhaa</u>limiina wayaf\'alu <b>al</b>l<u>aa</u>hu m<u>aa</u> yasy<u>aa</u>/<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1778, '14:28', 14, 28, ' alam tara il<u>aa</u> <b>al</b>la<u>dz</u>iina baddaluu ni\'mata <b>al</b>l<u>aa</u>hi kufran wa-a<u>h</u>alluu qawmahum d<u>aa</u>ra <b>a</b>lbaw<u>aa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1779, '14:29', 14, 29, ' jahannama ya<u>sh</u>lawnah<u>aa</u> wabi/sa <b>a</b>lqar<u>aa</u>r<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1780, '14:30', 14, 30, ' waja\'aluu lill<u>aa</u>hi and<u>aa</u>dan liyu<u>dh</u>illuu \'an sabiilihi qul tamatta\'uu fa-inna ma<u>sh</u>iirakum il<u>aa</u> <b>al</b>nn<u>aa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1781, '14:31', 14, 31, ' qul li\'ib<u>aa</u>diya <b>al</b>la<u>dz</u>iina <u>aa</u>manuu yuqiimuu <b>al</b><u>shsh</u>al<u>aa</u>ta wayunfiquu mimm<u>aa</u> razaqn<u>aa</u>hum sirran wa\'al<u>aa</u>niyatan min qabli an ya/tiya yawmun l<u>aa</u> bay\'un fiihi wal<u>aa</u> khil<u>aa</u>l<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1782, '14:32', 14, 32, ' <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a wa-anzala mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an fa-akhraja bihi mina <b>al</b>tstsamar<u>aa</u>ti rizqan lakum wasakhkhara lakumu <b>a</b>lfulka litajriya fii <b>a</b>lba<u>h</u>ri bi-amrihi wasakhkhara lakumu <b>a</b>l-anh<u>aa</u>r<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1783, '14:33', 14, 33, ' wasakhkhara lakumu <b>al</b>sysyamsa wa<b>a</b>lqamara d<u>aa</u>-ibayni wasakhkhara lakumu <b>al</b>layla wa<b>al</b>nnah<u>aa</u>r<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1784, '14:34', 14, 34, ' wa<u>aa</u>t<u>aa</u>kum min kulli m<u>aa</u> sa-altumuuhu wa-in ta\'udduu ni\'mata <b>al</b>l<u>aa</u>hi l<u>aa</u> tu<u>hs</u>uuh<u>aa</u> inna <b>a</b>l-ins<u>aa</u>na la<u>zh</u>aluumun kaff<u>aa</u>r<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1785, '14:35', 14, 35, ' wa-i<u>dz</u> q<u>aa</u>la ibr<u>aa</u>hiimu rabbi ij\'al h<u>aadzaa</u> <b>a</b>lbalada <u>aa</u>minan wa<b>u</b>jnubnii wabaniyya an na\'buda <b>a</b>l-a<u>sh</u>n<u>aa</u>m<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1786, '14:36', 14, 36, ' rabbi innahunna a<u>dh</u>lalna katsiiran mina <b>al</b>nn<u>aa</u>si faman tabi\'anii fa-innahu minnii waman \'a<u>shaa</u>nii fa-innaka ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1787, '14:37', 14, 37, ' rabban<u>aa</u> innii askantu min <u>dz</u>urriyyatii biw<u>aa</u>din ghayri <u>dz</u>ii zar\'in \'inda baytika <b>a</b>lmu<u>h</u>arrami rabban<u>aa</u> liyuqiimuu <b>al</b><u>shsh</u>al<u>aa</u>ta fa<b>i</b>j\'al af-idatan mina <b>al</b>nn<u>aa</u>si tahwii ilayhim wa<b>u</b>rzuqhum mina <b>al</b>tstsamar<u>aa</u>ti la\'allahum yasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1788, '14:38', 14, 38, ' rabban<u>aa</u> innaka ta\'lamu m<u>aa</u> nukhfii wam<u>aa</u> nu\'linu wam<u>aa</u> yakhf<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi min syay-in fii <b>a</b>l-ar<u>dh</u>i wal<u>aa</u> fii <b>al</b>ssam<u>aa</u>/-<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1789, '14:39', 14, 39, ' al<u>h</u>amdu lill<u>aa</u>hi <b>al</b>la<u>dz</u>ii wahaba lii \'al<u>aa</u> <b>a</b>lkibari ism<u>aa</u>\'iila wa-is<u>haa</u>qa inna rabbii lasamii\'u <b>al</b>ddu\'<u>aa</u>/-<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1790, '14:40', 14, 40, ' rabbi ij\'alnii muqiima <b>al</b><u>shsh</u>al<u>aa</u>ti wamin <u>dz</u>urriyyatii rabban<u>aa</u> wtaqabbal du\'<u>aa</u>/-<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1791, '14:41', 14, 41, ' rabban<u>aa</u> ighfir lii waliw<u>aa</u>lidayya walilmu/miniina yawma yaquumu <b>a</b>l<u>h</u>is<u>aa</u>b<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1792, '14:42', 14, 42, ' wal<u>aa</u> ta<u>h</u>sabanna <b>al</b>l<u>aa</u>ha gh<u>aa</u>filan \'amm<u>aa</u> ya\'malu <b>al</b><u>zhzhaa</u>limuuna innam<u>aa</u> yu-akhkhiruhum liyawmin tasykha<u>sh</u>u fiihi <b>a</b>l-ab<u>shaa</u>r<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1793, '14:43', 14, 43, ' muh<u>th</u>i\'iina muqni\'ii ruuusihim l<u>aa</u> yartaddu ilayhim <u>th</u>arfuhum wa-af-idatuhum haw<u>aa</u>/<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1794, '14:44', 14, 44, ' wa-an<u>dz</u>iri <b>al</b>nn<u>aa</u>sa yawma ya/tiihimu <b>a</b>l\'a<u>dzaa</u>bu fayaquulu <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu rabban<u>aa</u> akhkhirn<u>aa</u> il<u>aa</u> ajalin qariibin nujib da\'wataka wanattabi\'i <b>al</b>rrusula awa lam takuunuu aqsamtum min qablu m<u>aa</u> lakum min zaw<u>aa</u>l<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1795, '14:45', 14, 45, ' wasakantum fii mas<u>aa</u>kini <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu anfusahum watabayyana lakum kayfa fa\'aln<u>aa</u> bihim wa<u>dh</u>arabn<u>aa</u> lakumu <b>a</b>l-amts<u>aa</u>l<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1796, '14:46', 14, 46, ' waqad makaruu makrahum wa\'inda <b>al</b>l<u>aa</u>hi makruhum wa-in k<u>aa</u>na makruhum litazuula minhu <b>a</b>ljib<u>aa</u>l<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1797, '14:47', 14, 47, ' fal<u>aa</u> ta<u>h</u>sabanna <b>al</b>l<u>aa</u>ha mukhlifa wa\'dihi rusulahu inna <b>al</b>l<u>aa</u>ha \'aziizun <u>dz</u>uu intiq<u>aa</u>m<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1798, '14:48', 14, 48, ' yawma tubaddalu <b>a</b>l-ar<u>dh</u>u ghayra <b>a</b>l-ar<u>dh</u>i wa<b>al</b>ssam<u>aa</u>w<u>aa</u>tu wabarazuu lill<u>aa</u>hi <b>a</b>lw<u>aah</u>idi <b>a</b>lqahh<u>aa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1799, '14:49', 14, 49, ' watar<u>aa</u> <b>a</b>lmujrimiina yawma-i<u>dz</u>in muqarraniina fii <b>a</b>l-a<u>sh</u>f<u>aa</u>d<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1800, '14:50', 14, 50, ' sar<u>aa</u>biiluhum min qa<u>th</u>r<u>aa</u>nin wataghsy<u>aa</u> wujuuhahumu <b>al</b>nn<u>aa</u>r<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1801, '14:51', 14, 51, ' liyajziya <b>al</b>l<u>aa</u>hu kulla nafsin m<u>aa</u> kasabat inna <b>al</b>l<u>aa</u>ha sarii\'u <b>a</b>l<u>h</u>is<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1802, '14:52', 14, 52, ' h<u>aadzaa</u> bal<u>aa</u>ghun li<b>l</b>nn<u>aa</u>si waliyun<u>dz</u>aruu bihi waliya\'lamuu annam<u>aa</u> huwa il<u>aa</u>hun w<u>aah</u>idun waliya<u>dzdz</u>akkara uluu <b>a</b>l-alb<u>aa</u>b<b>i</b><dir>\r\r\n');
INSERT INTO quranLatin VALUES (1803, '15:1', 15, 1, ' alif-l<u>aa</u>m-r<u>aa</u> tilka <u>aa</u>y<u>aa</u>tu <b>a</b>lkit<u>aa</u>bi waqur-<u>aa</u>nin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1804, '15:2', 15, 2, ' rubam<u>aa</u> yawaddu <b>al</b>la<u>dz</u>iina kafaruu law k<u>aa</u>nuu muslimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1805, '15:3', 15, 3, ' <u>dz</u>arhum ya/kuluu wayatamatta\'uu wayulhihimu <b>a</b>l-amalu fasawfa ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1806, '15:4', 15, 4, ' wam<u>aa</u> ahlakn<u>aa</u> min qaryatin ill<u>aa</u> walah<u>aa</u> kit<u>aa</u>bun ma\'luum<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1807, '15:5', 15, 5, ' m<u>aa</u> tasbiqu min ummatin ajalah<u>aa</u> wam<u>aa</u> yasta/khiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1808, '15:6', 15, 6, ' waq<u>aa</u>luu y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>ii nuzzila \'alayhi <b>al</b><u>dzdz</u>ikru innaka lamajnuun<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1809, '15:7', 15, 7, ' law m<u>aa</u> ta/tiin<u>aa</u> bi<b>a</b>lmal<u>aa</u>-ikati in kunta mina <b>al</b><u>shshaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1810, '15:8', 15, 8, ' m<u>aa</u> nunazzilu <b>a</b>lmal<u>aa</u>-ikata ill<u>aa</u> bi<b>a</b>l<u>h</u>aqqi wam<u>aa</u> k<u>aa</u>nuu i<u>dz</u>an mun<u>zh</u>ariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1811, '15:9', 15, 9, ' inn<u>aa</u> na<u>h</u>nu nazzaln<u>aa</u> <b>al</b><u>dzdz</u>ikra wa-inn<u>aa</u> lahu la<u>haa</u>fi<u>zh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1812, '15:10', 15, 10, ' walaqad arsaln<u>aa</u> min qablika fii syiya\'i <b>a</b>l-awwaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1813, '15:11', 15, 11, ' wam<u>aa</u> ya/tiihim min rasuulin ill<u>aa</u> k<u>aa</u>nuu bihi yastahzi-uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1814, '15:12', 15, 12, ' ka<u>dzaa</u>lika naslukuhu fii quluubi <b>a</b>lmujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1815, '15:13', 15, 13, ' l<u>aa</u> yu/minuuna bihi waqad khalat sunnatu <b>a</b>l-awwaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1816, '15:14', 15, 14, ' walaw fata<u>h</u>n<u>aa</u> \'alayhim b<u>aa</u>ban mina <b>al</b>ssam<u>aa</u>-i fa<u>zh</u>alluu fiihi ya\'rujuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1817, '15:15', 15, 15, ' laq<u>aa</u>luu innam<u>aa</u> sukkirat ab<u>shaa</u>run<u>aa</u> bal na<u>h</u>nu qawmun mas<u>h</u>uuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1818, '15:16', 15, 16, ' walaqad ja\'aln<u>aa</u> fii <b>al</b>ssam<u>aa</u>-i buruujan wazayyann<u>aa</u>h<u>aa</u> li<b>l</b>nn<u>aats</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1819, '15:17', 15, 17, ' wa<u>h</u>afi<u>zh</u>n<u>aa</u>h<u>aa</u> min kulli syay<u>th</u><u>aa</u>nin rajiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1820, '15:18', 15, 18, ' ill<u>aa</u> mani istaraqa <b>al</b>ssam\'a fa-atba\'ahu syih<u>aa</u>bun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1821, '15:19', 15, 19, ' wa<b>a</b>l-ar<u>dh</u>a madadn<u>aa</u>h<u>aa</u> wa-alqayn<u>aa</u> fiih<u>aa</u> raw<u>aa</u>siya wa-anbatn<u>aa</u> fiih<u>aa</u> min kulli syay-in mawzuun<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1822, '15:20', 15, 20, ' waja\'aln<u>aa</u> lakum fiih<u>aa</u> ma\'<u>aa</u>yisya waman lastum lahu bir<u>aa</u>ziqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1823, '15:21', 15, 21, ' wa-in min syay-in ill<u>aa</u> \'indan<u>aa</u> khaz<u>aa</u>-inuhu wam<u>aa</u> nunazziluhu ill<u>aa</u> biqadarin ma\'luum<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1824, '15:22', 15, 22, ' wa-arsaln<u>aa</u> <b>al</b>rriy<u>aah</u>a law<u>aa</u>qi<u>h</u>a fa-anzaln<u>aa</u> mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an fa-asqayn<u>aa</u>kumuuhu wam<u>aa</u> antum lahu bikh<u>aa</u>ziniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1825, '15:23', 15, 23, ' wa-inn<u>aa</u> lana<u>h</u>nu nu<u>h</u>yii wanumiitu wana<u>h</u>nu <b>a</b>lw<u>aa</u>ritsuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1826, '15:24', 15, 24, ' walaqad \'alimn<u>aa</u> <b>a</b>lmustaqdimiina minkum walaqad \'alimn<u>aa</u> <b>a</b>lmusta/khiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1827, '15:25', 15, 25, ' wa-inna rabbaka huwa ya<u>h</u>syuruhum innahu <u>h</u>akiimun \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1828, '15:26', 15, 26, ' walaqad khalaqn<u>aa</u> <b>a</b>l-ins<u>aa</u>na min <u>sh</u>al<u>shaa</u>lin min <u>h</u>ama-in masnuun<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1829, '15:27', 15, 27, ' wa<b>a</b>lj<u>aa</u>nna khalaqn<u>aa</u>hu min qablu min n<u>aa</u>ri <b>al</b>ssamuum<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1830, '15:28', 15, 28, ' wa-i<u>dz</u> q<u>aa</u>la rabbuka lilmal<u>aa</u>-ikati innii kh<u>aa</u>liqun basyaran min <u>sh</u>al<u>shaa</u>lin min <u>h</u>ama-in masnuun<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1831, '15:29', 15, 29, ' fa-i<u>dzaa</u> sawwaytuhu wanafakhtu fiihi min ruu<u>h</u>ii faqa\'uu lahu s<u>aa</u>jidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1832, '15:30', 15, 30, ' fasajada <b>a</b>lmal<u>aa</u>-ikatu kulluhum ajma\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1833, '15:31', 15, 31, ' ill<u>aa</u> ibliisa ab<u>aa</u> an yakuuna ma\'a <b>al</b>ss<u>aa</u>jidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1834, '15:32', 15, 32, ' q<u>aa</u>la y<u>aa</u> ibliisu m<u>aa</u> laka <b>al</b>l<u>aa</u> takuuna ma\'a <b>al</b>ss<u>aa</u>jidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1835, '15:33', 15, 33, ' q<u>aa</u>la lam akun li-asjuda libasyarin khalaqtahu min <u>sh</u>al<u>shaa</u>lin min <u>h</u>ama-in masnuun<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1836, '15:34', 15, 34, ' q<u>aa</u>la fa<b>u</b>khruj minh<u>aa</u> fa-innaka rajiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1837, '15:35', 15, 35, ' wa-inna \'alayka <b>al</b>la\'nata il<u>aa</u> yawmi <b>al</b>ddiin<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1838, '15:36', 15, 36, ' q<u>aa</u>la rabbi fa-an<u>zh</u>irnii il<u>aa</u> yawmi yub\'atsuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1839, '15:37', 15, 37, ' q<u>aa</u>la fa-innaka mina <b>a</b>lmun<u>zh</u>ariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1840, '15:38', 15, 38, ' il<u>aa</u> yawmi <b>a</b>lwaqti <b>a</b>lma\'luum<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1841, '15:39', 15, 39, ' q<u>aa</u>la rabbi bim<u>aa</u> aghwaytanii lauzayyinanna lahum fii <b>a</b>l-ar<u>dh</u>i walaughwiyannahum ajma\'iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1842, '15:40', 15, 40, ' ill<u>aa</u> \'ib<u>aa</u>daka minhumu <b>a</b>lmukhla<u>sh</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1843, '15:41', 15, 41, ' q<u>aa</u>la h<u>aadzaa</u> <u>sh</u>ir<u>aath</u>un \'alayya mustaqiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1844, '15:42', 15, 42, ' inna \'ib<u>aa</u>dii laysa laka \'alayhim sul<u>th</u><u>aa</u>nun ill<u>aa</u> mani ittaba\'aka mina <b>a</b>lgh<u>aa</u>wiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1845, '15:43', 15, 43, ' wa-inna jahannama lamaw\'iduhum ajma\'iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1846, '15:44', 15, 44, ' lah<u>aa</u> sab\'atu abw<u>aa</u>bin likulli b<u>aa</u>bin minhum juz-un maqsuum<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (1847, '15:45', 15, 45, ' inna <b>a</b>lmuttaqiina fii jann<u>aa</u>tin wa\'uyuun<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1848, '15:46', 15, 46, ' udkhuluuh<u>aa</u> bisal<u>aa</u>min <u>aa</u>miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1849, '15:47', 15, 47, ' wanaza\'n<u>aa</u> m<u>aa</u> fii <u>sh</u>uduurihim min ghillin ikhw<u>aa</u>nan \'al<u>aa</u> sururin mutaq<u>aa</u>biliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1850, '15:48', 15, 48, ' l<u>aa</u> yamassuhum fiih<u>aa</u> na<u>sh</u>abun wam<u>aa</u> hum minh<u>aa</u> bimukhrajiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1851, '15:49', 15, 49, ' nabbi/ \'ib<u>aa</u>dii annii an<u>aa</u> <b>a</b>lghafuuru <b>al</b>rra<u>h</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1852, '15:50', 15, 50, ' wa-anna \'a<u>dzaa</u>bii huwa <b>a</b>l\'a<u>dzaa</u>bu <b>a</b>l-aliim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (1853, '15:51', 15, 51, ' wanabbi/hum \'an <u>dh</u>ayfi ibr<u>aa</u>hiim<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1854, '15:52', 15, 52, ' i<u>dz</u> dakhaluu \'alayhi faq<u>aa</u>luu sal<u>aa</u>man q<u>aa</u>la inn<u>aa</u> minkum wajiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1855, '15:53', 15, 53, ' q<u>aa</u>luu l<u>aa</u> tawjal inn<u>aa</u> nubasysyiruka bighul<u>aa</u>min \'aliim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (1856, '15:54', 15, 54, ' q<u>aa</u>la abasysyartumuunii \'al<u>aa</u> an massaniya <b>a</b>lkibaru fabima tubasysyiruun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (1857, '15:55', 15, 55, ' q<u>aa</u>luu basysyarn<u>aa</u>ka bi<b>a</b>l<u>h</u>aqqi fal<u>aa</u> takun mina <b>a</b>lq<u>aa</u>ni<u>th</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1858, '15:56', 15, 56, ' q<u>aa</u>la waman yaqna<u>th</u>u min ra<u>h</u>mati rabbihi ill<u>aa</u> <b>al</b><u>dhdhaa</u>lluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1859, '15:57', 15, 57, ' q<u>aa</u>la fam<u>aa</u> kha<u>th</u>bukum ayyuh<u>aa</u> <b>a</b>lmursaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1860, '15:58', 15, 58, ' q<u>aa</u>luu inn<u>aa</u> ursiln<u>aa</u> il<u>aa</u> qawmin mujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1861, '15:59', 15, 59, ' ill<u>aa</u> <u>aa</u>la luu<u>th</u>in inn<u>aa</u> lamunajjuuhum ajma\'iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1862, '15:60', 15, 60, ' ill<u>aa</u> imra-atahu qaddarn<u>aa</u> innah<u>aa</u> lamina <b>a</b>lgh<u>aa</u>biriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1863, '15:61', 15, 61, ' falamm<u>aa</u> j<u>aa</u>-a <u>aa</u>la luu<u>th</u>in <b>a</b>lmursaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1864, '15:62', 15, 62, ' q<u>aa</u>la innakum qawmun munkaruun<b>a</b>\r\r\n');
