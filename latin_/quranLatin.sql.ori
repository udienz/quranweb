INSERT INTO quranLatin VALUES (5321, '68:50', 68, 50, ' fa<b>i</b>jtab<u>aa</u>hu rabbuhu faja\'alahu mina <b>al</b><u>shshaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
