INSERT INTO quranLatin VALUES (3327, '28:75', 28, 75, ' wanaza\'n<u>aa</u> min kulli ummatin syahiidan faquln<u>aa</u> h<u>aa</u>tuu burh<u>aa</u>nakum fa\'alimuu anna <b>a</b>l<u>h</u>aqqa lill<u>aa</u>hi wa<u>dh</u>alla \'anhum m<u>aa</u> k<u>aa</u>nuu yaftaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3328, '28:76', 28, 76, ' inna q<u>aa</u>ruuna k<u>aa</u>na min qawmi muus<u>aa</u> fabagh<u>aa</u> \'alayhim wa<u>aa</u>tayn<u>aa</u>hu mina <b>a</b>lkunuuzi m<u>aa</u> inna maf<u>aa</u>ti<u>h</u>ahu latanuu-u bi<b>a</b>l\'u<u>sh</u>bati ulii <b>a</b>lquwwati i<u>dz</u> q<u>aa</u>la lahu qawmuhu l<u>aa</u> tafra<u>h</u> inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yu<u>h</u>ibbu <b>a</b>lfari<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3329, '28:77', 28, 77, ' wa<b>i</b>btaghi fiim<u>aa</u> <u>aa</u>t<u>aa</u>ka <b>al</b>l<u>aa</u>hu <b>al</b>dd<u>aa</u>ra <b>a</b>l-<u>aa</u>khirata wal<u>aa</u> tansa na<u>sh</u>iibaka mina <b>al</b>dduny<u>aa</u> wa-a<u>h</u>sin kam<u>aa</u> a<u>h</u>sana <b>al</b>l<u>aa</u>hu ilayka wal<u>aa</u> tabghi <b>a</b>lfas<u>aa</u>da fii <b>a</b>l-ar<u>dh</u>i inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yu<u>h</u>ibbu <b>a</b>lmufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3330, '28:78', 28, 78, ' q<u>aa</u>la innam<u>aa</u> uutiituhu \'al<u>aa</u> \'ilmin \'indii awa lam ya\'lam anna <b>al</b>l<u>aa</u>ha qad ahlaka min qablihi mina <b>a</b>lquruuni man huwa asyaddu minhu quwwatan wa-aktsaru jam\'an wal<u>aa</u> yus-alu \'an <u>dz</u>unuubihimu <b>a</b>lmujrimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3331, '28:79', 28, 79, ' fakharaja \'al<u>aa</u> qawmihi fii ziinatihi q<u>aa</u>la <b>al</b>la<u>dz</u>iina yuriiduuna <b>a</b>l<u>h</u>ay<u>aa</u>ta <b>al</b>dduny<u>aa</u> y<u>aa</u> layta lan<u>aa</u> mitsla m<u>aa</u> uutiya q<u>aa</u>ruunu innahu la<u>dz</u>uu <u>h</u>a<u>zhzh</u>in \'a<u>zh</u>iim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3332, '28:80', 28, 80, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>iina uutuu <b>a</b>l\'ilma waylakum tsaw<u>aa</u>bu <b>al</b>l<u>aa</u>hi khayrun liman <u>aa</u>mana wa\'amila <u>shaa</u>li<u>h</u>an wal<u>aa</u> yulaqq<u>aa</u>h<u>aa</u> ill<u>aa</u> <b>al</b><u>shshaa</u>biruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3333, '28:81', 28, 81, ' fakhasafn<u>aa</u> bihi wabid<u>aa</u>rihi <b>a</b>l-ar<u>dh</u>a fam<u>aa</u> k<u>aa</u>na lahu min fi-atin yan<u>sh</u>uruunahu min duuni <b>al</b>l<u>aa</u>hi wam<u>aa</u> k<u>aa</u>na mina <b>a</b>lmunta<u>sh</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3334, '28:82', 28, 82, ' wa-a<u>sh</u>ba<u>h</u>a <b>al</b>la<u>dz</u>iina tamannaw mak<u>aa</u>nahu bi<b>a</b>l-amsi yaquuluuna wayka-anna <b>al</b>l<u>aa</u>ha yabsu<u>th</u>u <b>al</b>rrizqa liman yasy<u>aa</u>u min \'ib<u>aa</u>dihi wayaqdiru lawl<u>aa</u> an manna <b>al</b>l<u>aa</u>hu \'alayn<u>aa</u> lakhasafa bin<u>aa</u> wayka-annahu l<u>aa</u> yufli<u>h</u>u <b>a</b>lk<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3335, '28:83', 28, 83, ' tilka <b>al</b>dd<u>aa</u>ru <b>a</b>l-<u>aa</u>khiratu naj\'aluh<u>aa</u> lilla<u>dz</u>iina l<u>aa</u> yuriiduuna \'uluwwan fii <b>a</b>l-ar<u>dh</u>i wal<u>aa</u> fas<u>aa</u>dan wa<b>a</b>l\'<u>aa</u>qibatu lilmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3336, '28:84', 28, 84, ' man j<u>aa</u>-a bi<b>a</b>l<u>h</u>asanati falahu khayrun minh<u>aa</u> waman j<u>aa</u>-a bi<b>al</b>ssayyi-ati fal<u>aa</u> yujz<u>aa</u> <b>al</b>la<u>dz</u>iina \'amiluu <b>al</b>ssayyi-<u>aa</u>ti ill<u>aa</u> m<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3337, '28:85', 28, 85, ' inna <b>al</b>la<u>dz</u>ii fara<u>dh</u>a \'alayka <b>a</b>lqur-<u>aa</u>na lar<u>aa</u>dduka il<u>aa</u> ma\'<u>aa</u>din qul rabbii a\'lamu man j<u>aa</u>-a bi<b>a</b>lhud<u>aa</u> waman huwa fii <u>dh</u>al<u>aa</u>lin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3338, '28:86', 28, 86, ' wam<u>aa</u> kunta tarjuu an yulq<u>aa</u> ilayka <b>a</b>lkit<u>aa</u>bu ill<u>aa</u> ra<u>h</u>matan min rabbika fal<u>aa</u> takuunanna <u>zh</u>ahiiran lilk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3339, '28:87', 28, 87, ' wal<u>aa</u> ya<u>sh</u>uddunnaka \'an <u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi ba\'da i<u>dz</u> unzilat ilayka wa<b>u</b>d\'u il<u>aa</u> rabbika wal<u>aa</u> takuunanna mina <b>a</b>lmusyrikiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3340, '28:88', 28, 88, ' wal<u>aa</u> tad\'u ma\'a <b>al</b>l<u>aa</u>hi il<u>aa</u>han <u>aa</u>khara l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa kullu syay-in h<u>aa</u>likun ill<u>aa</u> wajhahu lahu <b>a</b>l<u>h</u>ukmu wa-ilayhi turja\'uun<b>a</b><dir>\r\r\n');
INSERT INTO quranLatin VALUES (3341, '29:1', 29, 1, ' alif-l<u>aa</u>m-miim</u>\r\r\n');
INSERT INTO quranLatin VALUES (3342, '29:2', 29, 2, ' a<u>h</u>asiba <b>al</b>nn<u>aa</u>su an yutrakuu an yaquuluu <u>aa</u>mann<u>aa</u> wahum l<u>aa</u> yuftanuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3343, '29:3', 29, 3, ' walaqad fatann<u>aa</u> <b>al</b>la<u>dz</u>iina min qablihim falaya\'lamanna <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>iina <u>sh</u>adaquu walaya\'lamanna <b>a</b>lk<u>aadz</u>ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3344, '29:4', 29, 4, ' am <u>h</u>asiba <b>al</b>la<u>dz</u>iina ya\'maluuna <b>al</b>ssayyi-<u>aa</u>ti an yasbiquun<u>aa</u> s<u>aa</u>-a m<u>aa</u> ya<u>h</u>kumuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3345, '29:5', 29, 5, ' man k<u>aa</u>na yarjuu liq<u>aa</u>-a <b>al</b>l<u>aa</u>hi fa-inna ajala <b>al</b>l<u>aa</u>hi la<u>aa</u>tin wahuwa <b>al</b>ssamii\'u <b>a</b>l\'aliim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3346, '29:6', 29, 6, ' waman j<u>aa</u>hada fa-innam<u>aa</u> yuj<u>aa</u>hidu linafsihi inna <b>al</b>l<u>aa</u>ha laghaniyyun \'ani <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3347, '29:7', 29, 7, ' wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti lanukaffiranna \'anhum sayyi-<u>aa</u>tihim walanajziyannahum a<u>h</u>sana <b>al</b>la<u>dz</u>ii k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3348, '29:8', 29, 8, ' wawa<u>shsh</u>ayn<u>aa</u> <b>a</b>l-ins<u>aa</u>na biw<u>aa</u>lidayhi <u>h</u>usnan wa-in j<u>aa</u>had<u>aa</u>ka litusyrika bii m<u>aa</u> laysa laka bihi \'ilmun fal<u>aa</u> tu<u>th</u>i\'hum<u>aa</u> ilayya marji\'ukum fa-unabbi-ukum bim<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3349, '29:9', 29, 9, ' wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti lanudkhilannahum fii <b>al</b><u>shshaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3350, '29:10', 29, 10, ' wamina <b>al</b>nn<u>aa</u>si man yaquulu <u>aa</u>mann<u>aa</u> bi<b>al</b>l<u>aa</u>hi fa-i<u>dzaa</u> uu<u>dz</u>iya fii <b>al</b>l<u>aa</u>hi ja\'ala fitnata <b>al</b>nn<u>aa</u>si ka\'a<u>dzaa</u>bi <b>al</b>l<u>aa</u>hi wala-in j<u>aa</u>-a na<u>sh</u>run min rabbika layaquulunna inn<u>aa</u> kunn<u>aa</u> ma\'akum awa laysa <b>al</b>l<u>aa</u>hu bi-a\'lama bim<u>aa</u> fii <u>sh</u>uduuri <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3351, '29:11', 29, 11, ' walaya\'lamanna <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>iina <u>aa</u>manuu walaya\'lamanna <b>a</b>lmun<u>aa</u>fiqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3352, '29:12', 29, 12, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu lilla<u>dz</u>iina <u>aa</u>manuu ittabi\'uu sabiilan<u>aa</u> walna<u>h</u>mil kha<u>th</u><u>aa</u>y<u>aa</u>kum wam<u>aa</u> hum bi<u>haa</u>miliina min kha<u>th</u><u>aa</u>y<u>aa</u>hum min syay-in innahum lak<u>aadz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3353, '29:13', 29, 13, ' walaya<u>h</u>milunna atsq<u>aa</u>lahum wa-atsq<u>aa</u>lan ma\'a atsq<u>aa</u>lihim walayus-alunna yawma <b>a</b>lqiy<u>aa</u>mati \'amm<u>aa</u> k<u>aa</u>nuu yaftaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3354, '29:14', 29, 14, ' walaqad arsaln<u>aa</u> nuu<u>h</u>an il<u>aa</u> qawmihi falabitsa fiihim <b>a</b>lfa sanatin ill<u>aa</u> khamsiina \'<u>aa</u>man fa-akha<u>dz</u>ahumu <b>al</b><u>ththh</u>uuf<u>aa</u>nu wahum <u>zhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3355, '29:15', 29, 15, ' fa-anjayn<u>aa</u>hu wa-a<u>sh</u>-<u>haa</u>ba <b>al</b>ssafiinati waja\'aln<u>aa</u>h<u>aa</u> <u>aa</u>yatan lil\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3356, '29:16', 29, 16, ' wa-ibr<u>aa</u>hiima i<u>dz</u> q<u>aa</u>la liqawmihi u\'buduu <b>al</b>l<u>aa</u>ha wa<b>i</b>ttaquuhu <u>dzaa</u>likum khayrun lakum in kuntum ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3357, '29:17', 29, 17, ' innam<u>aa</u> ta\'buduuna min duuni <b>al</b>l<u>aa</u>hi awts<u>aa</u>nan watakhluquuna ifkan inna <b>al</b>la<u>dz</u>iina ta\'buduuna min duuni <b>al</b>l<u>aa</u>hi l<u>aa</u> yamlikuuna lakum rizqan fa<b>i</b>btaghuu \'inda <b>al</b>l<u>aa</u>hi <b>al</b>rrizqa wa<b>u</b>\'buduuhu wa<b>u</b>sykuruu lahu ilayhi turja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3358, '29:18', 29, 18, ' wa-in tuka<u>dzdz</u>ibuu faqad ka<u>dzdz</u>aba umamun min qablikum wam<u>aa</u> \'al<u>aa</u> <b>al</b>rrasuuli ill<u>aa</u> <b>a</b>lbal<u>aa</u>ghu <b>a</b>lmubiin<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3359, '29:19', 29, 19, ' awa lam yaraw kayfa yubdi-u <b>al</b>l<u>aa</u>hu <b>a</b>lkhalqa tsumma yu\'iiduhu inna <u>dzaa</u>lika \'al<u>aa</u> <b>al</b>l<u>aa</u>hi yasiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3360, '29:20', 29, 20, ' qul siiruu fii <b>a</b>l-ar<u>dh</u>i fa<b>u</b>n<u>zh</u>uruu kayfa bada-a <b>a</b>lkhalqa tsumma <b>al</b>l<u>aa</u>hu yunsyi-u <b>al</b>nnasy-ata <b>a</b>l-<u>aa</u>khirata inna <b>al</b>l<u>aa</u>ha \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3361, '29:21', 29, 21, ' yu\'a<u>dzdz</u>ibu man yasy<u>aa</u>u wayar<u>h</u>amu man yasy<u>aa</u>u wa-ilayhi tuqlabuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3362, '29:22', 29, 22, ' wam<u>aa</u> antum bimu\'jiziina fii <b>a</b>l-ar<u>dh</u>i wal<u>aa</u> fii <b>al</b>ssam<u>aa</u>-i wam<u>aa</u> lakum min duuni <b>al</b>l<u>aa</u>hi min waliyyin wal<u>aa</u> na<u>sh</u>iir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3363, '29:23', 29, 23, ' wa<b>a</b>lla<u>dz</u>iina kafaruu bi-<u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi waliq<u>aa</u>-ihi ul<u>aa</u>-ika ya-isuu min ra<u>h</u>matii waul<u>aa</u>-ika lahum \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3364, '29:24', 29, 24, ' fam<u>aa</u> k<u>aa</u>na jaw<u>aa</u>ba qawmihi ill<u>aa</u> an q<u>aa</u>luu uqtuluuhu aw <u>h</u>arriquuhu fa-anj<u>aa</u>hu <b>al</b>l<u>aa</u>hu mina <b>al</b>nn<u>aa</u>ri inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin liqawmin yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3365, '29:25', 29, 25, ' waq<u>aa</u>la innam<u>aa</u> ittakha<u>dz</u>tum min duuni <b>al</b>l<u>aa</u>hi awts<u>aa</u>nan mawaddata baynikum fii <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> tsumma yawma <b>a</b>lqiy<u>aa</u>mati yakfuru ba\'<u>dh</u>ukum biba\'<u>dh</u>in wayal\'anu ba\'<u>dh</u>ukum ba\'<u>dh</u>an wama-w<u>aa</u>kumu <b>al</b>nn<u>aa</u>ru wam<u>aa</u> lakum min n<u>aas</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3366, '29:26', 29, 26, ' fa<u>aa</u>mana lahu luu<u>th</u>un waq<u>aa</u>la innii muh<u>aa</u>jirun il<u>aa</u> rabbii innahu huwa <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3367, '29:27', 29, 27, ' wawahabn<u>aa</u> lahu is<u>haa</u>qa waya\'quuba waja\'aln<u>aa</u> fii <u>dz</u>urriyyatihi <b>al</b>nnubuwwata wa<b>a</b>lkit<u>aa</u>ba wa<u>aa</u>tayn<u>aa</u>hu ajrahu fii <b>al</b>dduny<u>aa</u> wa-innahu fii <b>a</b>l-<u>aa</u>khirati lamina <b>al</b><u>shshaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3368, '29:28', 29, 28, ' waluu<u>th</u>an i<u>dz</u> q<u>aa</u>la liqawmihi innakum lata/tuuna <b>a</b>lf<u>aah</u>isyata m<u>aa</u> sabaqakum bih<u>aa</u> min a<u>h</u>adin mina <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3369, '29:29', 29, 29, ' a-innakum lata/tuuna <b>al</b>rrij<u>aa</u>la wataq<u>th</u>a\'uuna <b>al</b>ssabiila wata/tuuna fii n<u>aa</u>diikumu <b>a</b>lmunkara fam<u>aa</u> k<u>aa</u>na jaw<u>aa</u>ba qawmihi ill<u>aa</u> an q<u>aa</u>luu i/tin<u>aa</u> bi\'a<u>dzaa</u>bi <b>al</b>l<u>aa</u>hi in kunta mina <b>al</b><u>shshaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3370, '29:30', 29, 30, ' q<u>aa</u>la rabbi un<u>sh</u>urnii \'al<u>aa</u> <b>a</b>lqawmi <b>a</b>lmufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3371, '29:31', 29, 31, ' walamm<u>aa</u> j<u>aa</u>-at rusulun<u>aa</u> ibr<u>aa</u>hiima bi<b>a</b>lbusyr<u>aa</u> q<u>aa</u>luu inn<u>aa</u> muhlikuu ahli h<u>aadz</u>ihi <b>a</b>lqaryati inna ahlah<u>aa</u> k<u>aa</u>nuu <u>zhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3372, '29:32', 29, 32, ' q<u>aa</u>la inna fiih<u>aa</u> luu<u>th</u>an q<u>aa</u>luu na<u>h</u>nu a\'lamu biman fiih<u>aa</u> lanunajjiyannahu wa-ahlahu ill<u>aa</u> imra-atahu k<u>aa</u>nat mina <b>a</b>lgh<u>aa</u>biriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3373, '29:33', 29, 33, ' walamm<u>aa</u> an j<u>aa</u>-at rusulun<u>aa</u> luu<u>th</u>an sii-a bihim wa<u>daa</u>qa bihim <u>dz</u>ar\'an waq<u>aa</u>luu l<u>aa</u> takhaf wal<u>aa</u> ta<u>h</u>zan inn<u>aa</u> munajjuuka wa-ahlaka ill<u>aa</u> imra-ataka k<u>aa</u>nat mina <b>a</b>lgh<u>aa</u>biriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3374, '29:34', 29, 34, ' inn<u>aa</u> munziluuna \'al<u>aa</u> ahli h<u>aadz</u>ihi <b>a</b>lqaryati rijzan mina <b>al</b>ssam<u>aa</u>-i bim<u>aa</u> k<u>aa</u>nuu yafsuquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3375, '29:35', 29, 35, ' walaqad tarakn<u>aa</u> minh<u>aa</u> <u>aa</u>yatan bayyinatan liqawmin ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3376, '29:36', 29, 36, ' wa-il<u>aa</u> madyana akh<u>aa</u>hum syu\'ayban faq<u>aa</u>la y<u>aa</u> qawmi u\'buduu <b>al</b>l<u>aa</u>ha wa<b>u</b>rjuu <b>a</b>lyawma <b>a</b>l-<u>aa</u>khira wal<u>aa</u> ta\'tsaw fii <b>a</b>l-ar<u>dh</u>i mufsidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3377, '29:37', 29, 37, ' faka<u>dzdz</u>abuuhu fa-akha<u>dz</u>at-humu <b>al</b>rrajfatu fa-a<u>sh</u>ba<u>h</u>uu fii d<u>aa</u>rihim j<u>aa</u>tsimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3378, '29:38', 29, 38, ' wa\'<u>aa</u>dan watsamuuda waqad tabayyana lakum min mas<u>aa</u>kinihim wazayyana lahumu <b>al</b>sysyay<u>th</u><u>aa</u>nu a\'m<u>aa</u>lahum fa<u>sh</u>addahum \'ani <b>al</b>ssabiili wak<u>aa</u>nuu mustab<u>sh</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3379, '29:39', 29, 39, ' waq<u>aa</u>ruuna wafir\'awna wah<u>aa</u>m<u>aa</u>na walaqad j<u>aa</u>-ahum muus<u>aa</u> bi<b>a</b>lbayyin<u>aa</u>ti fa<b>i</b>stakbaruu fii <b>a</b>l-ar<u>dh</u>i wam<u>aa</u> k<u>aa</u>nuu s<u>aa</u>biqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3380, '29:40', 29, 40, ' fakullan akha<u>dz</u>n<u>aa</u> bi<u>dz</u>anbihi faminhum man arsaln<u>aa</u> \'alayhi <u>has</u>iban waminhum man akha<u>dz</u>at-hu <b>al</b><u>shsh</u>ay<u>h</u>atu waminhum man khasafn<u>aa</u> bihi <b>a</b>l-ar<u>dh</u>a waminhum man aghraqn<u>aa</u> wam<u>aa</u> k<u>aa</u>na <b>al</b>l<u>aa</u>hu liya<u>zh</u>limahum wal<u>aa</u>kin k<u>aa</u>nuu anfusahum ya<u>zh</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3381, '29:41', 29, 41, ' matsalu <b>al</b>la<u>dz</u>iina ittakha<u>dz</u>uu min duuni <b>al</b>l<u>aa</u>hi awliy<u>aa</u>-a kamatsali <b>a</b>l\'ankabuuti ittakha<u>dz</u>at baytan wa-inna awhana <b>a</b>lbuyuuti labaytu <b>a</b>l\'ankabuuti law k<u>aa</u>nuu ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3382, '29:42', 29, 42, ' inna <b>al</b>l<u>aa</u>ha ya\'lamu m<u>aa</u> yad\'uuna min duunihi min syay-in wahuwa <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3383, '29:43', 29, 43, ' watilka <b>a</b>l-amts<u>aa</u>lu na<u>dh</u>ribuh<u>aa</u> li<b>l</b>nn<u>aa</u>si wam<u>aa</u> ya\'qiluh<u>aa</u> ill<u>aa</u> <b>a</b>l\'<u>aa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3384, '29:44', 29, 44, ' khalaqa <b>al</b>l<u>aa</u>hu <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a bi<b>a</b>l<u>h</u>aqqi inna fii <u>dzaa</u>lika la<u>aa</u>yatan lilmu-miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3385, '29:45', 29, 45, ' utlu m<u>aa</u> uu<u>h</u>iya ilayka mina <b>a</b>lkit<u>aa</u>bi wa-aqimi <b>al</b><u>shsh</u>al<u>aa</u>ta inna <b>al</b><u>shsh</u>al<u>aa</u>ta tanh<u>aa</u> \'ani <b>a</b>lfa<u>h</u>sy<u>aa</u>-i wa<b>a</b>lmunkari wala<u>dz</u>ikru <b>al</b>l<u>aa</u>hi akbaru wa<b>al</b>l<u>aa</u>hu ya\'lamu m<u>aa</u> ta<u>sh</u>na\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3386, '29:46', 29, 46, ' wal<u>aa</u> tuj<u>aa</u>diluu ahla <b>a</b>lkit<u>aa</u>bi ill<u>aa</u> bi<b>a</b>llatii hiya a<u>h</u>sanu ill<u>aa</u> <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu minhum waquuluu <u>aa</u>mann<u>aa</u> bi<b>a</b>lla<u>dz</u>ii unzila ilayn<u>aa</u> waunzila ilaykum wa-il<u>aa</u>hun<u>aa</u> wa-il<u>aa</u>hukum w<u>aah</u>idun wana<u>h</u>nu lahu muslimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3387, '29:47', 29, 47, ' waka<u>dzaa</u>lika anzaln<u>aa</u> ilayka <b>a</b>lkit<u>aa</u>ba fa<b>a</b>lla<u>dz</u>iina <u>aa</u>tayn<u>aa</u>humu <b>a</b>lkit<u>aa</u>ba yu/minuuna bihi wamin h<u>aa</u>ul<u>aa</u>-i man yu/minu bihi wam<u>aa</u> yaj<u>h</u>adu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> ill<u>aa</u> <b>a</b>lk<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3388, '29:48', 29, 48, ' wam<u>aa</u> kunta tatluu min qablihi min kit<u>aa</u>bin wal<u>aa</u> takhu<u>ththh</u>uhu biyamiinika i<u>dz</u>an la<b>i</b>rt<u>aa</u>ba <b>a</b>lmub<u>th</u>iluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3389, '29:49', 29, 49, ' bal huwa <u>aa</u>y<u>aa</u>tun bayyin<u>aa</u>tun fii <u>sh</u>uduuri <b>al</b>la<u>dz</u>iina uutuu <b>a</b>l\'ilma wam<u>aa</u> yaj<u>h</u>adu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> ill<u>aa</u> <b>al</b><u>zhzhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3390, '29:50', 29, 50, ' waq<u>aa</u>luu lawl<u>aa</u> unzila \'alayhi <u>aa</u>y<u>aa</u>tun min rabbihi qul innam<u>aa</u> <b>a</b>l-<u>aa</u>y<u>aa</u>tu \'inda <b>al</b>l<u>aa</u>hi wa-innam<u>aa</u> an<u>aa</u> na<u>dz</u>iirun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3391, '29:51', 29, 51, ' awa lam yakfihim ann<u>aa</u> anzaln<u>aa</u> \'alayka <b>a</b>lkit<u>aa</u>ba yutl<u>aa</u> \'alayhim inna fii <u>dzaa</u>lika lara<u>h</u>matan wa<u>dz</u>ikr<u>aa</u> liqawmin yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3392, '29:52', 29, 52, ' qul kaf<u>aa</u> bi<b>al</b>l<u>aa</u>hi baynii wabaynakum syahiidan ya\'lamu m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu bi<b>a</b>lb<u>aath</u>ili wakafaruu bi<b>al</b>l<u>aa</u>hi ul<u>aa</u>-ika humu <b>a</b>lkh<u>aa</u>siruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3393, '29:53', 29, 53, ' wayasta\'jiluunaka bi<b>a</b>l\'a<u>dzaa</u>bi walawl<u>aa</u> ajalun musamman laj<u>aa</u>-ahumu <b>a</b>l\'a<u>dzaa</u>bu walaya/tiyannahum baghtatan wahum l<u>aa</u> yasy\'uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3394, '29:54', 29, 54, ' yasta\'jiluunaka bi<b>a</b>l\'a<u>dzaa</u>bi wa-inna jahannama lamu<u>h</u>ii<u>th</u>atun bi<b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3395, '29:55', 29, 55, ' yawma yaghsy<u>aa</u>humu <b>a</b>l\'a<u>dzaa</u>bu min fawqihim wamin ta<u>h</u>ti arjulihim wayaquulu <u>dz</u>uuquu m<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3396, '29:56', 29, 56, ' y<u>aa</u> \'ib<u>aa</u>diya <b>al</b>la<u>dz</u>iina <u>aa</u>manuu inna ar<u>dh</u>ii w<u>aa</u>si\'atun fa-iyy<u>aa</u>ya fa<b>u</b>\'buduun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3397, '29:57', 29, 57, ' kullu nafsin <u>dzaa</u>-iqatu <b>a</b>lmawti tsumma ilayn<u>aa</u> turja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3398, '29:58', 29, 58, ' wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti lanubawwi-annahum mina <b>a</b>ljannati ghurafan tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru kh<u>aa</u>lidiina fiih<u>aa</u> ni\'ma ajru <b>a</b>l\'<u>aa</u>miliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3399, '29:59', 29, 59, ' <b>al</b>la<u>dz</u>iina <u>sh</u>abaruu wa\'al<u>aa</u> rabbihim yatawakkaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3400, '29:60', 29, 60, ' waka-ayyin min d<u>aa</u>bbatin l<u>aa</u> ta<u>h</u>milu rizqah<u>aa</u> <b>al</b>l<u>aa</u>hu yarzuquh<u>aa</u> wa-iyy<u>aa</u>kum wahuwa <b>al</b>ssamii\'u <b>a</b>l\'aliim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3401, '29:61', 29, 61, ' wala-in sa-altahum man khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a wasakhkhara <b>al</b>sysyamsa wa<b>a</b>lqamara layaquulunna <b>al</b>l<u>aa</u>hu fa-ann<u>aa</u> yu/fakuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3402, '29:62', 29, 62, ' <b>al</b>l<u>aa</u>hu yabsu<u>th</u>u <b>al</b>rrizqa liman yasy<u>aa</u>u min \'ib<u>aa</u>dihi wayaqdiru lahu inna <b>al</b>l<u>aa</u>ha bikulli syay-in \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3403, '29:63', 29, 63, ' wala-in sa-altahum man nazzala mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an fa-a<u>h</u>y<u>aa</u> bihi <b>a</b>l-ar<u>dh</u>a min ba\'di mawtih<u>aa</u> layaquulunna <b>al</b>l<u>aa</u>hu quli <b>a</b>l<u>h</u>amdu lill<u>aa</u>hi bal aktsaruhum l<u>aa</u> ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3404, '29:64', 29, 64, ' wam<u>aa</u> h<u>aadz</u>ihi <b>a</b>l<u>h</u>ay<u>aa</u>tu <b>al</b>dduny<u>aa</u> ill<u>aa</u> lahwun wala\'ibun wa-inna <b>al</b>dd<u>aa</u>ra <b>a</b>l-<u>aa</u>khirata lahiya <b>a</b>l<u>h</u>ayaw<u>aa</u>nu law k<u>aa</u>nuu ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3405, '29:65', 29, 65, ' fa-i<u>dzaa</u> rakibuu fii <b>a</b>lfulki da\'awuu <b>al</b>l<u>aa</u>ha mukhli<u>sh</u>iina lahu <b>al</b>ddiina falamm<u>aa</u> najj<u>aa</u>hum il<u>aa</u> <b>a</b>lbarri i<u>dzaa</u> hum yusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3406, '29:66', 29, 66, ' liyakfuruu bim<u>aa</u> <u>aa</u>tayn<u>aa</u>hum waliyatamatta\'uu fasawfa ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3407, '29:67', 29, 67, ' awa lam yaraw ann<u>aa</u> ja\'aln<u>aa</u> <u>h</u>araman <u>aa</u>minan wayutakha<u>ththh</u>afu <b>al</b>nn<u>aa</u>su min <u>h</u>awlihim afabi<b>a</b>lb<u>aath</u>ili yu/minuuna wabini\'mati <b>al</b>l<u>aa</u>hi yakfuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3408, '29:68', 29, 68, ' waman a<u>zh</u>lamu mimmani iftar<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ka<u>dz</u>iban aw ka<u>dzdz</u>aba bi<b>a</b>l<u>h</u>aqqi lamm<u>aa</u> j<u>aa</u>-ahu alaysa fii jahannama matswan lilk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3409, '29:69', 29, 69, ' wa<b>a</b>lla<u>dz</u>iina j<u>aa</u>haduu fiin<u>aa</u> lanahdiyannahum subulan<u>aa</u> wa-inna <b>al</b>l<u>aa</u>ha lama\'a <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3410, '30:1', 30, 1, ' alif-l<u>aa</u>m-miim</u>\r\r\n');
INSERT INTO quranLatin VALUES (3411, '30:2', 30, 2, ' ghulibati <b>al</b>rruum<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3412, '30:3', 30, 3, ' fii adn<u>aa</u> <b>a</b>l-ar<u>dh</u>i wahum min ba\'di ghalabihim sayaghlibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3413, '30:4', 30, 4, ' fii bi<u>dh</u>\'i siniina lill<u>aa</u>hi <b>a</b>l-amru min qablu wamin ba\'du wayawma-i<u>dz</u>in yafra<u>h</u>u <b>a</b>lmu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3414, '30:5', 30, 5, ' bina<u>sh</u>ri <b>al</b>l<u>aa</u>hi yan<u>sh</u>uru man yasy<u>aa</u>u wahuwa <b>a</b>l\'aziizu <b>al</b>rra<u>h</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3415, '30:6', 30, 6, ' wa\'da <b>al</b>l<u>aa</u>hi l<u>aa</u> yukhlifu <b>al</b>l<u>aa</u>hu wa\'dahu wal<u>aa</u>kinna aktsara <b>al</b>nn<u>aa</u>si l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3416, '30:7', 30, 7, ' ya\'lamuuna <u>zhaa</u>hiran mina <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> wahum \'ani <b>a</b>l-<u>aa</u>khirati hum gh<u>aa</u>filuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3417, '30:8', 30, 8, ' awa lam yatafakkaruu fii anfusihim m<u>aa</u> khalaqa <b>al</b>l<u>aa</u>hu <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a wam<u>aa</u> baynahum<u>aa</u> ill<u>aa</u> bi<b>a</b>l<u>h</u>aqqi wa-ajalin musamman wa-inna katsiiran mina <b>al</b>nn<u>aa</u>si biliq<u>aa</u>-i rabbihim lak<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3418, '30:9', 30, 9, ' awa lam yasiiruu fii <b>a</b>l-ar<u>dh</u>i fayan<u>zh</u>uruu kayfa k<u>aa</u>na \'<u>aa</u>qibatu <b>al</b>la<u>dz</u>iina min qablihim k<u>aa</u>nuu asyadda minhum quwwatan wa-ats<u>aa</u>ruu <b>a</b>l-ar<u>dh</u>a wa\'amaruuh<u>aa</u> aktsara mimm<u>aa</u> \'amaruuh<u>aa</u> waj<u>aa</u>-at-hum rusuluhum bi<b>a</b>lbayyin<u>aa</u>ti fam<u>aa</u> k<u>aa</u>na <b>al</b>l<u>aa</u>hu liya<u>zh</u>limahum wal<u>aa</u>kin k<u>aa</u>nuu anfusahum ya<u>zh</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3419, '30:10', 30, 10, ' tsumma k<u>aa</u>na \'<u>aa</u>qibata <b>al</b>la<u>dz</u>iina as<u>aa</u>uu <b>al</b>ssuu-<u>aa</u> an ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi wak<u>aa</u>nuu bih<u>aa</u> yastahzi-uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3420, '30:11', 30, 11, ' <b>al</b>l<u>aa</u>hu yabdau <b>a</b>lkhalqa tsumma yu\'iiduhu tsumma ilayhi turja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3421, '30:12', 30, 12, ' wayawma taquumu <b>al</b>ss<u>aa</u>\'atu yublisu <b>a</b>lmujrimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3422, '30:13', 30, 13, ' walam yakun lahum min syurak<u>aa</u>-ihim syufa\'<u>aa</u>u wak<u>aa</u>nuu bisyurak<u>aa</u>-ihim k<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3423, '30:14', 30, 14, ' wayawma taquumu <b>al</b>ss<u>aa</u>\'atu yawma-i<u>dz</u>in yatafarraquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3424, '30:15', 30, 15, ' fa-amm<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti fahum fii raw<u>dh</u>atin yu<u>h</u>baruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3425, '30:16', 30, 16, ' wa-amm<u>aa</u> <b>al</b>la<u>dz</u>iina kafaruu waka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> waliq<u>aa</u>-i <b>a</b>l-<u>aa</u>khirati faul<u>aa</u>-ika fii <b>a</b>l\'a<u>dzaa</u>bi mu<u>hd</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3426, '30:17', 30, 17, ' fasub<u>haa</u>na <b>al</b>l<u>aa</u>hi <u>h</u>iina tumsuuna wa<u>h</u>iina tu<u>sh</u>bi<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3427, '30:18', 30, 18, ' walahu <b>a</b>l<u>h</u>amdu fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wa\'asyiyyan wa<u>h</u>iina tu<u>zh</u>hiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3428, '30:19', 30, 19, ' yukhriju <b>a</b>l<u>h</u>ayya mina <b>a</b>lmayyiti wayukhriju <b>a</b>lmayyita mina <b>a</b>l<u>h</u>ayyi wayu<u>h</u>yii <b>a</b>l-ar<u>dh</u>a ba\'da mawtih<u>aa</u> waka<u>dzaa</u>lika tukhrajuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3429, '30:20', 30, 20, ' wamin <u>aa</u>y<u>aa</u>tihi an khalaqakum min tur<u>aa</u>bin tsumma i<u>dzaa</u> antum basyarun tantasyiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3430, '30:21', 30, 21, ' wamin <u>aa</u>y<u>aa</u>tihi an khalaqa lakum min anfusikum azw<u>aa</u>jan litaskunuu ilayh<u>aa</u> waja\'ala baynakum mawaddatan wara<u>h</u>matan inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin liqawmin yatafakkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3431, '30:22', 30, 22, ' wamin <u>aa</u>y<u>aa</u>tihi khalqu <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wa<b>i</b>khtil<u>aa</u>fu <b>a</b>lsinatikum wa-alw<u>aa</u>nikum inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin lil\'<u>aa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3432, '30:23', 30, 23, ' wamin <u>aa</u>y<u>aa</u>tihi man<u>aa</u>mukum bi<b>a</b>llayli wa<b>al</b>nnah<u>aa</u>ri wa<b>i</b>btigh<u>aa</u>ukum min fa<u>dh</u>lihi inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin liqawmin yasma\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3433, '30:24', 30, 24, ' wamin <u>aa</u>y<u>aa</u>tihi yuriikumu <b>a</b>lbarqa khawfan wa<u>th</u>ama\'an wayunazzilu mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an fayu<u>h</u>yii bihi <b>a</b>l-ar<u>dh</u>a ba\'da mawtih<u>aa</u> inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin liqawmin ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3434, '30:25', 30, 25, ' wamin <u>aa</u>y<u>aa</u>tihi an taquuma <b>al</b>ssam<u>aa</u>u wa<b>a</b>l-ar<u>dh</u>u bi-amrihi tsumma i<u>dzaa</u> da\'<u>aa</u>kum da\'watan mina <b>a</b>l-ar<u>dh</u>i i<u>dzaa</u> antum takhrujuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3435, '30:26', 30, 26, ' walahu man fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i kullun lahu q<u>aa</u>nituun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3436, '30:27', 30, 27, ' wahuwa <b>al</b>la<u>dz</u>ii yabdau <b>a</b>lkhalqa tsumma yu\'iiduhu wahuwa ahwanu \'alayhi walahu <b>a</b>lmatsalu <b>a</b>l-a\'l<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wahuwa <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3437, '30:28', 30, 28, ' <u>dh</u>araba lakum matsalan min anfusikum hal lakum mimm<u>aa</u> malakat aym<u>aa</u>nukum min syurak<u>aa</u>-a fii m<u>aa</u> razaqn<u>aa</u>kum fa-antum fiihi saw<u>aa</u>un takh<u>aa</u>fuunahum kakhiifatikum anfusakum ka<u>dzaa</u>lika nufa<u>shsh</u>ilu <b>a</b>l-<u>aa</u>y<u>aa</u>ti liqawmin ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3438, '30:29', 30, 29, ' bali ittaba\'a <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu ahw<u>aa</u>-ahum bighayri \'ilmin faman yahdii man a<u>dh</u>alla <b>al</b>l<u>aa</u>hu wam<u>aa</u> lahum min n<u>aas</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3439, '30:30', 30, 30, ' fa-aqim wajhaka li<b>l</b>ddiini <u>h</u>aniifan fi<u>th</u>rata <b>al</b>l<u>aa</u>hi <b>al</b>latii fa<u>th</u>ara <b>al</b>nn<u>aa</u>sa \'alayh<u>aa</u> l<u>aa</u> tabdiila likhalqi <b>al</b>l<u>aa</u>hi <u>dzaa</u>lika <b>al</b>ddiinu <b>a</b>lqayyimu wal<u>aa</u>kinna aktsara <b>al</b>nn<u>aa</u>si l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3440, '30:31', 30, 31, ' muniibiina ilayhi wa<b>i</b>ttaquuhu wa-aqiimuu <b>al</b><u>shsh</u>al<u>aa</u>ta wal<u>aa</u> takuunuu mina <b>a</b>lmusyrikiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3441, '30:32', 30, 32, ' mina <b>al</b>la<u>dz</u>iina farraquu diinahum wak<u>aa</u>nuu syiya\'an kullu <u>h</u>izbin bim<u>aa</u> ladayhim fari<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3442, '30:33', 30, 33, ' wa-i<u>dzaa</u> massa <b>al</b>nn<u>aa</u>sa <u>dh</u>urrun da\'aw rabbahum muniibiina ilayhi tsumma i<u>dzaa</u> a<u>dzaa</u>qahum minhu ra<u>h</u>matan i<u>dzaa</u> fariiqun minhum birabbihim yusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3443, '30:34', 30, 34, ' liyakfuruu bim<u>aa</u> <u>aa</u>tayn<u>aa</u>hum fatamatta\'uu fasawfa ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3444, '30:35', 30, 35, ' am anzaln<u>aa</u> \'alayhim sul<u>th</u><u>aa</u>nan fahuwa yatakallamu bim<u>aa</u> k<u>aa</u>nuu bihi yusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3445, '30:36', 30, 36, ' wa<b>i</b><u>dzaa</u> a<u>dz</u>aqn<u>aa</u> <b>al</b>nn<u>aa</u>sa ra<u>h</u>matan fari<u>h</u>uu bih<u>aa</u> wa-in tu<u>sh</u>ibhum sayyi-atun bim<u>aa</u> qaddamat aydiihim i<u>dzaa</u> hum yaqna<u>th</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3446, '30:37', 30, 37, ' awa lam yaraw anna <b>al</b>l<u>aa</u>ha yabsu<u>th</u>u <b>al</b>rrizqa liman yasy<u>aa</u>u wayaqdiru inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin liqawmin yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3447, '30:38', 30, 38, ' fa<u>aa</u>ti <u>dzaa</u> <b>a</b>lqurb<u>aa</u> <u>h</u>aqqahu wa<b>a</b>lmiskiina wa<b>i</b>bna <b>al</b>ssabiili <u>dzaa</u>lika khayrun lilla<u>dz</u>iina yuriiduuna wajha <b>al</b>l<u>aa</u>hi waul<u>aa</u>-ika humu <b>a</b>lmufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3448, '30:39', 30, 39, ' wam<u>aa</u> <u>aa</u>taytum min riban liyarbuwa fii amw<u>aa</u>li <b>al</b>nn<u>aa</u>si fal<u>aa</u> yarbuu \'inda <b>al</b>l<u>aa</u>hi wam<u>aa</u> <u>aa</u>taytum min zak<u>aa</u>tin turiiduuna wajha <b>al</b>l<u>aa</u>hi faul<u>aa</u>-ika humu <b>a</b>lmu<u>dh</u>\'ifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3449, '30:40', 30, 40, ' <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii khalaqakum tsumma razaqakum tsumma yumiitukum tsumma yu<u>h</u>yiikum hal min syurak<u>aa</u>-ikum man yaf\'alu min <u>dzaa</u>likum min syay-in sub<u>haa</u>nahu wata\'<u>aa</u>l<u>aa</u> \'amm<u>aa</u> yusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3450, '30:41', 30, 41, ' <u>zh</u>ahara <b>a</b>lfas<u>aa</u>du fii <b>a</b>lbarri wa<b>a</b>lba<u>h</u>ri bim<u>aa</u> kasabat aydii <b>al</b>nn<u>aa</u>si liyu<u>dz</u>iiqahum ba\'<u>dh</u>a <b>al</b>la<u>dz</u>ii \'amiluu la\'allahum yarji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3451, '30:42', 30, 42, ' qul siiruu fii <b>a</b>l-ar<u>dh</u>i fa<b>u</b>n<u>zh</u>uruu kayfa k<u>aa</u>na \'<u>aa</u>qibatu <b>al</b>la<u>dz</u>iina min qablu k<u>aa</u>na aktsaruhum musyrikiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3452, '30:43', 30, 43, ' fa-aqim wajhaka li<b>l</b>ddiini <b>a</b>lqayyimi min qabli an ya/tiya yawmun l<u>aa</u> maradda lahu mina <b>al</b>l<u>aa</u>hi yawma-i<u>dz</u>in ya<u>shsh</u>adda\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3453, '30:44', 30, 44, ' man kafara fa\'alayhi kufruhu waman \'amila <u>shaa</u>li<u>h</u>an fali-anfusihim yamhaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3454, '30:45', 30, 45, ' liyajziya <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti min fa<u>dh</u>lihi innahu l<u>aa</u> yu<u>h</u>ibbu <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3455, '30:46', 30, 46, ' wamin <u>aa</u>y<u>aa</u>tihi an yursila <b>al</b>rriy<u>aah</u>a mubasysyir<u>aa</u>tin waliyu<u>dz</u>iiqakum min ra<u>h</u>matihi walitajriya <b>a</b>lfulku bi-amrihi walitabtaghuu min fa<u>dh</u>lihi wala\'allakum tasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3456, '30:47', 30, 47, ' walaqad arsaln<u>aa</u> min qablika rusulan il<u>aa</u> qawmihim faj<u>aa</u>uuhum bi<b>a</b>lbayyin<u>aa</u>ti fa<b>i</b>ntaqamn<u>aa</u> mina <b>al</b>la<u>dz</u>iina ajramuu wak<u>aa</u>na <u>h</u>aqqan \'alayn<u>aa</u> na<u>sh</u>ru <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3457, '30:48', 30, 48, ' <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii yursilu <b>al</b>rriy<u>aah</u>a fatutsiiru sa<u>haa</u>ban fayabsu<u>th</u>uhu fii <b>al</b>ssam<u>aa</u>-i kayfa yasy<u>aa</u>u wayaj\'aluhu kisafan fatar<u>aa</u> <b>a</b>lwadqa yakhruju min khil<u>aa</u>lihi fa-i<u>dzaa</u> a<u>shaa</u>ba bihi man yasy<u>aa</u>u min \'ib<u>aa</u>dihi i<u>dzaa</u> hum yastabsyiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3458, '30:49', 30, 49, ' wa-in k<u>aa</u>nuu min qabli an yunazzala \'alayhim min qablihi lamublisiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3459, '30:50', 30, 50, ' fa<b>u</b>n<u>zh</u>ur il<u>aa</u> <u>aa</u>ts<u>aa</u>ri ra<u>h</u>mati <b>al</b>l<u>aa</u>hi kayfa yu<u>h</u>yii <b>a</b>l-ar<u>dh</u>a ba\'da mawtih<u>aa</u> inna <u>dzaa</u>lika lamu<u>h</u>yii <b>a</b>lmawt<u>aa</u> wahuwa \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3460, '30:51', 30, 51, ' wala-in arsaln<u>aa</u> rii<u>h</u>an fara-awhu mu<u>sh</u>farran la<u>zh</u>alluu min ba\'dihi yakfuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3461, '30:52', 30, 52, ' fa-innaka l<u>aa</u> tusmi\'u <b>a</b>lmawt<u>aa</u> wal<u>aa</u> tusmi\'u <b>al</b><u>shsh</u>umma <b>al</b>ddu\'<u>aa</u>-a i<u>dzaa</u> wallaw mudbiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3462, '30:53', 30, 53, ' wam<u>aa</u> anta bih<u>aa</u>di <b>a</b>l\'umyi \'an <u>dh</u>al<u>aa</u>latihim in tusmi\'u ill<u>aa</u> man yu/minu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> fahum muslimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3463, '30:54', 30, 54, ' <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii khalaqakum min <u>dh</u>a\'fin tsumma ja\'ala min ba\'di <u>dh</u>a\'fin quwwatan tsumma ja\'ala min ba\'di quwwatin <u>dh</u>a\'fan wasyaybatan yakhluqu m<u>aa</u> yasy<u>aa</u>u wahuwa <b>a</b>l\'aliimu <b>a</b>lqadiir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3464, '30:55', 30, 55, ' wayawma taquumu <b>al</b>ss<u>aa</u>\'atu yuqsimu <b>a</b>lmujrimuuna m<u>aa</u> labitsuu ghayra s<u>aa</u>\'atin ka<u>dzaa</u>lika k<u>aa</u>nuu yu/fakuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3465, '30:56', 30, 56, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>iina uutuu <b>a</b>l\'ilma wa<b>a</b>l-iim<u>aa</u>na laqad labitstum fii kit<u>aa</u>bi <b>al</b>l<u>aa</u>hi il<u>aa</u> yawmi <b>a</b>lba\'tsi fah<u>aadzaa</u> yawmu <b>a</b>lba\'tsi wal<u>aa</u>kinnakum kuntum l<u>aa</u> ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3466, '30:57', 30, 57, ' fayawma-i<u>dz</u>in l<u>aa</u> yanfa\'u <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu ma\'<u>dz</u>iratuhum wal<u>aa</u> hum yusta\'tabuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3467, '30:58', 30, 58, ' walaqad <u>dh</u>arabn<u>aa</u> li<b>l</b>nn<u>aa</u>si fii h<u>aadzaa</u> <b>a</b>lqur-<u>aa</u>ni min kulli matsalin wala-in ji/tahum bi-<u>aa</u>yatin layaquulanna <b>al</b>la<u>dz</u>iina kafaruu in antum ill<u>aa</u> mub<u>th</u>iluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3468, '30:59', 30, 59, ' ka<u>dzaa</u>lika ya<u>th</u>ba\'u <b>al</b>l<u>aa</u>hu \'al<u>aa</u> quluubi <b>al</b>la<u>dz</u>iina l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3469, '30:60', 30, 60, ' fa<b>i</b><u>sh</u>bir inna wa\'da <b>al</b>l<u>aa</u>hi <u>h</u>aqqun wal<u>aa</u> yastakhiffannaka <b>al</b>la<u>dz</u>iina l<u>aa</u> yuuqinuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3470, '31:1', 31, 1, ' alif-l<u>aa</u>m-miim</u>\r\r\n');
INSERT INTO quranLatin VALUES (3471, '31:2', 31, 2, ' tilka <u>aa</u>y<u>aa</u>tu <b>a</b>lkit<u>aa</u>bi <b>a</b>l<u>h</u>akiim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3472, '31:3', 31, 3, ' hudan wara<u>h</u>matan lilmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3473, '31:4', 31, 4, ' <b>al</b>la<u>dz</u>iina yuqiimuuna <b>al</b><u>shsh</u>al<u>aa</u>ta wayu/tuuna <b>al</b>zzak<u>aa</u>ta wahum bi<b>a</b>l-<u>aa</u>khirati hum yuuqinuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3474, '31:5', 31, 5, ' ul<u>aa</u>-ika \'al<u>aa</u> hudan min rabbihim waul<u>aa</u>-ika humu <b>a</b>lmufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3475, '31:6', 31, 6, ' wamina <b>al</b>nn<u>aa</u>si man yasytarii lahwa <b>a</b>l<u>h</u>adiitsi liyu<u>dh</u>illa \'an sabiili <b>al</b>l<u>aa</u>hi bighayri \'ilmin wayattakhi<u>dz</u>ah<u>aa</u> huzuwan ul<u>aa</u>-ika lahum \'a<u>dzaa</u>bun muhiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3476, '31:7', 31, 7, ' wa-i<u>dzaa</u> tutl<u>aa</u> \'alayhi <u>aa</u>y<u>aa</u>tun<u>aa</u> wall<u>aa</u> mustakbiran ka-an lam yasma\'h<u>aa</u> ka-anna fii u<u>dz</u>unayhi waqran fabasysyirhu bi\'a<u>dzaa</u>bin <b>a</b>liim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3477, '31:8', 31, 8, ' inna <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti lahum jann<u>aa</u>tu <b>al</b>nna\'iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3478, '31:9', 31, 9, ' kh<u>aa</u>lidiina fiih<u>aa</u> wa\'da <b>al</b>l<u>aa</u>hi <u>h</u>aqqan wahuwa <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3479, '31:10', 31, 10, ' khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti bighayri \'amadin tarawnah<u>aa</u> wa-alq<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i raw<u>aa</u>siya an tamiida bikum wabatstsa fiih<u>aa</u> min kulli d<u>aa</u>bbatin wa-anzaln<u>aa</u> mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an fa-anbatn<u>aa</u> fiih<u>aa</u> min kulli zawjin kariim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3480, '31:11', 31, 11, ' h<u>aadzaa</u> khalqu <b>al</b>l<u>aa</u>hi fa-aruunii m<u>aatsaa</u> khalaqa <b>al</b>la<u>dz</u>iina min duunihi bali <b>al</b><u>zhzhaa</u>limuuna fii <u>dh</u>al<u>aa</u>lin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3481, '31:12', 31, 12, ' walaqad <u>aa</u>tayn<u>aa</u> luqm<u>aa</u>na <b>a</b>l<u>h</u>ikmata ani usykur lill<u>aa</u>hi waman yasykur fa-innam<u>aa</u> yasykuru linafsihi waman kafara fa-inna <b>al</b>l<u>aa</u>ha ghaniyyun <u>h</u>amiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3482, '31:13', 31, 13, ' wa-i<u>dz</u> q<u>aa</u>la luqm<u>aa</u>nu li<b>i</b>bnihi wahuwa ya\'i<u>zh</u>uhu y<u>aa</u> bunayya l<u>aa</u> tusyrik bi<b>al</b>l<u>aa</u>hi inna <b>al</b>sysyirka la<u>zh</u>ulmun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3483, '31:14', 31, 14, ' wawa<u>shsh</u>ayn<u>aa</u> <b>a</b>l-ins<u>aa</u>na biw<u>aa</u>lidayhi <u>h</u>amalat-hu ummuhu wahnan \'al<u>aa</u> wahnin wafi<u>shaa</u>luhu fii \'<u>aa</u>mayni ani usykur lii waliw<u>aa</u>lidayka ilayya <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3484, '31:15', 31, 15, ' wa-in j<u>aa</u>had<u>aa</u>ka \'al<u>aa</u> an tusyrika bii m<u>aa</u> laysa laka bihi \'ilmun fal<u>aa</u> tu<u>th</u>i\'hum<u>aa</u> wa<u>shaah</u>ibhum<u>aa</u> fii <b>al</b>dduny<u>aa</u> ma\'ruufan wa<b>i</b>ttabi\' sabiila man an<u>aa</u>ba ilayya tsumma ilayya marji\'ukum fa-unabbi-ukum bim<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3485, '31:16', 31, 16, ' y<u>aa</u> bunayya innah<u>aa</u> in taku mitsq<u>aa</u>la <u>h</u>abbatin min khardalin fatakun fii <u>sh</u>akhratin aw fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti aw fii <b>a</b>l-ar<u>dh</u>i ya/ti bih<u>aa</u> <b>al</b>l<u>aa</u>hu inna <b>al</b>l<u>aa</u>ha la<u>th</u>iifun khabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3486, '31:17', 31, 17, ' y<u>aa</u> bunayya aqimi <b>al</b><u>shsh</u>al<u>aa</u>ta wa/mur bi<b>a</b>lma\'ruufi wa<b>i</b>nha \'ani <b>a</b>lmunkari wa<b>i</b><u>sh</u>bir \'al<u>aa</u> m<u>aa</u> a<u>shaa</u>baka inna <u>dzaa</u>lika min \'azmi <b>a</b>l-umuur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3487, '31:18', 31, 18, ' wal<u>aa</u> tu<u>sh</u>a\'\'ir khaddaka li<b>l</b>nn<u>aa</u>si wal<u>aa</u> tamsyi fii <b>a</b>l-ar<u>dh</u>i mara<u>h</u>an inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yu<u>h</u>ibbu kulla mukht<u>aa</u>lin fakhuur<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3488, '31:19', 31, 19, ' wa<b>i</b>q<u>sh</u>id fii masyyika wa<b>u</b>gh<u>dh</u>u<u>dh</u> min <u>sh</u>awtika inna ankara <b>a</b>l-a<u>sh</u>w<u>aa</u>ti la<u>sh</u>awtu <b>a</b>l<u>h</u>amiir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3489, '31:20', 31, 20, ' alam taraw anna <b>al</b>l<u>aa</u>ha sakhkhara lakum m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i wa-asbagha \'alaykum ni\'amahu <u>zhaa</u>hiratan wab<u>aath</u>inatan wamina <b>al</b>nn<u>aa</u>si man yuj<u>aa</u>dilu fii <b>al</b>l<u>aa</u>hi bighayri \'ilmin wal<u>aa</u> hudan wal<u>aa</u> kit<u>aa</u>bin muniir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3490, '31:21', 31, 21, ' wa-i<u>dzaa</u> qiila lahumu ittabi\'uu m<u>aa</u> anzala <b>al</b>l<u>aa</u>hu q<u>aa</u>luu bal nattabi\'u m<u>aa</u> wajadn<u>aa</u> \'alayhi <u>aa</u>b<u>aa</u>-an<u>aa</u> awa law k<u>aa</u>na <b>al</b>sysyay<u>th</u><u>aa</u>nu yad\'uuhum il<u>aa</u> \'a<u>dzaa</u>bi <b>al</b>ssa\'iir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3491, '31:22', 31, 22, ' waman yuslim wajhahu il<u>aa</u> <b>al</b>l<u>aa</u>hi wahuwa mu<u>h</u>sinun faqadi istamsaka bi<b>a</b>l\'urwati <b>a</b>lwutsq<u>aa</u> wa-il<u>aa</u> <b>al</b>l<u>aa</u>hi \'<u>aa</u>qibatu <b>a</b>l-umuur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3492, '31:23', 31, 23, ' waman kafara fal<u>aa</u> ya<u>h</u>zunka kufruhu ilayn<u>aa</u> marji\'uhum fanunabbi-uhum bim<u>aa</u> \'amiluu inna <b>al</b>l<u>aa</u>ha \'aliimun bi<u>dzaa</u>ti <b>al</b><u>shsh</u>uduur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (3493, '31:24', 31, 24, ' numatti\'uhum qaliilan tsumma na<u>dth</u>arruhum il<u>aa</u> \'a<u>dzaa</u>bin ghalii<u>zh</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3494, '31:25', 31, 25, ' wala-in sa-altahum man khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a layaquulunna <b>al</b>l<u>aa</u>hu quli <b>a</b>l<u>h</u>amdu lill<u>aa</u>hi bal aktsaruhum l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3495, '31:26', 31, 26, ' lill<u>aa</u>hi m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i inna <b>al</b>l<u>aa</u>ha huwa <b>a</b>lghaniyyu <b>a</b>l<u>h</u>amiid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3496, '31:27', 31, 27, ' walaw annam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i min syajaratin aql<u>aa</u>mun wa<b>a</b>lba<u>h</u>ru yamudduhu min ba\'dihi sab\'atu ab<u>h</u>urin m<u>aa</u> nafidat kalim<u>aa</u>tu <b>al</b>l<u>aa</u>hi inna <b>al</b>l<u>aa</u>ha \'aziizun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3497, '31:28', 31, 28, ' m<u>aa</u> khalqukum wal<u>aa</u> ba\'tsukum ill<u>aa</u> kanafsin w<u>aah</u>idatin inna <b>al</b>l<u>aa</u>ha samii\'un ba<u>sh</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3498, '31:29', 31, 29, ' alam tara anna <b>al</b>l<u>aa</u>ha yuuliju <b>al</b>layla fii <b>al</b>nnah<u>aa</u>ri wayuuliju <b>al</b>nnah<u>aa</u>ra fii <b>al</b>layli wasakhkhara <b>al</b>sysyamsa wa<b>a</b>lqamara kullun yajrii il<u>aa</u> ajalin musamman wa-anna <b>al</b>l<u>aa</u>ha bim<u>aa</u> ta\'maluuna khabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (3499, '31:30', 31, 30, ' <u>dzaa</u>lika bi-anna <b>al</b>l<u>aa</u>ha huwa <b>a</b>l<u>h</u>aqqu wa-anna m<u>aa</u> yad\'uuna min duunihi <b>a</b>lb<u>aath</u>ilu wa-anna <b>al</b>l<u>aa</u>ha huwa <b>a</b>l\'aliyyu <b>a</b>lkabiir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3500, '31:31', 31, 31, ' alam tara anna <b>a</b>lfulka tajrii fii <b>a</b>lba<u>h</u>ri bini\'mati <b>al</b>l<u>aa</u>hi liyuriyakum min <u>aa</u>y<u>aa</u>tihi inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin likulli <u>sh</u>abb<u>aa</u>rin syakuur<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3501, '31:32', 31, 32, ' wa-i<u>dzaa</u> ghasyiyahum mawjun ka<b>al</b><u>zhzh</u>ulali da\'awuu <b>al</b>l<u>aa</u>ha mukhli<u>sh</u>iina lahu <b>al</b>ddiina falamm<u>aa</u> najj<u>aa</u>hum il<u>aa</u> <b>a</b>lbarri faminhum muqta<u>sh</u>idun wam<u>aa</u> yaj<u>h</u>adu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> ill<u>aa</u> kullu khatt<u>aa</u>rin kafuur<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3502, '31:33', 31, 33, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su ittaquu rabbakum wa<b>i</b>khsyaw yawman l<u>aa</u> yajzii w<u>aa</u>lidun \'an waladihi wal<u>aa</u> mawluudun huwa j<u>aa</u>zin \'an w<u>aa</u>lidihi syay-an inna wa\'da <b>al</b>l<u>aa</u>hi <u>h</u>aqqun fal<u>aa</u> taghurrannakumu <b>a</b>l<u>h</u>ay<u>aa</u>tu <b>al</b>dduny<u>aa</u> wal<u>aa</u> yaghurrannakum bi<b>al</b>l<u>aa</u>hi <b>a</b>lgharuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3503, '31:34', 31, 34, ' inna <b>al</b>l<u>aa</u>ha \'indahu \'ilmu <b>al</b>ss<u>aa</u>\'ati wayunazzilu <b>a</b>lghaytsa waya\'lamu m<u>aa</u> fii <b>a</b>l-ar<u>haa</u>mi wam<u>aa</u> tadrii nafsun m<u>aatsaa</u> taksibu ghadan wam<u>aa</u> tadrii nafsun bi-ayyi ar<u>dh</u>in tamuutu inna <b>al</b>l<u>aa</u>ha \'aliimun khabiir<b>un</b><dir>\r\r\n');
INSERT INTO quranLatin VALUES (3504, '32:1', 32, 1, ' alif-l<u>aa</u>m-miim</u>\r\r\n');
INSERT INTO quranLatin VALUES (3505, '32:2', 32, 2, ' tanziilu <b>a</b>lkit<u>aa</u>bi l<u>aa</u> rayba fiihi min rabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3506, '32:3', 32, 3, ' am yaquuluuna iftar<u>aa</u>hu bal huwa <b>a</b>l<u>h</u>aqqu min rabbika litun<u>dz</u>ira qawman m<u>aa</u> at<u>aa</u>hum min na<u>dz</u>iirin min qablika la\'allahum yahtaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3507, '32:4', 32, 4, ' <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a wam<u>aa</u> baynahum<u>aa</u> fii sittati ayy<u>aa</u>min tsumma istaw<u>aa</u> \'al<u>aa</u> <b>a</b>l\'arsyi m<u>aa</u> lakum min duunihi min waliyyin wal<u>aa</u> syafii\'in afal<u>aa</u> tata<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3508, '32:5', 32, 5, ' yudabbiru <b>a</b>l-amra mina <b>al</b>ssam<u>aa</u>-i il<u>aa</u> <b>a</b>l-ar<u>dh</u>i tsumma ya\'ruju ilayhi fii yawmin k<u>aa</u>na miqd<u>aa</u>ruhu <b>a</b>lfa sanatin mimm<u>aa</u> ta\'udduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3509, '32:6', 32, 6, ' <u>dzaa</u>lika \'<u>aa</u>limu <b>a</b>lghaybi wa<b>al</b>sysyah<u>aa</u>dati <b>a</b>l\'aziizu <b>al</b>rra<u>h</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (3510, '32:7', 32, 7, ' <b>al</b>la<u>dz</u>ii a<u>h</u>sana kulla syay-in khalaqahu wabada-a khalqa <b>a</b>l-ins<u>aa</u>ni min <u>th</u>iin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3511, '32:8', 32, 8, ' tsumma ja\'ala naslahu min sul<u>aa</u>latin min m<u>aa</u>-in mahiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (3512, '32:9', 32, 9, ' tsumma saww<u>aa</u>hu wanafakha fiihi min ruu<u>h</u>ihi waja\'ala lakumu <b>al</b>ssam\'a wa<b>a</b>l-ab<u>shaa</u>ra wa<b>a</b>l-af-idata qaliilan m<u>aa</u> tasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3513, '32:10', 32, 10, ' waq<u>aa</u>luu a-i<u>dzaa</u> <u>dh</u>al<u>aa</u>ln<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i a-inn<u>aa</u> lafii khalqin jadiidin bal hum biliq<u>aa</u>-i rabbihim k<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3514, '32:11', 32, 11, ' qul yatawaff<u>aa</u>kum malaku <b>a</b>lmawti <b>al</b>la<u>dz</u>ii wukkila bikum tsumma il<u>aa</u> rabbikum turja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3515, '32:12', 32, 12, ' walaw tar<u>aa</u> i<u>dz</u>i <b>a</b>lmujrimuuna n<u>aa</u>kisuu ruuusihim \'inda rabbihim rabban<u>aa</u> ab<u>sh</u>arn<u>aa</u> wasami\'n<u>aa</u> fa-arji\'n<u>aa</u> na\'mal <u>shaa</u>li<u>h</u>an inn<u>aa</u> muuqinuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3516, '32:13', 32, 13, ' walaw syi-n<u>aa</u> la<u>aa</u>tayn<u>aa</u> kulla nafsin hud<u>aa</u>h<u>aa</u> wal<u>aa</u>kin <u>h</u>aqqa <b>a</b>lqawlu minnii la-amla-anna jahannama mina <b>a</b>ljinnati wa<b>al</b>nn<u>aa</u>si ajma\'iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3517, '32:14', 32, 14, ' fa<u>dz</u>uuquu bim<u>aa</u> nasiitum liq<u>aa</u>-a yawmikum h<u>aadzaa</u> inn<u>aa</u> nasiin<u>aa</u>kum wa<u>dz</u>uuquu \'a<u>dzaa</u>ba <b>a</b>lkhuldi bim<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3518, '32:15', 32, 15, ' innam<u>aa</u> yu/minu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> <b>al</b>la<u>dz</u>iina i<u>dzaa</u> <u>dz</u>ukkiruu bih<u>aa</u> kharruu sujjadan wasabba<u>h</u>uu bi<u>h</u>amdi rabbihim wahum l<u>aa</u> yastakbiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3519, '32:16', 32, 16, ' tataj<u>aa</u>f<u>aa</u> junuubuhum \'ani <b>a</b>lma<u>daa</u>ji\'i yad\'uuna rabbahum khawfan wa<u>th</u>ama\'an wamimm<u>aa</u> razaqn<u>aa</u>hum yunfiquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3520, '32:17', 32, 17, ' fal<u>aa</u> ta\'lamu nafsun m<u>aa</u> ukhfiya lahum min qurrati a\'yunin jaz<u>aa</u>-an bim<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3521, '32:18', 32, 18, ' afaman k<u>aa</u>na mu/minan kaman k<u>aa</u>na f<u>aa</u>siqan l<u>aa</u> yastawuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3522, '32:19', 32, 19, ' amm<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti falahum jann<u>aa</u>tu <b>a</b>lma/w<u>aa</u> nuzulan bim<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3523, '32:20', 32, 20, ' wa-amm<u>aa</u> <b>al</b>la<u>dz</u>iina fasaquu fama/w<u>aa</u>humu <b>al</b>nn<u>aa</u>ru kullam<u>aa</u> ar<u>aa</u>duu an yakhrujuu minh<u>aa</u> u\'iiduu fiih<u>aa</u> waqiila lahum <u>dz</u>uuquu \'a<u>dzaa</u>ba <b>al</b>nn<u>aa</u>ri <b>al</b>la<u>dz</u>ii kuntum bihi tuka<u>dzdz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3524, '32:21', 32, 21, ' walanu<u>dz</u>iiqannahum mina <b>a</b>l\'a<u>dzaa</u>bi <b>a</b>l-adn<u>aa</u> duuna <b>a</b>l\'a<u>dzaa</u>bi <b>a</b>l-akbari la\'allahum yarji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3525, '32:22', 32, 22, ' waman a<u>zh</u>lamu mimman <u>dz</u>ukkira bi-<u>aa</u>y<u>aa</u>ti rabbihi tsumma a\'ra<u>dh</u>a \'anh<u>aa</u> inn<u>aa</u> mina <b>a</b>lmujrimiina muntaqimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3526, '32:23', 32, 23, ' walaqad <u>aa</u>tayn<u>aa</u> muus<u>aa</u> <b>a</b>lkit<u>aa</u>ba fal<u>aa</u> takun fii miryatin min liq<u>aa</u>-ihi waja\'aln<u>aa</u>hu hudan libanii isr<u>aa</u>-iil<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3527, '32:24', 32, 24, ' waja\'aln<u>aa</u> minhum a-immatan yahduuna bi-amrin<u>aa</u> lamm<u>aa</u> <u>sh</u>abaruu wak<u>aa</u>nuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> yuuqinuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3528, '32:25', 32, 25, ' inna rabbaka huwa yaf<u>sh</u>ilu baynahum yawma <b>a</b>lqiy<u>aa</u>mati fiim<u>aa</u> k<u>aa</u>nuu fiihi yakhtalifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3529, '32:26', 32, 26, ' awa lam yahdi lahum kam ahlakn<u>aa</u> min qablihim mina <b>a</b>lquruuni yamsyuuna fii mas<u>aa</u>kinihim inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin afal<u>aa</u> yasma\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3530, '32:27', 32, 27, ' awa lam yaraw ann<u>aa</u> nasuuqu <b>a</b>lm<u>aa</u>-a il<u>aa</u> <b>a</b>l-ar<u>dh</u>i <b>a</b>ljuruzi fanukhriju bihi zar\'an ta-kulu minhu an\'<u>aa</u>muhum wa-anfusuhum afal<u>aa</u> yub<u>sh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3531, '32:28', 32, 28, ' wayaquuluuna mat<u>aa</u> h<u>aadzaa</u> <b>a</b>lfat<u>h</u>u in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3532, '32:29', 32, 29, ' qul yawma <b>a</b>lfat<u>h</u>i l<u>aa</u> yanfa\'u <b>al</b>la<u>dz</u>iina kafaruu iim<u>aa</u>nuhum wal<u>aa</u> hum yun<u>zh</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3533, '32:30', 32, 30, ' fa-a\'ri<u>dh</u> \'anhum wa<b>i</b>nta<u>zh</u>ir innahum munta<u>zh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3534, '33:1', 33, 1, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nnabiyyu ittaqi <b>al</b>l<u>aa</u>ha wal<u>aa</u> tu<u>th</u>i\'i <b>a</b>lk<u>aa</u>firiina wa<b>a</b>lmun<u>aa</u>fiqiina inna <b>al</b>l<u>aa</u>ha k<u>aa</u>na \'aliiman <u>h</u>akiim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3535, '33:2', 33, 2, ' wa<b>i</b>ttabi\' m<u>aa</u> yuu<u>haa</u> ilayka min rabbika inna <b>al</b>l<u>aa</u>ha k<u>aa</u>na bim<u>aa</u> ta\'maluuna khabiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3536, '33:3', 33, 3, ' watawakkal \'al<u>aa</u> <b>al</b>l<u>aa</u>hi wakaf<u>aa</u> bi<b>al</b>l<u>aa</u>hi wakiil<u>aa</u><b>n </b>\r\r\n');
INSERT INTO quranLatin VALUES (3537, '33:4', 33, 4, ' m<u>aa</u> ja\'ala <b>al</b>l<u>aa</u>hu lirajulin min qalbayni fii jawfihi wam<u>aa</u> ja\'ala azw<u>aa</u>jakumu <b>al</b>l<u>aa</u>-ii tu<u>zhaa</u>hiruuna minhunna ummah<u>aa</u>tikum wam<u>aa</u> ja\'ala ad\'iy<u>aa</u>-akum abn<u>aa</u>-akum <u>dzaa</u>likum qawlukum bi-afw<u>aa</u>hikum wa<b>al</b>l<u>aa</u>hu yaquulu <b>a</b>l<u>h</u>aqqa wahuwa yahdii <b>al</b>ssabiil<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3538, '33:5', 33, 5, ' ud\'uuhum li-<u>aa</u>b<u>aa</u>-ihim huwa aqsa<u>th</u>u \'inda <b>al</b>l<u>aa</u>hi fa-in lam ta\'lamuu <u>aa</u>b<u>aa</u>-ahum fa-ikhw<u>aa</u>nukum fii <b>al</b>ddiini wamaw<u>aa</u>liikum walaysa \'alaykum jun<u>aah</u>un fiim<u>aa</u> akh<u>th</u>a/tum bihi wal<u>aa</u>kin m<u>aa</u> ta\'ammadat quluubukum wak<u>aa</u>na <b>al</b>l<u>aa</u>hu ghafuuran ra<u>h</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3539, '33:6', 33, 6, ' a<b>l</b>nnabiyyu awl<u>aa</u> bi<b>a</b>lmu/miniina min anfusihim wa-azw<u>aa</u>juhu ummah<u>aa</u>tuhum wauluu <b>a</b>l-ar<u>haa</u>mi ba\'<u>dh</u>uhum awl<u>aa</u> biba\'<u>dh</u>in fii kit<u>aa</u>bi <b>al</b>l<u>aa</u>hi mina <b>a</b>lmu/miniina wa<b>a</b>lmuh<u>aa</u>jiriina ill<u>aa</u> an taf\'aluu il<u>aa</u> awliy<u>aa</u>-ikum ma\'ruufan k<u>aa</u>na <u>dzaa</u>lika fii <b>a</b>lkit<u>aa</u>bi mas<u>th</u>uur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3540, '33:7', 33, 7, ' wa-i<u>dz</u> akha<u>dz</u>n<u>aa</u> mina <b>al</b>nnabiyyiina miits<u>aa</u>qahum waminka wamin nuu<u>h</u>in wa-ibr<u>aa</u>hiima wamuus<u>aa</u> wa\'iis<u>aa</u> ibni maryama wa-akha<u>dz</u>n<u>aa</u> minhum miits<u>aa</u>qan ghalii<u>zhaa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3541, '33:8', 33, 8, ' liyas-ala <b>al</b><u>shshaa</u>diqiina \'an <u>sh</u>idqihim wa-a\'adda lilk<u>aa</u>firiina \'a<u>dzaa</u>ban <b>a</b>liim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3542, '33:9', 33, 9, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu u<u>dz</u>kuruu ni\'mata <b>al</b>l<u>aa</u>hi \'alaykum i<u>dz</u> j<u>aa</u>-atkum junuudun fa-arsaln<u>aa</u> \'alayhim rii<u>h</u>an wajunuudan lam tarawh<u>aa</u> wak<u>aa</u>na <b>al</b>l<u>aa</u>hu bim<u>aa</u> ta\'maluuna ba<u>sh</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3543, '33:10', 33, 10, ' i<u>dz</u> j<u>aa</u>uukum min fawqikum wamin asfala minkum wa-i<u>dz</u> z<u>aa</u>ghati <b>a</b>l-ab<u>shaa</u>ru wabalaghati <b>a</b>lquluubu <b>a</b>l<u>h</u>an<u>aa</u>jira wata<u>zh</u>unnuuna bi<b>al</b>l<u>aa</u>hi <b>al</b><u>zhzh</u>unuun<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (3544, '33:11', 33, 11, ' hun<u>aa</u>lika ibtuliya <b>a</b>lmu-minuuna wazulziluu zilz<u>aa</u>lan syadiid<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3545, '33:12', 33, 12, ' wa-i<u>dz</u> yaquulu <b>a</b>lmun<u>aa</u>fiquuna wa<b>a</b>lla<u>dz</u>iina fii quluubihim mara<u>dh</u>un m<u>aa</u> wa\'adan<u>aa</u> <b>al</b>l<u>aa</u>hu warasuuluhu ill<u>aa</u> ghuruur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3546, '33:13', 33, 13, ' wa-i<u>dz</u> q<u>aa</u>lat <u>th</u><u>aa</u>-ifatun minhum y<u>aa</u> ahla yatsriba l<u>aa</u> muq<u>aa</u>ma lakum fa<b>i</b>rji\'uu wayasta/<u>dz</u>inu fariiqun minhumu <b>al</b>nnabiyya yaquuluuna inna buyuutan<u>aa</u> \'awratun wam<u>aa</u> hiya bi\'awratin in yuriiduuna ill<u>aa</u> fir<u>aa</u>r<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3547, '33:14', 33, 14, ' walaw dukhilat \'alayhim min aq<u>th</u><u>aa</u>rih<u>aa</u> tsumma su-iluu <b>a</b>lfitnata la<u>aa</u>tawh<u>aa</u> wam<u>aa</u> talabbatsuu bih<u>aa</u> ill<u>aa</u> yasiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3548, '33:15', 33, 15, ' walaqad k<u>aa</u>nuu \'<u>aa</u>haduu <b>al</b>l<u>aa</u>ha min qablu l<u>aa</u> yuwalluuna <b>a</b>l-adb<u>aa</u>ra wak<u>aa</u>na \'ahdu <b>al</b>l<u>aa</u>hi mas-uul<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3549, '33:16', 33, 16, ' qul lan yanfa\'akumu <b>a</b>lfir<u>aa</u>ru in farartum mina <b>a</b>lmawti awi <b>a</b>lqatli wa-i<u>dz</u>an l<u>aa</u> tumatta\'uuna ill<u>aa</u> qaliil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3550, '33:17', 33, 17, ' qul man <u>dzaa</u> <b>al</b>la<u>dz</u>ii ya\'<u>sh</u>imukum mina <b>al</b>l<u>aa</u>hi in ar<u>aa</u>da bikum suu-an aw ar<u>aa</u>da bikum ra<u>h</u>matan wal<u>aa</u> yajiduuna lahum min duuni <b>al</b>l<u>aa</u>hi waliyyan wal<u>aa</u> na<u>sh</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3551, '33:18', 33, 18, ' qad ya\'lamu <b>al</b>l<u>aa</u>hu <b>a</b>lmu\'awwiqiina minkum wa<b>a</b>lq<u>aa</u>-iliina li-ikhw<u>aa</u>nihim halumma ilayn<u>aa</u> wal<u>aa</u> ya/tuuna <b>a</b>lba/sa ill<u>aa</u> qaliil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3552, '33:19', 33, 19, ' asyi<u>hh</u>atan \'alaykum fa-i<u>dzaa</u> j<u>aa</u>-a <b>a</b>lkhawfu ra-aytahum yan<u>zh</u>uruuna ilayka taduuru a\'yunuhum ka<b>a</b>lla<u>dz</u>ii yughsy<u>aa</u> \'alayhi mina <b>a</b>lmawti fa-i<u>dzaa</u> <u>dz</u>ahaba <b>a</b>lkhawfu salaquukum bi-alsinatin <u>h</u>id<u>aa</u>din asyi<u>hh</u>atan \'al<u>aa</u> <b>a</b>lkhayri ul<u>aa</u>-ika lam yu/minuu fa-a<u>h</u>ba<u>th</u>a <b>al</b>l<u>aa</u>hu a\'m<u>aa</u>lahum wak<u>aa</u>na <u>dzaa</u>lika \'al<u>aa</u> <b>al</b>l<u>aa</u>hi yasiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3553, '33:20', 33, 20, ' ya<u>h</u>sabuuna <b>a</b>l-a<u>h</u>z<u>aa</u>ba lam ya<u>dz</u>habuu wa-in ya/ti <b>a</b>l-a<u>h</u>z<u>aa</u>bu yawadduu law annahum b<u>aa</u>duuna fii <b>a</b>l-a\'r<u>aa</u>bi yas-aluuna \'an anb<u>aa</u>-ikum walaw k<u>aa</u>nuu fiikum m<u>aa</u> q<u>aa</u>taluu ill<u>aa</u> qaliil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3554, '33:21', 33, 21, ' laqad k<u>aa</u>na lakum fii rasuuli <b>al</b>l<u>aa</u>hi uswatun <u>h</u>asanatun liman k<u>aa</u>na yarjuu <b>al</b>l<u>aa</u>ha wa<b>a</b>lyawma <b>a</b>l-<u>aa</u>khira wa<u>dz</u>akara <b>al</b>l<u>aa</u>ha katsiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3555, '33:22', 33, 22, ' walamm<u>aa</u> ra<u>aa</u> <b>a</b>lmu/minuuna <b>a</b>l-a<u>h</u>z<u>aa</u>ba q<u>aa</u>luu h<u>aadzaa</u> m<u>aa</u> wa\'adan<u>aa</u> <b>al</b>l<u>aa</u>hu warasuuluhu wa<u>sh</u>adaqa <b>al</b>l<u>aa</u>hu warasuuluhu wam<u>aa</u> z<u>aa</u>dahum ill<u>aa</u> iim<u>aa</u>nan watasliim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3556, '33:23', 33, 23, ' mina <b>a</b>lmu/miniina rij<u>aa</u>lun <u>sh</u>adaquu m<u>aa</u> \'<u>aa</u>haduu <b>al</b>l<u>aa</u>ha \'alayhi faminhum man qa<u>daa</u> na<u>h</u>bahu waminhum man yanta<u>zh</u>iru wam<u>aa</u> baddaluu tabdiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3557, '33:24', 33, 24, ' liyajziya <b>al</b>l<u>aa</u>hu <b>al</b><u>shshaa</u>diqiina bi<u>sh</u>idqihim wayu\'a<u>dzdz</u>iba <b>a</b>lmun<u>aa</u>fiqiina in sy<u>aa</u>-a aw yatuuba \'alayhim inna <b>al</b>l<u>aa</u>ha k<u>aa</u>na ghafuuran ra<u>h</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3558, '33:25', 33, 25, ' waradda <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>iina kafaruu bighay<u>zh</u>ihim lam yan<u>aa</u>luu khayran wakaf<u>aa</u> <b>al</b>l<u>aa</u>hu <b>a</b>lmu/miniina <b>a</b>lqit<u>aa</u>la wak<u>aa</u>na <b>al</b>l<u>aa</u>hu qawiyyan \'aziiz<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3559, '33:26', 33, 26, ' wa-anzala <b>al</b>la<u>dz</u>iina <u>zhaa</u>haruuhum min ahli <b>a</b>lkit<u>aa</u>bi min <u>sh</u>ay<u>aas</u>iihim waqa<u>dz</u>afa fii quluubihimu <b>al</b>rru\'ba fariiqan taqtuluuna wata/siruuna fariiq<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3560, '33:27', 33, 27, ' wa-awratsakum ar<u>dh</u>ahum wadiy<u>aa</u>rahum wa-amw<u>aa</u>lahum wa-ar<u>dh</u>an lam ta<u>th</u>auuh<u>aa</u> wak<u>aa</u>na <b>al</b>l<u>aa</u>hu \'al<u>aa</u> kulli syay-in qadiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3561, '33:28', 33, 28, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nnabiyyu qul li-azw<u>aa</u>jika in kuntunna turidna <b>a</b>l<u>h</u>ay<u>aa</u>ta <b>al</b>dduny<u>aa</u> waziinatah<u>aa</u> fata\'<u>aa</u>layna umatti\'kunna wausarri<u>h</u>kunna sar<u>aah</u>an jamiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3562, '33:29', 33, 29, ' wa-in kuntunna turidna <b>al</b>l<u>aa</u>ha warasuulahu wa<b>al</b>dd<u>aa</u>ra <b>a</b>l-<u>aa</u>khirata fa-inna <b>al</b>l<u>aa</u>ha a\'adda lilmu<u>h</u>sin<u>aa</u>ti minkunna ajran \'a<u>zh</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3563, '33:30', 33, 30, ' y<u>aa</u> nis<u>aa</u>-a <b>al</b>nnabiyyi man ya/ti minkunna bif<u>aah</u>isyatin mubayyinatin yu<u>daa</u>\'af lah<u>aa</u> <b>a</b>l\'a<u>dzaa</u>bu <u>dh</u>i\'fayni wak<u>aa</u>na <u>dzaa</u>lika \'al<u>aa</u> <b>al</b>l<u>aa</u>hi yasiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3564, '33:31', 33, 31, ' waman yaqnut minkunna lill<u>aa</u>hi warasuulihi wata\'mal <u>shaa</u>li<u>h</u>an nu/tih<u>aa</u> ajrah<u>aa</u> marratayni wa-a\'tadn<u>aa</u> lah<u>aa</u> rizqan kariim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3565, '33:32', 33, 32, ' y<u>aa</u> nis<u>aa</u>-a <b>al</b>nnabiyyi lastunna ka-a<u>h</u>adin mina <b>al</b>nnis<u>aa</u>-i ini ittaqaytunna fal<u>aa</u> takh<u>dh</u>a\'na bi<b>a</b>lqawli faya<u>th</u>ma\'a <b>al</b>la<u>dz</u>ii fii qalbihi mara<u>dh</u>un waqulna qawlan ma\'ruuf<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3566, '33:33', 33, 33, ' waqarna fii buyuutikunna wal<u>aa</u> tabarrajna tabarruja <b>a</b>lj<u>aa</u>hiliyyati <b>a</b>l-uul<u>aa</u> wa-aqimna <b>al</b><u>shsh</u>al<u>aa</u>ta wa<u>aa</u>tiina <b>al</b>zzak<u>aa</u>ta wa-a<u>th</u>i\'na <b>al</b>l<u>aa</u>ha warasuulahu innam<u>aa</u> yuriidu <b>al</b>l<u>aa</u>hu liyu<u>dz</u>hiba \'ankumu <b>al</b>rrijsa ahla <b>a</b>lbayti wayu<u>th</u>ahhirakum ta<u>th</u>hiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3567, '33:34', 33, 34, ' wa<b>u</b><u>dz</u>kurna m<u>aa</u> yutl<u>aa</u> fii buyuutikunna min <u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi wa<b>a</b>l<u>h</u>ikmati inna <b>al</b>l<u>aa</u>ha k<u>aa</u>na la<u>th</u>iifan khabiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3568, '33:35', 33, 35, ' inna <b>a</b>lmuslimiina wa<b>a</b>lmuslim<u>aa</u>ti wa<b>a</b>lmu/miniina wa<b>a</b>lmu/min<u>aa</u>ti wa<b>a</b>lq<u>aa</u>nitiina wa<b>a</b>lq<u>aa</u>nit<u>aa</u>ti wa<b>al</b><u>shshaa</u>diqiina wa<b>al</b><u>shshaa</u>diq<u>aa</u>ti wa<b>al</b><u>shshaa</u>biriina wa<b>al</b><u>shshaa</u>bir<u>aa</u>ti wa<b>a</b>lkh<u>aa</u>syi\'iina wa<b>a</b>lkh<u>aa</u>syi\'<u>aa</u>ti wa<b>a</b>lmuta<u>sh</u>addiqiina wa<b>a</b>lmuta<u>sh</u>addiq<u>aa</u>ti wa<b>al</b><u>shshaa</u>-imiina wa<b>al</b><u>shshaa</u>-im<u>aa</u>ti wa<b>a</b>l<u>haa</u>fi<u>zh</u>iina furuujahum wa<b>a</b>l<u>haa</u>fi<u>zhaa</u>ti wa<b>al</b><u>dzdzaa</u>kiriina <b>al</b>l<u>aa</u>ha katsiiran wa<b>al</b><u>dzdzaa</u>kir<u>aa</u>ti a\'adda <b>al</b>l<u>aa</u>hu lahum maghfiratan wa-ajran \'a<u>zh</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3569, '33:36', 33, 36, ' wam<u>aa</u> k<u>aa</u>na limu/minin wal<u>aa</u> mu/minatin i<u>dzaa</u> qa<u>daa</u> <b>al</b>l<u>aa</u>hu warasuuluhu amran an yakuuna lahumu <b>a</b>lkhiyaratu min amrihim waman ya\'<u>sh</u>i <b>al</b>l<u>aa</u>ha warasuulahu faqad <u>dh</u>alla <u>dh</u>al<u>aa</u>lan mubiin<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (3570, '33:37', 33, 37, ' wa-i<u>dz</u> taquulu lilla<u>dz</u>ii an\'ama <b>al</b>l<u>aa</u>hu \'alayhi wa-an\'amta \'alayhi amsik \'alayka zawjaka wa<b>i</b>ttaqi <b>al</b>l<u>aa</u>ha watukhfii fii nafsika m<u>aa</u> <b>al</b>l<u>aa</u>hu mubdiihi watakhsy<u>aa</u> <b>al</b>nn<u>aa</u>sa wa<b>al</b>l<u>aa</u>hu a<u>h</u>aqqu an takhsy<u>aa</u>hu falamm<u>aa</u> qa<u>daa</u> zaydun minh<u>aa</u> wa<u>th</u>aran zawwajn<u>aa</u>kah<u>aa</u> likay l<u>aa</u> yakuuna \'al<u>aa</u> <b>a</b>lmu/miniina <u>h</u>arajun fii azw<u>aa</u>ji ad\'iy<u>aa</u>-ihim i<u>dzaa</u> qa<u>dh</u>aw minhunna wa<u>th</u>aran wak<u>aa</u>na amru <b>al</b>l<u>aa</u>hi maf\'uul<u>aa</u><b>n</b>\r\r\n');
