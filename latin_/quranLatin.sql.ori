INSERT INTO quranLatin VALUES (2216, '18:76', 18, 76, ' q<u>aa</u>la in sa-altuka \'an syay-in ba\'dah<u>aa</u> fal<u>aa</u> tu<u>shaah</u>ibnii qad balaghta min ladunnii \'u<u>dz</u>r<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2217, '18:77', 18, 77, ' fa<b>i</b>n<u>th</u>alaq<u>aa</u> <u>h</u>att<u>aa</u> i<u>dzaa</u> atay<u>aa</u> ahla qaryatin ista<u>th</u>\'am<u>aa</u> ahlah<u>aa</u> fa-abaw an yu<u>dh</u>ayyifuuhum<u>aa</u> fawajad<u>aa</u> fiih<u>aa</u> jid<u>aa</u>ran yuriidu an yanqa<u>dhdh</u>a fa-aq<u>aa</u>mahu q<u>aa</u>la law syi/ta la<b>i</b>ttakha<u>dz</u>ta \'alayhi ajr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2218, '18:78', 18, 78, ' q<u>aa</u>la h<u>aadzaa</u> fir<u>aa</u>qu baynii wabaynika sa-unabbi-uka bita/wiili m<u>aa</u> lam tasta<u>th</u>i\' \'alayhi <u>sh</u>abr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2219, '18:79', 18, 79, ' amm<u>aa</u> <b>al</b>ssafiinatu fak<u>aa</u>nat limas<u>aa</u>kiina ya\'maluuna fii <b>a</b>lba<u>h</u>ri fa-aradtu an a\'iibah<u>aa</u> wak<u>aa</u>na war<u>aa</u>-ahum malikun ya/khu<u>dz</u>u kulla safiinatin gha<u>sh</u>b<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2220, '18:80', 18, 80, ' wa-amm<u>aa</u> <b>a</b>lghul<u>aa</u>mu fak<u>aa</u>na abaw<u>aa</u>hu mu/minayni fakhasyiin<u>aa</u> an yurhiqahum<u>aa</u> <u>th</u>ughy<u>aa</u>nan wakufr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2221, '18:81', 18, 81, ' fa-aradn<u>aa</u> an yubdilahum<u>aa</u> rabbuhum<u>aa</u> khayran minhu zak<u>aa</u>tan wa-aqraba ru<u>h</u>m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2222, '18:82', 18, 82, ' wa-amm<u>aa</u> <b>a</b>ljid<u>aa</u>ru fak<u>aa</u>na lighul<u>aa</u>mayni yatiimayni fii <b>a</b>lmadiinati wak<u>aa</u>na ta<u>h</u>tahu kanzun lahum<u>aa</u> wak<u>aa</u>na abuuhum<u>aa</u> <u>shaa</u>li<u>h</u>an fa-ar<u>aa</u>da rabbuka an yablugh<u>aa</u> asyuddahum<u>aa</u> wayastakhrij<u>aa</u> kanzahum<u>aa</u> ra<u>h</u>matan min rabbika wam<u>aa</u> fa\'altuhu \'an amrii <u>dzaa</u>lika ta/wiilu m<u>aa</u> lam tas<u>th</u>i\' \'alayhi <u>sh</u>abr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2223, '18:83', 18, 83, ' wayas-aluunaka \'an <u>dz</u>ii <b>a</b>lqarnayni qul sa-atluu \'alaykum minhu <u>dz</u>ikr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2224, '18:84', 18, 84, ' inn<u>aa</u> makkann<u>aa</u> lahu fii <b>a</b>l-ar<u>dh</u>i wa<u>aa</u>tayn<u>aa</u>hu min kulli syay-in sabab<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2225, '18:85', 18, 85, ' fa-atba\'a sabab<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2226, '18:86', 18, 86, ' <u>h</u>att<u>aa</u> i<u>dzaa</u> balagha maghriba <b>al</b>sysyamsi wajadah<u>aa</u> taghrubu fii \'aynin <u>h</u>ami-atin wawajada \'indah<u>aa</u> qawman quln<u>aa</u> y<u>aa</u> <u>dzaa</u> <b>a</b>lqarnayni imm<u>aa</u> an tu\'a<u>dzdz</u>iba wa-imm<u>aa</u> an tattakhi<u>dz</u>a fiihim <u>h</u>usn<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2227, '18:87', 18, 87, ' q<u>aa</u>la amm<u>aa</u> man <u>zh</u>alama fasawfa nu\'a<u>dzdz</u>ibuhu tsumma yuraddu il<u>aa</u> rabbihi fayu\'a<u>dzdz</u>ibuhu \'a<u>dzaa</u>ban nukr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2228, '18:88', 18, 88, ' wa-amm<u>aa</u> man <u>aa</u>mana wa\'amila <u>shaa</u>li<u>h</u>an falahu jaz<u>aa</u>-an <b>a</b>l<u>h</u>usn<u>aa</u> wasanaquulu lahu min amrin<u>aa</u> yusr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2229, '18:89', 18, 89, ' tsumma atba\'a sabab<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2230, '18:90', 18, 90, ' <u>h</u>att<u>aa</u> i<u>dzaa</u> balagha ma<u>th</u>li\'a <b>al</b>sysyamsi wajadah<u>aa</u> ta<u>th</u>lu\'u \'al<u>aa</u> qawmin lam naj\'al lahum min duunih<u>aa</u> sitr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2231, '18:91', 18, 91, ' ka<u>dzaa</u>lika waqad a<u>h</u>a<u>th</u>n<u>aa</u> bim<u>aa</u> ladayhi khubr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2232, '18:92', 18, 92, ' tsumma atba\'a sabab<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2233, '18:93', 18, 93, ' <u>h</u>att<u>aa</u> i<u>dzaa</u> balagha bayna <b>al</b>ssaddayni wajada min duunihim<u>aa</u> qawman l<u>aa</u> yak<u>aa</u>duuna yafqahuuna qawl<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2234, '18:94', 18, 94, ' q<u>aa</u>luu y<u>aa</u> <u>dzaa</u> <b>a</b>lqarnayni inna ya/juuja wama/juuja mufsiduuna fii <b>a</b>l-ar<u>dh</u>i fahal naj\'alu laka kharjan \'al<u>aa</u> an taj\'ala baynan<u>aa</u> wabaynahum sadd<u>aa</u><b>n</b>\r\r\n');
