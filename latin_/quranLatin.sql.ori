INSERT INTO quranLatin VALUES (626, '4:133', 4, 133, ' in yasya/ yu<u>dz</u>hibkum ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su waya/ti bi-<u>aa</u>khariina wak<u>aa</u>na <b>al</b>l<u>aa</u>hu \'al<u>aa</u> <u>dzaa</u>lika qadiir<u>aa</u><b>n</b>\r\r\n');
