INSERT INTO quranLatin VALUES (5271, '67:30', 67, 30, ' qul ara-aytum in a<u>sh</u>ba<u>h</u>a m<u>aa</u>ukum ghawran faman ya/tiikum bim<u>aa</u>-in ma\'iin<b>in</b>\r\r\n');
