INSERT INTO quranLatin VALUES (3320, '28:68', 28, 68, ' warabbuka yakhluqu m<u>aa</u> yasy<u>aa</u>u wayakht<u>aa</u>ru m<u>aa</u> k<u>aa</u>na lahumu <b>a</b>lkhiyaratu sub<u>haa</u>na <b>al</b>l<u>aa</u>hi wata\'<u>aa</u>l<u>aa</u> \'amm<u>aa</u> yusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3321, '28:69', 28, 69, ' warabbuka ya\'lamu m<u>aa</u> tukinnu <u>sh</u>uduuruhum wam<u>aa</u> yu\'linuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3322, '28:70', 28, 70, ' wahuwa <b>al</b>l<u>aa</u>hu l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa lahu <b>a</b>l<u>h</u>amdu fii <b>a</b>l-uul<u>aa</u> wa<b>a</b>l-<u>aa</u>khirati walahu <b>a</b>l<u>h</u>ukmu wa-ilayhi turja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (3323, '28:71', 28, 71, ' qul ara-aytum in ja\'ala <b>al</b>l<u>aa</u>hu \'alaykumu <b>al</b>layla sarmadan il<u>aa</u> yawmi <b>a</b>lqiy<u>aa</u>mati man il<u>aa</u>hun ghayru <b>al</b>l<u>aa</u>hi ya/tiikum bi<u>dh</u>iy<u>aa</u>-in afal<u>aa</u> tasma\'uun<b>a</b>\r\r\n');
