INSERT INTO quranLatin VALUES (175, '2:168', 2, 168, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su kuluu mimm<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i <u>h</u>al<u>aa</u>lan <u>th</u>ayyiban wal<u>aa</u> tattabi\'uu khu<u>th</u>uw<u>aa</u>ti <b>al</b>sysyay<u>th</u><u>aa</u>ni innahu lakum \'aduwwun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (176, '2:169', 2, 169, ' innam<u>aa</u> ya/murukum bi<b>al</b>ssuu-i wa<b>a</b>lfa<u>h</u>sy<u>aa</u>-i wa-an taquuluu \'al<u>aa</u> <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (177, '2:170', 2, 170, ' wa-i<u>dzaa</u> qiila lahumu ittabi\'uu m<u>aa</u> anzala <b>al</b>l<u>aa</u>hu q<u>aa</u>luu bal nattabi\'u m<u>aa</u> <b>a</b>lfayn<u>aa</u> \'alayhi <u>aa</u>b<u>aa</u>-an<u>aa</u> awa law k<u>aa</u>na <u>aa</u>b<u>aa</u>uhum l<u>aa</u> ya\'qiluuna syay-an wal<u>aa</u> yahtaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (178, '2:171', 2, 171, ' wamatsalu <b>al</b>la<u>dz</u>iina kafaruu kamatsali <b>al</b>la<u>dz</u>ii yan\'iqu bim<u>aa</u> l<u>aa</u> yasma\'u ill<u>aa</u> du\'<u>aa</u>-an wanid<u>aa</u>-an <u>sh</u>ummun bukmun \'umyun fahum l<u>aa</u> ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (179, '2:172', 2, 172, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu kuluu min <u>th</u>ayyib<u>aa</u>ti m<u>aa</u> razaqn<u>aa</u>kum wa<b>u</b>sykuruu lill<u>aa</u>hi in kuntum iyy<u>aa</u>hu ta\'buduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (180, '2:173', 2, 173, ' innam<u>aa</u> <u>h</u>arrama \'alaykumu <b>a</b>lmaytata wa<b>al</b>ddama wala<u>h</u>ma <b>a</b>lkhinziiri wam<u>aa</u> uhilla bihi lighayri <b>al</b>l<u>aa</u>hi famani i<u>dth</u>urra ghayra b<u>aa</u>ghin wal<u>aa</u> \'<u>aa</u>din fal<u>aa</u> itsma \'alayhi inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b> \r\r\n');
INSERT INTO quranLatin VALUES (181, '2:174', 2, 174, ' inna <b>al</b>la<u>dz</u>iina yaktumuuna m<u>aa</u> anzala <b>al</b>l<u>aa</u>hu mina <b>a</b>lkit<u>aa</u>bi wayasytaruuna bihi tsamanan qaliilan ul<u>aa</u>-ika m<u>aa</u> ya/kuluuna fii bu<u>th</u>uunihim ill<u>aa</u> <b>al</b>nn<u>aa</u>ra wal<u>aa</u> yukallimuhumu <b>al</b>l<u>aa</u>hu yawma <b>a</b>lqiy<u>aa</u>mati wal<u>aa</u> yuzakkiihim walahum \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (182, '2:175', 2, 175, ' ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina isytarawuu <b>al</b><u>dhdh</u>al<u>aa</u>lata bi<b>a</b>lhud<u>aa</u> wa<b>a</b>l\'a<u>dzaa</u>ba bi<b>a</b>lmaghfirati fam<u>aa</u> a<u>sh</u>barahum \'al<u>aa</u> <b>al</b>nn<u>aa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (183, '2:176', 2, 176, ' <u>dzaa</u>lika bi-anna <b>al</b>l<u>aa</u>ha nazzala <b>a</b>lkit<u>aa</u>ba bi<b>a</b>l<u>h</u>aqqi wa-inna <b>al</b>la<u>dz</u>iina ikhtalafuu fii <b>a</b>lkit<u>aa</u>bi lafii syiq<u>aa</u>qin ba\'iid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (184, '2:177', 2, 177, ' laysa <b>a</b>lbirra an tuwalluu wujuuhakum qibala <b>a</b>lmasyriqi wa<b>a</b>lmaghribi wal<u>aa</u>kinna <b>a</b>lbirra man <u>aa</u>mana bi<b>al</b>l<u>aa</u>hi wa<b>a</b>lyawmi <b>a</b>l-<u>aa</u>khiri wa<b>a</b>lmal<u>aa</u>-ikati wa<b>a</b>lkit<u>aa</u>bi wa<b>al</b>nnabiyyiina wa<u>aa</u>t<u>aa</u>\r <b>a</b>lm<u>aa</u>la \'al<u>aa</u> <u>h</u>ubbihi <u>dz</u>awii <b>a</b>lqurb<u>aa</u> wa<b>a</b>lyat<u>aa</u>m<u>aa</u> wa<b>a</b>lmas<u>aa</u>kiina wa<b>i</b>bna <b>a</b>lssabiili wa<b>al</b>ss<u>aa</u>-iliina wafii <b>al</b>rriq<u>aa</u>bi wa-aq<u>aa</u>ma <b>al</b><u>shsh</u>al<u>aa</u>ta wa<u>aa</u>t<u>aa</u> <b>al</b>zzak<u>aa</u>ta wa<b>a</b>lmuufuuna bi\'ahdihim i<u>dzaa</u> \'<u>aa</u>haduu wa<b>al</b><u>shshaa</u>biriina fii <b>a</b>lba/s<u>aa</u>-i wa<b>al</b><u>dhdh</u>arr<u>aa</u>-i wa<u>h</u>iina <b>a</b>lba/si ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina <u>sh</u>adaquu waul<u>aa</u>-ika humu <b>a</b>lmuttaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (185, '2:178', 2, 178, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu kutiba \'alaykumu <b>a</b>lqi<u>sas</u>u fii <b>a</b>lqatl<u>aa</u> <b>a</b>l<u>h</u>urru bi<b>a</b>l<u>h</u>urri wa<b>a</b>l\'abdu bi<b>a</b>l\'abdi wa<b>a</b>lunts<u>aa</u> bi<b>a</b>lunts<u>aa</u> faman \'ufiya lahu min akhiihi syay-un fa<b>i</b>ttib<u>aa</u>\'un bi<b>a</b>lma\'ruufi wa-ad<u>aa</u>un ilayhi bi-i<u>h</u>s<u>aa</u>nin <u>dzaa</u>lika takhfiifun min rabbikum wara<u>h</u>matun famani i\'tad<u>aa</u> ba\'da <u>dzaa</u>lika falahu \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (186, '2:179', 2, 179, ' walakum fii <b>a</b>lqi<u>sas</u>i <u>h</u>ay<u>aa</u>tun y<u>aa</u> ulii <b>a</b>l-alb<u>aa</u>bi la\'allakum tattaquun<b>a</b>\r\n');
INSERT INTO quranLatin VALUES (187, '2:180', 2, 180, ' kutiba \'alaykum i<u>dzaa</u> <u>h</u>a<u>dh</u>ara a<u>h</u>adakumu <b>a</b>lmawtu in taraka khayran <b>a</b>lwa<u>sh</u>iyyatu lilw<u>aa</u>lidayni wa<b>a</b>l-aqrabiina bi<b>a</b>lma\'ruufi <u>h</u>aqqan \'al<u>aa</u> <b>a</b>lmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (188, '2:181', 2, 181, ' faman baddalahu ba\'da m<u>aa</u> sami\'ahu fa-innam<u>aa</u> itsmuhu \'al<u>aa</u> <b>al</b>la<u>dz</u>iina yubaddiluunahu inna <b>al</b>l<u>aa</u>ha samii\'un \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (189, '2:182', 2, 182, ' faman kh<u>aa</u>fa min muu<u>sh</u>in janafan aw itsman fa-a<u>sh</u>la<u>h</u>a baynahum fal<u>aa</u> itsma \'alayhi inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (190, '2:183', 2, 183, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu kutiba \'alaykumu <b>al</b><u>shsh</u>iy<u>aa</u>mu kam<u>aa</u> kutiba \'al<u>aa</u> <b>al</b>la<u>dz</u>iina min qablikum la\'allakum tattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (191, '2:184', 2, 184, ' ayy<u>aa</u>man ma\'duud<u>aa</u>tin faman k<u>aa</u>na minkum marii<u>dh</u>an aw \'al<u>aa</u> safarin fa\'iddatun min ayy<u>aa</u>min ukhara wa\'al<u>aa</u> <b>al</b>la<u>dz</u>iina yu<u>th</u>iiquunahu fidyatun <u>th</u>a\'<u>aa</u>mu miskiinin faman ta<u>th</u>awwa\'a khayran fahuwa khayrun lahu wa-an ta<u>sh</u>uumuu khayrun lakum in kuntum ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (192, '2:185', 2, 185, ' syahru rama<u>daa</u>na <b>al</b>la<u>dz</u>ii unzila fiihi <b>a</b>lqur-<u>aa</u>nu hudan li<b>l</b>nn<u>aa</u>si wabayyin<u>aa</u>tin mina <b>a</b>lhud<u>aa</u> wa<b>a</b>lfurq<u>aa</u>ni faman syahida minkumu <b>al</b>sysyahra falya<u>sh</u>umhu waman k<u>aa</u>na marii<u>dh</u>an aw \'al<u>aa</u> safarin fa\'iddatun min ayy<u>aa</u>min ukhara yuriidu <b>al</b>l<u>aa</u>hu bikumu <b>a</b>lyusra wal<u>aa</u> yuriidu bikumu <b>a</b>l\'usra walitukmiluu <b>a</b>l\'iddata walitukabbiruu <b>al</b>l<u>aa</u>ha \'al<u>aa</u> m<u>aa</u> had<u>aa</u>kum wala\'allakum tasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (193, '2:186', 2, 186, ' wa-i<u>dzaa</u> sa-alaka \'ib<u>aa</u>dii \'annii fa-innii qariibun ujiibu da\'wata <b>al</b>dd<u>aa</u>\'i i<u>dzaa</u> da\'<u>aa</u>ni falyastajiibuu lii walyu/minuu bii la\'allahum yarsyuduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (194, '2:187', 2, 187, ' u<u>h</u>illa lakum laylata <b>al</b><u>shsh</u>iy<u>aa</u>mi <b>al</b>rrafatsu il<u>aa</u> nis<u>aa</u>-ikum hunna lib<u>aa</u>sun lakum wa-antum lib<u>aa</u>sun lahunna \'alima <b>al</b>l<u>aa</u>hu annakum kuntum takht<u>aa</u>nuuna anfusakum fat<u>aa</u>ba \'alaykum wa\'af<u>aa</u> \'ankum fa<b>a</b>l-<u>aa</u>na b<u>aa</u>syiruuhunna wa<b>i</b>btaghuu m<u>aa</u> kataba <b>al</b>l<u>aa</u>hu lakum wakuluu wa<b>i</b>syrabuu <u>h</u>att<u>aa</u> yatabayyana lakumu <b>a</b>lkhay<u>th</u>u <b>a</b>l-abya<u>dh</u>u mina <b>a</b>lkhay<u>th</u>i <b>a</b>l-aswadi mina <b>a</b>lfajri tsumma atimmuu <b>al</b><u>shsh</u>iy<u>aa</u>ma il<u>aa</u> <b>al</b>layli wal<u>aa</u> tub<u>aa</u>syiruuhunna wa-antum \'<u>aa</u>kifuuna fii <b>a</b>lmas<u>aa</u>jidi tilka <u>h</u>uduudu <b>al</b>l<u>aa</u>hi fal<u>aa</u> taqrabuuh<u>aa</u> ka<u>dzaa</u>lika yubayyinu <b>al</b>l<u>aa</u>hu <u>aa</u>y<u>aa</u>tihi li<b>l</b>nn<u>aa</u>si la\'allahum yattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (195, '2:188', 2, 188, ' wal<u>aa</u> ta/kuluu amw<u>aa</u>lakum baynakum bi<b>a</b>lb<u>aath</u>ili watudluu bih<u>aa</u> il<u>aa</u> <b>a</b>l<u>h</u>ukk<u>aa</u>mi lita/kuluu fariiqan min amw<u>aa</u>li <b>al</b>nn<u>aa</u>si bi<b>a</b>l-itsmi wa-antum ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (196, '2:189', 2, 189, ' yas-aluunaka \'ani <b>a</b>l-ahillati qul hiya maw<u>aa</u>qiitu li<b>l</b>nn<u>aa</u>si wa<b>a</b>l<u>h</u>ajji walaysa <b>a</b>lbirru bi-an ta/tuu <b>a</b>lbuyuuta min <u>zh</u>uhuurih<u>aa</u> wal<u>aa</u>kinna <b>a</b>lbirra mani ittaq<u>aa</u> wa/tuu <b>a</b>lbuyuuta min abw<u>aa</u>bih<u>aa</u> wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha la\'allakum tufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (197, '2:190', 2, 190, ' waq<u>aa</u>tiluu fii sabiili <b>al</b>l<u>aa</u>hi <b>al</b>la<u>dz</u>iina yuq<u>aa</u>tiluunakum wal<u>aa</u> ta\'taduu inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yu<u>h</u>ibbu <b>a</b>lmu\'tadiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (198, '2:191', 2, 191, ' wa<b>u</b>qtuluuhum <u>h</u>aytsu tsaqiftumuuhum wa-akhrijuuhum min <u>h</u>aytsu akhrajuukum wa<b>a</b>lfitnatu asyaddu mina <b>a</b>lqatli wal<u>aa</u> tuq<u>aa</u>tiluuhum \'inda <b>a</b>lmasjidi <b>a</b>l<u>h</u>ar<u>aa</u>mi <u>h</u>att<u>aa</u> yuq<u>aa</u>tiluukum fiihi fa-in q<u>aa</u>taluukum fa<b>u</b>qtuluuhum ka<u>dzaa</u>lika jaz<u>aa</u>u <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (199, '2:192', 2, 192, ' fa-ini intahaw fa-inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (200, '2:193', 2, 193, ' waq<u>aa</u>tiluuhum <u>h</u>att<u>aa</u> l<u>aa</u> takuuna fitnatun wayakuuna <b>al</b>ddiinu lill<u>aa</u>hi fa-ini intahaw fal<u>aa</u> \'udw<u>aa</u>na ill<u>aa</u> \'al<u>aa</u> <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (201, '2:194', 2, 194, ' a<b>l</b>sysyahru <b>a</b>l<u>h</u>ar<u>aa</u>mu bi<b>al</b>sysyahri <b>a</b>l<u>h</u>ar<u>aa</u>mi wa<b>a</b>l<u>h</u>urum<u>aa</u>tu qi<u>sas</u>un famani i\'tad<u>aa</u> \'alaykum fa<b>i</b>\'taduu \'alayhi bimitsli m<u>aa</u> i\'tad<u>aa</u> \'alaykum wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha wa<b>i</b>\'lamuu anna <b>al</b>l<u>aa</u>ha ma\'a <b>a</b>lmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (202, '2:195', 2, 195, ' wa-anfiquu fii sabiili <b>al</b>l<u>aa</u>hi wal<u>aa</u> tulquu bi-aydiikum il<u>aa</u> <b>al</b>ttahlukati wa-a<u>h</u>sinuu inna <b>al</b>l<u>aa</u>ha yu<u>h</u>ibbu <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (203, '2:196', 2, 196, ' wa-atimmuu <b>a</b>l<u>h</u>ajja wa<b>a</b>l\'umrata lill<u>aa</u>hi fa-in u<u>hs</u>irtum fam<u>aa</u> istaysara mina <b>a</b>lhadyi wal<u>aa</u> ta<u>h</u>liquu ruuusakum <u>h</u>att<u>aa</u> yablugha <b>a</b>lhadyu ma<u>h</u>illahu faman k<u>aa</u>na minkum marii<u>dh</u>an aw bihi a<u>dz</u>an min ra/sihi fafidyatun min <u>sh</u>iy<u>aa</u>min aw <u>sh</u>adaqatin aw nusukin fa-i<u>dzaa</u> amintum faman tamatta\'a bi<b>a</b>l\'umrati il<u>aa</u> <b>a</b>l<u>h</u>ajji fam<u>aa</u> istaysara mina <b>a</b>lhadyi faman lam yajid fa<u>sh</u>iy<u>aa</u>mu tsal<u>aa</u>tsati ayy<u>aa</u>min fii <b>a</b>l<u>h</u>ajji wasab\'atin i<u>dzaa</u> raja\'tum tilka \'asyaratun k<u>aa</u>milatun <u>dzaa</u>lika liman lam yakun ahluhu <u>had</u>irii <b>a</b>lmasjidi <b>a</b>l<u>h</u>ar<u>aa</u>mi wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha wa<b>i</b>\'lamuu anna <b>al</b>l<u>aa</u>ha syadiidu <b>a</b>l\'iq<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (204, '2:197', 2, 197, ' al<u>h</u>ajju asyhurun ma\'luum<u>aa</u>tun faman fara<u>dh</u>a fiihinna <b>a</b>l<u>h</u>ajja fal<u>aa</u> rafatsa wal<u>aa</u> fusuuqa wal<u>aa</u> jid<u>aa</u>la fii <b>a</b>l<u>h</u>ajji wam<u>aa</u> taf\'aluu min khayrin ya\'lamhu <b>al</b>l<u>aa</u>hu watazawwaduu fa-inna khayra <b>al</b>zz<u>aa</u>di <b>al</b>ttaqw<u>aa</u> wa<b>i</b>ttaquuni y<u>aa</u> ulii <b>a</b>l-alb<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (205, '2:198', 2, 198, ' laysa \'alaykum jun<u>aah</u>un an tabtaghuu fa<u>dh</u>lan min rabbikum fa-i<u>dzaa</u> afa<u>dh</u>tum min \'araf<u>aa</u>tin fa<b>u</b><u>dz</u>kuruu <b>al</b>l<u>aa</u>ha \'inda <b>a</b>lmasy\'ari <b>a</b>l<u>h</u>ar<u>aa</u>mi wa<b>u</b><u>dz</u>kuruuhu kam<u>aa</u> had<u>aa</u>kum wa-in kuntum min qablihi lamina <b>al</b><u>dhdhaa</u>lliin<b>a</b>\r \r\r\n');
INSERT INTO quranLatin VALUES (206, '2:199', 2, 199, ' tsumma afii<u>dh</u>uu min <u>h</u>aytsu af<u>aad</u>a <b>al</b>nn<u>aa</u>su wa<b>i</b>staghfiruu <b>al</b>l<u>aa</u>ha inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (207, '2:200', 2, 200, ' fa-i<u>dzaa</u> qa<u>dh</u>aytum man<u>aa</u>sikakum fa<b>u</b><u>dz</u>kuruu <b>al</b>l<u>aa</u>ha ka<u>dz</u>ikrikum <u>aa</u>b<u>aa</u>-akum aw asyadda <u>dz</u>ikran famina <b>al</b>nn<u>aa</u>si man yaquulu rabban<u>aa</u> <u>aa</u>tin<u>aa</u> fii <b>al</b>dduny<u>aa</u> wam<u>aa</u> lahu fii <b>a</b>l-<u>aa</u>khirati min khal<u>aa</u>q<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (208, '2:201', 2, 201, ' waminhum man yaquulu rabban<u>aa</u> <u>aa</u>tin<u>aa</u> fii <b>al</b>dduny<u>aa</u> <u>h</u>asanatan wafii <b>a</b>l-<u>aa</u>khirati <u>h</u>asanatan waqin<u>aa</u> \'a<u>dzaa</u>ba <b>al</b>nn<u>aa</u>r<b>i</b>\r\n');
INSERT INTO quranLatin VALUES (209, '2:202', 2, 202, ' ul<u>aa</u>-ika lahum na<u>sh</u>iibun mimm<u>aa</u> kasabuu wa<b>al</b>l<u>aa</u>hu sarii\'u <b>a</b>l<u>h</u>is<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (210, '2:203', 2, 203, ' wa<b>u</b><u>dz</u>kuruu <b>al</b>l<u>aa</u>ha fii ayy<u>aa</u>min ma\'duud<u>aa</u>tin faman ta\'ajjala fii yawmayni fal<u>aa</u> itsma \'alayhi waman ta-akhkhara fal<u>aa</u> itsma \'alayhi limani ittaq<u>aa</u> wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha wa<b>i</b>\'lamuu annakum ilayhi tu<u>h</u>syaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (211, '2:204', 2, 204, ' wamina <b>al</b>nn<u>aa</u>si man yu\'jibuka qawluhu fii al<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> wayusyhidu <b>al</b>l<u>aa</u>ha \'al<u>aa</u> m<u>aa</u> fii qalbihi wahuwa aladdu alkhi<u>shaa</u>m<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (212, '2:205', 2, 205, ' wa-i<u>dzaa</u> tawall<u>aa</u> sa\'<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i liyufsida fiih<u>aa</u> wayuhlika <b>a</b>l<u>h</u>artsa wa<b>al</b>nnasla wa<b>al</b>l<u>aa</u>hu l<u>aa</u> yu<u>h</u>ibbu <b>a</b>lfas<u>aa</u>d<b>a</b>\r\n');
