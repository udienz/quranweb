INSERT INTO quranLatin VALUES (3313, '28:61', 28, 61, ' afaman wa\'adn<u>aa</u>hu wa\'dan <u>h</u>asanan fahuwa l<u>aa</u>qiihi kaman matta\'n<u>aa</u>hu mat<u>aa</u>\'a <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> tsumma huwa yawma <b>a</b>lqiy<u>aa</u>mati mina <b>a</b>lmu<u>hd</u>ariin<b>a</b>\r\r\n');
