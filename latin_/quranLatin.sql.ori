INSERT INTO quranLatin VALUES (4481, '45:8', 45, 8, ' yasma\'u <u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi tutl<u>aa</u> \'alayhi tsumma yu<u>sh</u>irru mustakbiran ka-an lam yasma\'h<u>aa</u> fabasysyirhu bi\'a<u>dzaa</u>bin <b>a</b>liim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4482, '45:9', 45, 9, ' wa-i<u>dzaa</u> \'alima min <u>aa</u>y<u>aa</u>tin<u>aa</u> syay-an ittakha<u>dz</u>ah<u>aa</u> huzuwan ul<u>aa</u>-ika lahum \'a<u>dzaa</u>bun muhiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4483, '45:10', 45, 10, ' min war<u>aa</u>-ihim jahannamu wal<u>aa</u> yughnii \'anhum m<u>aa</u> kasabuu syay-an wal<u>aa</u> m<u>aa</u> ittakha<u>dz</u>uu min duuni <b>al</b>l<u>aa</u>hi awliy<u>aa</u>-a walahum \'a<u>dzaa</u>bun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4484, '45:11', 45, 11, ' h<u>aadzaa</u> hudan wa<b>a</b>lla<u>dz</u>iina kafaruu bi-<u>aa</u>y<u>aa</u>ti rabbihim lahum \'a<u>dzaa</u>bun min rijzin <b>a</b>liim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4485, '45:12', 45, 12, ' <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii sakhkhara lakumu <b>a</b>lba<u>h</u>ra litajriya <b>a</b>lfulku fiihi bi-amrihi walitabtaghuu min fa<u>dh</u>lihi wala\'allakum tasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4486, '45:13', 45, 13, ' wasakhkhara lakum m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i jamii\'an minhu inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin liqawmin yatafakkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4487, '45:14', 45, 14, ' qul lilla<u>dz</u>iina <u>aa</u>manuu yaghfiruu lilla<u>dz</u>iina l<u>aa</u> yarjuuna ayy<u>aa</u>ma <b>al</b>l<u>aa</u>hi liyajziya qawman bim<u>aa</u> k<u>aa</u>nuu yaksibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4488, '45:15', 45, 15, ' man \'amila <u>shaa</u>li<u>h</u>an falinafsihi waman as<u>aa</u>-a fa\'alayh<u>aa</u> tsumma il<u>aa</u> rabbikum turja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4489, '45:16', 45, 16, ' walaqad <u>aa</u>tayn<u>aa</u> banii isr<u>aa</u>-iila <b>a</b>lkit<u>aa</u>ba wa<b>a</b>l<u>h</u>ukma wa<b>al</b>nnubuwwata warazaqn<u>aa</u>hum mina <b>al</b><u>ththh</u>ayyib<u>aa</u>ti wafa<u>dhdh</u>aln<u>aa</u>hum \'al<u>aa</u> <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4490, '45:17', 45, 17, ' wa<u>aa</u>tayn<u>aa</u>hum bayyin<u>aa</u>tin mina <b>a</b>l-amri fam<u>aa</u> ikhtalafuu ill<u>aa</u> min ba\'di m<u>aa</u> j<u>aa</u>-ahumu <b>a</b>l\'ilmu baghyan baynahum inna rabbaka yaq<u>dh</u>ii baynahum yawma <b>a</b>lqiy<u>aa</u>mati fiim<u>aa</u> k<u>aa</u>nuu fiihi yakhtalifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4491, '45:18', 45, 18, ' tsumma ja\'aln<u>aa</u>ka \'al<u>aa</u> syarii\'atin mina <b>a</b>l-amri fa<b>i</b>ttabi\'h<u>aa</u> wal<u>aa</u> tattabi\' ahw<u>aa</u>-a <b>al</b>la<u>dz</u>iina l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4492, '45:19', 45, 19, ' innahum lan yughnuu \'anka mina <b>al</b>l<u>aa</u>hi syay-an wa-inna <b>al</b><u>zhzhaa</u>limiina ba\'<u>dh</u>uhum awliy<u>aa</u>u ba\'<u>dh</u>in wa<b>al</b>l<u>aa</u>hu waliyyu <b>a</b>lmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4493, '45:20', 45, 20, ' h<u>aadzaa</u> ba<u>shaa</u>-iru li<b>l</b>nn<u>aa</u>si wahudan wara<u>h</u>matun liqawmin yuuqinuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4494, '45:21', 45, 21, ' am <u>h</u>asiba <b>al</b>la<u>dz</u>iina ijtara<u>h</u>uu <b>al</b>ssayyi-<u>aa</u>ti an naj\'alahum ka<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti saw<u>aa</u>-an ma<u>h</u>y<u>aa</u>hum wamam<u>aa</u>tuhum s<u>aa</u>-a m<u>aa</u> ya<u>h</u>kumuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4495, '45:22', 45, 22, ' wakhalaqa <b>al</b>l<u>aa</u>hu <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a bi<b>a</b>l<u>h</u>aqqi walitujz<u>aa</u> kullu nafsin bim<u>aa</u> kasabat wahum l<u>aa</u> yu<u>zh</u>lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4496, '45:23', 45, 23, ' afara-ayta mani ittakha<u>dz</u>a il<u>aa</u>hahu haw<u>aa</u>hu wa-a<u>dh</u>allahu <b>al</b>l<u>aa</u>hu \'al<u>aa</u> \'ilmin wakhatama \'al<u>aa</u> sam\'ihi waqalbihi waja\'ala \'al<u>aa</u> ba<u>sh</u>arihi ghisy<u>aa</u>watan faman yahdiihi min ba\'di <b>al</b>l<u>aa</u>hi afal<u>aa</u> ta<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4497, '45:24', 45, 24, ' waq<u>aa</u>luu m<u>aa</u> hiya ill<u>aa</u> <u>h</u>ay<u>aa</u>tun<u>aa</u> <b>al</b>dduny<u>aa</u> namuutu wana<u>h</u>y<u>aa</u> wam<u>aa</u> yuhlikun<u>aa</u> ill<u>aa</u> <b>al</b>ddahru wam<u>aa</u> lahum bi<u>dzaa</u>lika min \'ilmin in hum ill<u>aa</u> ya<u>zh</u>unnuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4498, '45:25', 45, 25, ' wa-i<u>dzaa</u> tutl<u>aa</u> \'alayhim <u>aa</u>y<u>aa</u>tun<u>aa</u> bayyin<u>aa</u>tin m<u>aa</u> k<u>aa</u>na <u>h</u>ujjatahum ill<u>aa</u> an q<u>aa</u>luu i/tuu bi-<u>aa</u>b<u>aa</u>-in<u>aa</u> in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4499, '45:26', 45, 26, ' quli <b>al</b>l<u>aa</u>hu yu<u>h</u>yiikum tsumma yumiitukum tsumma yajma\'ukum il<u>aa</u> yawmi <b>a</b>lqiy<u>aa</u>mati l<u>aa</u> rayba fiihi wal<u>aa</u>kinna aktsara <b>al</b>nn<u>aa</u>si l<u>aa</u> ya\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4500, '45:27', 45, 27, ' walill<u>aa</u>hi mulku <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wayawma taquumu <b>al</b>ss<u>aa</u>\'atu yawma-i<u>dz</u>in yakhsaru <b>a</b>lmub<u>th</u>iluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4501, '45:28', 45, 28, ' watar<u>aa</u> kulla ummatin j<u>aa</u>tsiyatan kullu ummatin tud\'<u>aa</u> il<u>aa</u> kit<u>aa</u>bih<u>aa</u> <b>a</b>lyawma tujzawna m<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4502, '45:29', 45, 29, ' h<u>aadzaa</u> kit<u>aa</u>bun<u>aa</u> yan<u>th</u>iqu \'alaykum bi<b>a</b>l<u>h</u>aqqi inn<u>aa</u> kunn<u>aa</u> nastansikhu m<u>aa</u> kuntum ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4503, '45:30', 45, 30, ' fa-amm<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti fayudkhiluhum rabbuhum fii ra<u>h</u>matihi <u>dzaa</u>lika huwa <b>a</b>lfawzu <b>a</b>lmubiin<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (4504, '45:31', 45, 31, ' wa-amm<u>aa</u> <b>al</b>la<u>dz</u>iina kafaruu afalam takun <u>aa</u>y<u>aa</u>tii tutl<u>aa</u> \'alaykum fa<b>i</b>stakbartum wakuntum qawman mujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4505, '45:32', 45, 32, ' wa-i<u>dzaa</u> qiila inna wa\'da <b>al</b>l<u>aa</u>hi <u>h</u>aqqun wa<b>al</b>ss<u>aa</u>\'atu l<u>aa</u> rayba fiih<u>aa</u> qultum m<u>aa</u> nadrii m<u>aa</u> <b>al</b>ss<u>aa</u>\'atu in na<u>zh</u>unnu ill<u>aa</u> <u>zh</u>annan wam<u>aa</u> na<u>h</u>nu bimustayqiniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4506, '45:33', 45, 33, ' wabad<u>aa</u> lahum sayyi-<u>aa</u>tu m<u>aa</u> \'amiluu wa<u>haa</u>qa bihim m<u>aa</u> k<u>aa</u>nuu bihi yastahzi-uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4507, '45:34', 45, 34, ' waqiila <b>a</b>lyawma nans<u>aa</u>kum kam<u>aa</u> nasiitum liq<u>aa</u>-a yawmikum h<u>aadzaa</u> wama/w<u>aa</u>kumu <b>al</b>nn<u>aa</u>ru wam<u>aa</u> lakum min n<u>aas</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4508, '45:35', 45, 35, ' <u>dzaa</u>likum bi-annakumu ittakha<u>dz</u>tum <u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi huzuwan wagharratkumu <b>a</b>l<u>h</u>ay<u>aa</u>tu <b>al</b>dduny<u>aa</u> fa<b>a</b>lyawma l<u>aa</u> yukhrajuuna minh<u>aa</u> wal<u>aa</u> hum yusta\'tabuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4509, '45:36', 45, 36, ' falill<u>aa</u>hi <b>a</b>l<u>h</u>amdu rabbi <b>al</b>ssam<u>aa</u>w<u>aa</u>ti warabbi <b>a</b>l-ar<u>dh</u>i rabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4510, '45:37', 45, 37, ' walahu <b>a</b>lkibriy<u>aa</u>u fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wahuwa <b>a</b>l\'aziizu <b>a</b>l<u>h</u>akiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (4511, '46:1', 46, 1, ' <u>haa</u>-miim</u>\r\r\n');
INSERT INTO quranLatin VALUES (4512, '46:2', 46, 2, ' tanziilu <b>a</b>lkit<u>aa</u>bi mina <b>al</b>l<u>aa</u>hi <b>a</b>l\'aziizi <b>a</b>l<u>h</u>akiim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4513, '46:3', 46, 3, ' m<u>aa</u> khalaqn<u>aa</u> <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a wam<u>aa</u> baynahum<u>aa</u> ill<u>aa</u> bi<b>a</b>l<u>h</u>aqqi wa-ajalin musamman wa<b>a</b>lla<u>dz</u>iina kafaruu \'amm<u>aa</u> un<u>dz</u>iruu mu\'ri<u>dh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4514, '46:4', 46, 4, ' qul ara-aytum m<u>aa</u> tad\'uuna min duuni <b>al</b>l<u>aa</u>hi aruunii m<u>aatsaa</u> khalaquu mina <b>a</b>l-ar<u>dh</u>i am lahum syirkun fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti iituunii bikit<u>aa</u>bin min qabli h<u>aadzaa</u> aw ats<u>aa</u>ratin min \'ilmin in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4515, '46:5', 46, 5, ' waman a<u>dh</u>allu mimman yad\'uu min duuni <b>al</b>l<u>aa</u>hi man l<u>aa</u> yastajiibu lahu il<u>aa</u> yawmi <b>a</b>lqiy<u>aa</u>mati wahum \'an du\'<u>aa</u>-ihim gh<u>aa</u>filuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4516, '46:6', 46, 6, ' wa-i<u>dzaa</u> <u>h</u>usyira <b>al</b>nn<u>aa</u>su k<u>aa</u>nuu lahum a\'d<u>aa</u>-an wak<u>aa</u>nuu bi\'ib<u>aa</u>datihim k<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4517, '46:7', 46, 7, ' wa-i<u>dzaa</u> tutl<u>aa</u> \'alayhim <u>aa</u>y<u>aa</u>tun<u>aa</u> bayyin<u>aa</u>tin q<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu lil<u>h</u>aqqi lamm<u>aa</u> j<u>aa</u>-ahum h<u>aadzaa</u> si<u>h</u>run mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4518, '46:8', 46, 8, ' am yaquuluuna iftar<u>aa</u>hu qul ini iftaraytuhu fal<u>aa</u> tamlikuuna lii mina <b>al</b>l<u>aa</u>hi syay-an huwa a\'lamu bim<u>aa</u> tufii<u>dh</u>uuna fiihi kaf<u>aa</u> bihi syahiidan baynii wabaynakum wahuwa <b>a</b>lghafuuru <b>al</b>rra<u>h</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (4519, '46:9', 46, 9, ' qul m<u>aa</u> kuntu bid\'an mina <b>al</b>rrusuli wam<u>aa</u> adrii m<u>aa</u> yuf\'alu bii wal<u>aa</u> bikum in attabi\'u ill<u>aa</u> m<u>aa</u> yuu<u>haa</u> ilayya wam<u>aa</u> an<u>aa</u> ill<u>aa</u> na<u>dz</u>iirun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4520, '46:10', 46, 10, ' qul ara-aytum in k<u>aa</u>na min \'indi <b>al</b>l<u>aa</u>hi wakafartum bihi wasyahida sy<u>aa</u>hidun min banii isr<u>aa</u>-iila \'al<u>aa</u> mitslihi fa<u>aa</u>mana wa<b>i</b>stakbartum inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yahdii <b>a</b>lqawma <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4521, '46:11', 46, 11, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu lilla<u>dz</u>iina <u>aa</u>manuu law k<u>aa</u>na khayran m<u>aa</u> sabaquun<u>aa</u> ilayhi wa-i<u>dz</u> lam yahtaduu bihi fasayaquuluuna h<u>aadzaa</u> ifkun qadiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4522, '46:12', 46, 12, ' wamin qablihi kit<u>aa</u>bu muus<u>aa</u> im<u>aa</u>man wara<u>h</u>matan wah<u>aadzaa</u> kit<u>aa</u>bun mu<u>sh</u>addiqun lis<u>aa</u>nan \'arabiyyan liyun<u>dz</u>ira <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu wabusyr<u>aa</u> lilmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4523, '46:13', 46, 13, ' inna <b>al</b>la<u>dz</u>iina q<u>aa</u>luu rabbun<u>aa</u> <b>al</b>l<u>aa</u>hu tsumma istaq<u>aa</u>muu fal<u>aa</u> khawfun \'alayhim wal<u>aa</u> hum ya<u>h</u>zanuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4524, '46:14', 46, 14, ' ul<u>aa</u>-ika a<u>sh</u>-<u>haa</u>bu <b>a</b>ljannati kh<u>aa</u>lidiina fiih<u>aa</u> jaz<u>aa</u>-an bim<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4525, '46:15', 46, 15, ' wawa<u>shsh</u>ayn<u>aa</u> <b>a</b>l-ins<u>aa</u>na biw<u>aa</u>lidayhi i<u>h</u>s<u>aa</u>nan <u>h</u>amalat-hu ummuhu kurhan wawa<u>dh</u>a\'at-hu kurhan wa<u>h</u>amluhu wafi<u>shaa</u>luhu tsal<u>aa</u>tsuuna syahran <u>h</u>att<u>aa</u> i<u>dzaa</u> balagha asyuddahu wabalagha arba\'iina sanatan q<u>aa</u>la rabbi awzi\'nii an asykura ni\'mataka <b>al</b>latii an\'amta \'alayya wa\'al<u>aa</u> w<u>aa</u>lidayya wa-an a\'mala <u>shaa</u>li<u>h</u>an tar<u>daa</u>hu wa-a<u>sh</u>li<u>h</u> lii fii <u>dz</u>urriyyatii innii tubtu ilayka wa-innii mina <b>a</b>lmuslimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4526, '46:16', 46, 16, ' ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina nataqabbalu \'anhum a<u>h</u>sana m<u>aa</u> \'amiluu wanataj<u>aa</u>wazu \'an sayyi-<u>aa</u>tihim fii a<u>sh</u>-<u>haa</u>bi <b>a</b>ljannati wa\'da <b>al</b><u>shsh</u>idqi <b>al</b>la<u>dz</u>ii k<u>aa</u>nuu yuu\'aduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4527, '46:17', 46, 17, ' wa<b>a</b>lla<u>dz</u>ii q<u>aa</u>la liw<u>aa</u>lidayhi uffin lakum<u>aa</u> ata\'id<u>aa</u>ninii an ukhraja waqad khalati <b>a</b>lquruunu min qablii wahum<u>aa</u> yastaghiits<u>aa</u>ni <b>al</b>l<u>aa</u>ha waylaka <u>aa</u>min inna wa\'da <b>al</b>l<u>aa</u>hi <u>h</u>aqqun fayaquulu m<u>aa</u> h<u>aadzaa</u> ill<u>aa</u> as<u>aath</u>iiru <b>a</b>l-awwaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4528, '46:18', 46, 18, ' ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina <u>h</u>aqqa \'alayhimu <b>a</b>lqawlu fii umamin qad khalat min qablihim mina <b>a</b>ljinni wa<b>a</b>l-insi innahum k<u>aa</u>nuu kh<u>aa</u>siriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4529, '46:19', 46, 19, ' walikullin daraj<u>aa</u>tun mimm<u>aa</u> \'amiluu waliyuwaffiyahum a\'m<u>aa</u>lahum wahum l<u>aa</u> yu<u>zh</u>lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4530, '46:20', 46, 20, ' wayawma yu\'ra<u>dh</u>u <b>al</b>la<u>dz</u>iina kafaruu \'al<u>aa</u> <b>al</b>nn<u>aa</u>ri a<u>dz</u>habtum <u>th</u>ayyib<u>aa</u>tikum fii <u>h</u>ay<u>aa</u>tikumu <b>al</b>dduny<u>aa</u> wa<b>i</b>stamta\'tum bih<u>aa</u> fa<b>a</b>lyawma tujzawna \'a<u>dzaa</u>ba <b>a</b>lhuuni bim<u>aa</u> kuntum tastakbiruuna fii <b>a</b>l-ar<u>dh</u>i bighayri <b>a</b>l<u>h</u>aqqi wabim<u>aa</u> kuntum tafsuquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4531, '46:21', 46, 21, ' wa<b>u</b><u>dz</u>kur akh<u>aa</u> \'<u>aa</u>din i<u>dz</u> an<u>dz</u>ara qawmahu bi<b>a</b>l-a<u>h</u>q<u>aa</u>fi waqad khalati <b>al</b>nnu<u>dz</u>uru min bayni yadayhi wamin khalfihi <b>al</b>l<u>aa</u> ta\'buduu ill<u>aa</u> <b>al</b>l<u>aa</u>ha innii akh<u>aa</u>fu \'alaykum \'a<u>dzaa</u>ba yawmin \'a<u>zh</u>iim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4532, '46:22', 46, 22, ' q<u>aa</u>luu aji/tan<u>aa</u> lita/fikan<u>aa</u> \'an <u>aa</u>lihatin<u>aa</u> fa/tin<u>aa</u> bim<u>aa</u> ta\'idun<u>aa</u> in kunta mina <b>al</b><u>shshaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4533, '46:23', 46, 23, ' q<u>aa</u>la innam<u>aa</u> <b>a</b>l\'ilmu \'inda <b>al</b>l<u>aa</u>hi wauballighukum m<u>aa</u> ursiltu bihi wal<u>aa</u>kinnii ar<u>aa</u>kum qawman tajhaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4534, '46:24', 46, 24, ' falamm<u>aa</u> ra-awhu \'<u>aa</u>ri<u>dh</u>an mustaqbila awdiyatihim q<u>aa</u>luu h<u>aadzaa</u> \'<u>aa</u>ri<u>dh</u>un mum<u>th</u>irun<u>aa</u> bal huwa m<u>aa</u> ista\'jaltum bihi rii<u>h</u>un fiih<u>aa</u> \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4535, '46:25', 46, 25, ' tudammiru kulla syay-in bi-amri rabbih<u>aa</u> fa-a<u>sh</u>ba<u>h</u>uu l<u>aa</u> yur<u>aa</u> ill<u>aa</u> mas<u>aa</u>kinuhum ka<u>dzaa</u>lika najzii <b>a</b>lqawma <b>a</b>lmujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4536, '46:26', 46, 26, ' walaqad makkann<u>aa</u>hum fiim<u>aa</u> in makkann<u>aa</u>kum fiihi waja\'aln<u>aa</u> lahum sam\'an wa-ab<u>shaa</u>ran wa-af-idatan fam<u>aa</u> aghn<u>aa</u> \'anhum sam\'uhum wal<u>aa</u> ab<u>shaa</u>ruhum wal<u>aa</u> af-idatuhum min syay-in i<u>dz</u> k<u>aa</u>nuu yaj<u>h</u>aduuna bi-<u>aa</u>y<u>aa</u>ti <b>al</b>l<u>aa</u>hi wa<u>haa</u>qa bihim m<u>aa</u> k<u>aa</u>nuu bihi yastahzi-uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4537, '46:27', 46, 27, ' walaqad ahlakn<u>aa</u> m<u>aa</u> <u>h</u>awlakum mina <b>a</b>lqur<u>aa</u> wa<u>sh</u>arrafn<u>aa</u> <b>a</b>l-<u>aa</u>y<u>aa</u>ti la\'allahum yarji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4538, '46:28', 46, 28, ' falawl<u>aa</u> na<u>sh</u>arahumu <b>al</b>la<u>dz</u>iina ittakha<u>dz</u>uu min duuni <b>al</b>l<u>aa</u>hi qurb<u>aa</u>nan <u>aa</u>lihatan bal <u>dh</u>alluu \'anhum wa<u>dzaa</u>lika ifkuhum wam<u>aa</u> k<u>aa</u>nuu yaftaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4539, '46:29', 46, 29, ' wa-i<u>dz</u> <u>sh</u>arafn<u>aa</u> ilayka nafaran mina <b>a</b>ljinni yastami\'uuna <b>a</b>lqur-<u>aa</u>na falamm<u>aa</u> <u>h</u>a<u>dh</u>aruuhu q<u>aa</u>luu an<u>sh</u>ituu falamm<u>aa</u> qu<u>dh</u>iya wallaw il<u>aa</u> qawmihim mun<u>dz</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4540, '46:30', 46, 30, ' q<u>aa</u>luu y<u>aa</u> qawman<u>aa</u> inn<u>aa</u> sami\'n<u>aa</u> kit<u>aa</u>ban unzila min ba\'di muus<u>aa</u> mu<u>sh</u>addiqan lim<u>aa</u> bayna yadayhi yahdii il<u>aa</u> <b>a</b>l<u>h</u>aqqi wa-il<u>aa</u> <u>th</u>ariiqin mustaqiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4541, '46:31', 46, 31, ' y<u>aa</u> qawman<u>aa</u> ajiibuu d<u>aa</u>\'iya <b>al</b>l<u>aa</u>hi wa<u>aa</u>minuu bihi yaghfir lakum min <u>dz</u>unuubikum wayujirkum min \'a<u>dzaa</u>bin <b>a</b>liim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4542, '46:32', 46, 32, ' waman l<u>aa</u> yujib d<u>aa</u>\'iya <b>al</b>l<u>aa</u>hi falaysa bimu\'jizin fii <b>a</b>l-ar<u>dh</u>i walaysa lahu min duunihi awliy<u>aa</u>-a ul<u>aa</u>-ika fii <u>dh</u>al<u>aa</u>lin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4543, '46:33', 46, 33, ' awa lam yaraw anna <b>al</b>l<u>aa</u>ha <b>al</b>la<u>dz</u>ii khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a walam ya\'ya bikhalqihinna biq<u>aa</u>dirin \'al<u>aa</u> an yu<u>h</u>yiya <b>a</b>lmawt<u>aa</u> bal<u>aa</u> innahu \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4544, '46:34', 46, 34, ' wayawma yu\'ra<u>dh</u>u <b>al</b>la<u>dz</u>iina kafaruu \'al<u>aa</u> <b>al</b>nn<u>aa</u>ri alaysa h<u>aadzaa</u> bi<b>a</b>l<u>h</u>aqqi q<u>aa</u>luu bal<u>aa</u> warabbin<u>aa</u> q<u>aa</u>la fa<u>dz</u>uuquu al\'a<u>dzaa</u>ba bim<u>aa</u> kuntum takfuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4545, '46:35', 46, 35, ' fa<b>i</b><u>sh</u>bir kam<u>aa</u> <u>sh</u>abara uluu <b>a</b>l\'azmi mina <b>al</b>rrusuli wal<u>aa</u> tasta\'jil lahum ka-annahum yawma yarawna m<u>aa</u> yuu\'aduuna lam yalbatsuu ill<u>aa</u> s<u>aa</u>\'atan min nah<u>aa</u>rin bal<u>aa</u>ghun fahal yuhlaku ill<u>aa</u> <b>a</b>lqawmu <b>a</b>lf<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4546, '47:1', 47, 1, ' <b>al</b>la<u>dz</u>iina kafaruu wa<u>sh</u>adduu \'an sabiili <b>al</b>l<u>aa</u>hi a<u>dh</u>alla a\'m<u>aa</u>lahum </u>\r\r\n');
INSERT INTO quranLatin VALUES (4547, '47:2', 47, 2, ' wa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti wa<u>aa</u>manuu bim<u>aa</u> nuzzila \'al<u>aa</u> mu<u>h</u>ammadin wahuwa <b>a</b>l<u>h</u>aqqu min rabbihim kaffara \'anhum sayyi-<u>aa</u>tihim wa-a<u>sh</u>la<u>h</u>a b<u>aa</u>lahum </u>\r\r\n');
INSERT INTO quranLatin VALUES (4548, '47:3', 47, 3, ' <u>dzaa</u>lika bi-anna <b>al</b>la<u>dz</u>iina kafaruu ittaba\'uu <b>a</b>lb<u>aath</u>ila wa-anna <b>al</b>la<u>dz</u>iina <u>aa</u>manuu ittaba\'uu <b>a</b>l<u>h</u>aqqa min rabbihim ka<u>dzaa</u>lika ya<u>dh</u>ribu <b>al</b>l<u>aa</u>hu li<b>l</b>nn<u>aa</u>si amts<u>aa</u>lahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4549, '47:4', 47, 4, ' fa-i<u>dzaa</u> laqiitumu <b>al</b>la<u>dz</u>iina kafaruu fa<u>dh</u>arba <b>al</b>rriq<u>aa</u>bi <u>h</u>att<u>aa</u> i<u>dzaa</u> atskhantumuuhum fasyudduu <b>a</b>lwats<u>aa</u>qa fa-imm<u>aa</u> mannan ba\'du wa-imm<u>aa</u> fid<u>aa</u>-an <u>h</u>att<u>aa</u> ta<u>dh</u>a\'a <b>a</b>l<u>h</u>arbu awz<u>aa</u>rah<u>aa</u> <u>dzaa</u>lika walaw yasy<u>aa</u>u <b>al</b>l<u>aa</u>hu la<b>i</b>nta<u>sh</u>ara minhum wal<u>aa</u>kin liyabluwa ba\'<u>dh</u>akum biba\'<u>dh</u>in wa<b>a</b>lla<u>dz</u>iina qutiluu fii sabiili <b>al</b>l<u>aa</u>hi falan yu<u>dh</u>illa a\'m<u>aa</u>lahum sayahdiihim wayu<u>sh</u>li<u>h</u>u b<u>aa</u>lahum </u>\r\r\n');
INSERT INTO quranLatin VALUES (4550, '47:5', 47, 5, ' sayahd<u>i</u>him wa yu<u>sh</u>li<u>h</u>u b<u>aa</u> lahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4551, '47:6', 47, 6, ' wayudkhiluhumu <b>a</b>ljannata \'arrafah<u>aa</u> lahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4552, '47:7', 47, 7, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu in tan<u>sh</u>uruu <b>al</b>l<u>aa</u>ha yan<u>sh</u>urkum wayutsabbit aqd<u>aa</u>makum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4553, '47:8', 47, 8, ' wa<b>a</b>lla<u>dz</u>iina kafaruu fata\'san lahum wa-a<u>dh</u>alla a\'m<u>aa</u>lahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4554, '47:9', 47, 9, ' <u>dzaa</u>lika bi-annahum karihuu m<u>aa</u> anzala <b>al</b>l<u>aa</u>hu fa-a<u>h</u>ba<u>th</u>a a\'m<u>aa</u>lahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4555, '47:10', 47, 10, ' afalam yasiiruu fii <b>a</b>l-ar<u>dh</u>i fayan<u>zh</u>uruu kayfa k<u>aa</u>na \'<u>aa</u>qibatu <b>al</b>la<u>dz</u>iina min qablihim dammara <b>al</b>l<u>aa</u>hu \'alayhim walilk<u>aa</u>firiina amts<u>aa</u>luh<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (4556, '47:11', 47, 11, ' <u>dzaa</u>lika bi-anna <b>al</b>l<u>aa</u>ha mawl<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa-anna <b>a</b>lk<u>aa</u>firiina l<u>aa</u> mawl<u>aa</u> lahum </u>\r\r\n');
INSERT INTO quranLatin VALUES (4557, '47:12', 47, 12, ' inna <b>al</b>l<u>aa</u>ha yudkhilu <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru wa<b>a</b>lla<u>dz</u>iina kafaruu yatamatta\'uuna waya/kuluuna kam<u>aa</u> ta/kulu <b>a</b>l-an\'<u>aa</u>mu wa<b>al</b>nn<u>aa</u>ru matswan lahum </u>\r\r\n');
INSERT INTO quranLatin VALUES (4558, '47:13', 47, 13, ' waka-ayyin min qaryatin hiya asyaddu quwwatan min qaryatika <b>al</b>latii akhrajatka ahlakn<u>aa</u>hum fal<u>aa</u> n<u>aas</u>ira lahum </u>\r\r\n');
INSERT INTO quranLatin VALUES (4559, '47:14', 47, 14, ' afaman k<u>aa</u>na \'al<u>aa</u> bayyinatin min rabbihi kaman zuyyina lahu suu-u \'amalihi wa<b>i</b>ttaba\'uu ahw<u>aa</u>-ahum </u>\r\r\n');
INSERT INTO quranLatin VALUES (4560, '47:15', 47, 15, ' matsalu <b>a</b>ljannati <b>al</b>latii wu\'ida <b>a</b>lmuttaquuna fiih<u>aa</u> anh<u>aa</u>run min m<u>aa</u>-in ghayri <u>aa</u>sinin wa-anh<u>aa</u>run min labanin lam yataghayyar <u>th</u>a\'muhu wa-anh<u>aa</u>run min khamrin la<u>dzdz</u>atin li<b>l</b>sysy<u>aa</u>ribiina wa-anh<u>aa</u>run min \'asalin mu<u>sh</u>affan walahum fiih<u>aa</u> min kulli <b>al</b>tstsamar<u>aa</u>ti wamaghfiratun min rabbihim kaman huwa kh<u>aa</u>lidun fii <b>al</b>nn<u>aa</u>ri wasuquu m<u>aa</u>-an <u>h</u>amiiman faqa<u>ththh</u>a\'a am\'<u>aa</u>-ahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4561, '47:16', 47, 16, ' waminhum man yastami\'u ilayka <u>h</u>att<u>aa</u> i<u>dzaa</u> kharajuu min \'indika q<u>aa</u>luu lilla<u>dz</u>iina uutuu <b>a</b>l\'ilma m<u>aatsaa</u> q<u>aa</u>la <u>aa</u>nifan ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina <u>th</u>aba\'a <b>al</b>l<u>aa</u>hu \'al<u>aa</u> quluubihim wa<b>i</b>ttaba\'uu ahw<u>aa</u>-ahum </u>\r\r\n');
INSERT INTO quranLatin VALUES (4562, '47:17', 47, 17, ' wa<b>a</b>lla<u>dz</u>iina ihtadaw z<u>aa</u>dahum hudan wa<u>aa</u>t<u>aa</u>hum taqw<u>aa</u>hum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4563, '47:18', 47, 18, ' fahal yan<u>zh</u>uruuna ill<u>aa</u> <b>al</b>ss<u>aa</u>\'ata an ta/tiyahum baghtatan faqad j<u>aa</u>-a asyr<u>aath</u>uh<u>aa</u> fa-ann<u>aa</u> lahum i<u>dzaa</u> j<u>aa</u>-at-hum <u>dz</u>ikr<u>aa</u>hum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4564, '47:19', 47, 19, ' fa<b>i</b>\'lam annahu l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> <b>al</b>l<u>aa</u>hu wa<b>i</b>staghfir li<u>dz</u>anbika walilmu/miniina wa<b>a</b>lmu/min<u>aa</u>ti wa<b>al</b>l<u>aa</u>hu ya\'lamu mutaqallabakum wamatsw<u>aa</u>kum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4565, '47:20', 47, 20, ' wayaquulu <b>al</b>la<u>dz</u>iina <u>aa</u>manuu lawl<u>aa</u> nuzzilat suuratun fa-i<u>dzaa</u> unzilat suuratun mu<u>h</u>kamatun wa<u>dz</u>ukira fiih<u>aa</u> <b>a</b>lqit<u>aa</u>lu ra-ayta <b>al</b>la<u>dz</u>iina fii quluubihim mara<u>dh</u>un yan<u>zh</u>uruuna ilayka na<u>zh</u>ara <b>a</b>lmaghsyiyyi \'alayhi mina <b>a</b>lmawti fa-awl<u>aa</u> lahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4566, '47:21', 47, 21, ' <u>th</u><u>aa</u>\'atun waqawlun ma\'ruufun fa-i<u>dzaa</u> \'azama <b>a</b>l-amru falaw <u>sh</u>adaquu <b>al</b>l<u>aa</u>ha lak<u>aa</u>na khayran lahum </u>\r\r\n');
INSERT INTO quranLatin VALUES (4567, '47:22', 47, 22, ' fahal \'asaytum in tawallaytum an tufsiduu fii <b>a</b>l-ar<u>dh</u>i watuqa<u>ththh</u>i\'uu ar<u>haa</u>makum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4568, '47:23', 47, 23, ' ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina la\'anahumu <b>al</b>l<u>aa</u>hu fa-a<u>sh</u>ammahum wa-a\'m<u>aa</u> ab<u>shaa</u>rahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4569, '47:24', 47, 24, ' afal<u>aa</u> yatadabbaruuna <b>a</b>lqur-<u>aa</u>na am \'al<u>aa</u> quluubin aqf<u>aa</u>luh<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (4570, '47:25', 47, 25, ' inna <b>al</b>la<u>dz</u>iina irtadduu \'al<u>aa</u> adb<u>aa</u>rihim min ba\'di m<u>aa</u> tabayyana lahumu <b>a</b>lhud<u>aa</u> <b>al</b>sysyay<u>th</u><u>aa</u>nu sawwala lahum wa-aml<u>aa</u> lahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4571, '47:26', 47, 26, ' <u>dzaa</u>lika bi-annahum q<u>aa</u>luu lilla<u>dz</u>iina karihuu m<u>aa</u> nazzala <b>al</b>l<u>aa</u>hu sanu<u>th</u>ii\'ukum fii ba\'<u>dh</u>i <b>a</b>l-amri wa<b>al</b>l<u>aa</u>hu ya\'lamu isr<u>aa</u>rahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4572, '47:27', 47, 27, ' fakayfa i<u>dzaa</u> tawaffat-humu <b>a</b>lmal<u>aa</u>-ikatu ya<u>dh</u>ribuuna wujuuhahum wa-adb<u>aa</u>rahum </u>\r\r\n');
INSERT INTO quranLatin VALUES (4573, '47:28', 47, 28, ' <u>dzaa</u>lika bi-annahumu ittaba\'uu m<u>aa</u> askha<u>th</u>a <b>al</b>l<u>aa</u>ha wakarihuu ri<u>dh</u>w<u>aa</u>nahu fa-a<u>h</u>ba<u>th</u>a a\'m<u>aa</u>lahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4574, '47:29', 47, 29, ' am <u>h</u>asiba <b>al</b>la<u>dz</u>iina fii quluubihim mara<u>dh</u>un an lan yukhrija <b>al</b>l<u>aa</u>hu a<u>dh</u>gh<u>aa</u>nahum </u>\r\r\n');
INSERT INTO quranLatin VALUES (4575, '47:30', 47, 30, ' walaw nasy<u>aa</u>u la-arayn<u>aa</u>kahum fala\'araftahum bisiim<u>aa</u>hum walata\'rifannahum fii la<u>h</u>ni <b>a</b>lqawli wa<b>al</b>l<u>aa</u>hu ya\'lamu a\'m<u>aa</u>lakum </u>\r\r\n');
INSERT INTO quranLatin VALUES (4576, '47:31', 47, 31, ' walanabluwannakum <u>h</u>att<u>aa</u> na\'lama <b>a</b>lmuj<u>aa</u>hidiina minkum wa<b>al</b><u>shshaa</u>biriina wanabluwa akhb<u>aa</u>rakum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4577, '47:32', 47, 32, ' inna <b>al</b>la<u>dz</u>iina kafaruu wa<u>sh</u>adduu \'an sabiili <b>al</b>l<u>aa</u>hi wasy<u>aa</u>qquu <b>al</b>rrasuula min ba\'di m<u>aa</u> tabayyana lahumu <b>a</b>lhud<u>aa</u> lan ya<u>dh</u>uruu <b>al</b>l<u>aa</u>ha syay-an wasayu<u>h</u>bi<u>th</u>u a\'m<u>aa</u>lahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4578, '47:33', 47, 33, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu a<u>th</u>ii\'uu <b>al</b>l<u>aa</u>ha wa-a<u>th</u>ii\'uu <b>al</b>rrasuula wal<u>aa</u> tub<u>th</u>iluu a\'m<u>aa</u>lakum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4579, '47:34', 47, 34, ' inna <b>al</b>la<u>dz</u>iina kafaruu wa<u>sh</u>adduu \'an sabiili <b>al</b>l<u>aa</u>hi tsumma m<u>aa</u>tuu wahum kuff<u>aa</u>run falan yaghfira <b>al</b>l<u>aa</u>hu lahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4580, '47:35', 47, 35, ' fal<u>aa</u> tahinuu watad\'uu il<u>aa</u> <b>al</b>ssalmi wa-antumu <b>a</b>l-a\'lawna wa<b>al</b>l<u>aa</u>hu ma\'akum walan yatirakum a\'m<u>aa</u>lakum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4581, '47:36', 47, 36, ' innam<u>aa</u> <b>a</b>l<u>h</u>ay<u>aa</u>tu <b>al</b>dduny<u>aa</u> la\'ibun walahwun wa-in tu/minuu watattaquu yu/tikum ujuurakum wal<u>aa</u> yas-alkum amw<u>aa</u>lakum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4582, '47:37', 47, 37, ' in yas-alkumuuh<u>aa</u> fayu<u>h</u>fikum tabkhaluu wayukhrij a<u>dh</u>gh<u>aa</u>nakum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4583, '47:38', 47, 38, ' h<u>aa</u> antum h<u>aa</u>ul<u>aa</u>-i tud\'awna litunfiquu fii sabiili <b>al</b>l<u>aa</u>hi faminkum man yabkhalu waman yabkhal fa-innam<u>aa</u> yabkhalu \'an nafsihi wa<b>al</b>l<u>aa</u>hu <b>a</b>lghaniyyu wa-antumu <b>a</b>lfuqar<u>aa</u>u wa-in tatawallaw yastabdil qawman ghayrakum tsumma l<u>aa</u> yakuunuu amts<u>aa</u>lakum</u>\r\r\n');
INSERT INTO quranLatin VALUES (4584, '48:1', 48, 1, ' inn<u>aa</u> fata<u>h</u>n<u>aa</u> laka fat<u>h</u>an mubiin<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4585, '48:2', 48, 2, ' liyaghfira laka <b>al</b>l<u>aa</u>hu m<u>aa</u> taqaddama min <u>dz</u>anbika wam<u>aa</u> ta-akhkhara wayutimma ni\'matahu \'alayka wayahdiyaka <u>sh</u>ir<u>aath</u>an mustaqiim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4586, '48:3', 48, 3, ' wayan<u>sh</u>uraka <b>al</b>l<u>aa</u>hu na<u>sh</u>ran \'aziiz<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4587, '48:4', 48, 4, ' huwa <b>al</b>la<u>dz</u>ii anzala <b>al</b>ssakiinata fii quluubi <b>a</b>lmu/miniina liyazd<u>aa</u>duu iim<u>aa</u>nan ma\'a iim<u>aa</u>nihim walill<u>aa</u>hi junuudu <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wak<u>aa</u>na <b>al</b>l<u>aa</u>hu \'aliiman <u>h</u>akiim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4588, '48:5', 48, 5, ' liyudkhila <b>a</b>lmu/miniina wa<b>a</b>lmu/min<u>aa</u>ti jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru kh<u>aa</u>lidiina fiih<u>aa</u> wayukaffira \'anhum sayyi-<u>aa</u>tihim wak<u>aa</u>na <u>dzaa</u>lika \'inda <b>al</b>l<u>aa</u>hi fawzan \'a<u>zh</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4589, '48:6', 48, 6, ' wayu\'a<u>dzdz</u>iba <b>a</b>lmun<u>aa</u>fiqiina wa<b>a</b>lmun<u>aa</u>fiq<u>aa</u>ti wa<b>a</b>lmusyrikiina wa<b>a</b>lmusyrik<u>aa</u>ti <b>al</b><u>zhzhaa</u>nniina bi<b>al</b>l<u>aa</u>hi <u>zh</u>anna <b>al</b>ssaw-i \'alayhim d<u>aa</u>-iratu <b>al</b>ssaw-i wagha<u>dh</u>iba <b>al</b>l<u>aa</u>hu \'alayhim wala\'anahum wa-a\'adda lahum jahannama was<u>aa</u>-at ma<u>sh</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4590, '48:7', 48, 7, ' walill<u>aa</u>hi junuudu <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wak<u>aa</u>na <b>al</b>l<u>aa</u>hu \'aziizan <u>h</u>akiim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4591, '48:8', 48, 8, ' inn<u>aa</u> arsaln<u>aa</u>ka sy<u>aa</u>hidan wamubasysyiran wana<u>dz</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4592, '48:9', 48, 9, ' litu/minuu bi<b>al</b>l<u>aa</u>hi warasuulihi watu\'azziruuhu watuwaqqiruuhu watusabbi<u>h</u>uuhu bukratan wa-a<u>sh</u>iil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4593, '48:10', 48, 10, ' inna <b>al</b>la<u>dz</u>iina yub<u>aa</u>yi\'uunaka innam<u>aa</u> yub<u>aa</u>yi\'uuna <b>al</b>l<u>aa</u>ha yadu <b>al</b>l<u>aa</u>hi fawqa aydiihim faman nakatsa fa-innam<u>aa</u> yankutsu \'al<u>aa</u> nafsihi waman awf<u>aa</u> bim<u>aa</u> \'<u>aa</u>hada \'alayhu <b>al</b>l<u>aa</u>ha fasayu/tiihi ajran \'a<u>zh</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4594, '48:11', 48, 11, ' sayaquulu laka <b>a</b>lmukhallafuuna mina <b>a</b>l-a\'r<u>aa</u>bi syaghalatn<u>aa</u> amw<u>aa</u>lun<u>aa</u> wa-ahluun<u>aa</u> fa<b>i</b>staghfir lan<u>aa</u> yaquuluuna bi-alsinatihim m<u>aa</u> laysa fii quluubihim qul faman yamliku lakum mina <b>al</b>l<u>aa</u>hi syay-an in ar<u>aa</u>da bikum <u>dh</u>arran aw ar<u>aa</u>da bikum naf\'an bal k<u>aa</u>na <b>al</b>l<u>aa</u>hu bim<u>aa</u> ta\'maluuna khabiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4595, '48:12', 48, 12, ' bal <u>zh</u>anantum an lan yanqaliba <b>al</b>rrasuulu wa<b>a</b>lmu/minuuna il<u>aa</u> ahliihim abadan wazuyyina <u>dzaa</u>lika fii quluubikum wa<u>zh</u>anantum <u>zh</u>anna <b>al</b>ssaw-i wakuntum qawman buur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4596, '48:13', 48, 13, ' waman lam yu/min bi<b>al</b>l<u>aa</u>hi warasuulihi fa-inn<u>aa</u> a\'tadn<u>aa</u> lilk<u>aa</u>firiina sa\'iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4597, '48:14', 48, 14, ' walill<u>aa</u>hi mulku <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i yaghfiru liman yasy<u>aa</u>u wayu\'a<u>dzdz</u>ibu man yasy<u>aa</u>u wak<u>aa</u>na <b>al</b>l<u>aa</u>hu ghafuuran ra<u>h</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4598, '48:15', 48, 15, ' sayaquulu <b>a</b>lmukhallafuuna i<u>dzaa</u> in<u>th</u>alaqtum il<u>aa</u> magh<u>aa</u>nima lita/khu<u>dz</u>uuh<u>aa</u> <u>dz</u>aruun<u>aa</u> nattabi\'kum yuriiduuna an yubaddiluu kal<u>aa</u>ma <b>al</b>l<u>aa</u>hi qul lan tattabi\'uun<u>aa</u> ka<u>dzaa</u>likum q<u>aa</u>la <b>al</b>l<u>aa</u>hu min qablu fasayaquuluuna bal ta<u>h</u>suduunan<u>aa</u> bal k<u>aa</u>nuu l<u>aa</u> yafqahuuna ill<u>aa</u> qaliil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4599, '48:16', 48, 16, ' qul lilmukhallafiina mina <b>a</b>l-a\'r<u>aa</u>bi satud\'awna il<u>aa</u> qawmin ulii ba/sin syadiidin tuq<u>aa</u>tiluunahum aw yuslimuuna fa-in tu<u>th</u>ii\'uu yu/tikumu <b>al</b>l<u>aa</u>hu ajran <u>h</u>asanan wa-in tatawallaw kam<u>aa</u> tawallaytum min qablu yu\'a<u>dzdz</u>ibkum \'a<u>dzaa</u>ban <b>a</b>liim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4600, '48:17', 48, 17, ' laysa \'al<u>aa</u> <b>a</b>l-a\'m<u>aa</u> <u>h</u>arajun wal<u>aa</u> \'al<u>aa</u> <b>a</b>l-a\'raji <u>h</u>arajun wal<u>aa</u> \'al<u>aa</u> <b>a</b>lmarii<u>dh</u>i <u>h</u>arajun waman yu<u>th</u>i\'i <b>al</b>l<u>aa</u>ha warasuulahu yudkhilhu jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru waman yatawalla yu\'a<u>dzdz</u>ibhu \'a<u>dzaa</u>ban <b>a</b>liim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4601, '48:18', 48, 18, ' laqad ra<u>dh</u>iya <b>al</b>l<u>aa</u>hu \'ani <b>a</b>lmu/miniina i<u>dz</u> yub<u>aa</u>yi\'uunaka ta<u>h</u>ta <b>al</b>sysyajarati fa\'alima m<u>aa</u> fii quluubihim fa-anzala <b>al</b>sakiinata \'alayhim wa-ats<u>aa</u>bahum fat<u>h</u>an qariib<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4602, '48:19', 48, 19, ' wamagh<u>aa</u>nima katsiiratan ya/khu<u>dz</u>uunah<u>aa</u> wak<u>aa</u>na <b>al</b>l<u>aa</u>hu \'aziizan <u>h</u>akiim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4603, '48:20', 48, 20, ' wa\'adakumu <b>al</b>l<u>aa</u>hu magh<u>aa</u>nima katsiiratan ta/khu<u>dz</u>uunah<u>aa</u> fa\'ajjala lakum h<u>aadz</u>ihi wakaffa aydiya <b>al</b>nn<u>aa</u>si \'ankum walitakuuna <u>aa</u>yatan lilmu/miniina wayahdiyakum <u>sh</u>ir<u>aath</u>an mustaqiim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4604, '48:21', 48, 21, ' waukhr<u>aa</u> lam taqdiruu \'alayh<u>aa</u> qad a<u>hath</u>a <b>al</b>l<u>aa</u>hu bih<u>aa</u> wak<u>aa</u>na <b>al</b>l<u>aa</u>hu \'al<u>aa</u> kulli syay-in qadiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4605, '48:22', 48, 22, ' walaw q<u>aa</u>talakumu <b>al</b>la<u>dz</u>iina kafaruu lawallawuu <b>a</b>l-adb<u>aa</u>ra tsumma l<u>aa</u> yajiduuna waliyyan wal<u>aa</u> na<u>sh</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4606, '48:23', 48, 23, ' sunnata <b>al</b>l<u>aa</u>hi <b>al</b>latii qad khalat min qablu walan tajida lisunnati <b>al</b>l<u>aa</u>hi tabdiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4607, '48:24', 48, 24, ' wahuwa <b>al</b>la<u>dz</u>ii kaffa aydiyahum \'ankum wa-aydiyakum \'anhum biba<u>th</u>ni makkata min ba\'di an a<u>zh</u>farakum \'alayhim wak<u>aa</u>na <b>al</b>l<u>aa</u>hu bim<u>aa</u> ta\'maluuna ba<u>sh</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4608, '48:25', 48, 25, ' humu <b>al</b>la<u>dz</u>iina kafaruu wa<u>sh</u>adduukum \'ani <b>a</b>lmasjidi <b>a</b>l<u>h</u>ar<u>aa</u>mi wa<b>a</b>lhadya ma\'kuufan an yablugha ma<u>h</u>illahu walawl<u>aa</u> rij<u>aa</u>lun mu/minuuna wanis<u>aa</u>un mu/min<u>aa</u>tun lam ta\'lamuuhum an ta<u>th</u>auuhum fatu<u>sh</u>iibakum minhum ma\'arratun bighayri \'ilmin liyudkhila <b>al</b>l<u>aa</u>hu fii ra<u>h</u>matihi man yasy<u>aa</u>u law tazayyaluu la\'a<u>dzdz</u>abn<u>aa</u> <b>al</b>la<u>dz</u>iina kafaruu minhum \'a<u>dzaa</u>ban <b>a</b>liim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4609, '48:26', 48, 26, ' i<u>dz</u> ja\'ala <b>al</b>la<u>dz</u>iina kafaruu fii quluubihimu <b>a</b>l<u>h</u>amiyyata <u>h</u>amiyyata <b>a</b>lj<u>aa</u>hiliyyati fa-anzala <b>al</b>l<u>aa</u>hu sakiinatahu \'al<u>aa</u> rasuulihi wa\'al<u>aa</u> <b>a</b>lmu/miniina wa-alzamahum kalimata <b>al</b>ttaqw<u>aa</u> wak<u>aa</u>nuu a<u>h</u>aqqa bih<u>aa</u> wa-ahlah<u>aa</u> wak<u>aa</u>na <b>al</b>l<u>aa</u>hu bikulli syay-in \'aliim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4610, '48:27', 48, 27, ' laqad <u>sh</u>adaqa <b>al</b>l<u>aa</u>hu rasuulahu <b>al</b>rru/y<u>aa</u> bi<b>a</b>l<u>h</u>aqqi latadkhulunna <b>a</b>lmasjida <b>a</b>l<u>h</u>ar<u>aa</u>ma in sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu <u>aa</u>miniina mu<u>h</u>alliqiina ruuusakum wamuqa<u>shsh</u>iriina l<u>aa</u> takh<u>aa</u>fuuna fa\'alima m<u>aa</u> lam ta\'lamuu faja\'ala min duuni <u>dzaa</u>lika fat<u>h</u>an qariib<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4611, '48:28', 48, 28, ' huwa <b>al</b>la<u>dz</u>ii arsala rasuulahu bi<b>a</b>lhud<u>aa</u> wadiini <b>a</b>l<u>h</u>aqqi liyu<u>zh</u>hirahu \'al<u>aa</u> <b>al</b>ddiini kullihi wakaf<u>aa</u> bi<b>al</b>l<u>aa</u>hi syahiid<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4612, '48:29', 48, 29, ' mu<u>h</u>ammadun rasuulu <b>al</b>l<u>aa</u>hi wa<b>a</b>lla<u>dz</u>iina ma\'ahu asyidd<u>aa</u>u \'al<u>aa</u> <b>a</b>lkuff<u>aa</u>ri ru<u>h</u>am<u>aa</u>u baynahum tar<u>aa</u>hum rukka\'an sujjadan yabtaghuuna fa<u>dh</u>lan mina <b>al</b>l<u>aa</u>hi wari<u>dh</u>w<u>aa</u>nan siim<u>aa</u>hum fii wujuuhihim min atsari <b>al</b>ssujuudi <u>dzaa</u>lika matsaluhum fii <b>al</b>ttawr<u>aa</u>ti wamatsaluhum fii <b>a</b>l-injiili kazar\'in akhraja sya<u>th</u>-ahu fa<u>aa</u>zarahu fa<b>i</b>staghla<u>zh</u>a fa<b>i</b>staw<u>aa</u> \'al<u>aa</u> suuqihi yu\'jibu <b>al</b>zzurr<u>aa</u>\'a liyaghii<u>zh</u>a bihimu <b>a</b>lkuff<u>aa</u>ra wa\'ada <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti minhum maghfiratan wa-ajran \'a<u>zh</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4613, '49:1', 49, 1, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tuqaddimuu bayna yadayi <b>al</b>l<u>aa</u>hi warasuulihi wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha inna <b>al</b>l<u>aa</u>ha samii\'un \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4614, '49:2', 49, 2, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tarfa\'uu a<u>sh</u>w<u>aa</u>takum fawqa <u>sh</u>awti <b>al</b>nnabiyyi wal<u>aa</u> tajharuu lahu bi<b>a</b>lqawli kajahri ba\'<u>dh</u>ikum liba\'<u>dh</u>in an ta<u>h</u>ba<u>th</u>a a\'m<u>aa</u>lukum wa-antum l<u>aa</u> tasy\'uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4615, '49:3', 49, 3, ' inna <b>al</b>la<u>dz</u>iina yaghu<u>dhdh</u>uuna a<u>sh</u>w<u>aa</u>tahum \'inda rasuuli <b>al</b>l<u>aa</u>hi ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina imta<u>h</u>ana <b>al</b>l<u>aa</u>hu quluubahum li<b>l</b>ttaqw<u>aa</u> lahum maghfiratun wa-ajrun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4616, '49:4', 49, 4, ' inna <b>al</b>la<u>dz</u>iina yun<u>aa</u>duunaka min war<u>aa</u>-i <b>a</b>l<u>h</u>ujur<u>aa</u>ti aktsaruhum l<u>aa</u> ya\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4617, '49:5', 49, 5, ' walaw annahum <u>sh</u>abaruu <u>h</u>att<u>aa</u> takhruja ilayhim lak<u>aa</u>na khayran lahum wa<b>al</b>l<u>aa</u>hu ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4618, '49:6', 49, 6, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu in j<u>aa</u>-akum f<u>aa</u>siqun binaba-in fatabayyanuu an tu<u>sh</u>iibuu qawman bijah<u>aa</u>latin fatu<u>sh</u>bi<u>h</u>uu \'al<u>aa</u> m<u>aa</u> fa\'altum n<u>aa</u>dimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4619, '49:7', 49, 7, ' wa<b>i</b>\'lamuu anna fiikum rasuula <b>al</b>l<u>aa</u>hi law yu<u>th</u>ii\'ukum fii katsiirin mina <b>a</b>l-amri la\'anittum wal<u>aa</u>kinna <b>al</b>l<u>aa</u>ha <u>h</u>abbaba ilaykumu <b>a</b>l-iim<u>aa</u>na wazayyanahu fii quluubikum wakarraha ilaykumu <b>a</b>lkufra wa<b>a</b>lfusuuqa wa<b>a</b>l\'i<u>sh</u>y<u>aa</u>na ul<u>aa</u>-ika humu <b>al</b>rr<u>aa</u>syiduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4620, '49:8', 49, 8, ' fa<u>dh</u>lan mina <b>al</b>l<u>aa</u>hi wani\'matan wa<b>al</b>l<u>aa</u>hu \'aliimun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4621, '49:9', 49, 9, ' wa-in <u>th</u><u>aa</u>-ifat<u>aa</u>ni mina <b>a</b>lmu/miniina iqtataluu fa-a<u>sh</u>li<u>h</u>uu baynahum<u>aa</u> fa-in baghat i<u>h</u>d<u>aa</u>hum<u>aa</u> \'al<u>aa</u> <b>a</b>l-ukhr<u>aa</u> faq<u>aa</u>tiluu <b>al</b>latii tabghii <u>h</u>att<u>aa</u> tafii-a il<u>aa</u> amri <b>al</b>l<u>aa</u>hi fa-in f<u>aa</u>-at fa-a<u>sh</u>li<u>h</u>uu baynahum<u>aa</u> bi<b>a</b>l\'adli wa-aqsi<u>th</u>uu inna <b>al</b>l<u>aa</u>ha yu<u>h</u>ibbu <b>a</b>lmuqsi<u>th</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4622, '49:10', 49, 10, ' innam<u>aa</u> <b>a</b>lmu/minuuna ikhwatun fa-a<u>sh</u>li<u>h</u>uu bayna akhawaykum wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha la\'allakum tur<u>h</u>amuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4623, '49:11', 49, 11, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> yaskhar qawmun min qawmin \'as<u>aa</u> an yakuunuu khayran minhum wal<u>aa</u> nis<u>aa</u>un min nis<u>aa</u>-in \'as<u>aa</u> an yakunna khayran minhunna wal<u>aa</u> talmizuu anfusakum wal<u>aa</u> tan<u>aa</u>bazuu bi<b>a</b>l-alq<u>aa</u>bi bi/sa <b>a</b>l-ismu <b>a</b>lfusuuqu ba\'da <b>a</b>l-iim<u>aa</u>ni waman lam yatub faul<u>aa</u>-ika humu <b>al</b><u>zhzhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4624, '49:12', 49, 12, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu ijtanibuu katsiiran mina <b>al</b><u>zhzh</u>anni inna ba\'<u>dh</u>a <b>al</b><u>zhzh</u>anni itsmun wal<u>aa</u> tajassasuu wal<u>aa</u> yaghtab ba\'<u>dh</u>ukum ba\'<u>dh</u>an ayu<u>h</u>ibbu a<u>h</u>adukum an ya/kula la<u>h</u>ma akhiihi maytan fakarihtumuuhu wa<b>i</b>ttaquu <b>al</b>l<u>aa</u>ha inna <b>al</b>l<u>aa</u>ha taww<u>aa</u>bun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4625, '49:13', 49, 13, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su inn<u>aa</u> khalaqn<u>aa</u>kum min <u>dz</u>akarin waunts<u>aa</u> waja\'aln<u>aa</u>kum syu\'uuban waqab<u>aa</u>-ila lita\'<u>aa</u>rafuu inna akramakum \'inda <b>al</b>l<u>aa</u>hi atq<u>aa</u>kum inna <b>al</b>l<u>aa</u>ha \'aliimun khabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4626, '49:14', 49, 14, ' q<u>aa</u>lati <b>a</b>l-a\'r<u>aa</u>bu <u>aa</u>mann<u>aa</u> qul lam tu/minuu wal<u>aa</u>kin quuluu aslamn<u>aa</u> walamm<u>aa</u> yadkhuli <b>a</b>l-iim<u>aa</u>nu fii quluubikum wa-in tu<u>th</u>ii\'uu <b>al</b>l<u>aa</u>ha warasuulahu l<u>aa</u> yalitkum min a\'m<u>aa</u>likum syay-an inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4627, '49:15', 49, 15, ' innam<u>aa</u> <b>a</b>lmu/minuuna <b>al</b>la<u>dz</u>iina <u>aa</u>manuu bi<b>al</b>l<u>aa</u>hi warasuulihi tsumma lam yart<u>aa</u>buu waj<u>aa</u>haduu bi-amw<u>aa</u>lihim wa-anfusihim fii sabiili <b>al</b>l<u>aa</u>hi ul<u>aa</u>-ika humu <b>al</b><u>shshaa</u>diquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4628, '49:16', 49, 16, ' qul atu\'allimuuna <b>al</b>l<u>aa</u>ha bidiinikum wa<b>al</b>l<u>aa</u>hu ya\'lamu m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i wa<b>al</b>l<u>aa</u>hu bikulli syay-in \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4629, '49:17', 49, 17, ' yamunnuuna \'alayka an aslamuu qul l<u>aa</u> tamunnuu \'alayya isl<u>aa</u>makum bali <b>al</b>l<u>aa</u>hu yamunnu \'alaykum an had<u>aa</u>kum lil-iim<u>aa</u>ni in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4630, '49:18', 49, 18, ' inna <b>al</b>l<u>aa</u>ha ya\'lamu ghayba <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wa<b>al</b>l<u>aa</u>hu ba<u>sh</u>iirun bim<u>aa</u> ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4631, '50:1', 50, 1, ' q<u>aa</u>f wa<b>a</b>lqur-<u>aa</u>ni <b>a</b>lmajiid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4632, '50:2', 50, 2, ' bal \'ajibuu an j<u>aa</u>-ahum mun<u>dz</u>irun minhum faq<u>aa</u>la <b>a</b>lk<u>aa</u>firuuna h<u>aadzaa</u> syay-un \'ajiib<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4633, '50:3', 50, 3, ' a-i<u>dzaa</u> mitn<u>aa</u> wakunn<u>aa</u> tur<u>aa</u>ban <u>dzaa</u>lika raj\'un ba\'iid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4634, '50:4', 50, 4, ' qad \'alimn<u>aa</u> m<u>aa</u> tanqu<u>sh</u>u <b>a</b>l-ar<u>dh</u>u minhum wa\'indan<u>aa</u> kit<u>aa</u>bun <u>h</u>afii<u>zh</u><b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4635, '50:5', 50, 5, ' bal ka<u>dzdz</u>abuu bi<b>a</b>l<u>h</u>aqqi lamm<u>aa</u> j<u>aa</u>-ahum fahum fii amrin mariij<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4636, '50:6', 50, 6, ' afalam yan<u>zh</u>uruu il<u>aa</u> <b>al</b>ssam<u>aa</u>-i fawqahum kayfa banayn<u>aa</u>h<u>aa</u> wazayyann<u>aa</u>h<u>aa</u> wam<u>aa</u> lah<u>aa</u> min furuuj<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4637, '50:7', 50, 7, ' wa<b>a</b>l-ar<u>dh</u>a madadn<u>aa</u>h<u>aa</u> wa-alqayn<u>aa</u> fiih<u>aa</u> raw<u>aa</u>siya wa-anbatn<u>aa</u> fiih<u>aa</u> min kulli zawjin bahiij<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4638, '50:8', 50, 8, ' tab<u>sh</u>iratan wa<u>dz</u>ikr<u>aa</u> likulli \'abdin muniib<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4639, '50:9', 50, 9, ' wanazzaln<u>aa</u> mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an mub<u>aa</u>rakan fa-anbatn<u>aa</u> bihi jann<u>aa</u>tin wa<u>h</u>abba <b>a</b>l<u>h</u>a<u>sh</u>iid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4640, '50:10', 50, 10, ' wa<b>al</b>nnakhla b<u>aa</u>siq<u>aa</u>tin lah<u>aa</u> <u>th</u>al\'un na<u>dh</u>iid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4641, '50:11', 50, 11, ' rizqan lil\'ib<u>aa</u>di wa-a<u>h</u>yayn<u>aa</u> bihi baldatan maytan ka<u>dzaa</u>lika <b>a</b>lkhuruuj<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (4642, '50:12', 50, 12, ' ka<u>dzdz</u>abat qablahum qawmu nuu<u>h</u>in wa-a<u>sh</u>-<u>haa</u>bu <b>al</b>rrassi watsamuud<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (4643, '50:13', 50, 13, ' wa\'<u>aa</u>dun wafir\'awnu wa-ikhw<u>aa</u>nu luu<u>th</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4644, '50:14', 50, 14, ' wa-a<u>sh</u>-<u>haa</u>bu <b>a</b>l-aykati waqawmu tubba\'in kullun ka<u>dzdz</u>aba <b>al</b>rrusula fa<u>h</u>aqqa wa\'iid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4645, '50:15', 50, 15, ' afa\'ayiin<u>aa</u> bi<b>a</b>lkhalqi <b>a</b>l-awwali bal hum fii labsin min khalqin jadiid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4646, '50:16', 50, 16, ' walaqad khalaqn<u>aa</u> <b>a</b>l-ins<u>aa</u>na wana\'lamu m<u>aa</u> tuwaswisu bihi nafsuhu wana<u>h</u>nu aqrabu ilayhi min <u>h</u>abli <b>a</b>lwariid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4647, '50:17', 50, 17, ' i<u>dz</u> yatalaqq<u>aa</u> <b>a</b>lmutalaqqiy<u>aa</u>ni \'ani <b>a</b>lyamiini wa\'ani <b>al</b>sysyim<u>aa</u>li qa\'iid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4648, '50:18', 50, 18, ' m<u>aa</u> yalfi<u>zh</u>u min qawlin ill<u>aa</u> ladayhi raqiibun \'atiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4649, '50:19', 50, 19, ' waj<u>aa</u>-at sakratu <b>a</b>lmawti bi<b>a</b>l<u>h</u>aqqi <u>dzaa</u>lika m<u>aa</u> kunta minhu ta<u>h</u>iid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (4650, '50:20', 50, 20, ' wanufikha fii <b>al</b><u>shsh</u>uuri <u>dzaa</u>lika yawmu <b>a</b>lwa\'iid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4651, '50:21', 50, 21, ' waj<u>aa</u>-at kullu nafsin ma\'ah<u>aa</u> s<u>aa</u>-iqun wasyahiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4652, '50:22', 50, 22, ' laqad kunta fii ghaflatin min h<u>aadzaa</u> fakasyafn<u>aa</u> \'anka ghi<u>th</u><u>aa</u>-aka faba<u>sh</u>aruka <b>a</b>lyawma <u>h</u>adiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4653, '50:23', 50, 23, ' waq<u>aa</u>la qariinuhu h<u>aadzaa</u> m<u>aa</u> ladayya \'atiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4654, '50:24', 50, 24, ' alqiy<u>aa</u> fii jahannama kulla kaff<u>aa</u>rin \'aniid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4655, '50:25', 50, 25, ' mann<u>aa</u>\'in lilkhayri mu\'tadin muriib<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4656, '50:26', 50, 26, ' <b>al</b>la<u>dz</u>ii ja\'ala ma\'a <b>al</b>l<u>aa</u>hi il<u>aa</u>han <u>aa</u>khara fa-alqiy<u>aa</u>hu fii <b>a</b>l\'a<u>dzaa</u>bi <b>al</b>sysyadiid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4657, '50:27', 50, 27, ' q<u>aa</u>la qariinuhu rabban<u>aa</u> m<u>aa</u> a<u>th</u>ghaytuhu wal<u>aa</u>kin k<u>aa</u>na fii <u>dh</u>al<u>aa</u>lin ba\'iid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4658, '50:28', 50, 28, ' q<u>aa</u>la l<u>aa</u> takhta<u>sh</u>imuu ladayya waqad qaddamtu ilaykum bi<b>a</b>lwa\'iid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4659, '50:29', 50, 29, ' m<u>aa</u> yubaddalu <b>a</b>lqawlu ladayya wam<u>aa</u> an<u>aa</u> bi<u>zh</u>all<u>aa</u>min lil\'abiid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4660, '50:30', 50, 30, ' yawma naquulu lijahannama hali imtala/ti wataquulu hal min maziid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4661, '50:31', 50, 31, ' wauzlifati <b>a</b>ljannatu lilmuttaqiina ghayra ba\'iid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4662, '50:32', 50, 32, ' h<u>aadzaa</u> m<u>aa</u> tuu\'aduuna likulli aww<u>aa</u>bin <u>h</u>afii<u>zh</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4663, '50:33', 50, 33, ' man khasyiya <b>al</b>rra<u>h</u>m<u>aa</u>na bi<b>a</b>lghaybi waj<u>aa</u>-a biqalbin muniib<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4664, '50:34', 50, 34, ' udkhuluuh<u>aa</u> bisal<u>aa</u>min <u>dzaa</u>lika yawmu <b>a</b>lkhuluud<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4665, '50:35', 50, 35, ' lahum m<u>aa</u> yasy<u>aa</u>uuna fiih<u>aa</u> waladayn<u>aa</u> maziid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4666, '50:36', 50, 36, ' wakam ahlakn<u>aa</u> qablahum min qarnin hum asyaddu minhum ba<u>th</u>syan fanaqqabuu fii <b>a</b>lbil<u>aa</u>di hal min ma<u>h</u>ii<u>sh</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4667, '50:37', 50, 37, ' inna fii <u>dzaa</u>lika la<u>dz</u>ikr<u>aa</u> liman k<u>aa</u>na lahu qalbun aw <b>a</b>lq<u>aa</u> <b>al</b>ssam\'a wahuwa syahiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4668, '50:38', 50, 38, ' walaqad khalaqn<u>aa</u> <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a wam<u>aa</u> baynahum<u>aa</u> fii sittati ayy<u>aa</u>min wam<u>aa</u> massan<u>aa</u> min lughuub<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4669, '50:39', 50, 39, ' fa<b>i</b><u>sh</u>bir \'al<u>aa</u> m<u>aa</u> yaquuluuna wasabbi<u>h</u> bi<u>h</u>amdi rabbika qabla <u>th</u>uluu\'i <b>al</b>sysyamsi waqabla <b>a</b>lghuruub<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4670, '50:40', 50, 40, ' wamina <b>al</b>layli fasabbi<u>h</u>hu wa-adb<u>aa</u>ra <b>al</b>ssujuud<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4671, '50:41', 50, 41, ' wa<b>i</b>stami\' yawma yun<u>aa</u>di <b>a</b>lmun<u>aa</u>di min mak<u>aa</u>nin qariib<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4672, '50:42', 50, 42, ' yawma yasma\'uuna <b>al</b><u>shsh</u>ay<u>h</u>ata bi<b>a</b>l<u>h</u>aqqi <u>dzaa</u>lika yawmu <b>a</b>lkhuruuj<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4673, '50:43', 50, 43, ' inn<u>aa</u> na<u>h</u>nu nu<u>h</u>yii wanumiitu wa-ilayn<u>aa</u> <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (4674, '50:44', 50, 44, ' yawma tasyaqqaqu <b>a</b>l-ar<u>dh</u>u \'anhum sir<u>aa</u>\'an <u>dzaa</u>lika <u>h</u>asyrun \'alayn<u>aa</u> yasiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4675, '50:45', 50, 45, ' na<u>h</u>nu a\'lamu bim<u>aa</u> yaquuluuna wam<u>aa</u> anta \'alayhim bijabb<u>aa</u>rin fa<u>dz</u>akkir bi<b>a</b>lqur-<u>aa</u>ni man yakh<u>aa</u>fu wa\'iid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4676, '51:1', 51, 1, ' wa<b>al</b><u>dzdzaa</u>riy<u>aa</u>ti <u>dz</u>arw<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4677, '51:2', 51, 2, ' fa<b>a</b>l<u>haa</u>mil<u>aa</u>ti wiqr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4678, '51:3', 51, 3, ' fa<b>a</b>lj<u>aa</u>riy<u>aa</u>ti yusr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4679, '51:4', 51, 4, ' fa<b>a</b>lmuqassim<u>aa</u>ti amr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (4680, '51:5', 51, 5, ' innam<u>aa</u> tuu\'aduuna la<u>shaa</u>diq<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4681, '51:6', 51, 6, ' wa-inna <b>al</b>ddiina law<u>aa</u>qi\'<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4682, '51:7', 51, 7, ' wa<b>al</b>ssam<u>aa</u>-i <u>dzaa</u>ti <b>a</b>l<u>h</u>ubuk<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4683, '51:8', 51, 8, ' innakum lafii qawlin mukhtalif<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4684, '51:9', 51, 9, ' yu/faku \'anhu man ufik<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4685, '51:10', 51, 10, ' qutila <b>a</b>lkharr<u>aas</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4686, '51:11', 51, 11, ' <b>al</b>la<u>dz</u>iina hum fii ghamratin s<u>aa</u>huun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4687, '51:12', 51, 12, ' yas-aluuna ayy<u>aa</u>na yawmu <b>al</b>ddiin<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4688, '51:13', 51, 13, ' yawma hum \'al<u>aa</u> <b>al</b>nn<u>aa</u>ri yuftanuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4689, '51:14', 51, 14, ' <u>dz</u>uuquu fitnatakum h<u>aadzaa</u> <b>al</b>la<u>dz</u>ii kuntum bihi tasta\'jiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4690, '51:15', 51, 15, ' inna <b>a</b>lmuttaqiina fii jann<u>aa</u>tin wa\'uyuun<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4691, '51:16', 51, 16, ' <u>aa</u>khi<u>dz</u>iina m<u>aa</u> <u>aa</u>t<u>aa</u>hum rabbuhum innahum k<u>aa</u>nuu qabla <u>dzaa</u>lika mu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4692, '51:17', 51, 17, ' k<u>aa</u>nuu qaliilan mina <b>al</b>layli m<u>aa</u> yahja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4693, '51:18', 51, 18, ' wabi<b>a</b>l-as<u>haa</u>ri hum yastaghfiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4694, '51:19', 51, 19, ' wafii amw<u>aa</u>lihim <u>h</u>aqqun li<b>l</b>ss<u>aa</u>-ili wa<b>a</b>lma<u>h</u>ruum<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4695, '51:20', 51, 20, ' wafii <b>a</b>l-ar<u>dh</u>i <u>aa</u>y<u>aa</u>tun lilmuuqiniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4696, '51:21', 51, 21, ' wafii anfusikum afal<u>aa</u> tub<u>sh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4697, '51:22', 51, 22, ' wafii <b>al</b>ssam<u>aa</u>-i rizqukum wam<u>aa</u> tuu\'aduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4698, '51:23', 51, 23, ' fawarabbi <b>al</b>ssam<u>aa</u>-i wa<b>a</b>l-ar<u>dh</u>i innahu la<u>h</u>aqqun mitsla m<u>aa</u> annakum tan<u>th</u>iquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4699, '51:24', 51, 24, ' hal at<u>aa</u>ka <u>h</u>adiitsu <u>dh</u>ayfi ibr<u>aa</u>hiima <b>a</b>lmukramiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4700, '51:25', 51, 25, ' i<u>dz</u> dakhaluu \'alayhi faq<u>aa</u>luu sal<u>aa</u>man q<u>aa</u>la sal<u>aa</u>mun qawmun munkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4701, '51:26', 51, 26, ' far<u>aa</u>gha il<u>aa</u> ahlihi faj<u>aa</u>-a bi\'ijlin samiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4702, '51:27', 51, 27, ' faqarrabahu ilayhim q<u>aa</u>la <b>a</b>l<u>aa</u> ta/kuluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4703, '51:28', 51, 28, ' fa-awjasa minhum khiifatan q<u>aa</u>luu l<u>aa</u> takhaf wabasysyaruuhu bighul<u>aa</u>min \'aliim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4704, '51:29', 51, 29, ' fa-aqbalati imra-atuhu fii <u>sh</u>arratin fa<u>sh</u>akkat wajhah<u>aa</u> waq<u>aa</u>lat \'ajuuzun \'aqiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4705, '51:30', 51, 30, ' q<u>aa</u>luu ka<u>dzaa</u>liki q<u>aa</u>la rabbuki innahu huwa <b>a</b>l<u>h</u>akiimu <b>a</b>l\'aliim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (4706, '51:31', 51, 31, ' q<u>aa</u>la fam<u>aa</u> kha<u>th</u>bukum ayyuh<u>aa</u> <b>a</b>lmursaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4707, '51:32', 51, 32, ' q<u>aa</u>luu inn<u>aa</u> ursiln<u>aa</u> il<u>aa</u> qawmin mujrimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4708, '51:33', 51, 33, ' linursila \'alayhim <u>h</u>ij<u>aa</u>ratan min <u>th</u>iin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4709, '51:34', 51, 34, ' musawwamatan \'inda rabbika lilmusrifiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4710, '51:35', 51, 35, ' fa-akhrajn<u>aa</u> man k<u>aa</u>na fiih<u>aa</u> mina <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4711, '51:36', 51, 36, ' fam<u>aa</u> wajadn<u>aa</u> fiih<u>aa</u> ghayra baytin mina <b>a</b>lmuslimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4712, '51:37', 51, 37, ' watarakn<u>aa</u> fiih<u>aa</u> <u>aa</u>yatan lilla<u>dz</u>iina yakh<u>aa</u>fuuna <b>a</b>l\'a<u>dzaa</u>ba <b>a</b>l-aliim<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4713, '51:38', 51, 38, ' wafii muus<u>aa</u> i<u>dz</u> arsa<b>l</b>n<u>aa</u>hu il<u>aa</u> fir\'awna bisul<u>th</u><u>aa</u>nin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4714, '51:39', 51, 39, ' fatawall<u>aa</u> biruknihi waq<u>aa</u>la s<u>aah</u>irun aw majnuun<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4715, '51:40', 51, 40, ' fa-akha<u>dz</u>n<u>aa</u>hu wajunuudahu fanaba<u>dz</u>n<u>aa</u>hum fii <b>a</b>lyammi wahuwa muliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (4716, '51:41', 51, 41, ' wafii \'<u>aa</u>din i<u>dz</u> arsaln<u>aa</u> \'alayhimu <b>al</b>rrii<u>h</u>a <b>a</b>l\'aqiim<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4717, '51:42', 51, 42, ' m<u>aa</u> ta<u>dz</u>aru min syay-in atat \'alayhi ill<u>aa</u> ja\'alat-hu ka<b>al</b>rramiim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (4718, '51:43', 51, 43, ' wafii tsamuuda i<u>dz</u> qiila lahum tamatta\'uu <u>h</u>att<u>aa</u> <u>h</u>iin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (4719, '51:44', 51, 44, ' fa\'ataw \'an amri rabbihim fa-akha<u>dz</u>at-humu <b>al</b><u>shshaa</u>\'iqatu wahum yan<u>zh</u>uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4720, '51:45', 51, 45, ' fam<u>aa</u> ista<u>th</u><u>aa</u>\'uu min qiy<u>aa</u>min wam<u>aa</u> k<u>aa</u>nuu munta<u>sh</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4721, '51:46', 51, 46, ' waqawma nuu<u>h</u>in min qablu innahum k<u>aa</u>nuu qawman f<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4722, '51:47', 51, 47, ' wa<b>al</b>ssam<u>aa</u>-a banayn<u>aa</u>h<u>aa</u> bi-aydin wa-inn<u>aa</u> lamuusi\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4723, '51:48', 51, 48, ' wa<b>a</b>l-ar<u>dh</u>a farasyn<u>aa</u>h<u>aa</u> fani\'ma <b>a</b>lm<u>aa</u>hiduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (4724, '51:49', 51, 49, ' wamin kulli syay-in khalaqn<u>aa</u> zawjayni la\'allakum ta<u>dz</u>akkaruun<b>a</b>\r\r\n');
