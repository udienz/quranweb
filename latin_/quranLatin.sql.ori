INSERT INTO quranLatin VALUES (1912, '16:11', 16, 11, ' yunbitu lakum bihi <b>al</b>zzar\'a wa<b>al</b>zzaytuuna wa<b>al</b>nnakhiila wa<b>a</b>l-a\'n<u>aa</u>ba wamin kulli <b>al</b>tstsamar<u>aa</u>ti inna fii <u>dzaa</u>lika la<u>aa</u>yatan liqawmin yatafakkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (1913, '16:12', 16, 12, ' wasakhkhara lakumu <b>al</b>layla wa<b>al</b>nnah<u>aa</u>ra wa<b>al</b>sysyamsa wa<b>a</b>lqamara wa<b>al</b>nnujuumu musakhkhar<u>aa</u>tun bi-amrihi inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin liqawmin ya\'qiluun<b>a</b>\r\r\n');
