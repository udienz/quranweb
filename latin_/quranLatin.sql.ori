INSERT INTO quranLatin VALUES (2239, '18:99', 18, 99, ' watarakn<u>aa</u> ba\'<u>dh</u>ahum yawma-i<u>dz</u>in yamuuju fii ba\'<u>dh</u>in wanufikha fii <b>al</b><u>shsh</u>uuri fajama\'n<u>aa</u>hum jam\'<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2240, '18:100', 18, 100, ' wa\'ara<u>dh</u>n<u>aa</u> jahannama yawma-i<u>dz</u>in lilk<u>aa</u>firiina \'ar<u>daa</u><b>n </b>\r\r\n');
INSERT INTO quranLatin VALUES (2241, '18:101', 18, 101, ' <b>al</b>la<u>dz</u>iina k<u>aa</u>nat a\'yunuhum fii ghi<u>th</u><u>aa</u>-in \'an <u>dz</u>ikrii wak<u>aa</u>nuu l<u>aa</u> yasta<u>th</u>ii\'uuna sam\'<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2242, '18:102', 18, 102, ' afa<u>h</u>asiba <b>al</b>la<u>dz</u>iina kafaruu an yattakhi<u>dz</u>uu \'ib<u>aa</u>dii min duunii awliy<u>aa</u>-a inn<u>aa</u> a\'tadn<u>aa</u> jahannama lilk<u>aa</u>firiina nuzul<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2243, '18:103', 18, 103, ' qul hal nunabbi-ukum bi<b>a</b>l-akhsariina a\'m<u>aa</u>l<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2244, '18:104', 18, 104, ' <b>al</b>la<u>dz</u>iina <u>dh</u>alla sa\'yuhum fii <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> wahum ya<u>h</u>sabuuna annahum yu<u>h</u>sinuuna <u>sh</u>un\'<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2245, '18:105', 18, 105, ' ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina kafaruu bi-<u>aa</u>y<u>aa</u>ti rabbihim waliq<u>aa</u>-ihi fa<u>h</u>abi<u>th</u>at a\'m<u>aa</u>luhum fal<u>aa</u> nuqiimu lahum yawma <b>a</b>lqiy<u>aa</u>mati wazn<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2246, '18:106', 18, 106, ' <u>dzaa</u>lika jaz<u>aa</u>uhum jahannamu bim<u>aa</u> kafaruu wa<b>i</b>ttakha<u>dz</u>uu <u>aa</u>y<u>aa</u>tii warusulii huzuw<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2247, '18:107', 18, 107, ' inna <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti k<u>aa</u>nat lahum jann<u>aa</u>tu <b>a</b>lfirdawsi nuzul<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2248, '18:108', 18, 108, ' kh<u>aa</u>lidiina fiih<u>aa</u> l<u>aa</u> yabghuuna \'anh<u>aa</u> <u>h</u>iwal<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2249, '18:109', 18, 109, ' qul law k<u>aa</u>na <b>a</b>lba<u>h</u>ru mid<u>aa</u>dan likalim<u>aa</u>ti rabbii lanafida <b>a</b>lba<u>h</u>ru qabla an tanfada kalim<u>aa</u>tu rabbii walaw ji/n<u>aa</u> bimitslihi madad<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2250, '18:110', 18, 110, ' qul innam<u>aa</u> an<u>aa</u> basyarun mitslukum yuu<u>haa</u> ilayya annam<u>aa</u> il<u>aa</u>hukum il<u>aa</u>hun w<u>aah</u>idun faman k<u>aa</u>na yarjuu liq<u>aa</u>-a rabbihi falya\'mal \'amalan <u>shaa</u>li<u>h</u>an wal<u>aa</u> yusyrik bi\'ib<u>aa</u>dati rabbihi a<u>h</u>ad<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2251, '19:1', 19, 1, ' k<u>aa</u>f-h<u>aa</u>-y<u>aa</u>-\'ayn-<u>shaa</u>d</u>\r\r\n');
INSERT INTO quranLatin VALUES (2252, '19:2', 19, 2, ' <u>dz</u>ikru ra<u>h</u>mati rabbika \'abdahu zakariyy<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2253, '19:3', 19, 3, ' i<u>dz</u> n<u>aa</u>d<u>aa</u> rabbahu nid<u>aa</u>-an khafiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2254, '19:4', 19, 4, ' q<u>aa</u>la rabbi innii wahana <b>a</b>l\'a<u>zh</u>mu minnii wa<b>i</b>syta\'ala <b>al</b>rra/su syayban walam akun bidu\'<u>aa</u>-ika rabbi syaqiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2255, '19:5', 19, 5, ' wa-innii khiftu <b>a</b>lmaw<u>aa</u>liya min war<u>aa</u>-ii wak<u>aa</u>nati imra-atii \'<u>aa</u>qiran fahab lii min ladunka waliyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2256, '19:6', 19, 6, ' yaritsunii wayaritsu min <u>aa</u>li ya\'quuba wa<b>i</b>j\'alhu rabbi ra<u>dh</u>iyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2257, '19:7', 19, 7, ' y<u>aa</u> zakariyy<u>aa</u> inn<u>aa</u> nubasysyiruka bighul<u>aa</u>min ismuhu ya<u>h</u>y<u>aa</u> lam naj\'al lahu min qablu samiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2258, '19:8', 19, 8, ' q<u>aa</u>la rabbi ann<u>aa</u> yakuunu lii ghul<u>aa</u>mun wak<u>aa</u>nati imra-atii \'<u>aa</u>qiran waqad balaghtu mina <b>a</b>lkibari \'itiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2259, '19:9', 19, 9, ' q<u>aa</u>la ka<u>dzaa</u>lika q<u>aa</u>la rabbuka huwa \'alayya hayyinun waqad khalaqtuka min qablu walam taku syay-<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2260, '19:10', 19, 10, ' q<u>aa</u>la rabbi ij\'al lii <u>aa</u>yatan q<u>aa</u>la <u>aa</u>yatuka <b>al</b>l<u>aa</u> tukallima <b>al</b>nn<u>aa</u>sa tsal<u>aa</u>tsa lay<u>aa</u>lin sawiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2261, '19:11', 19, 11, ' fakharaja \'al<u>aa</u> qawmihi mina <b>a</b>lmi<u>h</u>r<u>aa</u>bi fa-aw<u>haa</u> ilayhim an sabbi<u>h</u>uu bukratan wa\'asyiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2262, '19:12', 19, 12, ' y<u>aa</u> ya<u>h</u>y<u>aa</u> khu<u>dz</u>i <b>a</b>lkit<u>aa</u>ba biquwwatin wa<u>aa</u>tayn<u>aa</u>hu <b>a</b>l<u>h</u>ukma <u>sh</u>abiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2263, '19:13', 19, 13, ' wa<u>h</u>an<u>aa</u>nan min ladunn<u>aa</u> wazak<u>aa</u>tan wak<u>aa</u>na taqiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2264, '19:14', 19, 14, ' wabarran biw<u>aa</u>lidayhi walam yakun jabb<u>aa</u>ran \'a<u>sh</u>iyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2265, '19:15', 19, 15, ' wasal<u>aa</u>mun \'alayhi yawma wulida wayawma yamuutu wayawma yub\'atsu <u>h</u>ayy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2266, '19:16', 19, 16, ' wa<b>u</b><u>dz</u>kur fii <b>a</b>lkit<u>aa</u>bi maryama i<u>dz</u>i intaba<u>dz</u>at min ahlih<u>aa</u> mak<u>aa</u>nan syarqiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2267, '19:17', 19, 17, ' fa<b>i</b>ttakha<u>dz</u>at min duunihim <u>h</u>ij<u>aa</u>ban fa-arsaln<u>aa</u> ilayh<u>aa</u> ruu<u>h</u>an<u>aa</u> fatamatstsala lah<u>aa</u> basyaran sawiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2268, '19:18', 19, 18, ' q<u>aa</u>lat innii a\'uu<u>dz</u>u bi<b>al</b>rra<u>h</u>m<u>aa</u>ni minka in kunta taqiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2269, '19:19', 19, 19, ' q<u>aa</u>la innam<u>aa</u> an<u>aa</u> rasuulu rabbiki li-ahaba laki ghul<u>aa</u>man zakiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2270, '19:20', 19, 20, ' q<u>aa</u>lat ann<u>aa</u> yakuunu lii ghul<u>aa</u>mun walam yamsasnii basyarun walam aku baghiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2271, '19:21', 19, 21, ' q<u>aa</u>la ka<u>dzaa</u>liki q<u>aa</u>la rabbuki huwa \'alayya hayyinun walinaj\'alahu <u>aa</u>yatan li<b>l</b>nn<u>aa</u>si wara<u>h</u>matan minn<u>aa</u> wak<u>aa</u>na amran maq<u>dh</u>iyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2272, '19:22', 19, 22, ' fa<u>h</u>amalat-hu fa<b>i</b>ntaba<u>dz</u>at bihi mak<u>aa</u>nan qa<u>sh</u>iyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2273, '19:23', 19, 23, ' fa-aj<u>aa</u>-ah<u>aa</u> <b>a</b>lmakh<u>aad</u>u il<u>aa</u> ji<u>dz</u>\'i <b>al</b>nnakhlati q<u>aa</u>lat y<u>aa</u> laytanii mittu qabla h<u>aadzaa</u> wakuntu nasyan mansiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2274, '19:24', 19, 24, ' fan<u>aa</u>d<u>aa</u>h<u>aa</u> min ta<u>h</u>tih<u>aa</u> <b>al</b>l<u>aa</u> ta<u>h</u>zanii qad ja\'ala rabbuki ta<u>h</u>taki sariyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2275, '19:25', 19, 25, ' wahuzzii ilayki biji<u>dz</u>\'i <b>al</b>nnakhlati tus<u>aa</u>qi<u>th</u> \'alayki ru<u>th</u>aban janiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2276, '19:26', 19, 26, ' fakulii wa<b>i</b>syrabii waqarrii \'aynan fa-imm<u>aa</u> tarayinna mina <b>a</b>lbasyari a<u>h</u>adan faquulii innii na<u>dz</u>artu li<b>l</b>rra<u>h</u>m<u>aa</u>ni <u>sh</u>awman falan ukallima <b>a</b>lyawma insiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2277, '19:27', 19, 27, ' fa-atat bihi qawmah<u>aa</u> ta<u>h</u>miluhu q<u>aa</u>luu y<u>aa</u> maryamu laqad ji/ti syay-an fariyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2278, '19:28', 19, 28, ' y<u>aa</u> ukhta h<u>aa</u>ruuna m<u>aa</u> k<u>aa</u>na abuuki imra-a saw-in wam<u>aa</u> k<u>aa</u>nat ummuki baghiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2279, '19:29', 19, 29, ' fa-asy<u>aa</u>rat ilayhi q<u>aa</u>luu kayfa nukallimu man k<u>aa</u>na fii <b>a</b>lmahdi <u>sh</u>abiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2280, '19:30', 19, 30, ' q<u>aa</u>la innii \'abdu <b>al</b>l<u>aa</u>hi <u>aa</u>t<u>aa</u>niya <b>a</b>lkit<u>aa</u>ba waja\'alanii nabiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2281, '19:31', 19, 31, ' waja\'alanii mub<u>aa</u>rakan aynam<u>aa</u> kuntu wa-aw<u>shaa</u>nii bi<b>al</b><u>shsh</u>al<u>aa</u>ti wa<b>al</b>zzak<u>aa</u>ti m<u>aa</u> dumtu <u>h</u>ayy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2282, '19:32', 19, 32, ' wabarran biw<u>aa</u>lidatii walam yaj\'alnii jabb<u>aa</u>ran syaqiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2283, '19:33', 19, 33, ' wa<b>al</b>ssal<u>aa</u>mu \'alayya yawma wulidtu wayawma amuutu wayawma ub\'atsu <u>h</u>ayy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2284, '19:34', 19, 34, ' <u>dzaa</u>lika \'iis<u>aa</u> ibnu maryama qawla <b>a</b>l<u>h</u>aqqi <b>al</b>la<u>dz</u>ii fiihi yamtaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2285, '19:35', 19, 35, ' m<u>aa</u> k<u>aa</u>na lill<u>aa</u>hi an yattakhi<u>dz</u>a min waladin sub<u>haa</u>nahu i<u>dzaa</u> qa<u>daa</u> amran fa-innam<u>aa</u> yaquulu lahu kun fayakuun<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2286, '19:36', 19, 36, ' wa-inna <b>al</b>l<u>aa</u>ha rabbii warabbukum fa<b>u</b>\'buduuhu h<u>aadzaa</u> <u>sh</u>ir<u>aath</u>un mustaqiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2287, '19:37', 19, 37, ' fa<b>i</b>khtalafa <b>a</b>l-a<u>h</u>z<u>aa</u>bu min baynihim fawaylun lilla<u>dz</u>iina kafaruu min masyhadi yawmin \'a<u>zh</u>iim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2288, '19:38', 19, 38, ' asmi\' bihim wa-ab<u>sh</u>ir yawma ya/tuunan<u>aa</u> l<u>aa</u>kini <b>al</b><u>zhzhaa</u>limuuna <b>a</b>lyawma fii <u>dh</u>al<u>aa</u>lin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2289, '19:39', 19, 39, ' wa-an<u>dz</u>irhum yawma <b>a</b>l<u>h</u>asrati i<u>dz</u> qu<u>dh</u>iya <b>a</b>l-amru wahum fii ghaflatin wahum l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2290, '19:40', 19, 40, ' inn<u>aa</u> na<u>h</u>nu naritsu <b>a</b>l-ar<u>dh</u>a waman \'alayh<u>aa</u> wa-ilayn<u>aa</u> yurja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2291, '19:41', 19, 41, ' wa<b>u</b><u>dz</u>kur fii <b>a</b>lkit<u>aa</u>bi ibr<u>aa</u>hiima innahu k<u>aa</u>na <u>sh</u>iddiiqan nabiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2292, '19:42', 19, 42, ' i<u>dz</u> q<u>aa</u>la li-abiihi y<u>aa</u> abati lima ta\'budu m<u>aa</u> l<u>aa</u> yasma\'u wal<u>aa</u> yub<u>sh</u>iru wal<u>aa</u> yughnii \'anka syay-<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2293, '19:43', 19, 43, ' y<u>aa</u> abati innii qad j<u>aa</u>-anii mina <b>a</b>l\'ilmi m<u>aa</u> lam ya/tika fa<b>i</b>ttabi\'nii ahdika <u>sh</u>ir<u>aath</u>an sawiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2294, '19:44', 19, 44, ' y<u>aa</u> abati l<u>aa</u> ta\'budi <b>al</b>sysyay<u>th</u><u>aa</u>na inna <b>al</b>sysyay<u>th</u><u>aa</u>na k<u>aa</u>na li<b>l</b>rra<u>h</u>m<u>aa</u>ni \'a<u>sh</u>iyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2295, '19:45', 19, 45, ' y<u>aa</u> abati innii akh<u>aa</u>fu an yamassaka \'a<u>dzaa</u>bun mina <b>al</b>rra<u>h</u>m<u>aa</u>ni fatakuuna li<b>l</b>sysyya<u>th</u><u>aa</u>ni waliyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2296, '19:46', 19, 46, ' q<u>aa</u>la ar<u>aa</u>ghibun anta \'an <u>aa</u>lihatii y<u>aa</u> ibr<u>aa</u>hiimu la-in lam tantahi la-arjumannaka wa<b>u</b>hjurnii maliyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2297, '19:47', 19, 47, ' q<u>aa</u>la sal<u>aa</u>mun \'alayka sa-astaghfiru laka rabbii innahu k<u>aa</u>na bii <u>h</u>afiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2298, '19:48', 19, 48, ' wa-a\'tazilukum wam<u>aa</u> tad\'uuna min duuni <b>al</b>l<u>aa</u>hi wa-ad\'uu rabbii \'as<u>aa</u> <b>al</b>l<u>aa</u> akuuna bidu\'<u>aa</u>-i rabbii syaqiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2299, '19:49', 19, 49, ' falamm<u>aa</u> i\'tazalahum wam<u>aa</u> ya\'buduuna min duuni <b>al</b>l<u>aa</u>hi wahabn<u>aa</u> lahu is<u>haa</u>qa waya\'quuba wakullan ja\'aln<u>aa</u> nabiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2300, '19:50', 19, 50, ' wawahabn<u>aa</u> lahum min ra<u>h</u>matin<u>aa</u> waja\'aln<u>aa</u> lahum lis<u>aa</u>na <u>sh</u>idqin \'aliyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2301, '19:51', 19, 51, ' wa<b>u</b><u>dz</u>kur fii <b>a</b>lkit<u>aa</u>bi muus<u>aa</u> innahu k<u>aa</u>na mukhla<u>sh</u>an wak<u>aa</u>na rasuulan nabiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2302, '19:52', 19, 52, ' wan<u>aa</u>dayn<u>aa</u>hu min j<u>aa</u>nibi <b>al</b><u>ththh</u>uuri <b>a</b>l-aymani waqarrabn<u>aa</u>hu najiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2303, '19:53', 19, 53, ' wawahabn<u>aa</u> lahu min ra<u>h</u>matin<u>aa</u> akh<u>aa</u>hu h<u>aa</u>ruuna nabiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2304, '19:54', 19, 54, ' wa<b>u</b><u>dz</u>kur fii <b>a</b>lkit<u>aa</u>bi ism<u>aa</u>\'iila innahu k<u>aa</u>na <u>shaa</u>diqa <b>a</b>lwa\'di wak<u>aa</u>na rasuulan nabiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2305, '19:55', 19, 55, ' wak<u>aa</u>na ya/muru ahlahu bi<b>al</b><u>shsh</u>al<u>aa</u>ti wa<b>al</b>zzak<u>aa</u>ti wak<u>aa</u>na \'inda rabbihi mar<u>dh</u>iyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2306, '19:56', 19, 56, ' wa<b>u</b><u>dz</u>kur fii <b>a</b>lkit<u>aa</u>bi idriisa innahu k<u>aa</u>na <u>sh</u>iddiiqan nabiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2307, '19:57', 19, 57, ' warafa\'n<u>aa</u>hu mak<u>aa</u>nan \'aliyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2308, '19:58', 19, 58, ' ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina an\'ama <b>al</b>l<u>aa</u>hu \'alayhim mina <b>al</b>nnabiyyiina min <u>dz</u>urriyyati <u>aa</u>dama wamimman <u>h</u>amaln<u>aa</u> ma\'a nuu<u>h</u>in wamin <u>dz</u>urriyyati ibr<u>aa</u>hiima wa-isr<u>aa</u>-iila wamimman hadayn<u>aa</u> wa<b>i</b>jtabayn<u>aa</u> i<u>dzaa</u> tutl<u>aa</u> \'alayhim <u>aa</u>y<u>aa</u>tu <b>al</b>rra<u>h</u>m<u>aa</u>ni kharruu sujjadan wabukiyy<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2309, '19:59', 19, 59, ' fakhalafa min ba\'dihim khalfun a<u>daa</u>\'uu <b>al</b><u>shsh</u>al<u>aa</u>ta wa<b>i</b>ttaba\'uu <b>al</b>sysyahaw<u>aa</u>ti fasawfa yalqawna ghayy<u>aa</u><b>n</b>\r\r\n');
