INSERT INTO quranLatin VALUES (606, '4:113', 4, 113, ' walawl<u>aa</u> fa<u>dh</u>lu <b>al</b>l<u>aa</u>hi \'alayka wara<u>h</u>matuhu lahammat <u>th</u><u>aa</u>-ifatun minhum an yu<u>dh</u>illuuka wam<u>aa</u> yu<u>dh</u>illuuna ill<u>aa</u> anfusahum wam<u>aa</u> ya<u>dh</u>urruunaka min syay-in wa-anzala <b>al</b>l<u>aa</u>hu \'alayka <b>a</b>lkit<u>aa</u>ba wa<b>a</b>l<u>h</u>ikmata wa\'allamaka m<u>aa</u> lam takun ta\'lamu wak<u>aa</u>na fa<u>dh</u>lu <b>al</b>l<u>aa</u>hi \'alayka \'a<u>zh</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (607, '4:114', 4, 114, ' l<u>aa</u> khayra fii katsiirin min najw<u>aa</u>hum ill<u>aa</u> man amara bi<u>sh</u>adaqatin aw ma\'ruufin aw i<u>sh</u>l<u>aah</u>in bayna <b>al</b>nn<u>aa</u>si waman yaf\'al <u>dzaa</u>lika ibtigh<u>aa</u>-a mar<u>daa</u>ti <b>al</b>l<u>aa</u>hi fasawfa nu/tiihi ajran \'a<u>zh</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (608, '4:115', 4, 115, ' waman yusy<u>aa</u>qiqi <b>al</b>rrasuula min ba\'di m<u>aa</u> tabayyana lahu <b>a</b>lhud<u>aa</u> wayattabi\' ghayra sabiili <b>a</b>lmu/miniina nuwallihi m<u>aa</u> tawall<u>aa</u> wanu<u>sh</u>lihi jahannama was<u>aa</u>-at ma<u>sh</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (609, '4:116', 4, 116, ' inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yaghfiru an yusyraka bihi wayaghfiru m<u>aa</u> duuna <u>dzaa</u>lika liman yasy<u>aa</u>u waman yusyrik bi<b>al</b>l<u>aa</u>hi faqad <u>dh</u>alla <u>dh</u>al<u>aa</u>lan ba\'iid<u>aa</u><b>n</b>\r\r\n');
