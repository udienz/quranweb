INSERT INTO quranLatin VALUES (5304, '68:33', 68, 33, ' ka<u>dzaa</u>lika <b>a</b>l\'a<u>dzaa</u>bu wala\'a<u>dzaa</u>bu <b>a</b>l-<u>aa</u>khirati akbaru law k<u>aa</u>nuu ya\'lamuun<b>a</b>\r\r\n');
