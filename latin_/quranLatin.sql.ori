INSERT INTO quranLatin VALUES (2846, '24:55', 24, 55, ' wa\'ada <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>iina <u>aa</u>manuu minkum wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti layastakhlifannahum fii <b>a</b>l-ar<u>dh</u>i kam<u>aa</u> istakhlafa <b>al</b>la<u>dz</u>iina min qablihim walayumakkinanna lahum diinahumu <b>al</b>la<u>dz</u>ii irta<u>daa</u> lahum walayubaddilannahum min ba\'di khawfihim amnan ya\'buduunanii l<u>aa</u> yusyrikuuna bii syay-an waman kafara ba\'da <u>dzaa</u>lika faul<u>aa</u>-ika humu <b>a</b>lf<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2847, '24:56', 24, 56, ' wa-aqiimuu <b>al</b><u>shsh</u>al<u>aa</u>ta wa<u>aa</u>tuu <b>al</b>zzak<u>aa</u>ta wa-a<u>th</u>ii\'uu <b>al</b>rrasuula la\'allakum tur<u>h</u>amuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2848, '24:57', 24, 57, ' l<u>aa</u> ta<u>h</u>sabanna <b>al</b>la<u>dz</u>iina kafaruu mu\'jiziina fii <b>a</b>l-ar<u>dh</u>i wama/w<u>aa</u>humu <b>al</b>nn<u>aa</u>ru walabi/sa <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2849, '24:58', 24, 58, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu liyasta/<u>dz</u>inkumu <b>al</b>la<u>dz</u>iina malakat aym<u>aa</u>nukum wa<b>a</b>lla<u>dz</u>iina lam yablughuu <b>a</b>l<u>h</u>uluma minkum tsal<u>aa</u>tsa marr<u>aa</u>tin min qabli <u>sh</u>al<u>aa</u>ti <b>a</b>lfajri wa<u>h</u>iina ta<u>dh</u>a\'uuna tsiy<u>aa</u>bakum mina <b>al</b><u>zhzh</u>ahiirati wamin ba\'di <u>sh</u>al<u>aa</u>ti <b>a</b>l\'isy<u>aa</u>-i tsal<u>aa</u>tsu \'awr<u>aa</u>tin lakum laysa \'alaykum wal<u>aa</u> \'alayhim jun<u>aah</u>un ba\'dahunna <u>th</u>aww<u>aa</u>fuuna \'alaykum ba\'<u>dh</u>ukum \'al<u>aa</u> ba\'<u>dh</u>in ka<u>dzaa</u>lika yubayyinu <b>al</b>l<u>aa</u>hu lakumu <b>a</b>l-<u>aa</u>y<u>aa</u>ti wa<b>al</b>l<u>aa</u>hu \'aliimun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2850, '24:59', 24, 59, ' wa-i<u>dzaa</u> balagha <b>a</b>l-a<u>th</u>f<u>aa</u>lu minkumu <b>a</b>l<u>h</u>uluma falyasta/<u>dz</u>inuu kam<u>aa</u> ista/<u>dz</u>ana <b>al</b>la<u>dz</u>iina min qablihim ka<u>dzaa</u>lika yubayyinu <b>al</b>l<u>aa</u>hu lakum <u>aa</u>y<u>aa</u>tihi wa<b>al</b>l<u>aa</u>hu \'aliimun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2851, '24:60', 24, 60, ' wa<b>a</b>lqaw<u>aa</u>\'idu mina <b>al</b>nnis<u>aa</u>-i <b>al</b>l<u>aa</u>tii l<u>aa</u> yarjuuna nik<u>aah</u>an falaysa \'alayhinna jun<u>aah</u>un an ya<u>dh</u>a\'na tsiy<u>aa</u>bahunna ghayra mutabarrij<u>aa</u>tin biziinatin wa-an yasta\'fifna khayrun lahunna wa<b>al</b>l<u>aa</u>hu samii\'un \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2852, '24:61', 24, 61, ' laysa \'al<u>aa</u> <b>a</b>l-a\'m<u>aa</u> <u>h</u>arajun wal<u>aa</u> \'al<u>aa</u> <b>a</b>l-a\'raji <u>h</u>arajun wal<u>aa</u> \'al<u>aa</u> <b>a</b>lmarii<u>dh</u>i <u>h</u>arajun wal<u>aa</u> \'al<u>aa</u> anfusikum an ta/kuluu min buyuutikum aw buyuuti <u>aa</u>b<u>aa</u>-ikum aw buyuuti ummah<u>aa</u>tikum aw buyuuti ikhw<u>aa</u>nikum aw buyuuti akhaw<u>aa</u>tikum aw buyuuti a\'m<u>aa</u>mikum aw buyuuti \'amm<u>aa</u>tikum aw buyuuti akhw<u>aa</u>likum aw buyuuti kh<u>aa</u>l<u>aa</u>tikum aw m<u>aa</u> malaktum maf<u>aa</u>ti<u>h</u>ahu aw <u>sh</u>adiiqikum laysa \'alaykum jun<u>aah</u>un an ta/kuluu jamii\'an aw asyt<u>aa</u>tan fa-i<u>dzaa</u> dakhaltum buyuutan fasallimuu \'al<u>aa</u> anfusikum ta<u>h</u>iyyatan min \'indi <b>al</b>l<u>aa</u>hi mub<u>aa</u>rakatan <u>th</u>ayyibatan ka<u>dzaa</u>lika yubayyinu <b>al</b>l<u>aa</u>hu lakumu <b>a</b>l-<u>aa</u>y<u>aa</u>ti la\'allakum ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2853, '24:62', 24, 62, ' innam<u>aa</u> <b>a</b>lmu/minuuna <b>al</b>la<u>dz</u>iina <u>aa</u>manuu bi<b>al</b>l<u>aa</u>hi warasuulihi wa-i<u>dzaa</u> k<u>aa</u>nuu ma\'ahu \'al<u>aa</u> amrin j<u>aa</u>mi\'in lam ya<u>dz</u>habuu <u>h</u>att<u>aa</u> yasta/<u>dz</u>inuuhu inna <b>al</b>la<u>dz</u>iina yasta/<u>dz</u>inuunaka ul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina yu/minuuna bi<b>al</b>l<u>aa</u>hi warasuulihi fa-i<u>dzaa</u> ista/<u>dz</u>anuuka liba\'<u>dh</u>i sya/nihim fa/<u>dz</u>an liman syi/ta minhum wa<b>i</b>staghfir lahumu <b>al</b>l<u>aa</u>ha inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2854, '24:63', 24, 63, ' l<u>aa</u> taj\'aluu du\'<u>aa</u>-a <b>al</b>rrasuuli baynakum kadu\'<u>aa</u>-i ba\'<u>dh</u>ikum ba\'<u>dh</u>an qad ya\'lamu <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>iina yatasallaluuna minkum liw<u>aadz</u>an falya<u>hts</u>ari <b>al</b>la<u>dz</u>iina yukh<u>aa</u>lifuuna \'an amrihi an tu<u>sh</u>iibahum fitnatun aw yu<u>sh</u>iibahum \'a<u>dzaa</u>bun <b>a</b>liim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2855, '24:64', 24, 64, ' al<u>aa</u> inna lill<u>aa</u>hi m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i qad ya\'lamu m<u>aa</u> antum \'alayhi wayawma yurja\'uuna ilayhi fayunabbi-uhum bim<u>aa</u> \'amiluu wa<b>al</b>l<u>aa</u>hu bikulli syay-in \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2856, '25:1', 25, 1, ' tab<u>aa</u>raka <b>al</b>la<u>dz</u>ii nazzala <b>a</b>lfurq<u>aa</u>na \'al<u>aa</u> \'abdihi liyakuuna lil\'<u>aa</u>lamiina na<u>dz</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2857, '25:2', 25, 2, ' <b>al</b>la<u>dz</u>ii lahu mulku <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i walam yattakhi<u>dz</u> waladan walam yakun lahu syariikun fii <b>a</b>lmulki wakhalaqa kulla syay-in faqaddarahu taqdiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2858, '25:3', 25, 3, ' wa<b>i</b>ttakha<u>dz</u>uu min duunihi <u>aa</u>lihatan l<u>aa</u> yakhluquuna syay-an wahum yukhlaquuna wal<u>aa</u> yamlikuuna li-anfusihim <u>dh</u>arran wal<u>aa</u> naf\'an wal<u>aa</u> yamlikuuna mawtan wal<u>aa</u> <u>h</u>ay<u>aa</u>tan wal<u>aa</u> nusyuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2859, '25:4', 25, 4, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu in h<u>aadzaa</u> ill<u>aa</u> ifkun iftar<u>aa</u>hu wa-a\'<u>aa</u>nahu \'alayhi qawmun <u>aa</u>kharuuna faqad j<u>aa</u>uu <u>zh</u>ulman wazuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2860, '25:5', 25, 5, ' waq<u>aa</u>luu as<u>aath</u>iiru <b>a</b>l-awwaliina iktatabah<u>aa</u> fahiya tuml<u>aa</u> \'alayhi bukratan wa-a<u>sh</u>iil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2861, '25:6', 25, 6, ' qul anzalahu <b>al</b>la<u>dz</u>ii ya\'lamu <b>al</b>ssirra fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i innahu k<u>aa</u>na ghafuuran ra<u>h</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2862, '25:7', 25, 7, ' waq<u>aa</u>luu m<u>aa</u>li h<u>aadzaa</u> <b>al</b>rrasuuli ya/kulu <b>al</b><u>ththh</u>a\'<u>aa</u>ma wayamsyii fii <b>a</b>l-asw<u>aa</u>qi lawl<u>aa</u> unzila ilayhi malakun fayakuuna ma\'ahu na<u>dz</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2863, '25:8', 25, 8, ' aw yulq<u>aa</u> ilayhi kanzun aw takuunu lahu jannatun ya/kulu minh<u>aa</u> waq<u>aa</u>la <b>al</b><u>zhzhaa</u>limuuna in tattabi\'uuna ill<u>aa</u> rajulan mas<u>h</u>uur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2864, '25:9', 25, 9, ' un<u>zh</u>ur kayfa <u>dh</u>arabuu laka <b>a</b>l-amts<u>aa</u>la fa<u>dh</u>alluu fal<u>aa</u> yasta<u>th</u>ii\'uuna sabiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2865, '25:10', 25, 10, ' tab<u>aa</u>raka <b>al</b>la<u>dz</u>ii in sy<u>aa</u>-a ja\'ala laka khayran min <u>dzaa</u>lika jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru wayaj\'al laka qu<u>sh</u>uur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2866, '25:11', 25, 11, ' bal ka<u>dzdz</u>abuu bi<b>al</b>ss<u>aa</u>\'ati wa-a\'tadn<u>aa</u> liman ka<u>dzdz</u>aba bi<b>al</b>ss<u>aa</u>\'ati sa\'iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2867, '25:12', 25, 12, ' i<u>dzaa</u> ra-at-hum min mak<u>aa</u>nin ba\'iidin sami\'uu lah<u>aa</u> taghayyu<u>zh</u>an wazafiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2868, '25:13', 25, 13, ' wa-i<u>dzaa</u> ulquu minh<u>aa</u> mak<u>aa</u>nan <u>dh</u>ayyiqan muqarraniina da\'aw hun<u>aa</u>lika tsubuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2869, '25:14', 25, 14, ' l<u>aa</u> tad\'uu <b>a</b>lyawma tsubuuran w<u>aah</u>idan wa<b>u</b>d\'uu tsubuuran katsiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2870, '25:15', 25, 15, ' qul a<u>dzaa</u>lika khayrun am jannatu <b>a</b>lkhuldi <b>al</b>latii wu\'ida <b>a</b>lmuttaquuna k<u>aa</u>nat lahum jaz<u>aa</u>-an wama<u>sh</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2871, '25:16', 25, 16, ' lahum fiih<u>aa</u> m<u>aa</u> yasy<u>aa</u>uuna kh<u>aa</u>lidiina k<u>aa</u>na \'al<u>aa</u> rabbika wa\'dan mas-uul<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2872, '25:17', 25, 17, ' wayawma ya<u>h</u>syuruhum wam<u>aa</u> ya\'buduuna min duuni <b>al</b>l<u>aa</u>hi fayaquulu a-antum a<u>dh</u>laltum \'ib<u>aa</u>dii h<u>aa</u>ul<u>aa</u>-i am hum <u>dh</u>alluu <b>al</b>ssabiil<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2873, '25:18', 25, 18, ' q<u>aa</u>luu sub<u>haa</u>naka m<u>aa</u> k<u>aa</u>na yanbaghii lan<u>aa</u> an nattakhi<u>dz</u>a min duunika min awliy<u>aa</u>-a wal<u>aa</u>kin matta\'tahum wa<u>aa</u>b<u>aa</u>-ahum <u>h</u>att<u>aa</u> nasuu <b>al</b><u>dzdz</u>ikra wak<u>aa</u>nuu qawman buur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2874, '25:19', 25, 19, ' faqad ka<u>dzdz</u>abuukum bim<u>aa</u> taquuluuna fam<u>aa</u> tasta<u>th</u>ii\'uuna <u>sh</u>arfan wal<u>aa</u> na<u>sh</u>ran waman ya<u>zh</u>lim minkum nu<u>dz</u>iqhu \'a<u>dzaa</u>ban kabiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2875, '25:20', 25, 20, ' wam<u>aa</u> arsaln<u>aa</u> qablaka mina <b>a</b>lmursaliina ill<u>aa</u> innahum laya/kuluuna <b>al</b><u>ththh</u>a\'<u>aa</u>ma wayamsyuuna fii <b>a</b>l-asw<u>aa</u>qi waja\'aln<u>aa</u> ba\'<u>dh</u>akum liba\'<u>dh</u>in fitnatan ata<u>sh</u>biruuna wak<u>aa</u>na rabbuka ba<u>sh</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2876, '25:21', 25, 21, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>iina l<u>aa</u> yarjuuna liq<u>aa</u>-an<u>aa</u> lawl<u>aa</u> unzila \'alayn<u>aa</u> <b>a</b>lmal<u>aa</u>-ikatu aw nar<u>aa</u> rabban<u>aa</u> laqadi istakbaruu fii anfusihim wa\'ataw \'utuwwan kabiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2877, '25:22', 25, 22, ' yawma yarawna <b>a</b>lmal<u>aa</u>-ikata l<u>aa</u> busyr<u>aa</u> yawma-i<u>dz</u>in lilmujrimiina wayaquuluuna <u>h</u>ijran ma<u>h</u>juur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2878, '25:23', 25, 23, ' waqadimn<u>aa</u> il<u>aa</u> m<u>aa</u> \'amiluu min \'amalin faja\'aln<u>aa</u>hu hab<u>aa</u>-an mantsuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2879, '25:24', 25, 24, ' a<u>sh</u>-<u>haa</u>bu <b>a</b>ljannati yawma-i<u>dz</u>in khayrun mustaqarran wa-a<u>h</u>sanu maqiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2880, '25:25', 25, 25, ' wayawma tasyaqqaqu <b>al</b>ssam<u>aa</u>u bi<b>a</b>lgham<u>aa</u>mi wanuzzila <b>a</b>lmal<u>aa</u>-ikatu tanziil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2881, '25:26', 25, 26, ' almulku yawma-i<u>dz</u>in <b>a</b>l<u>h</u>aqqu li<b>l</b>rra<u>h</u>m<u>aa</u>ni wak<u>aa</u>na yawman \'al<u>aa</u> <b>a</b>lk<u>aa</u>firiina \'asiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2882, '25:27', 25, 27, ' wayawma ya\'a<u>dhdh</u>u <b>al</b><u>zhzhaa</u>limu \'al<u>aa</u> yadayhi yaquulu y<u>aa</u> laytanii ittakha<u>dz</u>tu ma\'a <b>al</b>rrasuuli sabiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2883, '25:28', 25, 28, ' y<u>aa</u> waylat<u>aa</u> laytanii lam attakhi<u>dz</u> ful<u>aa</u>nan khaliil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2884, '25:29', 25, 29, ' laqad a<u>dh</u>allanii \'ani <b>al</b><u>dzdz</u>ikri ba\'da i<u>dz</u> j<u>aa</u>-anii wak<u>aa</u>na <b>al</b>sysyay<u>th</u><u>aa</u>nu lil-ins<u>aa</u>ni kha<u>dz</u>uul<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2885, '25:30', 25, 30, ' waq<u>aa</u>la <b>al</b>rrasuulu y<u>aa</u> rabbi inna qawmii ittakha<u>dz</u>uu h<u>aadzaa</u> <b>a</b>lqur-<u>aa</u>na mahjuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2886, '25:31', 25, 31, ' waka<u>dzaa</u>lika ja\'aln<u>aa</u> likulli nabiyyin \'aduwwan mina <b>a</b>lmujrimiina wakaf<u>aa</u> birabbika h<u>aa</u>diyan wana<u>sh</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2887, '25:32', 25, 32, ' waq<u>aa</u>la <b>al</b>la<u>dz</u>iina kafaruu lawl<u>aa</u> nuzzila \'alayhi <b>a</b>lqur-<u>aa</u>nu jumlatan w<u>aah</u>idatan ka<u>dzaa</u>lika linutsabbita bihi fu-<u>aa</u>daka warattaln<u>aa</u>hu tartiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2888, '25:33', 25, 33, ' wal<u>aa</u> ya/tuunaka bimatsalin ill<u>aa</u> ji/n<u>aa</u>ka bi<b>a</b>l<u>h</u>aqqi wa-a<u>h</u>sana tafsiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2889, '25:34', 25, 34, ' <b>al</b>la<u>dz</u>iina yu<u>h</u>syaruuna \'al<u>aa</u> wujuuhihim il<u>aa</u> jahannama ul<u>aa</u>-ika syarrun mak<u>aa</u>nan wa-a<u>dh</u>allu sabiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2890, '25:35', 25, 35, ' walaqad <u>aa</u>tayn<u>aa</u> muus<u>aa</u> <b>a</b>lkit<u>aa</u>ba waja\'aln<u>aa</u> ma\'ahu akh<u>aa</u>hu h<u>aa</u>ruuna waziir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2891, '25:36', 25, 36, ' faquln<u>aa</u> i<u>dz</u>hab<u>aa</u> il<u>aa</u> <b>a</b>lqawmi <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> fadammarn<u>aa</u>hum tadmiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2892, '25:37', 25, 37, ' waqawma nuu<u>h</u>in lamm<u>aa</u> ka<u>dzdz</u>abuu <b>al</b>rrusula aghraqn<u>aa</u>hum waja\'aln<u>aa</u>hum li<b>l</b>nn<u>aa</u>si <u>aa</u>yatan wa-a\'tadn<u>aa</u> li<b>l</b><u>zhzhaa</u>limiina \'a<u>dzaa</u>ban <b>a</b>liim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2893, '25:38', 25, 38, ' wa\'<u>aa</u>dan watsamuuda wa-a<u>sh</u>-<u>haa</u>ba <b>al</b>rrassi waquruunan bayna <u>dzaa</u>lika katsiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2894, '25:39', 25, 39, ' wakullan <u>dh</u>arabn<u>aa</u> lahu <b>a</b>l-amts<u>aa</u>la wakullan tabbarn<u>aa</u> tatbiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2895, '25:40', 25, 40, ' walaqad ataw \'al<u>aa</u> <b>a</b>lqaryati <b>al</b>latii um<u>th</u>irat ma<u>th</u>ara <b>al</b>ssaw-i afalam yakuunuu yarawnah<u>aa</u> bal k<u>aa</u>nuu l<u>aa</u> yarjuuna nusyuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2896, '25:41', 25, 41, ' wa-i<u>dzaa</u> ra-awka in yattakhi<u>dz</u>uunaka ill<u>aa</u> huzuwan ah<u>aadzaa</u> <b>al</b>la<u>dz</u>ii ba\'atsa <b>al</b>l<u>aa</u>hu rasuul<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2897, '25:42', 25, 42, ' in k<u>aa</u>da layu<u>dh</u>illun<u>aa</u> \'an <u>aa</u>lihatin<u>aa</u> lawl<u>aa</u> an <u>sh</u>abarn<u>aa</u> \'alayh<u>aa </u>wasawfa ya\'lamuuna <u>h</u>iina yarawna <b>a</b>l\'a<u>dzaa</u>ba man a<u>dh</u>allu sabiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2898, '25:43', 25, 43, ' ara-ayta mani ittakha<u>dz</u>a il<u>aa</u>hahu haw<u>aa</u>hu afa-anta takuunu \'alayhi wakiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2899, '25:44', 25, 44, ' am ta<u>h</u>sabu anna aktsarahum yasma\'uuna aw ya\'qiluuna in hum ill<u>aa</u> ka<b>a</b>l-an\'<u>aa</u>mi bal hum a<u>dh</u>allu sabiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2900, '25:45', 25, 45, ' alam tara il<u>aa</u> rabbika kayfa madda <b>al</b><u>zhzh</u>illa walaw sy<u>aa</u>-a laja\'alahu s<u>aa</u>kinan tsumma ja\'aln<u>aa</u> <b>al</b>sysyamsa \'alayhi daliil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2901, '25:46', 25, 46, ' tsumma qaba<u>dh</u>n<u>aa</u>hu ilayn<u>aa</u> qab<u>dh</u>an yasiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2902, '25:47', 25, 47, ' wahuwa <b>al</b>la<u>dz</u>ii ja\'ala lakumu <b>al</b>layla lib<u>aa</u>san wa<b>al</b>nnawma sub<u>aa</u>tan waja\'ala <b>al</b>nnah<u>aa</u>ra nusyuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2903, '25:48', 25, 48, ' wahuwa <b>al</b>la<u>dz</u>ii arsala <b>al</b>rriy<u>aah</u>a busyran bayna yaday ra<u>h</u>matihi wa-anzaln<u>aa</u> mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an <u>th</u>ahuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2904, '25:49', 25, 49, ' linu<u>h</u>yiya bihi baldatan maytan wanusqiyahu mimm<u>aa</u> khalaqn<u>aa</u> an\'<u>aa</u>man wa-an<u>aa</u>siyya katsiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2905, '25:50', 25, 50, ' walaqad <u>sh</u>arrafn<u>aa</u>hu baynahum liya<u>dzdz</u>akkaruu fa-ab<u>aa</u> aktsaru <b>al</b>nn<u>aa</u>si ill<u>aa</u> kufuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2906, '25:51', 25, 51, ' walaw syi/n<u>aa</u> laba\'atsn<u>aa</u> fii kulli qaryatin na<u>dz</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2907, '25:52', 25, 52, ' fal<u>aa</u> tu<u>th</u>i\'i <b>a</b>lk<u>aa</u>firiina waj<u>aa</u>hidhum bihi jih<u>aa</u>dan kabiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2908, '25:53', 25, 53, ' wahuwa <b>al</b>la<u>dz</u>ii maraja <b>a</b>lba<u>h</u>rayni h<u>aadzaa</u> \'a<u>dz</u>bun fur<u>aa</u>tun wah<u>aadzaa</u> mil<u>h</u>un uj<u>aa</u>jun waja\'ala baynahum<u>aa</u> barzakhan wa<u>h</u>ijran ma<u>h</u>juur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2909, '25:54', 25, 54, ' wahuwa <b>al</b>la<u>dz</u>ii khalaqa mina <b>a</b>lm<u>aa</u>-i basyaran faja\'alahu nasaban wa<u>sh</u>ihran wak<u>aa</u>na rabbuka qadiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2910, '25:55', 25, 55, ' waya\'buduuna min duuni <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> yanfa\'uhum wal<u>aa</u> ya<u>dh</u>urruhum wak<u>aa</u>na <b>a</b>lk<u>aa</u>firu \'al<u>aa</u> rabbihi <u>zh</u>ahiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2911, '25:56', 25, 56, ' wam<u>aa</u> arsaln<u>aa</u>ka ill<u>aa</u> mubasysyiran wana<u>dz</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2912, '25:57', 25, 57, ' qul m<u>aa</u> as-alukum \'alayhi min ajrin ill<u>aa</u> man sy<u>aa</u>-a an yattakhi<u>dz</u>a il<u>aa</u> rabbihi sabiil<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2913, '25:58', 25, 58, ' watawakkal \'al<u>aa</u> <b>a</b>l<u>h</u>ayyi <b>al</b>la<u>dz</u>ii l<u>aa</u> yamuutu wasabbi<u>h</u> bi<u>h</u>amdihi wakaf<u>aa</u> bihi bi<u>dz</u>unuubi \'ib<u>aa</u>dihi khabiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2914, '25:59', 25, 59, ' <b>al</b>la<u>dz</u>ii khalaqa <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a wam<u>aa</u> baynahum<u>aa</u> fii sittati ayy<u>aa</u>min tsumma istaw<u>aa</u> \'al<u>aa</u> <b>a</b>l\'arsyi <b>al</b>rra<u>h</u>m<u>aa</u>nu fa<b>i</b>s-al bihi khabiir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2915, '25:60', 25, 60, ' wa-i<u>dzaa</u> qiila lahumu usjuduu li<b>l</b>rra<u>h</u>m<u>aa</u>ni q<u>aa</u>luu wam<u>aa</u> <b>al</b>rra<u>h</u>m<u>aa</u>nu anasjudu lim<u>aa</u> ta/murun<u>aa</u> waz<u>aa</u>dahum nufuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2916, '25:61', 25, 61, ' tab<u>aa</u>raka <b>al</b>la<u>dz</u>ii ja\'ala fii <b>al</b>ssam<u>aa</u>-i buruujan waja\'ala fiih<u>aa</u> sir<u>aa</u>jan waqamaran muniir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2917, '25:62', 25, 62, ' wahuwa <b>al</b>la<u>dz</u>ii ja\'ala <b>al</b>layla wa<b>al</b>nnah<u>aa</u>ra khilfatan liman ar<u>aa</u>da an ya<u>dzdz</u>akkara aw ar<u>aa</u>da syukuur<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2918, '25:63', 25, 63, ' wa\'ib<u>aa</u>du <b>al</b>rra<u>h</u>m<u>aa</u>ni <b>al</b>la<u>dz</u>iina yamsyuuna \'al<u>aa</u> <b>a</b>l-ar<u>dh</u>i hawnan wa-i<u>dzaa</u> kh<u>aath</u>abahumu <b>a</b>lj<u>aa</u>hiluuna q<u>aa</u>luu sal<u>aa</u>m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2919, '25:64', 25, 64, ' wa<b>a</b>lla<u>dz</u>iina yabiituuna lirabbihim sujjadan waqiy<u>aa</u>m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2920, '25:65', 25, 65, ' wa<b>a</b>lla<u>dz</u>iina yaquuluuna rabban<u>aa</u> i<u>sh</u>rif \'ann<u>aa</u> \'a<u>dzaa</u>ba jahannama inna \'a<u>dzaa</u>bah<u>aa</u> k<u>aa</u>na ghar<u>aa</u>m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2921, '25:66', 25, 66, ' innah<u>aa</u> s<u>aa</u>-at mustaqarran wamuq<u>aa</u>m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2922, '25:67', 25, 67, ' wa<b>a</b>lla<u>dz</u>iina i<u>dzaa</u> anfaquu lam yusrifuu walam yaqturuu wak<u>aa</u>na bayna <u>dzaa</u>lika qaw<u>aa</u>m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2923, '25:68', 25, 68, ' wa<b>a</b>lla<u>dz</u>iina l<u>aa</u> yad\'uuna ma\'<u>aa</u> <b>al</b>l<u>aa</u>hi il<u>aa</u>han <u>aa</u>khara wal<u>aa</u> yaqtuluuna <b>al</b>nnafsa <b>al</b>latii <u>h</u>arrama <b>al</b>l<u>aa</u>hu ill<u>aa</u> bi<b>a</b>l<u>h</u>aqqi wal<u>aa</u> yaznuuna waman yaf\'al <u>dzaa</u>lika yalqa ats<u>aa</u>m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2924, '25:69', 25, 69, ' yu<u>daa</u>\'af lahu <b>a</b>l\'a<u>dzaa</u>bu yawma <b>a</b>lqiy<u>aa</u>mati wayakhlud fiihi muh<u>aa</u>n<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2925, '25:70', 25, 70, ' ill<u>aa</u> man t<u>aa</u>ba wa<u>aa</u>mana wa\'amila \'amalan <u>shaa</u>li<u>h</u>an faul<u>aa</u>-ika yubaddilu <b>al</b>l<u>aa</u>hu sayyi-<u>aa</u>tihim <u>h</u>asan<u>aa</u>tin wak<u>aa</u>na <b>al</b>l<u>aa</u>hu ghafuuran ra<u>h</u>iim<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2926, '25:71', 25, 71, ' waman t<u>aa</u>ba wa\'amila <u>shaa</u>li<u>h</u>an fa-innahu yatuubu il<u>aa</u> <b>al</b>l<u>aa</u>hi mat<u>aa</u>b<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2927, '25:72', 25, 72, ' wa<b>a</b>lla<u>dz</u>iina l<u>aa</u> yasyhaduuna <b>al</b>zzuura wa-i<u>dzaa</u> marruu bi<b>a</b>llaghwi marruu kir<u>aa</u>m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2928, '25:73', 25, 73, ' wa<b>a</b>lla<u>dz</u>iina i<u>dzaa</u> <u>dz</u>ukkiruu bi-<u>aa</u>y<u>aa</u>ti rabbihim lam yakhirruu \'alayh<u>aa</u> <u>sh</u>umman wa\'umy<u>aa</u>n<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2929, '25:74', 25, 74, ' wa<b>a</b>lla<u>dz</u>iina yaquuluuna rabban<u>aa</u> hab lan<u>aa</u> min azw<u>aa</u>jin<u>aa</u> wa<u>dz</u>urriyy<u>aa</u>tin<u>aa</u> qurrata a\'yunin wa<b>i</b>j\'aln<u>aa</u> lilmuttaqiina im<u>aa</u>m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2930, '25:75', 25, 75, ' ul<u>aa</u>-ika yujzawna <b>a</b>lghurfata bim<u>aa</u> <u>sh</u>abaruu wayulaqqawna fiih<u>aa</u> ta<u>h</u>iyyatan wasal<u>aa</u>m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2931, '25:76', 25, 76, ' kh<u>aa</u>lidiina fiih<u>aa</u> <u>h</u>asunat mustaqarran wamuq<u>aa</u>m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2932, '25:77', 25, 77, ' qul m<u>aa</u> ya\'bau bikum rabbii lawl<u>aa</u> du\'<u>aa</u>ukum faqad ka<u>dzdz</u>abtum fasawfa yakuunu liz<u>aa</u>m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2933, '26:1', 26, 1, ' <u>th</u><u>aa</u>-siin-miim</u>\r\r\n');
INSERT INTO quranLatin VALUES (2934, '26:2', 26, 2, ' tilka <u>aa</u>y<u>aa</u>tu <b>a</b>lkit<u>aa</u>bi <b>a</b>lmubiin<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2935, '26:3', 26, 3, ' la\'allaka b<u>aa</u>khi\'un nafsaka <b>al</b>l<u>aa</u> yakuunuu mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2936, '26:4', 26, 4, ' in nasya/ nunazzil \'alayhim mina <b>al</b>ssam<u>aa</u>-i <u>aa</u>yatan fa<u>zh</u>allat a\'n<u>aa</u>quhum lah<u>aa</u> kh<u>aad</u>i\'iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2937, '26:5', 26, 5, ' wam<u>aa</u> ya/tiihim min <u>dz</u>ikrin mina <b>al</b>rra<u>h</u>m<u>aa</u>ni mu<u>h</u>datsin ill<u>aa</u> k<u>aa</u>nuu \'anhu mu\'ri<u>dh</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2938, '26:6', 26, 6, ' faqad ka<u>dzdz</u>abuu fasaya/tiihim anb<u>aa</u>u m<u>aa</u> k<u>aa</u>nuu bihi yastahzi-uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2939, '26:7', 26, 7, ' awa lam yaraw il<u>aa</u> <b>a</b>l-ar<u>dh</u>i kam anbatn<u>aa</u> fiih<u>aa</u> min kulli zawjin kariim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2940, '26:8', 26, 8, ' inna fii <u>dzaa</u>lika la<u>aa</u>yatan wam<u>aa</u> k<u>aa</u>na aktsaruhum mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2941, '26:9', 26, 9, ' wa-inna rabbaka lahuwa <b>a</b>l\'aziizu <b>al</b>rra<u>h</u>iim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2942, '26:10', 26, 10, ' wa-i<u>dz</u> n<u>aa</u>d<u>aa</u> rabbuka muus<u>aa</u> ani i/ti <b>a</b>lqawma <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2943, '26:11', 26, 11, ' qawma fir\'awna <b>a</b>l<u>aa</u> yattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2944, '26:12', 26, 12, ' q<u>aa</u>la rabbi innii akh<u>aa</u>fu an yuka<u>dzdz</u>ibuun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2945, '26:13', 26, 13, ' waya<u>dh</u>iiqu <u>sh</u>adrii wal<u>aa</u> yan<u>th</u>aliqu lis<u>aa</u>nii fa-arsil il<u>aa</u> h<u>aa</u>ruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2946, '26:14', 26, 14, ' walahum \'alayya <u>dz</u>anbun fa-akh<u>aa</u>fu an yaqtuluun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2947, '26:15', 26, 15, ' q<u>aa</u>la kall<u>aa</u> fa<b>i</b><u>dz</u>hab<u>aa</u> bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> inn<u>aa</u> ma\'akum mustami\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2948, '26:16', 26, 16, ' fa/tiy<u>aa</u> fir\'awna faquul<u>aa</u> inn<u>aa</u> rasuulu rabbi <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2949, '26:17', 26, 17, ' an arsil ma\'an<u>aa</u> banii isr<u>aa</u>-iil<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2950, '26:18', 26, 18, ' q<u>aa</u>la alam nurabbika fiin<u>aa</u> waliidan walabitsta fiin<u>aa</u> min \'umurika siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2951, '26:19', 26, 19, ' wafa\'alta fa\'lataka <b>al</b>latii fa\'alta wa-anta mina <b>a</b>lk<u>aa</u>firiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2952, '26:20', 26, 20, ' q<u>aa</u>la fa\'altuh<u>aa</u> i<u>dz</u>an wa-an<u>aa</u> mina <b>al</b><u>dhdhaa</u>lliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2953, '26:21', 26, 21, ' fafarartu minkum lamm<u>aa</u> khiftukum fawahaba lii rabbii <u>h</u>ukman waja\'alanii mina <b>a</b>lmursaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2954, '26:22', 26, 22, ' watilka ni\'matun tamunnuh<u>aa</u> \'alayya an \'abbadta banii isr<u>aa</u>-iil<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2955, '26:23', 26, 23, ' q<u>aa</u>la fir\'awnu wam<u>aa</u> rabbu <b>a</b>l\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2956, '26:24', 26, 24, ' q<u>aa</u>la rabbu <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wam<u>aa</u> baynahum<u>aa</u> in kuntum muuqiniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2957, '26:25', 26, 25, ' q<u>aa</u>la liman <u>h</u>awlahu <b>a</b>l<u>aa</u> tastami\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2958, '26:26', 26, 26, ' q<u>aa</u>la rabbukum warabbu <u>aa</u>b<u>aa</u>-ikumu <b>a</b>l-awwaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2959, '26:27', 26, 27, ' q<u>aa</u>la inna rasuulakumu <b>al</b>la<u>dz</u>ii ursila ilaykum lamajnuun<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2960, '26:28', 26, 28, ' q<u>aa</u>la rabbu <b>a</b>lmasyriqi wa<b>a</b>lmaghribi wam<u>aa</u> baynahum<u>aa</u> in kuntum ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2961, '26:29', 26, 29, ' q<u>aa</u>la la-ini ittakha<u>dz</u>ta il<u>aa</u>han ghayrii la-aj\'alannaka mina <b>a</b>lmasjuuniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2962, '26:30', 26, 30, ' q<u>aa</u>la awa law ji/tuka bisyay-in mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2963, '26:31', 26, 31, ' q<u>aa</u>la fa/ti bihi in kunta mina <b>al</b><u>shshaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2964, '26:32', 26, 32, ' fa-alq<u>aa</u> \'a<u>shaa</u>hu fa-i<u>dzaa</u> hiya tsu\'b<u>aa</u>nun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2965, '26:33', 26, 33, ' wanaza\'a yadahu fa-i<u>dzaa</u> hiya bay<u>daa</u>u li<b>l</b>nn<u>aats</u>iriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2966, '26:34', 26, 34, ' q<u>aa</u>la lilmala-i <u>h</u>awlahu inna h<u>aadzaa</u> las<u>aah</u>irun \'aliim<b>un</b>\r\r\n');
