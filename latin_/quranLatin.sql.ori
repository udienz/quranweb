INSERT INTO quranLatin VALUES (2386, '20:38', 20, 38, ' i<u>dz</u> aw<u>h</u>ayn<u>aa</u> il<u>aa</u> ummika m<u>aa</u> yuu<u>haa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2387, '20:39', 20, 39, ' ani iq<u>dz</u>ifiihi fii <b>al</b>tt<u>aa</u>buuti fa<b>i</b>q<u>dz</u>ifiihi fii <b>a</b>lyammi falyulqihi <b>a</b>lyammu bi<b>al</b>ss<u>aah</u>ili ya/khu<u>dz</u>hu \'aduwwun lii wa\'aduwwun lahu wa-alqaytu \'alayka ma<u>h</u>abbatan minnii walitu<u>sh</u>na\'a \'al<u>aa</u> \'aynii</u>\r\r\n');
INSERT INTO quranLatin VALUES (2388, '20:40', 20, 40, ' i<u>dz</u> tamsyii ukhtuka fataquulu hal adullukum \'al<u>aa</u> man yakfuluhu faraja\'n<u>aa</u>ka il<u>aa</u> ummika kay taqarra \'aynuh<u>aa</u> wal<u>aa</u> ta<u>h</u>zana waqatalta nafsan fanajjayn<u>aa</u>ka mina <b>a</b>lghammi wafatann<u>aa</u>ka futuunan falabitsta siniina fii ahli madyana tsumma ji/ta \'al<u>aa</u> qadarin y<u>aa</u> muus<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2389, '20:41', 20, 41, ' wa<b>i</b><u>sth</u>ana\'tuka linafsii</u>\r\r\n');
INSERT INTO quranLatin VALUES (2390, '20:42', 20, 42, ' i<u>dz</u>hab anta wa-akhuuka bi-<u>aa</u>y<u>aa</u>tii wal<u>aa</u> taniy<u>aa</u> fii <u>dz</u>ikrii</u>\r\r\n');
INSERT INTO quranLatin VALUES (2391, '20:43', 20, 43, ' i<u>dz</u>hab<u>aa</u> il<u>aa</u> fir\'awna innahu <u>th</u>agh<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2392, '20:44', 20, 44, ' faquul<u>aa</u> lahu qawlan layyinan la\'allahu yata<u>dz</u>akkaru aw yakhsy<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2393, '20:45', 20, 45, ' q<u>aa</u>l<u>aa</u> rabban<u>aa</u> innan<u>aa</u> nakh<u>aa</u>fu an yafru<u>th</u>a \'alayn<u>aa</u> aw an ya<u>th</u>gh<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2394, '20:46', 20, 46, ' q<u>aa</u>la l<u>aa</u> takh<u>aa</u>f<u>aa</u> innanii ma\'akum<u>aa</u> asma\'u wa-ar<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2395, '20:47', 20, 47, ' fa/tiy<u>aa</u>hu faquul<u>aa</u> inn<u>aa</u> rasuul<u>aa</u> rabbika fa-arsil ma\'an<u>aa</u> banii isr<u>aa</u>-iila wal<u>aa</u> tu\'a<u>dzdz</u>ibhum qad ji/n<u>aa</u>ka bi-<u>aa</u>yatin min rabbika wa<b>al</b>ssal<u>aa</u>mu \'al<u>aa</u> mani ittaba\'a <b>a</b>lhud<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2396, '20:48', 20, 48, ' inn<u>aa</u> qad uu<u>h</u>iya ilayn<u>aa</u> anna <b>a</b>l\'a<u>dzaa</u>ba \'al<u>aa</u> man ka<u>dzdz</u>aba watawall<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2397, '20:49', 20, 49, ' q<u>aa</u>la faman rabbukum<u>aa</u> y<u>aa</u> muus<u>aa</u> </u>\r\r\n');
INSERT INTO quranLatin VALUES (2398, '20:50', 20, 50, ' q<u>aa</u>la rabbun<u>aa</u> <b>al</b>la<u>dz</u>ii a\'<u>th</u><u>aa</u> kulla syay-in khalqahu tsumma had<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2399, '20:51', 20, 51, ' q<u>aa</u>la fam<u>aa</u> b<u>aa</u>lu <b>a</b>lquruuni <b>a</b>l-uul<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2400, '20:52', 20, 52, ' q<u>aa</u>la \'ilmuh<u>aa</u> \'inda rabbii fii kit<u>aa</u>bin l<u>aa</u> ya<u>dh</u>illu rabbii wal<u>aa</u> yans<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2401, '20:53', 20, 53, ' <b>al</b>la<u>dz</u>ii ja\'ala lakumu <b>a</b>l-ar<u>dh</u>a mahdan wasalaka lakum fiih<u>aa</u> subulan wa-anzala mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an fa-akhrajn<u>aa</u> bihi azw<u>aa</u>jan min nab<u>aa</u>tin syatt<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2402, '20:54', 20, 54, ' kuluu wa<b>i</b>r\'aw an\'<u>aa</u>makum inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin li-ulii <b>al</b>nnuh<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2403, '20:55', 20, 55, ' minh<u>aa</u> khalaqn<u>aa</u>kum wafiih<u>aa</u> nu\'iidukum waminh<u>aa</u> nukhrijukum t<u>aa</u>ratan ukhr<u>aa</u> </u>\r\r\n');
INSERT INTO quranLatin VALUES (2404, '20:56', 20, 56, ' walaqad arayn<u>aa</u>hu <u>aa</u>y<u>aa</u>tin<u>aa</u> kullah<u>aa</u> faka<u>dzdz</u>aba wa-ab<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2405, '20:57', 20, 57, ' q<u>aa</u>la aji/tan<u>aa</u> litukhrijan<u>aa</u> min ar<u>dh</u>in<u>aa</u> bisi<u>h</u>rika y<u>aa</u> muus<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2406, '20:58', 20, 58, ' falana/tiyannaka bisi<u>h</u>rin mitslihi fa<b>i</b>j\'al baynan<u>aa</u> wabaynaka maw\'idan l<u>aa</u> nukhlifuhu na<u>h</u>nu wal<u>aa</u> anta mak<u>aa</u>nan suw<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2407, '20:59', 20, 59, ' q<u>aa</u>la maw\'idukum yawmu <b>al</b>zziinati wa-an yu<u>h</u>syara <b>al</b>nn<u>aa</u>su <u>dh</u>u<u>haa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2408, '20:60', 20, 60, ' fatawall<u>aa</u> fir\'awnu fajama\'a kaydahu tsumma at<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2409, '20:61', 20, 61, ' q<u>aa</u>la lahum muus<u>aa</u> waylakum l<u>aa</u> taftaruu \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ka<u>dz</u>iban fayus<u>h</u>itakum bi\'a<u>dzaa</u>bin waqad kh<u>aa</u>ba mani iftar<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2410, '20:62', 20, 62, ' fatan<u>aa</u>za\'uu amrahum baynahum wa-asarruu <b>al</b>nnajw<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2411, '20:63', 20, 63, ' q<u>aa</u>luu in h<u>aadzaa</u>ni las<u>aah</u>ir<u>aa</u>ni yuriid<u>aa</u>ni an yukhrij<u>aa</u>kum min ar<u>dh</u>ikum bisi<u>h</u>rihim<u>aa</u> waya<u>dz</u>hab<u>aa</u> bi<u>th</u>ariiqatikumu <b>a</b>lmutsl<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2412, '20:64', 20, 64, ' fa-ajmi\'uu kaydakum tsumma i/tuu <u>sh</u>affan waqad afla<u>h</u>a <b>a</b>lyawma mani ista\'l<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2413, '20:65', 20, 65, ' q<u>aa</u>luu y<u>aa</u> muus<u>aa</u> imm<u>aa</u> an tulqiya wa-imm<u>aa</u> an nakuuna awwala man <b>a</b>lq<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2414, '20:66', 20, 66, ' q<u>aa</u>la bal <b>a</b>lquu fa-i<u>dzaa</u> <u>h</u>ib<u>aa</u>luhum wa\'i<u>sh</u>iyyuhum yukhayyalu ilayhi min si<u>h</u>rihim annah<u>aa</u> tas\'<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2415, '20:67', 20, 67, ' fa-awjasa fii nafsihi khiifatan muus<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2416, '20:68', 20, 68, ' quln<u>aa</u> l<u>aa</u> takhaf innaka anta <b>a</b>l-a\'l<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2417, '20:69', 20, 69, ' wa-alqi m<u>aa</u> fii yamiinika talqaf m<u>aa</u> <u>sh</u>ana\'uu innam<u>aa</u> <u>sh</u>ana\'uu kaydu s<u>aah</u>irin wal<u>aa</u> yufli<u>h</u>u <b>al</b>ss<u>aah</u>iru <u>h</u>aytsu at<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2418, '20:70', 20, 70, ' faulqiya <b>al</b>ssa<u>h</u>aratu sujjadan q<u>aa</u>luu <u>aa</u>mann<u>aa</u> birabbi h<u>aa</u>ruuna wamuus<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2419, '20:71', 20, 71, ' q<u>aa</u>la <u>aa</u>mantum lahu qabla an <u>aadz</u>ana lakum innahu lakabiirukumu <b>al</b>la<u>dz</u>ii \'allamakumu <b>al</b>ssi<u>h</u>ra falauqa<u>ththh</u>i\'anna aydiyakum wa-arjulakum min khil<u>aa</u>fin walau<u>sh</u>allibannakum fii ju<u>dz</u>uu\'i <b>al</b>nnakhli walata\'lamunna ayyun<u>aa</u> asyaddu \'a<u>dzaa</u>ban wa-abq<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2420, '20:72', 20, 72, ' q<u>aa</u>luu lan nu/tsiraka \'al<u>aa</u> m<u>aa</u> j<u>aa</u>-an<u>aa</u> mina <b>a</b>lbayyin<u>aa</u>ti wa<b>a</b>lla<u>dz</u>ii fa<u>th</u>aran<u>aa</u> fa<b>i</b>q<u>dh</u>i m<u>aa</u> anta q<u>aad</u>in innam<u>aa</u> taq<u>dh</u>ii h<u>aadz</u>ihi <b>a</b>l<u>h</u>ay<u>aa</u>ta <b>al</b>dduny<u>aa</u> </u>\r\r\n');
INSERT INTO quranLatin VALUES (2421, '20:73', 20, 73, ' inn<u>aa</u> <u>aa</u>mann<u>aa</u> birabbin<u>aa</u> liyaghfira lan<u>aa</u> kha<u>th</u><u>aa</u>y<u>aa</u>n<u>aa</u> wam<u>aa</u> akrahtan<u>aa</u> \'alayhi mina <b>al</b>ssi<u>h</u>ri wa<b>al</b>l<u>aa</u>hu khayrun wa-abq<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2422, '20:74', 20, 74, ' innahu man ya/ti rabbahu mujriman fa-inna lahu jahannama l<u>aa</u> yamuutu fiih<u>aa</u> wal<u>aa</u> ya<u>h</u>y<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2423, '20:75', 20, 75, ' waman ya/tihi mu/minan qad \'amila <b>al</b><u>shshaa</u>li<u>haa</u>ti faul<u>aa</u>-ika lahumu <b>al</b>ddaraj<u>aa</u>tu <b>a</b>l\'ul<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2424, '20:76', 20, 76, ' jann<u>aa</u>tu \'<u>aa</u>dnin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru kh<u>aa</u>lidiina fiih<u>aa</u> wa<u>dzaa</u>lika jaz<u>aa</u>u man tazakk<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2425, '20:77', 20, 77, ' walaqad aw<u>h</u>ayn<u>aa</u> il<u>aa</u> muus<u>aa</u> an asri bi\'ib<u>aa</u>dii fa<b>i</b><u>dh</u>rib lahum <u>th</u>ariiqan fii <b>a</b>lba<u>h</u>ri yabasan l<u>aa</u> takh<u>aa</u>fu darakan wal<u>aa</u> takhsy<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2426, '20:78', 20, 78, ' fa-atba\'ahum fir\'awnu bijunuudihi faghasyiyahum mina <b>a</b>lyammi m<u>aa</u> ghasyiyahum</u>\r\r\n');
INSERT INTO quranLatin VALUES (2427, '20:79', 20, 79, ' wa-a<u>dh</u>alla fir\'awnu qawmahu wam<u>aa</u> had<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2428, '20:80', 20, 80, ' y<u>aa</u> banii isr<u>aa</u>-iila qad anjayn<u>aa</u>kum min \'aduwwikum waw<u>aa</u>\'adn<u>aa</u>kum j<u>aa</u>niba <b>al</b><u>ththh</u>uuri <b>a</b>l-aymana wanazzaln<u>aa</u> \'alaykumu <b>a</b>lmanna wa<b>al</b>ssalw<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2429, '20:81', 20, 81, ' kuluu min <u>th</u>ayyib<u>aa</u>ti m<u>aa</u> razaqn<u>aa</u>kum wal<u>aa</u> ta<u>th</u>ghaw fiihi faya<u>h</u>illa \'alaykum gha<u>dh</u>abii waman ya<u>h</u>lil \'alayhi gha<u>dh</u>abii faqad haw<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2430, '20:82', 20, 82, ' wa-innii laghaff<u>aa</u>run liman t<u>aa</u>ba wa<u>aa</u>mana wa\'amila <u>shaa</u>li<u>h</u>an tsumma ihtad<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2431, '20:83', 20, 83, ' wam<u>aa</u> a\'jalaka \'an qawmika y<u>aa</u> muus<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2432, '20:84', 20, 84, ' q<u>aa</u>la hum ul<u>aa</u>-i \'al<u>aa</u> atsarii wa\'ajiltu ilayka rabbi litar<u>daa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2433, '20:85', 20, 85, ' q<u>aa</u>la fa-inn<u>aa</u> qad fatann<u>aa</u> qawmaka min ba\'dika wa-a<u>dh</u>allahumu <b>al</b>ss<u>aa</u>miriy<b>yu</b>\r\r\n');
INSERT INTO quranLatin VALUES (2434, '20:86', 20, 86, ' faraja\'a muus<u>aa</u> il<u>aa</u> qawmihi gha<u>dh</u>b<u>aa</u>na asifan q<u>aa</u>la y<u>aa</u> qawmi alam ya\'idkum rabbukum wa\'dan <u>h</u>asanan afa<u>th</u><u>aa</u>la \'alaykumu al\'ahdu am aradtum an ya<u>h</u>illa \'alaykum gha<u>dh</u>abun min rabbikum fa-akhlaftum maw\'idii</u>\r\r\n');
INSERT INTO quranLatin VALUES (2435, '20:87', 20, 87, ' q<u>aa</u>luu m<u>aa</u> akhlafn<u>aa</u> maw\'idaka bimalkin<u>aa</u> wal<u>aa</u>kinn<u>aa</u> <u>h</u>ummiln<u>aa</u> awz<u>aa</u>ran min ziinati <b>a</b>lqawmi faqa<u>dz</u>afn<u>aa</u>h<u>aa</u> faka<u>dzaa</u>lika <b>a</b>lq<u>aa</u> <b>al</b>ss<u>aa</u>miriy<b>yu</b>\r\r\n');
INSERT INTO quranLatin VALUES (2436, '20:88', 20, 88, ' fa-akhraja lahum \'ijlan jasadan lahu khuw<u>aa</u>run faq<u>aa</u>luu h<u>aadzaa</u> il<u>aa</u>hukum wa-il<u>aa</u>hu muus<u>aa</u> fanasiy<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2437, '20:89', 20, 89, ' afal<u>aa</u> yarawna <b>al</b>l<u>aa</u> yarji\'u ilayhim qawlan wal<u>aa</u> yamliku lahum <u>dh</u>arran wal<u>aa</u> naf\'<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2438, '20:90', 20, 90, ' walaqad q<u>aa</u>la lahum h<u>aa</u>ruunu min qablu y<u>aa</u> qawmi innam<u>aa</u> futintum bihi wa-inna rabbakumu <b>al</b>rra<u>h</u>m<u>aa</u>nu fa<b>i</b>ttabi\'uunii wa-a<u>th</u>ii\'uu amrii</u>\r\r\n');
INSERT INTO quranLatin VALUES (2439, '20:91', 20, 91, ' q<u>aa</u>luu lan nabra<u>h</u>a \'alayhi \'<u>aa</u>kifiina <u>h</u>att<u>aa</u> yarji\'a ilayn<u>aa</u> muus<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2440, '20:92', 20, 92, ' q<u>aa</u>la y<u>aa</u> h<u>aa</u>ruunu m<u>aa</u> mana\'aka i<u>dz</u> ra-aytahum <u>dh</u>alluu </u>\r\r\n');
INSERT INTO quranLatin VALUES (2441, '20:93', 20, 93, ' <b>al</b>l<u>aa</u> tattabi\'ani afa\'a<u>sh</u>ayta amrii</u>\r\r\n');
INSERT INTO quranLatin VALUES (2442, '20:94', 20, 94, ' q<u>aa</u>la yabnaumma l<u>aa</u> ta/khu<u>dz</u> bili<u>h</u>yatii wal<u>aa</u> bira/sii innii khasyiitu an taquula farraqta bayna banii isr<u>aa</u>-iila walam tarqub qawlii</u>\r\r\n');
INSERT INTO quranLatin VALUES (2443, '20:95', 20, 95, ' q<u>aa</u>la fam<u>aa</u> kha<u>th</u>buka y<u>aa</u> s<u>aa</u>miriy<b>yu</b>\r\r\n');
INSERT INTO quranLatin VALUES (2444, '20:96', 20, 96, ' q<u>aa</u>la ba<u>sh</u>urtu bim<u>aa</u> lam yab<u>sh</u>uruu bihi faqaba<u>dh</u>tu qab<u>dh</u>atan min atsari <b>al</b>rrasuuli fanaba<u>dz</u>tuh<u>aa</u> waka<u>dzaa</u>lika sawwalat lii nafsii</u>\r\r\n');
INSERT INTO quranLatin VALUES (2445, '20:97', 20, 97, ' q<u>aa</u>la fa<b>i</b><u>dz</u>hab fa-inna laka fii <b>a</b>l<u>h</u>ay<u>aa</u>ti an taquula l<u>aa</u> mis<u>aa</u>sa wa-inna laka maw\'idan lan tukhlafahu wa<b>u</b>n<u>zh</u>ur il<u>aa</u> il<u>aa</u>hika <b>al</b>la<u>dz</u>ii <u>zh</u>alta \'alayhi \'<u>aa</u>kifan lanu<u>h</u>arriqannahu tsumma lanansifannahu fii <b>a</b>lyammi nasf<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2446, '20:98', 20, 98, ' innam<u>aa</u> il<u>aa</u>hukumu <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>ii l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa wasi\'a kulla syay-in \'ilm<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2447, '20:99', 20, 99, ' ka<u>dzaa</u>lika naqu<u>shsh</u>u \'alayka min anb<u>aa</u>-i m<u>aa</u> qad sabaqa waqad <u>aa</u>tayn<u>aa</u>ka min ladunn<u>aa</u> tsikra<b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2448, '20:100', 20, 100, ' man a\'ra<u>dh</u>a \'anhu fa-innahu ya<u>h</u>milu yawma <b>a</b>lqiy<u>aa</u>mati wizr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2449, '20:101', 20, 101, ' kh<u>aa</u>lidiina fiihi was<u>aa</u>-a lahum yawma <b>a</b>lqiy<u>aa</u>mati <u>h</u>iml<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2450, '20:102', 20, 102, ' yawma yunfakhu fii <b>al</b><u>shsh</u>uuri wana<u>h</u>syuru <b>a</b>lmujrimiina yawma-i<u>dz</u>in zurq<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2451, '20:103', 20, 103, ' yatakh<u>aa</u>fatuuna baynahum in labitstum ill<u>aa</u> \'asyr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2452, '20:104', 20, 104, ' na<u>h</u>nu a\'lamu bim<u>aa</u> yaquuluuna i<u>dz</u> yaquulu amtsaluhum <u>th</u>ariiqatan in labitstum ill<u>aa</u> yawm<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2453, '20:105', 20, 105, ' wayas-aluunaka \'ani <b>a</b>ljib<u>aa</u>li faqul yansifuh<u>aa</u> rabbii nasf<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2454, '20:106', 20, 106, ' faya<u>dz</u>aruh<u>aa</u> q<u>aa</u>\'an <u>sh</u>af<u>sh</u>af<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2455, '20:107', 20, 107, ' l<u>aa</u> tar<u>aa</u> fiih<u>aa</u> \'iwajan wal<u>aa</u> amt<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2456, '20:108', 20, 108, ' yawma-i<u>dz</u>in yattabi\'uuna <b>al</b>dd<u>aa</u>\'iya l<u>aa</u> \'iwaja lahu wakhasya\'ati <b>a</b>l-a<u>sh</u>w<u>aa</u>tu li<b>l</b>rra<u>h</u>m<u>aa</u>ni fal<u>aa</u> tasma\'u ill<u>aa</u> hams<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2457, '20:109', 20, 109, ' yawma-i<u>dz</u>in l<u>aa</u> tanfa\'u <b>al</b>sysyaf<u>aa</u>\'atu ill<u>aa</u> man a<u>dz</u>ina lahu <b>al</b>rra<u>h</u>m<u>aa</u>nu wara<u>dh</u>iya lahu qawl<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2458, '20:110', 20, 110, ' ya\'lamu m<u>aa</u> bayna aydiihim wam<u>aa</u> khalfahum wal<u>aa</u> yu<u>h</u>ii<u>th</u>uuna bihi \'ilm<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2459, '20:111', 20, 111, ' wa\'anati <b>a</b>lwujuuhu lil<u>h</u>ayyi <b>a</b>lqayyuumi waqad kh<u>aa</u>ba man <u>h</u>amala <u>zh</u>ulm<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2460, '20:112', 20, 112, ' waman ya\'mal mina <b>al</b><u>shshaa</u>li<u>haa</u>ti wahuwa mu/minun fal<u>aa</u> yakh<u>aa</u>fu <u>zh</u>ulman wal<u>aa</u> ha<u>dh</u>m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2461, '20:113', 20, 113, ' waka<u>dzaa</u>lika anzaln<u>aa</u>hu qur-<u>aa</u>nan \'arabiyyan wa<u>sh</u>arrafn<u>aa</u> fiihi mina <b>a</b>lwa\'iidi la\'allahum yattaquuna aw yu<u>h</u>ditsu lahum <u>dz</u>ikr<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2462, '20:114', 20, 114, ' fata\'<u>aa</u>l<u>aa</u> <b>al</b>l<u>aa</u>hu <b>a</b>lmaliku <b>a</b>l<u>h</u>aqqu wal<u>aa</u> ta\'jal bi<b>a</b>lqur-<u>aa</u>ni min qabli an yuq<u>daa</u> ilayka wa<u>h</u>yuhu waqul rabbi zidnii \'ilm<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2463, '20:115', 20, 115, ' walaqad \'ahidn<u>aa</u> il<u>aa</u> <u>aa</u>dama min qablu fanasiya walam najid lahu \'azm<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2464, '20:116', 20, 116, ' wa-i<u>dz</u> quln<u>aa</u> lilmal<u>aa</u>-ikati usjuduu li-<u>aa</u>dama fasajaduu ill<u>aa</u> ibliisa ab<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2465, '20:117', 20, 117, ' faquln<u>aa</u> y<u>aa</u> <u>aa</u>damu inna h<u>aadzaa</u> \'aduwwun laka walizawjika fal<u>aa</u> yukhrijannakum<u>aa</u> mina <b>a</b>ljannati fatasyq<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2466, '20:118', 20, 118, ' inna laka <b>al</b>l<u>aa</u> tajuu\'a fiih<u>aa</u> wal<u>aa</u> ta\'r<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2467, '20:119', 20, 119, ' wa-annaka l<u>aa</u> ta<u>zh</u>mau fiih<u>aa</u> wal<u>aa</u> ta<u>dhaa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2468, '20:120', 20, 120, ' fawaswasa ilayhi <b>al</b>sysyay<u>th</u><u>aa</u>nu q<u>aa</u>la y<u>aa</u> <u>aa</u>damu hal adulluka \'al<u>aa</u> syajarati <b>a</b>lkhuldi wamulkin l<u>aa</u> yabl<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2469, '20:121', 20, 121, ' fa-akal<u>aa</u> minh<u>aa</u> fabadat lahum<u>aa</u> saw-<u>aa</u>tuhum<u>aa</u> wa<u>th</u>afiq<u>aa</u> yakh<u>sh</u>if<u>aa</u>ni \'alayhim<u>aa</u> min waraqi <b>a</b>ljannati wa\'a<u>shaa</u> <u>aa</u>damu rabbahu faghaw<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2470, '20:122', 20, 122, ' tsumma ijtab<u>aa</u>hu rabbuhu fat<u>aa</u>ba \'alayhi wahad<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2471, '20:123', 20, 123, ' q<u>aa</u>la ihbi<u>th</u><u>aa</u> minh<u>aa</u> jamii\'an ba\'<u>dh</u>ukum liba\'<u>dh</u>in \'aduwwun fa-imm<u>aa</u> ya/tiyannakum minnii hudan famani ittaba\'a hud<u>aa</u>ya fal<u>aa</u> ya<u>dh</u>illu wal<u>aa</u> yasyq<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2472, '20:124', 20, 124, ' waman a\'ra<u>dh</u>a \'an <u>dz</u>ikrii fa-inna lahu ma\'iisyatan <u>dh</u>ank<u>aa</u><b>n</b> wana<u>h</u>syuruhu yawma <b>a</b>lqiy<u>aa</u>mati a\'m<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2473, '20:125', 20, 125, ' q<u>aa</u>la rabbi lima <u>h</u>asyartanii a\'m<u>aa</u> waqad kuntu ba<u>sh</u>iir<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2474, '20:126', 20, 126, ' q<u>aa</u>la ka<u>dzaa</u>lika atatka <u>aa</u>y<u>aa</u>tun<u>aa</u> fanasiitah<u>aa</u> waka<u>dzaa</u>lika <b>a</b>lyawma tuns<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2475, '20:127', 20, 127, ' waka<u>dzaa</u>lika najzii man asrafa walam yu/min bi-<u>aa</u>y<u>aa</u>ti rabbihi wala\'a<u>dzaa</u>bu <b>a</b>l-<u>aa</u>khirati asyaddu wa-abq<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2476, '20:128', 20, 128, ' afalam yahdi lahum kam ahlakn<u>aa</u> qablahum mina <b>a</b>lquruuni yamsyuuna fii mas<u>aa</u>kinihim inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin li-ulii <b>al</b>nnuh<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2477, '20:129', 20, 129, ' walawl<u>aa</u> kalimatun sabaqat min rabbika lak<u>aa</u>na liz<u>aa</u>man wa-ajalun musamm<u>aa</u><b>n</b>\r\r\n');
INSERT INTO quranLatin VALUES (2478, '20:130', 20, 130, ' fa<b>i</b><u>sh</u>bir \'al<u>aa</u> m<u>aa</u> yaquuluuna wasabbi<u>h</u> bi<u>h</u>amdi rabbika qabla <u>th</u>uluu\'i <b>al</b>sysyamsi waqabla ghuruubih<u>aa</u> wamin <u>aa</u>n<u>aa</u>-i <b>al</b>layli fasabbi<u>h</u> wa-a<u>th</u>r<u>aa</u>fa <b>al</b>nnah<u>aa</u>ri la\'allaka tar<u>daa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2479, '20:131', 20, 131, ' wal<u>aa</u> tamuddanna \'aynayka il<u>aa</u> m<u>aa</u> matta\'n<u>aa</u> bihi azw<u>aa</u>jan minhum zahrata <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> linaftinahum fiihi warizqu rabbika khayrun wa-abq<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2480, '20:132', 20, 132, ' wa/mur ahlaka bi<b>al</b><u>shsh</u>al<u>aa</u>ti wa<b>i</b><u>sth</u>abir \'alayh<u>aa</u> l<u>aa</u> nas-aluka rizqan na<u>h</u>nu narzuquka wa<b>a</b>l\'<u>aa</u>qibatu li<b>l</b>ttaqw<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2481, '20:133', 20, 133, ' waq<u>aa</u>luu lawl<u>aa</u> ya/tiin<u>aa</u> bi-<u>aa</u>yatin min rabbihi awa lam ta/tihim bayyinatu m<u>aa</u> fii <b>al</b><u>shsh</u>u<u>h</u>ufi <b>a</b>l-uul<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2482, '20:134', 20, 134, ' walaw ann<u>aa</u> ahlakn<u>aa</u>hum bi\'a<u>dzaa</u>bin min qablihi laq<u>aa</u>luu rabban<u>aa</u> lawl<u>aa</u> arsalta ilayn<u>aa</u> rasuulan fanattabi\'a <u>aa</u>y<u>aa</u>tika min qabli an na<u>dz</u>illa wanakhz<u>aa</u>\r\r\n');
INSERT INTO quranLatin VALUES (2483, '20:135', 20, 135, ' qul kullun mutarabbi<u>sh</u>un fatarabba<u>sh</u>uu fasata\'lamuuna man a<u>sh</u>-<u>haa</u>bu <b>al</b><u>shsh</u>ir<u>aath</u>i <b>al</b>ssawiyyi wamani ihtad<u>aa</u></u>\r\r\n');
INSERT INTO quranLatin VALUES (2484, '21:1', 21, 1, ' iqtaraba li<b>l</b>nn<u>aa</u>si <u>h</u>is<u>aa</u>buhum wahum fii ghaflatin mu\'ri<u>dh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2485, '21:2', 21, 2, ' m<u>aa</u> ya/tiihim min <u>dz</u>ikrin min rabbihim mu<u>h</u>datsin ill<u>aa</u> istama\'uuhu wahum yal\'abuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2486, '21:3', 21, 3, ' l<u>aa</u>hiyatan quluubuhum wa-asarruu <b>al</b>nnajw<u>aa</u> <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu hal h<u>aadzaa</u> ill<u>aa</u> basyarun mitslukum afata/tuuna <b>al</b>ssi<u>h</u>ra wa-antum tub<u>sh</u>iruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2487, '21:4', 21, 4, ' q<u>aa</u>la rabbii ya\'lamu <b>a</b>lqawla fii <b>al</b>ssam<u>aa</u>-i wa<b>a</b>l-ar<u>dh</u>i wahuwa <b>al</b>ssamii\'u <b>a</b>l\'aliim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2488, '21:5', 21, 5, ' bal q<u>aa</u>luu a<u>dh</u>gh<u>aa</u>tsu a<u>h</u>l<u>aa</u>min bali iftar<u>aa</u>hu bal huwa sy<u>aa</u>\'irun falya/tin<u>aa</u> bi-<u>aa</u>yatin kam<u>aa</u> ursila <b>a</b>l-awwaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2489, '21:6', 21, 6, ' m<u>aa</u> <u>aa</u>manat qablahum min qaryatin ahlakn<u>aa</u>h<u>aa</u> afahum yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2490, '21:7', 21, 7, ' wam<u>aa</u> arsaln<u>aa</u> qablaka ill<u>aa</u> rij<u>aa</u>lan nuu<u>h</u>ii ilayhim fa<b>i</b>s-aluu ahla <b>al</b><u>dzdz</u>ikri in kuntum l<u>aa</u> ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2491, '21:8', 21, 8, ' wam<u>aa</u> ja\'aln<u>aa</u>hum jasadan l<u>aa</u> ya/kuluuna <b>al</b><u>ththh</u>a\'<u>aa</u>ma wam<u>aa</u> k<u>aa</u>nuu kh<u>aa</u>lidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2492, '21:9', 21, 9, ' tsumma <u>sh</u>adaqn<u>aa</u>humu <b>a</b>lwa\'da fa-anjayn<u>aa</u>hum waman nasy<u>aa</u>u wa-ahlakn<u>aa</u> <b>a</b>lmusrifiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2493, '21:10', 21, 10, ' laqad anzaln<u>aa</u> ilaykum kit<u>aa</u>ban fiihi <u>dz</u>ikrukum afal<u>aa</u> ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2494, '21:11', 21, 11, ' wakam qa<u>sh</u>amn<u>aa</u> min qaryatin k<u>aa</u>nat <u>zhaa</u>limatan wa-ansya/n<u>aa</u> ba\'dah<u>aa</u> qawman <u>aa</u>khariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2495, '21:12', 21, 12, ' falamm<u>aa</u> a<u>h</u>assuu ba/san<u>aa</u> i<u>dzaa</u> hum minh<u>aa</u> yarku<u>dh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2496, '21:13', 21, 13, ' l<u>aa</u> tarku<u>dh</u>uu wa<b>i</b>rji\'uu il<u>aa</u> m<u>aa</u> utriftum fiihi wamas<u>aa</u>kinikum la\'allakum tus-aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2497, '21:14', 21, 14, ' q<u>aa</u>luu y<u>aa</u> waylan<u>aa</u> inn<u>aa</u> kunn<u>aa</u> <u>zhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2498, '21:15', 21, 15, ' fam<u>aa</u> z<u>aa</u>lat tilka da\'w<u>aa</u>hum <u>h</u>att<u>aa</u> ja\'aln<u>aa</u>hum <u>h</u>a<u>sh</u>iidan kh<u>aa</u>midiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2499, '21:16', 21, 16, ' wam<u>aa</u> khalaqn<u>aa</u> <b>al</b>ssam<u>aa</u>-a wa<b>a</b>l-ar<u>dh</u>a wam<u>aa</u> baynahum<u>aa</u> l<u>aa</u>\'ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2500, '21:17', 21, 17, ' law aradn<u>aa</u> an nattakhi<u>dz</u>a lahwan la<b>i</b>ttakha<u>dz</u>n<u>aa</u>hu min ladunn<u>aa</u> in kunn<u>aa</u> f<u>aa</u>\'iliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2501, '21:18', 21, 18, ' bal naq<u>dz</u>ifu bi<b>a</b>l<u>h</u>aqqi \'al<u>aa</u> <b>a</b>lb<u>aath</u>ili fayadmaghuhu fa-i<u>dzaa</u> huwa z<u>aa</u>hiqun walakumu <b>a</b>lwaylu mimm<u>aa</u> ta<u>sh</u>ifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2502, '21:19', 21, 19, ' walahu man fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i waman \'indahu l<u>aa</u> yastakbiruuna \'an \'ib<u>aa</u>datihi wal<u>aa</u> yasta<u>h</u>siruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2503, '21:20', 21, 20, ' yusabbi<u>h</u>uuna <b>al</b>layla wa<b>al</b>nnah<u>aa</u>ra l<u>aa</u> yafturuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2504, '21:21', 21, 21, ' ami ittakha<u>dz</u>uu <u>aa</u>lihatan mina <b>a</b>l-ar<u>dh</u>i hum yunsyiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2505, '21:22', 21, 22, ' law k<u>aa</u>na fiihim<u>aa</u> <u>aa</u>lihatun ill<u>aa</u> <b>al</b>l<u>aa</u>hu lafasadat<u>aa</u> fasub<u>haa</u>na <b>al</b>l<u>aa</u>hi rabbi <b>a</b>l\'arsyi \'amm<u>aa</u> ya<u>sh</u>ifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2506, '21:23', 21, 23, ' l<u>aa</u> yus-alu \'amm<u>aa</u> yaf\'alu wahum yus-aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2507, '21:24', 21, 24, ' ami ittakha<u>dz</u>uu min duunihi <u>aa</u>lihatan qul h<u>aa</u>tuu burh<u>aa</u>nakum h<u>aadzaa</u> <u>dz</u>ikru man ma\'iya wa<u>dz</u>ikru man qablii bal aktsaruhum l<u>aa</u> ya\'lamuuna <b>a</b>l<u>h</u>aqqa fahum mu\'ri<u>dh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2508, '21:25', 21, 25, ' wam<u>aa</u> arsaln<u>aa</u> min qablika min rasuulin ill<u>aa</u> nuu<u>h</u>ii ilayhi annahu l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> an<u>aa</u> fa<b>u</b>\'buduun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2509, '21:26', 21, 26, ' waq<u>aa</u>luu ittakha<u>dz</u>a <b>al</b>rra<u>h</u>m<u>aa</u>nu waladan sub<u>haa</u>nahu bal \'ib<u>aa</u>dun mukramuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2510, '21:27', 21, 27, ' l<u>aa</u> yasbiquunahu bi<b>a</b>lqawli wahum bi-amrihi ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2511, '21:28', 21, 28, ' ya\'lamu m<u>aa</u> bayna aydiihim wam<u>aa</u> khalfahum wal<u>aa</u> yasyfa\'uuna ill<u>aa</u> limani irta<u>daa</u> wahum min khasyyatihi musyfiquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2512, '21:29', 21, 29, ' waman yaqul minhum innii il<u>aa</u>hun min duunihi fa<u>dzaa</u>lika najziihi jahannama ka<u>dzaa</u>lika najzii <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2513, '21:30', 21, 30, ' awa lam yar<u>aa</u> <b>al</b>la<u>dz</u>iina kafaruu anna <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>a k<u>aa</u>nat<u>aa</u> ratqan fafataqn<u>aa</u>hum<u>aa</u> waja\'aln<u>aa</u> mina <b>a</b>lm<u>aa</u>-i kulla syay-in <u>h</u>ayyin afal<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2514, '21:31', 21, 31, ' waja\'aln<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i raw<u>aa</u>siya an tamiida bihim waja\'aln<u>aa</u> fiih<u>aa </u>fij<u>aa</u>jan subulan la\'allahum yahtaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2515, '21:32', 21, 32, ' waja\'aln<u>aa</u> <b>al</b>ssam<u>aa</u>-a saqfan ma<u>h</u>fuu<u>zh</u>an wahum \'an <u>aa</u>y<u>aa</u>tih<u>aa</u> mu\'ri<u>dh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2516, '21:33', 21, 33, ' wahuwa <b>al</b>la<u>dz</u>ii khalaqa <b>al</b>layla wa<b>al</b>nnah<u>aa</u>ra wa<b>al</b>sysyamsa wa<b>a</b>lqamara kullun fii falakin yasba<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2517, '21:34', 21, 34, ' wam<u>aa</u> ja\'aln<u>aa</u> libasyarin min qablika <b>a</b>lkhulda afa-in mitta fahumu <b>a</b>lkh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2518, '21:35', 21, 35, ' kullu nafsin <u>dzaa</u>-iqatu <b>a</b>lmawti wanabluukum bi<b>al</b>sysyarri wa<b>a</b>lkhayri fitnatan wa-ilayn<u>aa</u> turja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2519, '21:36', 21, 36, ' wa-i<u>dzaa</u> ra<u>aa</u>ka <b>al</b>la<u>dz</u>iina kafaruu in yattakhi<u>dz</u>uunaka ill<u>aa</u> huzuwan ah<u>aadzaa</u> <b>al</b>la<u>dz</u>ii ya<u>dz</u>kuru <u>aa</u>lihatakum wahum bi<u>dz</u>ikri <b>al</b>rra<u>h</u>m<u>aa</u>ni hum k<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2520, '21:37', 21, 37, ' khuliqa <b>a</b>l-ins<u>aa</u>nu min \'ajalin sauriikum <u>aa</u>y<u>aa</u>tii fal<u>aa</u> tasta\'jiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2521, '21:38', 21, 38, ' wayaquuluuna mat<u>aa</u> h<u>aadzaa</u> <b>a</b>lwa\'du in kuntum <u>shaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2522, '21:39', 21, 39, ' law ya\'lamu <b>al</b>la<u>dz</u>iina kafaruu <u>h</u>iina l<u>aa</u> yakuffuuna \'an wujuuhihimu <b>al</b>nn<u>aa</u>ra wal<u>aa</u> \'an <u>zh</u>uhuurihim wal<u>aa</u> hum yun<u>sh</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2523, '21:40', 21, 40, ' bal ta/tiihim baghtatan fatabhatuhum fal<u>aa</u> yasta<u>th</u>ii\'uuna raddah<u>aa</u> wal<u>aa</u> hum yun<u>zh</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2524, '21:41', 21, 41, ' walaqadi istuhzi-a birusulin min qablika fa<u>haa</u>qa bi<b>a</b>lla<u>dz</u>iina sakhiruu minhum m<u>aa</u> k<u>aa</u>nuu bihi yastahzi-uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2525, '21:42', 21, 42, ' qul man yaklaukum bi<b>a</b>llayli wa<b>al</b>nnah<u>aa</u>ri mina <b>al</b>rra<u>h</u>m<u>aa</u>ni bal hum \'an <u>dz</u>ikri rabbihim mu\'ri<u>dh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2526, '21:43', 21, 43, ' am lahum <u>aa</u>lihatun tamna\'uhum min duunin<u>aa</u> l<u>aa</u> yasta<u>th</u>ii\'uuna na<u>sh</u>ra anfusihim wal<u>aa</u> hum minn<u>aa</u> yu<u>sy</u>abuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2527, '21:44', 21, 44, ' bal matta\'n<u>aa</u> h<u>aa</u>ul<u>aa</u>-i wa<u>aa</u>b<u>aa</u>-ahum <u>h</u>att<u>aa</u> <u>th</u><u>aa</u>la \'alayhimu <b>a</b>l\'umuru afal<u>aa</u> yarawna ann<u>aa</u> na/tii <b>a</b>l-ar<u>dh</u>a nanqu<u>sh</u>uh<u>aa</u> min a<u>th</u>r<u>aa</u>fih<u>aa</u> afahumu <b>a</b>lgh<u>aa</u>libuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2528, '21:45', 21, 45, ' qul innam<u>aa</u> un<u>dz</u>irukum bi<b>a</b>lwa<u>h</u>yi wal<u>aa</u> yasma\'u <b>al</b><u>shsh</u>ummu <b>al</b>ddu\'<u>aa</u>-a i<u>dzaa</u> m<u>aa</u> yun<u>dz</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2529, '21:46', 21, 46, ' wala-in massat-hum naf<u>h</u>atun min \'a<u>dzaa</u>bi rabbika layaquulunna y<u>aa </u>waylan<u>aa</u> inn<u>aa</u> kunn<u>aa</u> <u>zhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2530, '21:47', 21, 47, ' wana<u>dh</u>a\'u <b>a</b>lmaw<u>aa</u>ziina <b>a</b>lqis<u>th</u>a liyawmi <b>a</b>lqiy<u>aa</u>mati fal<u>aa</u> tu<u>zh</u>lamu nafsun syay-an wa-in k<u>aa</u>na mitsq<u>aa</u>la <u>h</u>abbatin min khardalin atayn<u>aa</u> bih<u>aa</u> wakaf<u>aa</u> bin<u>aa</u> <u>haa</u>sibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2531, '21:48', 21, 48, ' walaqad <u>aa</u>tayn<u>aa</u> muus<u>aa</u> wah<u>aa</u>ruuna <b>a</b>lfurq<u>aa</u>na wa<u>dh</u>iy<u>aa</u>-an wa<u>dz</u>ikran lilmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2532, '21:49', 21, 49, ' <b>al</b>la<u>dz</u>iina yakhsyawna rabbahum bi<b>a</b>lghaybi wahum mina <b>al</b>ss<u>aa</u>\'ati musyfiquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2533, '21:50', 21, 50, ' wah<u>aadzaa</u> <u>dz</u>ikrun mub<u>aa</u>rakun anzaln<u>aa</u>hu afa-antum lahu munkiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2534, '21:51', 21, 51, ' walaqad <u>aa</u>tayn<u>aa</u> ibr<u>aa</u>hiima rusydahu min qablu wakunn<u>aa</u> bihi \'<u>aa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2535, '21:52', 21, 52, ' i<u>dz</u> q<u>aa</u>la li-abiihi waqawmihi m<u>aa</u> h<u>aadz</u>ihi <b>al</b>ttam<u>aa</u>tsiilu <b>al</b>latii antum lah<u>aa</u> \'<u>aa</u>kifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2536, '21:53', 21, 53, ' q<u>aa</u>luu wajadn<u>aa</u> <u>aa</u>b<u>aa</u>-an<u>aa</u> lah<u>aa</u> \'<u>aa</u>bidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2537, '21:54', 21, 54, ' q<u>aa</u>la laqad kuntum antum wa<u>aa</u>b<u>aa</u>ukum fii <u>dh</u>al<u>aa</u>lin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2538, '21:55', 21, 55, ' q<u>aa</u>luu aji/tan<u>aa</u> bi<b>a</b>l<u>h</u>aqqi am anta mina <b>al</b>l<u>aa</u>\'ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2539, '21:56', 21, 56, ' q<u>aa</u>la bal rabbukum rabbu <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i <b>al</b>la<u>dz</u>ii fa<u>th</u>arahunna wa-an<u>aa</u> \'al<u>aa</u> <u>dzaa</u>likum mina <b>al</b>sysy<u>aa</u>hidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2540, '21:57', 21, 57, ' wata<b>al</b>l<u>aa</u>hi la-akiidanna a<u>sh</u>n<u>aa</u>makum ba\'da an tuwalluu mudbiriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2541, '21:58', 21, 58, ' faja\'alahum ju<u>dzats</u>an ill<u>aa</u> kabiiran lahum la\'allahum ilayhi yarji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2542, '21:59', 21, 59, ' q<u>aa</u>luu man fa\'ala h<u>aadzaa</u> bi-<u>aa</u>lihatin<u>aa</u> innahu lamina <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2543, '21:60', 21, 60, ' q<u>aa</u>luu sami\'n<u>aa</u> fatan ya<u>dz</u>kuruhum yuq<u>aa</u>lu lahu ibr<u>aa</u>hiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2544, '21:61', 21, 61, ' q<u>aa</u>luu fa/tuu bihi \'al<u>aa</u> a\'yuni <b>al</b>nn<u>aa</u>si la\'allahum yasyhaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2545, '21:62', 21, 62, ' q<u>aa</u>luu a-anta fa\'alta h<u>aadzaa</u> bi-<u>aa</u>lihatin<u>aa</u> y<u>aa</u> ibr<u>aa</u>hiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2546, '21:63', 21, 63, ' q<u>aa</u>la bal fa\'alahu kabiiruhum h<u>aadzaa</u> fa<b>i</b>s-aluuhum in k<u>aa</u>nuu yan<u>th</u>iquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2547, '21:64', 21, 64, ' faraja\'uu il<u>aa</u> anfusihim faq<u>aa</u>luu innakum antumu <b>al</b><u>zhzhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2548, '21:65', 21, 65, ' tsumma nukisuu \'al<u>aa</u> ruuusihim laqad \'alimta m<u>aa</u> h<u>aa</u>ul<u>aa</u>-i yan<u>th</u>iquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2549, '21:66', 21, 66, ' q<u>aa</u>la afata\'buduuna min duuni <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> yanfa\'ukum syay-an wal<u>aa</u> ya<u>dh</u>urrukum</u>\r\r\n');
INSERT INTO quranLatin VALUES (2550, '21:67', 21, 67, ' uffin lakum walim<u>aa</u> ta\'buduuna min duuni <b>al</b>l<u>aa</u>hi afal<u>aa</u> ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2551, '21:68', 21, 68, ' q<u>aa</u>luu <u>h</u>arriquuhu wa<b>u</b>n<u>sh</u>uruu <u>aa</u>lihatakum in kuntum f<u>aa</u>\'iliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2552, '21:69', 21, 69, ' quln<u>aa</u> y<u>aa</u> n<u>aa</u>ru kuunii bardan wasal<u>aa</u>man \'al<u>aa</u> ibr<u>aa</u>hiim<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2553, '21:70', 21, 70, ' wa-ar<u>aa</u>duu bihi kaydan faja\'aln<u>aa</u>humu <b>a</b>l-akhsariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2554, '21:71', 21, 71, ' wanajjayn<u>aa</u>hu waluu<u>th</u>an il<u>aa</u> <b>a</b>l-ar<u>dh</u>i <b>al</b>latii b<u>aa</u>rakn<u>aa</u> fiih<u>aa</u> lil\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2555, '21:72', 21, 72, ' wawahabn<u>aa</u> lahu is<u>haa</u>qa waya\'quuba n<u>aa</u>filatan wakullan ja\'aln<u>aa</u> <u>shaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2556, '21:73', 21, 73, ' waja\'aln<u>aa</u>hum a-immatan yahduuna bi-amrin<u>aa</u> wa-aw<u>h</u>ayn<u>aa</u> ilayhim fi\'la <b>a</b>lkhayr<u>aa</u>ti wa-iq<u>aa</u>ma <b>al</b><u>shsh</u>al<u>aa</u>ti wa-iit<u>aa</u>-a <b>al</b>zzak<u>aa</u>ti wak<u>aa</u>nuu lan<u>aa</u> \'<u>aa</u>bidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2557, '21:74', 21, 74, ' waluu<u>th</u>an <u>aa</u>tayn<u>aa</u>hu <u>h</u>ukman wa\'ilman wanajjayn<u>aa</u>hu mina <b>a</b>lqaryati <b>al</b>latii k<u>aa</u>nat ta\'malu <b>a</b>lkhab<u>aa</u>-itsa innahum k<u>aa</u>nuu qawma saw-in f<u>aa</u>siqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2558, '21:75', 21, 75, ' wa-adkhaln<u>aa</u>hu fii ra<u>h</u>matin<u>aa</u> innahu mina <b>al</b><u>shshaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2559, '21:76', 21, 76, ' wanuu<u>h</u>an i<u>dz</u> n<u>aa</u>d<u>aa</u> min qablu fa<b>i</b>stajabn<u>aa</u> lahu fanajjayn<u>aa</u>hu wa-ahlahu mina <b>a</b>lkarbi <b>a</b>l\'a<u>zh</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2560, '21:77', 21, 77, ' wana<u>sh</u>arn<u>aa</u>hu mina <b>a</b>lqawmi <b>al</b>la<u>dz</u>iina ka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> innahum k<u>aa</u>nuu qawma saw-in fa-aghraqn<u>aa</u>hum ajma\'iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2561, '21:78', 21, 78, ' wad<u>aa</u>wuuda wasulaym<u>aa</u>na i<u>dz</u> ya<u>h</u>kum<u>aa</u>ni fii <b>a</b>l<u>h</u>artsi i<u>dz</u> nafasyat fiihi ghanamu <b>a</b>lqawmi wakunn<u>aa</u> li<u>h</u>ukmihim sy<u>aa</u>hidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2562, '21:79', 21, 79, ' fafahhamn<u>aa</u>h<u>aa</u> sulaym<u>aa</u>na wakullan <u>aa</u>tayn<u>aa</u> <u>h</u>ukman wa\'ilman wasakhkharn<u>aa</u> ma\'a d<u>aa</u>wuuda <b>a</b>ljib<u>aa</u>la yusabbi<u>h</u>na wa<b>al</b><u>ththh</u>ayra wakunn<u>aa</u> f<u>aa</u>\'iliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2563, '21:80', 21, 80, ' wa\'allamn<u>aa</u>hu <u>sh</u>an\'ata labuusin lakum litu<u>hs</u>inakum min ba/sikum fahal antum sy<u>aa</u>kiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2564, '21:81', 21, 81, ' walisulaym<u>aa</u>na <b>al</b>rrii<u>h</u>a \'<u>aas</u>ifatan tajrii bi-amrihi il<u>aa</u> <b>a</b>l-ar<u>dh</u>i <b>al</b>latii b<u>aa</u>rakn<u>aa</u> fiih<u>aa</u> wakunn<u>aa</u> bikulli syay-in \'<u>aa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2565, '21:82', 21, 82, ' wamina <b>al</b>sysyay<u>aath</u>iini man yaghuu<u>sh</u>uuna lahu waya\'maluuna \'amalan duuna <u>dzaa</u>lika wakunn<u>aa</u> lahum <u>haa</u>fi<u>zh</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2566, '21:83', 21, 83, ' wa-ayyuuba i<u>dz</u> n<u>aa</u>d<u>aa</u> rabbahu annii massaniya <b>al</b><u>dhdh</u>urru wa-anta ar<u>h</u>amu <b>al</b>rr<u>aah</u>imiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2567, '21:84', 21, 84, ' fa<b>i</b>stajabn<u>aa</u> lahu fakasyafn<u>aa</u> m<u>aa</u> bihi min <u>dh</u>urrin wa<u>aa</u>tayn<u>aa</u>hu ahlahu wamitslahum ma\'ahum ra<u>h</u>matan min \'indin<u>aa</u> wa<u>dz</u>ikr<u>aa</u> lil\'<u>aa</u>bidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2568, '21:85', 21, 85, ' wa-ism<u>aa</u>\'iila wa-idriisa wa<u>dzaa</u> <b>a</b>lkifli kullun mina <b>al</b><u>shshaa</u>biriin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2569, '21:86', 21, 86, ' wa-adkhaln<u>aa</u>hum fii ra<u>h</u>matin<u>aa</u> innahum mina <b>al</b><u>shshaa</u>li<u>h</u>iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2570, '21:87', 21, 87, ' wa<u>dzaa</u> <b>al</b>nnuuni i<u>dz</u> <u>dz</u>ahaba mugh<u>aad</u>iban fa<u>zh</u>anna an lan naqdira \'alayhi fan<u>aa</u>d<u>aa</u> fii <b>al</b><u>zhzh</u>ulum<u>aa</u>ti an l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> anta sub<u>haa</u>naka innii kuntu mina <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2571, '21:88', 21, 88, ' fa<b>i</b>stajabn<u>aa</u> lahu wanajjayn<u>aa</u>hu mina <b>a</b>lghammi waka<u>dzaa</u>lika nunjii <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2572, '21:89', 21, 89, ' wazakariyy<u>aa</u> i<u>dz</u> n<u>aa</u>d<u>aa</u> rabbahu rabbi l<u>aa</u> ta<u>dz</u>arnii fardan wa-anta khayru <b>a</b>lw<u>aa</u>ritsiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2573, '21:90', 21, 90, ' fa<b>i</b>stajabn<u>aa</u> lahu wawahabn<u>aa</u> lahu ya<u>h</u>y<u>aa</u> wa-a<u>sh</u>la<u>h</u>n<u>aa</u> lahu zawjahu innahum k<u>aa</u>nuu yus<u>aa</u>ri\'uuna fii <b>a</b>lkhayr<u>aa</u>ti wayad\'uunan<u>aa</u> raghaban warahaban wak<u>aa</u>nuu lan<u>aa</u> kh<u>aa</u>syi\'iin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2574, '21:91', 21, 91, ' wa<b>a</b>llatii a<u>hs</u>anat farjah<u>aa</u> fanafakhn<u>aa</u> fiih<u>aa</u> min ruu<u>h</u>in<u>aa</u> waja\'aln<u>aa</u>h<u>aa</u> wa<b>i</b>bnah<u>aa</u> <u>aa</u>yatan lil\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2575, '21:92', 21, 92, ' inna h<u>aadz</u>ihi ummatukum ummatan w<u>aah</u>idatan wa-an<u>aa</u> rabbukum fau\'buduun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2576, '21:93', 21, 93, ' wataqa<u>ththh</u>a\'uu amrahum baynahum kullun ilayn<u>aa</u> r<u>aa</u>ji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2577, '21:94', 21, 94, ' faman ya\'mal mina <b>al</b><u>shshaa</u>li<u>haa</u>ti wahuwa mu/minun fal<u>aa</u> kufr<u>aa</u>na lisa\'yihi wa-inn<u>aa</u> lahu k<u>aa</u>tibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2578, '21:95', 21, 95, ' wa<u>h</u>ar<u>aa</u>mun \'al<u>aa</u> qaryatin ahlakn<u>aa</u>h<u>aa</u> annahum l<u>aa</u> yarji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2579, '21:96', 21, 96, ' <u>h</u>att<u>aa</u> i<u>dzaa</u> futi<u>h</u>at ya/juuju wama/juuju wahum min kulli <u>h</u>adabin yansiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2580, '21:97', 21, 97, ' wa<b>i</b>qtaraba <b>a</b>lwa\'du <b>a</b>l<u>h</u>aqqu fa-i<u>dzaa</u> hiya sy<u>aa</u>khi<u>sh</u>atun ab<u>shaa</u>ru <b>al</b>la<u>dz</u>iina kafaruu y<u>aa</u> waylan<u>aa</u> qad kunn<u>aa</u> fii ghaflatin min h<u>aadzaa</u> bal kunn<u>aa</u> <u>zhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2581, '21:98', 21, 98, ' innakum wam<u>aa</u> ta\'buduuna min duuni <b>al</b>l<u>aa</u>hi <u>h</u>a<u>sh</u>abu jahannama antum lah<u>aa</u> w<u>aa</u>riduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2582, '21:99', 21, 99, ' law k<u>aa</u>na h<u>aa</u>ul<u>aa</u>-i <u>aa</u>lihatan m<u>aa</u> waraduuh<u>aa</u> wakullun fiih<u>aa</u> kh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2583, '21:100', 21, 100, ' lahum fiih<u>aa</u> zafiirun wahum fiih<u>aa</u> l<u>aa</u> yasma\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2584, '21:101', 21, 101, ' inna <b>al</b>la<u>dz</u>iina sabaqat lahum minn<u>aa</u> <b>a</b>l<u>h</u>usn<u>aa</u> ul<u>aa</u>-ika \'anh<u>aa </u>mub\'aduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2585, '21:102', 21, 102, ' l<u>aa</u> yasma\'uuna <u>h</u>asiisah<u>aa</u> wahum fii m<u>aa</u> isytahat anfusuhum kh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2586, '21:103', 21, 103, ' l<u>aa</u> ya<u>h</u>zunuhumu <b>a</b>lfaza\'u <b>a</b>l-akbaru watatalaqq<u>aa</u>humu <b>a</b>lmal<u>aa</u>-ikatu h<u>aadzaa</u> yawmukumu <b>al</b>la<u>dz</u>ii kuntum tuu\'aduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2587, '21:104', 21, 104, ' yawma na<u>th</u>wii <b>al</b>ssam<u>aa</u>-a ka<u>th</u>ayyi <b>al</b>ssijlli lilkutubi kam<u>aa</u> bada/n<u>aa</u> awwala khalqin nu\'iiduhu wa\'dan \'alayn<u>aa</u> inn<u>aa</u> kunn<u>aa</u> f<u>aa</u>\'iliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2588, '21:105', 21, 105, ' walaqad katabn<u>aa</u> fii <b>al</b>zzabuuri min ba\'di <b>al</b><u>dzdz</u>ikri anna <b>a</b>l-ar<u>dh</u>a yaritsuh<u>aa</u> \'ib<u>aa</u>diya <b>al</b><u>shshaa</u>li<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2589, '21:106', 21, 106, ' inna fii h<u>aadzaa</u> labal<u>aa</u>ghan liqawmin \'<u>aa</u>bidiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2590, '21:107', 21, 107, ' wam<u>aa</u> arsaln<u>aa</u>ka ill<u>aa</u> ra<u>h</u>matan lil\'<u>aa</u>lamiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2591, '21:108', 21, 108, ' qul innam<u>aa</u> yuu<u>haa</u> ilayya annam<u>aa</u> il<u>aa</u>hukum il<u>aa</u>hun w<u>aah</u>idun fahal antum muslimuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2592, '21:109', 21, 109, ' fa-in tawallaw faqul <u>aadz</u>antukum \'al<u>aa</u> saw<u>aa</u>-in wa-in adrii aqariibun am ba\'iidun m<u>aa</u> tuu\'aduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2593, '21:110', 21, 110, ' innahu ya\'lamu <b>a</b>ljahra mina <b>a</b>lqawli waya\'lamu m<u>aa</u> taktumuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2594, '21:111', 21, 111, ' wa-in adrii la\'allahu fitnatun lakum wamat<u>aa</u>\'un il<u>aa</u> <u>h</u>iin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2595, '21:112', 21, 112, ' q<u>aa</u>la rabbi u<u>h</u>kum bi<b>a</b>l<u>h</u>aqqi warabbun<u>aa</u> <b>al</b>rra<u>h</u>m<u>aa</u>nu <b>a</b>lmusta\'<u>aa</u>nu \'al<u>aa</u> m<u>aa</u> ta<u>sh</u>ifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2596, '22:1', 22, 1, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su ittaquu rabbakum inna zalzalata <b>al</b>ss<u>aa</u>\'ati syay-un \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2597, '22:2', 22, 2, ' yawma tarawnah<u>aa</u> ta<u>dz</u>halu kullu mur<u>dh</u>i\'atin \'amm<u>aa</u> ar<u>dh</u>a\'at wata<u>dh</u>a\'u kullu <u>dzaa</u>ti <u>h</u>amlin <u>h</u>amlah<u>aa</u> watar<u>aa</u> <b>al</b>nn<u>aa</u>sa suk<u>aa</u>r<u>aa</u> wam<u>aa</u> hum bisuk<u>aa</u>r<u>aa</u> wal<u>aa</u>kinna \'a<u>dzaa</u>ba <b>al</b>l<u>aa</u>hi syadiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2598, '22:3', 22, 3, ' wamina <b>al</b>nn<u>aa</u>si man yuj<u>aa</u>dilu fii <b>al</b>l<u>aa</u>hi bighayri \'ilmin wayattabi\'u kulla syay<u>th</u><u>aa</u>nin mariid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2599, '22:4', 22, 4, ' kutiba \'alayhi annahu man tawall<u>aa</u>hu fa-annahu yu<u>dh</u>illuhu wayahdiihi il<u>aa</u> \'a<u>dzaa</u>bi <b>al</b>ssa\'iir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2600, '22:5', 22, 5, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su in kuntum fii raybin mina <b>a</b>lba\'tsi fa-inn<u>aa</u> khalaqn<u>aa</u>kum min tur<u>aa</u>bin tsumma min nu<u>th</u>fatin tsumma min \'alaqatin tsumma min mu<u>dh</u>ghatin mukhallaqatin waghayri mukhallaqatin linubayyina lakum wanuqirru fii <b>a</b>l-ar<u>haa</u>mi m<u>aa</u> nasy<u>aa</u>u il<u>aa</u> ajalin musamman tsumma nukhrijukum <u>th</u>iflan tsumma litablughuu asyuddakum waminkum man yutawaff<u>aa</u> waminkum man yuraddu il<u>aa</u> ar<u>dz</u>ali <b>a</b>l\'umuri likayl<u>aa</u> ya\'lama min ba\'di \'ilmin syay-an watar<u>aa</u> <b>a</b>l-ar<u>dh</u>a h<u>aa</u>midatan fa-i<u>dzaa</u> anzaln<u>aa</u> \'alayh<u>aa</u> <b>a</b>lm<u>aa</u>-a ihtazzat warabat wa-anbatat min kulli zawjin bahiij<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2601, '22:6', 22, 6, ' <u>dzaa</u>lika bi-anna <b>al</b>l<u>aa</u>ha huwa <b>a</b>l<u>h</u>aqqu wa-annahu yu<u>h</u>yii <b>a</b>lmawt<u>aa</u> wa-annahu \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2602, '22:7', 22, 7, ' wa-anna <b>al</b>ss<u>aa</u>\'ata <u>aa</u>tiyatun l<u>aa</u> rayba fiih<u>aa</u> wa-anna <b>al</b>l<u>aa</u>ha yab\'atsu man fii <b>a</b>lqubuur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2603, '22:8', 22, 8, ' wamina <b>al</b>nn<u>aa</u>si man yuj<u>aa</u>dilu fii <b>al</b>l<u>aa</u>hi bighayri \'ilmin wal<u>aa</u> hudan wal<u>aa</u> kit<u>aa</u>bin muniir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2604, '22:9', 22, 9, ' ts<u>aa</u>niya \'i<u>th</u>fihi liyu<u>dh</u>illa \'an sabiili <b>al</b>l<u>aa</u>hi lahu fii <b>al</b>dduny<u>aa</u> khizyun wanu<u>dz</u>iiquhu yawma <b>a</b>lqiy<u>aa</u>mati \'a<u>dzaa</u>ba <b>a</b>l<u>h</u>ariiq<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2605, '22:10', 22, 10, ' <u>dzaa</u>lika bim<u>aa</u> qaddamat yad<u>aa</u>ka wa-anna <b>al</b>l<u>aa</u>ha laysa bi<u>zh</u>all<u>aa</u>min lil\'abiid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2606, '22:11', 22, 11, ' wamina <b>al</b>nn<u>aa</u>si man ya\'budu <b>al</b>l<u>aa</u>ha \'al<u>aa</u> <u>h</u>arfin fa-in a<u>shaa</u>bahu khayrun i<u>th</u>ma-anna bihi wa-in a<u>shaa</u>bat-hu fitnatun inqalaba \'al<u>aa</u> wajhihi khasira <b>al</b>dduny<u>aa</u> wa<b>a</b>l-<u>aa</u>khirata <u>dzaa</u>lika huwa <b>a</b>lkhusr<u>aa</u>nu <b>a</b>lmubiin<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2607, '22:12', 22, 12, ' yad\'uu min duuni <b>al</b>l<u>aa</u>hi m<u>aa</u> l<u>aa</u> ya<u>dh</u>urruhu wam<u>aa</u> l<u>aa</u> yanfa\'uhu <u>dzaa</u>lika huwa <b>al</b><u>dhdh</u>al<u>aa</u>lu <b>a</b>lba\'iid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2608, '22:13', 22, 13, ' yad\'uu laman <u>dh</u>arruhu aqrabu min naf\'ihi labi/sa <b>a</b>lmawl<u>aa</u> walabi/sa <b>a</b>l\'asyiir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2609, '22:14', 22, 14, ' inna <b>al</b>l<u>aa</u>ha yudkhilu <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru inna <b>al</b>l<u>aa</u>ha yaf\'alu m<u>aa</u> yuriid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2610, '22:15', 22, 15, ' man k<u>aa</u>na ya<u>zh</u>unnu an lan yan<u>sh</u>urahu <b>al</b>l<u>aa</u>hu fii <b>al</b>dduny<u>aa</u> wa<b>a</b>l-<u>aa</u>khirati falyamdud bisababin il<u>aa</u> <b>al</b>ssam<u>aa</u>-i tsumma liyaq<u>th</u>a\' falyan<u>zh</u>ur hal yu<u>dz</u>hibanna kayduhu m<u>aa</u> yaghii<u>zh</u><b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2611, '22:16', 22, 16, ' waka<u>dzaa</u>lika anzaln<u>aa</u>hu <u>aa</u>y<u>aa</u>tin bayyin<u>aa</u>tin wa-anna <b>al</b>l<u>aa</u>ha yahdii man yuriid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2612, '22:17', 22, 17, ' inna <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa<b>a</b>lla<u>dz</u>iina h<u>aa</u>duu wa<b>al</b><u>shshaa</u>bi-iina wa<b>al</b>nna<u>shaa</u>r<u>aa</u> wa<b>a</b>lmajuusa wa<b>a</b>lla<u>dz</u>iina asyrakuu inna <b>al</b>l<u>aa</u>ha yaf<u>sh</u>ilu baynahum yawma <b>a</b>lqiy<u>aa</u>mati inna <b>al</b>l<u>aa</u>ha \'al<u>aa</u> kulli syay-in syahiid<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2613, '22:18', 22, 18, ' alam tara anna <b>al</b>l<u>aa</u>ha yasjudu lahu man fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti waman fii <b>a</b>l-ar<u>dh</u>i wa<b>al</b>sysyamsu wa<b>a</b>lqamaru wa<b>al</b>nnujuumu wa<b>a</b>ljib<u>aa</u>lu wa<b>al</b>sysyajaru wa<b>al</b>ddaw<u>aa</u>bbu wakatsiirun mina <b>al</b>nn<u>aa</u>si wakatsiirun <u>h</u>aqqa \'alayhi <b>a</b>l\'a<u>dzaa</u>bu waman yuhini <b>al</b>l<u>aa</u>hu fam<u>aa</u> lahu min mukrimin inna <b>al</b>l<u>aa</u>ha yaf\'alu m<u>aa</u> yasy<u>aa</u>/<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2614, '22:19', 22, 19, ' h<u>aadzaa</u>ni kha<u>sh</u>m<u>aa</u>ni ikhta<u>sh</u>amuu fii rabbihim fa<b>a</b>lla<u>dz</u>iina kafaruu qu<u>ththh</u>i\'at lahum tsiy<u>aa</u>bun min n<u>aa</u>rin yu<u>sh</u>abbu min fawqi ruuusihimu <b>a</b>l<u>h</u>amiim<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2615, '22:20', 22, 20, ' yu<u>sh</u>haru bihi m<u>aa</u> fii bu<u>th</u>uunihim wa<b>a</b>ljuluud<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2616, '22:21', 22, 21, ' walahum maq<u>aa</u>mi\'u min <u>h</u>adiid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2617, '22:22', 22, 22, ' kullam<u>aa</u> ar<u>aa</u>duu an yakhrujuu minh<u>aa</u> min ghammin u\'iiduu fiih<u>aa</u> wa<u>dz</u>uuquu \'a<u>dzaa</u>ba <b>a</b>l<u>h</u>ariiq<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2618, '22:23', 22, 23, ' inna <b>al</b>l<u>aa</u>ha yudkhilu <b>al</b>la<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti jann<u>aa</u>tin tajrii min ta<u>h</u>tih<u>aa</u> <b>a</b>l-anh<u>aa</u>ru yu<u>h</u>allawna fiih<u>aa</u> min as<u>aa</u>wira min <u>dz</u>ahabin walu/lu-an walib<u>aa</u>suhum fiih<u>aa</u> <u>h</u>ariir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2619, '22:24', 22, 24, ' wahuduu il<u>aa</u> <b>al</b><u>ththh</u>ayyibi mina <b>a</b>lqawli wahuduu il<u>aa</u> <u>sh</u>ir<u>aath</u>i <b>a</b>l<u>h</u>amiid<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2620, '22:25', 22, 25, ' inna <b>al</b>la<u>dz</u>iina kafaruu waya<u>sh</u>udduuna \'an sabiili <b>al</b>l<u>aa</u>hi wa<b>a</b>lmasjidi <b>a</b>l<u>h</u>ar<u>aa</u>mi <b>al</b>la<u>dz</u>ii ja\'aln<u>aa</u>hu li<b>l</b>nn<u>aa</u>si saw<u>aa</u>-an <b>a</b>l\'<u>aa</u>kifu fiihi wa<b>a</b>lb<u>aa</u>di waman yurid fiihi bi-il<u>haa</u>din bi<u>zh</u>ulmin nu<u>dz</u>iqhu min \'a<u>dzaa</u>bin <b>a</b>liim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2621, '22:26', 22, 26, ' wa-i<u>dz</u> bawwa/n<u>aa</u> li-ibr<u>aa</u>hiima mak<u>aa</u>na <b>a</b>lbayti an l<u>aa</u> tusyrik bii syay-an wa<u>th</u>ahhir baytiya li<b>l</b><u>ththaa</u>-ifiina wa<b>a</b>lq<u>aa</u>-imiina wa<b>al</b>rrukka\'i <b>al</b>ssujuud<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2622, '22:27', 22, 27, ' wa-a<u>dzdz</u>in fii <b>al</b>nn<u>aa</u>si bi<b>a</b>l<u>h</u>ajji ya/tuuka rij<u>aa</u>lan wa\'al<u>aa</u> kulli <u>daa</u>mirin ya/tiina min kulli fajjin \'amiiq<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2623, '22:28', 22, 28, ' liyasyhaduu man<u>aa</u>fi\'a lahum waya<u>dz</u>kuruu isma <b>al</b>l<u>aa</u>hi fii ayy<u>aa</u>min ma\'luum<u>aa</u>tin \'al<u>aa</u> m<u>aa</u> razaqahum min bahiimati <b>a</b>l-an\'<u>aa</u>mi fakuluu minh<u>aa</u> wa-a<u>th</u>\'imuu <b>a</b>lb<u>aa</u>-isa <b>a</b>lfaqiir<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2624, '22:29', 22, 29, ' tsumma lyaq<u>dh</u>uu tafatsahum walyuufuu nu<u>dz</u>uurahum walya<u>ththh</u>awwafuu bi<b>a</b>lbayti <b>a</b>l\'atiiq<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2625, '22:30', 22, 30, ' <u>dzaa</u>lika waman yu\'a<u>zhzh</u>im <u>h</u>urum<u>aa</u>ti <b>al</b>l<u>aa</u>hi fahuwa khayrun lahu \'inda rabbihi wau<u>h</u>illat lakumu <b>a</b>l-an\'<u>aa</u>mu ill<u>aa</u> m<u>aa</u> yutl<u>aa</u> \'alaykum fa<b>i</b>jtanibuu <b>al</b>rrijsa mina <b>a</b>l-awts<u>aa</u>ni wa<b>i</b>jtanibuu qawla <b>al</b>zzuur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2626, '22:31', 22, 31, ' <u>h</u>unaf<u>aa</u>-a lill<u>aa</u>hi ghayra musyrikiina bihi waman yusyrik bi<b>al</b>l<u>aa</u>hi faka-annam<u>aa</u> kharra mina <b>al</b>ssam<u>aa</u>-i fatakh<u>th</u>afuhu <b>al</b><u>ththh</u>ayru aw tahwii bihi <b>al</b>rrii<u>h</u>u fii mak<u>aa</u>nin sa<u>h</u>iiq<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2627, '22:32', 22, 32, ' <u>dzaa</u>lika waman yu\'a<u>zhzh</u>im sya\'<u>aa</u>-ira <b>al</b>l<u>aa</u>hi fa-innah<u>aa</u> min taqw<u>aa</u> <b>a</b>lquluub<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2628, '22:33', 22, 33, ' lakum fiih<u>aa</u> man<u>aa</u>fi\'u il<u>aa</u> ajalin musamman tsumma ma<u>h</u>illuh<u>aa</u> il<u>aa</u> <b>a</b>lbayti <b>a</b>l\'atiiq<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2629, '22:34', 22, 34, ' walikulli ummatin ja\'aln<u>aa</u> mansakan liya<u>dz</u>kuruu isma <b>al</b>l<u>aa</u>hi \'al<u>aa</u> m<u>aa</u> razaqahum min bahiimati <b>a</b>l-an\'<u>aa</u>mi fa-il<u>aa</u>hukum il<u>aa</u>hun w<u>aah</u>idun falahu aslimuu wabasysyiri <b>a</b>lmukhbitiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2630, '22:35', 22, 35, ' <b>al</b>la<u>dz</u>iina i<u>dzaa</u> <u>dz</u>ukira <b>al</b>l<u>aa</u>hu wajilat quluubuhum wa<b>al</b><u>shshaa</u>biriina \'al<u>aa</u> m<u>aa</u> a<u>shaa</u>bahum wa<b>a</b>lmuqiimii <b>al</b><u>shsh</u>al<u>aa</u>ti wamimm<u>aa</u> razaqn<u>aa</u>hum yunfiquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2631, '22:36', 22, 36, ' wa<b>a</b>lbudna ja\'aln<u>aa</u>h<u>aa</u> lakum min sya\'<u>aa</u>-iri <b>al</b>l<u>aa</u>hi lakum fiih<u>aa</u> khayrun fa<b>u</b><u>dz</u>kuruu isma <b>al</b>l<u>aa</u>hi \'alayh<u>aa</u> <u>sh</u>aw<u>aa</u>ffa fa-i<u>dzaa</u> wajabat junuubuh<u>aa</u> fakuluu minh<u>aa</u> wa-a<u>th</u>\'imuu <b>a</b>lq<u>aa</u>ni\'a wa<b>a</b>lmu\'tarra ka<u>dzaa</u>lika sakhkharn<u>aa</u>h<u>aa</u> lakum la\'allakum tasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2632, '22:37', 22, 37, ' lan yan<u>aa</u>la <b>al</b>l<u>aa</u>ha lu<u>h</u>uumuh<u>aa</u> wal<u>aa</u> dim<u>aa</u>uh<u>aa</u> wal<u>aa</u>kin yan<u>aa</u>luhu <b>al</b>ttaqw<u>aa</u> minkum ka<u>dzaa</u>lika sakhkharah<u>aa</u> lakum litukabbiruu <b>al</b>l<u>aa</u>ha \'al<u>aa</u> m<u>aa</u> had<u>aa</u>kum wabasysyiri <b>a</b>lmu<u>h</u>siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2633, '22:38', 22, 38, ' inna <b>al</b>l<u>aa</u>ha yud<u>aa</u>fi\'u \'ani <b>al</b>la<u>dz</u>iina <u>aa</u>manuu inna <b>al</b>l<u>aa</u>ha l<u>aa</u> yu<u>h</u>ibbu kulla khaww<u>aa</u>nin kafuur<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2634, '22:39', 22, 39, ' u<u>dz</u>ina lilla<u>dz</u>iina yuq<u>aa</u>taluuna bi-annahum <u>zh</u>ulimuu wa-inna <b>al</b>l<u>aa</u>ha \'al<u>aa</u> na<u>sh</u>rihim laqadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2635, '22:40', 22, 40, ' <b>al</b>la<u>dz</u>iina ukhrijuu min diy<u>aa</u>rihim bighayri <u>h</u>aqqin ill<u>aa</u> an yaquuluu rabbun<u>aa</u> <b>al</b>l<u>aa</u>hu walawl<u>aa</u> daf\'u <b>al</b>l<u>aa</u>hi <b>al</b>nn<u>aa</u>sa ba\'<u>dh</u>ahum biba\'<u>dh</u>in lahuddimat <u>sh</u>aw<u>aa</u>mi\'u wabiya\'un wa<u>sh</u>alaw<u>aa</u>tun wamas<u>aa</u>jidu yu<u>dz</u>karu fiih<u>aa</u> ismu <b>al</b>l<u>aa</u>hi katsiiran walayan<u>sh</u>uranna <b>al</b>l<u>aa</u>hu man yan<u>sh</u>uruhu inna <b>al</b>l<u>aa</u>ha laqawiyyun \'aziiz<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2636, '22:41', 22, 41, ' <b>al</b>la<u>dz</u>iina in makkann<u>aa</u>hum fii <b>a</b>l-ar<u>dh</u>i aq<u>aa</u>muu <b>al</b><u>shsh</u>al<u>aa</u>ta wa<u>aa</u>tawuu <b>al</b>zzak<u>aa</u>ta wa-amaruu bi<b>a</b>lma\'ruufi wanahaw \'ani <b>a</b>lmunkari walill<u>aa</u>hi \'<u>aa</u>qibatu <b>a</b>l-umuur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2637, '22:42', 22, 42, ' wa-in yuka<u>dzdz</u>ibuuka faqad ka<u>dzdz</u>abat qablahum qawmu nuu<u>h</u>in wa\'<u>aa</u>dun watsamuud<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2638, '22:43', 22, 43, ' waqawmu ibr<u>aa</u>hiima waqawmu luu<u>th</u><b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2639, '22:44', 22, 44, ' wa-a<u>sh</u>-<u>haa</u>bu madyana waku<u>dzdz</u>iba muus<u>aa</u> fa-amlaytu lilk<u>aa</u>firiina tsumma akha<u>dz</u>tuhum fakayfa k<u>aa</u>na nakiir<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2640, '22:45', 22, 45, ' faka-ayyin min qaryatin ahlakn<u>aa</u>h<u>aa</u> wahiya <u>zhaa</u>limatun fahiya kh<u>aa</u>wiyatun \'al<u>aa</u> \'uruusyih<u>aa</u> wabi/rin mu\'a<u>ththh</u>alatin waqa<u>sh</u>rin masyiid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2641, '22:46', 22, 46, ' afalam yasiiruu fii <b>a</b>l-ar<u>dh</u>i fatakuuna lahum quluubun ya\'qiluuna bih<u>aa</u> aw <u>aatsaa</u>nun yasma\'uuna bih<u>aa</u> fa-innah<u>aa</u> l<u>aa</u> ta\'m<u>aa</u> <b>a</b>l-ab<u>shaa</u>ru wal<u>aa</u>kin ta\'m<u>aa</u> <b>a</b>lquluubu <b>al</b>latii fii <b>al</b><u>shsh</u>uduur<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2642, '22:47', 22, 47, ' wayasta\'jiluunaka bi<b>a</b>l\'a<u>dzaa</u>bi walan yukhlifa <b>al</b>l<u>aa</u>hu wa\'dahu wa-inna yawman \'inda rabbika ka-alfi sanatin mimm<u>aa</u> ta\'udduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2643, '22:48', 22, 48, ' waka-ayyin min qaryatin amlaytu lah<u>aa</u> wahiya <u>zhaa</u>limatun tsumma akha<u>dz</u>tuh<u>aa</u> wa-ilayya <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2644, '22:49', 22, 49, ' qul y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su innam<u>aa</u> an<u>aa</u> lakum na<u>dz</u>iirun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2645, '22:50', 22, 50, ' fa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti lahum maghfiratun warizqun kariim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2646, '22:51', 22, 51, ' wa<b>a</b>lla<u>dz</u>iina sa\'aw fii <u>aa</u>y<u>aa</u>tin<u>aa</u> mu\'<u>aa</u>jiziina ul<u>aa</u>-ika a<u>sh</u>-<u>haa</u>bu <b>a</b>lja<u>h</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2647, '22:52', 22, 52, ' wam<u>aa</u> arsaln<u>aa</u> min qablika min rasuulin wal<u>aa</u> nabiyyin ill<u>aa</u> i<u>dzaa</u> tamann<u>aa</u> <b>a</b>lq<u>aa</u> <b>al</b>sysyay<u>th</u><u>aa</u>nu fii umniyyatihi fayansakhu <b>al</b>l<u>aa</u>hu m<u>aa</u> yulqii <b>al</b>sysyay<u>th</u><u>aa</u>nu tsumma yu<u>h</u>kimu <b>al</b>l<u>aa</u>hu <u>aa</u>y<u>aa</u>tihi wa<b>al</b>l<u>aa</u>hu \'aliimun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2648, '22:53', 22, 53, ' liyaj\'ala m<u>aa</u> yulqii <b>al</b>sysyay<u>th</u><u>aa</u>nu fitnatan lilla<u>dz</u>iina fii quluubihim mara<u>dh</u>un wa<b>a</b>lq<u>aa</u>siyati quluubuhum wa-inna <b>al</b><u>zhzhaa</u>limiina lafii syiq<u>aa</u>qin ba\'iid<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2649, '22:54', 22, 54, ' waliya\'lama <b>al</b>la<u>dz</u>iina uutuu <b>a</b>l\'ilma annahu <b>a</b>l<u>h</u>aqqu min rabbika fayu/minuu bihi fatukhbita lahu quluubuhum wa-inna <b>al</b>l<u>aa</u>ha lah<u>aa</u>di <b>al</b>la<u>dz</u>iina <u>aa</u>manuu il<u>aa</u> <u>sh</u>ir<u>aath</u>in mustaqiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2650, '22:55', 22, 55, ' wal<u>aa</u> yaz<u>aa</u>lu <b>al</b>la<u>dz</u>iina kafaruu fii miryatin minhu <u>h</u>att<u>aa</u> ta/tiyahumu <b>al</b>ss<u>aa</u>\'atu baghtatan aw ya/tiyahum \'a<u>dzaa</u>bu yawmin \'aqiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2651, '22:56', 22, 56, ' almulku yawma-i<u>dz</u>in lill<u>aa</u>hi ya<u>h</u>kumu baynahum fa<b>a</b>lla<u>dz</u>iina <u>aa</u>manuu wa\'amiluu <b>al</b><u>shshaa</u>li<u>haa</u>ti fii jann<u>aa</u>ti <b>al</b>nna\'iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2652, '22:57', 22, 57, ' wa<b>a</b>lla<u>dz</u>iina kafaruu waka<u>dzdz</u>abuu bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> faul<u>aa</u>-ika lahum \'a<u>dzaa</u>bun muhiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2653, '22:58', 22, 58, ' wa<b>a</b>lla<u>dz</u>iina h<u>aa</u>jaruu fii sabiili <b>al</b>l<u>aa</u>hi tsumma qutiluu aw m<u>aa</u>tuu layarzuqannahumu <b>al</b>l<u>aa</u>hu rizqan <u>h</u>asanan wa-inna <b>al</b>l<u>aa</u>ha lahuwa khayru <b>al</b>rr<u>aa</u>ziqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2654, '22:59', 22, 59, ' layudkhilannahum mudkhalan yar<u>dh</u>awnahu wa-inna <b>al</b>l<u>aa</u>ha la\'aliimun <u>h</u>aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2655, '22:60', 22, 60, ' <u>dzaa</u>lika waman \'<u>aa</u>qaba bimitsli m<u>aa</u> \'uuqiba bihi tsumma bughiya \'alayhi layan<u>sh</u>urannahu <b>al</b>l<u>aa</u>hu inna <b>al</b>l<u>aa</u>ha la\'afuwwun ghafuur<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2656, '22:61', 22, 61, ' <u>dzaa</u>lika bi-anna <b>al</b>l<u>aa</u>ha yuuliju <b>al</b>layla fii <b>al</b>nnah<u>aa</u>ri wayuuliju <b>al</b>nnah<u>aa</u>ra fii <b>al</b>layli wa-anna <b>al</b>l<u>aa</u>ha samii\'un ba<u>sh</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2657, '22:62', 22, 62, ' <u>dzaa</u>lika bi-anna <b>al</b>l<u>aa</u>ha huwa <b>a</b>l<u>h</u>aqqu wa-anna m<u>aa</u> yad\'uuna min duunihi huwa <b>a</b>lb<u>aath</u>ilu wa-anna <b>al</b>l<u>aa</u>ha huwa <b>a</b>l\'aliyyu <b>a</b>lkabiir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2658, '22:63', 22, 63, ' alam tara anna <b>al</b>l<u>aa</u>ha anzala mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an fatu<u>sh</u>bi<u>h</u>u <b>a</b>l-ar<u>dh</u>u mukh<u>dh</u>arratan inna <b>al</b>l<u>aa</u>ha la<u>th</u>iifun khabiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2659, '22:64', 22, 64, ' lahu m<u>aa</u> fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wam<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i wa-inna <b>al</b>l<u>aa</u>ha lahuwa <b>a</b>lghaniyyu <b>a</b>l<u>h</u>amiid<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2660, '22:65', 22, 65, ' alam tara anna <b>al</b>l<u>aa</u>ha sakhkhara lakum m<u>aa</u> fii <b>a</b>l-ar<u>dh</u>i wa<b>a</b>lfulka tajrii fii <b>a</b>lba<u>h</u>ri bi-amrihi wayumsiku <b>al</b>ssam<u>aa</u>-a an taqa\'a \'al<u>aa</u> <b>a</b>l-ar<u>dh</u>i ill<u>aa</u> bi-i<u>dz</u>nihi inna <b>al</b>l<u>aa</u>ha bi<b>al</b>nn<u>aa</u>si larauufun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2661, '22:66', 22, 66, ' wahuwa <b>al</b>la<u>dz</u>ii a<u>h</u>y<u>aa</u>kum tsumma yumiitukum tsumma yu<u>h</u>yiikum inna <b>a</b>l-ins<u>aa</u>na lakafuur<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2662, '22:67', 22, 67, ' likulli ummatin ja\'aln<u>aa</u> mansakan hum n<u>aa</u>sikuuhu fal<u>aa</u> yun<u>aa</u>zi\'unnaka fii <b>a</b>l-amri wa<b>u</b>d\'u il<u>aa</u> rabbika innaka la\'al<u>aa</u> hudan mustaqiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2663, '22:68', 22, 68, ' wa-in j<u>aa</u>daluuka faquli <b>al</b>l<u>aa</u>hu a\'lamu bim<u>aa</u> ta\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2664, '22:69', 22, 69, ' <b>al</b>l<u>aa</u>hu ya<u>h</u>kumu baynakum yawma <b>a</b>lqiy<u>aa</u>mati fiim<u>aa</u> kuntum fiihi takhtalifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2665, '22:70', 22, 70, ' alam ta\'lam anna <b>al</b>l<u>aa</u>ha ya\'lamu m<u>aa</u> fii <b>al</b>ssam<u>aa</u>-i wa<b>a</b>l-ar<u>dh</u>i inna <u>dzaa</u>lika fii kit<u>aa</u>bin inna <u>dzaa</u>lika \'al<u>aa</u> <b>al</b>l<u>aa</u>hi yasiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2666, '22:71', 22, 71, ' waya\'buduuna min duuni <b>al</b>l<u>aa</u>hi m<u>aa</u> lam yunazzil bihi sul<u>th</u><u>aa</u>nan wam<u>aa</u> laysa lahum bihi \'ilmun wam<u>aa</u> li<b>l</b><u>zhzhaa</u>limiina min na<u>sh</u>iir<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2667, '22:72', 22, 72, ' wa-i<u>dzaa</u> tutl<u>aa</u> \'alayhim <u>aa</u>y<u>aa</u>tun<u>aa</u> bayyin<u>aa</u>tin ta\'rifu fii wujuuhi <b>al</b>la<u>dz</u>iina kafaruu <b>a</b>lmunkara yak<u>aa</u>duuna yas<u>th</u>uuna bi<b>a</b>lla<u>dz</u>iina yatluuna \'alayhim <u>aa</u>y<u>aa</u>tin<u>aa</u> qul afa-unabbi-ukum bisyarrin min <u>dzaa</u>likum <b>al</b>nn<u>aa</u>ru wa\'adah<u>aa</u> <b>al</b>l<u>aa</u>hu <b>al</b>la<u>dz</u>iina kafaruu wabi/sa <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2668, '22:73', 22, 73, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>nn<u>aa</u>su <u>dh</u>uriba matsalun fa<b>i</b>stami\'uu lahu inna <b>al</b>la<u>dz</u>iina tad\'uuna min duuni <b>al</b>l<u>aa</u>hi lan yakhluquu <u>dz</u>ub<u>aa</u>ban walawi ijtama\'uu lahu wa-in yaslubuhumu <b>al</b><u>dzdz</u>ub<u>aa</u>bu syay-an l<u>aa</u> yastanqi<u>dz</u>uuhu minhu <u>dh</u>a\'ufa <b>al</b><u>ththaa</u>libu wa<b>a</b>lma<u>th</u>luub<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2669, '22:74', 22, 74, ' m<u>aa</u> qadaruu <b>al</b>l<u>aa</u>ha <u>h</u>aqqa qadrihi inna <b>al</b>l<u>aa</u>ha laqawiyyun \'aziiz<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2670, '22:75', 22, 75, ' <b>al</b>l<u>aa</u>hu ya<u>sth</u>afii mina <b>a</b>lmal<u>aa</u>-ikati rusulan wamina <b>al</b>nn<u>aa</u>si inna <b>al</b>l<u>aa</u>ha samii\'un ba<u>sh</u>iir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2671, '22:76', 22, 76, ' ya\'lamu m<u>aa</u> bayna aydiihim wam<u>aa</u> khalfahum wa-il<u>aa</u> <b>al</b>l<u>aa</u>hi turja\'u <b>a</b>l-umuur<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2672, '22:77', 22, 77, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu irka\'uu wa<b>u</b>sjuduu wa<b>u</b>\'buduu rabbakum wa<b>i</b>f\'aluu <b>a</b>lkhayra la\'allakum tufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2673, '22:78', 22, 78, ' waj<u>aa</u>hiduu fii <b>al</b>l<u>aa</u>hi <u>h</u>aqqa jih<u>aa</u>dihi huwa ijtab<u>aa</u>kum wam<u>aa</u> ja\'ala \'alaykum fii <b>al</b>ddiini min <u>h</u>arajin millata abiikum ibr<u>aa</u>hiima huwa samm<u>aa</u>kumu <b>a</b>lmuslimiina min qablu wafii h<u>aadzaa</u> liyakuuna <b>al</b>rrasuulu syahiidan \'alaykum watakuunuu syuhad<u>aa</u>-a \'al<u>aa</u> <b>al</b>nn<u>aa</u>si fa-aqiimuu <b>al</b><u>shsh</u>al<u>aa</u>ta wa<u>aa</u>tuu <b>al</b>zzak<u>aa</u>ta wa<b>i</b>\'ta<u>sh</u>imuu bi<b>al</b>l<u>aa</u>hi huwa mawl<u>aa</u>kum fani\'ma <b>a</b>lmawl<u>aa</u> wani\'ma <b>al</b>nna<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2674, '23:1', 23, 1, ' qad afla<u>h</u>a <b>a</b>lmu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2675, '23:2', 23, 2, ' <b>al</b>la<u>dz</u>iina hum fii <u>sh</u>al<u>aa</u>tihim kh<u>aa</u>syi\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2676, '23:3', 23, 3, ' wa<b>a</b>lla<u>dz</u>iina hum \'ani <b>al</b>laghwi mu\'ri<u>dh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2677, '23:4', 23, 4, ' wa<b>a</b>lla<u>dz</u>iina hum li<b>l</b>zzak<u>aa</u>ti f<u>aa</u>\'iluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2678, '23:5', 23, 5, ' wa<b>a</b>lla<u>dz</u>iina hum lifuruujihim <u>haa</u>fi<u>zh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2679, '23:6', 23, 6, ' ill<u>aa</u> \'al<u>aa</u> azw<u>aa</u>jihim aw m<u>aa</u> malakat aym<u>aa</u>nuhum fa-innahum ghayru maluumiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2680, '23:7', 23, 7, ' famani ibtagh<u>aa</u> war<u>aa</u>-a <u>dzaa</u>lika faul<u>aa</u>-ika humu <b>a</b>l\'<u>aa</u>duun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2681, '23:8', 23, 8, ' wa<b>a</b>lla<u>dz</u>iina hum li-am<u>aa</u>n<u>aa</u>tihim wa\'ahdihim r<u>aa</u>\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2682, '23:9', 23, 9, ' wa<b>a</b>lla<u>dz</u>iina hum \'al<u>aa</u> <u>sh</u>alaw<u>aa</u>tihim yu<u>haa</u>fi<u>zh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2683, '23:10', 23, 10, ' ul<u>aa</u>-ika humu <b>a</b>lw<u>aa</u>ritsuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2684, '23:11', 23, 11, ' <b>al</b>la<u>dz</u>iina yaritsuuna <b>a</b>lfirdawsa hum fiih<u>aa</u> kh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2685, '23:12', 23, 12, ' walaqad khalaqn<u>aa</u> <b>a</b>l-ins<u>aa</u>na min sul<u>aa</u>latin min <u>th</u>iin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2686, '23:13', 23, 13, ' tsumma ja\'aln<u>aa</u>hu nu<u>th</u>fatan fii qar<u>aa</u>rin makiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2687, '23:14', 23, 14, ' tsumma khalaqn<u>aa</u> <b>al</b>nnu<u>th</u>fata \'alaqatan fakhalaqn<u>aa</u> <b>a</b>l\'alaqata mu<u>dh</u>ghatan fakhalaqn<u>aa</u> <b>a</b>lmu<u>dh</u>ghata \'i<u>zhaa</u>man fakasawn<u>aa</u> <b>a</b>l\'i<u>zhaa</u>ma la<u>h</u>man tsumma ansya/n<u>aa</u>hu khalqan <u>aa</u>khara fatab<u>aa</u>raka <b>al</b>l<u>aa</u>hu a<u>h</u>sanu <b>a</b>lkh<u>aa</u>liqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2688, '23:15', 23, 15, ' tsumma innakum ba\'da <u>dzaa</u>lika lamayyituun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2689, '23:16', 23, 16, ' tsumma innakum yawma <b>a</b>lqiy<u>aa</u>mati tub\'atsuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2690, '23:17', 23, 17, ' walaqad khalaqn<u>aa</u> fawqakum sab\'a <u>th</u>ar<u>aa</u>-iqa wam<u>aa</u> kunn<u>aa</u> \'ani <b>a</b>lkhalqi gh<u>aa</u>filiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2691, '23:18', 23, 18, ' wa-anzaln<u>aa</u> mina <b>al</b>ssam<u>aa</u>-i m<u>aa</u>-an biqadarin fa-askann<u>aa</u>hu fii <b>a</b>l-ar<u>dh</u>i wa-inn<u>aa</u> \'al<u>aa</u> <u>dz</u>ah<u>aa</u>bin bihi laq<u>aa</u>diruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2692, '23:19', 23, 19, ' fa-ansya/n<u>aa</u> lakum bihi jann<u>aa</u>tin min nakhiilin wa-a\'n<u>aa</u>bin lakum fiih<u>aa</u> faw<u>aa</u>kihu katsiiratun waminh<u>aa</u> ta/kuluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2693, '23:20', 23, 20, ' wasyajaratan takhruju min <u>th</u>uuri sayn<u>aa</u>-a tanbutu bi<b>al</b>dduhni wa<u>sh</u>ibghin lil<u>aa</u>kiliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2694, '23:21', 23, 21, ' wa-inna lakum fii <b>a</b>l-an\'<u>aa</u>mi la\'ibratan nusqiikum mimm<u>aa</u> fii bu<u>th</u>uunih<u>aa</u> walakum fiih<u>aa</u> man<u>aa</u>fi\'u katsiiratun waminh<u>aa</u> ta/kuluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2695, '23:22', 23, 22, ' wa\'alayh<u>aa</u> wa\'al<u>aa</u> <b>a</b>lfulki tu<u>h</u>maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2696, '23:23', 23, 23, ' walaqad arsaln<u>aa</u> nuu<u>h</u>an il<u>aa</u> qawmihi faq<u>aa</u>la y<u>aa</u> qawmi u\'buduu <b>al</b>l<u>aa</u>ha m<u>aa</u> lakum min il<u>aa</u>hin ghayruhu afal<u>aa</u> tattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2697, '23:24', 23, 24, ' faq<u>aa</u>la <b>a</b>lmalau <b>al</b>la<u>dz</u>iina kafaruu min qawmihi m<u>aa</u> h<u>aadzaa</u> ill<u>aa</u> basyarun mitslukum yuriidu an yatafa<u>dhdh</u>ala \'alaykum walaw sy<u>aa</u>-a <b>al</b>l<u>aa</u>hu la-anzala mal<u>aa</u>-ikatan m<u>aa</u> sami\'n<u>aa</u> bih<u>aadzaa</u> fii <u>aa</u>b<u>aa</u>-in<u>aa</u> <b>a</b>l-awwaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2698, '23:25', 23, 25, ' in huwa ill<u>aa</u> rajulun bihi jinnatun fatarabba<u>sh</u>uu bihi <u>h</u>att<u>aa</u> <u>h</u>iin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2699, '23:26', 23, 26, ' q<u>aa</u>la rabbi un<u>sh</u>urnii bim<u>aa</u> ka<u>dzdz</u>abuun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2700, '23:27', 23, 27, ' fa-aw<u>h</u>ayn<u>aa</u> ilayhi ani i<u>sh</u>na\'i <b>a</b>lfulka bi-a\'yunin<u>aa</u> wawa<u>h</u>yin<u>aa</u> fa-i<u>dzaa</u> j<u>aa</u>-a amrun<u>aa</u> waf<u>aa</u>ra <b>al</b>ttannuuru fa<b>u</b>sluk fiih<u>aa</u> min kullin zawjayni itsnayni wa-ahlaka ill<u>aa</u> man sabaqa \'alayhi <b>a</b>lqawlu minhum wal<u>aa</u> tukh<u>aath</u>ibnii fii <b>al</b>la<u>dz</u>iina <u>zh</u>alamuu innahum mughraquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2701, '23:28', 23, 28, ' fa-i<u>dzaa</u> istawayta anta waman ma\'aka \'al<u>aa</u> <b>a</b>lfulki faquli <b>a</b>l<u>h</u>amdu lill<u>aa</u>hi <b>al</b>la<u>dz</u>ii najj<u>aa</u>n<u>aa</u> mina <b>a</b>lqawmi <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2702, '23:29', 23, 29, ' waqul rabbi anzilnii munzalan mub<u>aa</u>rakan wa-anta khayru <b>a</b>lmunziliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2703, '23:30', 23, 30, ' inna fii <u>dzaa</u>lika la<u>aa</u>y<u>aa</u>tin wa-in kunn<u>aa</u> lamubtaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2704, '23:31', 23, 31, ' tsumma ansya/n<u>aa</u> min ba\'dihim qarnan <u>aa</u>khariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2705, '23:32', 23, 32, ' fa-arsaln<u>aa</u> fiihim rasuulan minhum ani u\'buduu <b>al</b>l<u>aa</u>ha m<u>aa</u> lakum min il<u>aa</u>hin ghayruhu afal<u>aa</u> tattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2706, '23:33', 23, 33, ' waq<u>aa</u>la <b>a</b>lmalau min qawmihi <b>al</b>la<u>dz</u>iina kafaruu waka<u>dzdz</u>abuu biliq<u>aa</u>-i <b>a</b>l-<u>aa</u>khirati wa-atrafn<u>aa</u>hum fii <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> m<u>aa</u> h<u>aadzaa</u> ill<u>aa</u> basyarun mitslukum ya/kulu mimm<u>aa</u> ta/kuluuna minhu wayasyrabu mimm<u>aa</u> tasyrabuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2707, '23:34', 23, 34, ' wala-in a<u>th</u>a\'tum basyaran mitslakum innakum i<u>dz</u>an lakh<u>aa</u>siruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2708, '23:35', 23, 35, ' aya\'idukum annakum i<u>dzaa</u> mittum wakuntum tur<u>aa</u>ban wa\'i<u>zhaa</u>man annakum mukhrajuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2709, '23:36', 23, 36, ' hayh<u>aa</u>ta hayh<u>aa</u>ta lim<u>aa</u> tuu\'aduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2710, '23:37', 23, 37, ' in hiya ill<u>aa</u> <u>h</u>ay<u>aa</u>tun<u>aa</u> <b>al</b>dduny<u>aa</u> namuutu wana<u>h</u>y<u>aa</u> wam<u>aa</u> na<u>h</u>nu bimab\'uutsiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2711, '23:38', 23, 38, ' in huwa ill<u>aa</u> rajulun iftar<u>aa</u> \'al<u>aa</u> <b>al</b>l<u>aa</u>hi ka<u>dz</u>iban wam<u>aa</u> na<u>h</u>nu lahu bimu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2712, '23:39', 23, 39, ' q<u>aa</u>la rabbi un<u>sh</u>urnii bim<u>aa</u> ka<u>dzdz</u>abuun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2713, '23:40', 23, 40, ' q<u>aa</u>la \'amm<u>aa</u> qaliilin layu<u>sh</u>bi<u>h</u>unna n<u>aa</u>dimiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2714, '23:41', 23, 41, ' fa-akha<u>dz</u>at-humu <b>al</b><u>shsh</u>ay<u>h</u>atu bi<b>a</b>l<u>h</u>aqqi faja\'aln<u>aa</u>hum ghuts<u>aa</u>-an fabu\'dan lilqawmi <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2715, '23:42', 23, 42, ' tsumma ansya/n<u>aa</u> min ba\'dihim quruunan <u>aa</u>khariin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2716, '23:43', 23, 43, ' m<u>aa</u> tasbiqu min ummatin ajalah<u>aa</u> wam<u>aa</u> yasta/khiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2717, '23:44', 23, 44, ' tsumma arsaln<u>aa</u> rusulan<u>aa</u> tatr<u>aa</u> kulla m<u>aa</u> j<u>aa</u>-a ummatan rasuuluh<u>aa</u> ka<u>dzdz</u>abuuhu fa-atba\'n<u>aa</u> ba\'<u>dh</u>ahum ba\'<u>dh</u>an waja\'aln<u>aa</u>hum a<u>haa</u>diitsa fabu\'dan liqawmin l<u>aa</u> yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2718, '23:45', 23, 45, ' tsumma arsaln<u>aa</u> muus<u>aa</u> wa-akh<u>aa</u>hu h<u>aa</u>ruuna bi-<u>aa</u>y<u>aa</u>tin<u>aa</u> wasul<u>th</u><u>aa</u>nin mubiin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2719, '23:46', 23, 46, ' il<u>aa</u> fir\'awna wamala-ihi fa<b>i</b>stakbaruu wak<u>aa</u>nuu qawman \'<u>aa</u>liin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2720, '23:47', 23, 47, ' faq<u>aa</u>luu anu/minu libasyarayni mitslin<u>aa</u> waqawmuhum<u>aa</u> lan<u>aa</u> \'<u>aa</u>biduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2721, '23:48', 23, 48, ' faka<u>dzdz</u>abuuhum<u>aa</u> fak<u>aa</u>nuu mina <b>a</b>lmuhlakiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2722, '23:49', 23, 49, ' walaqad <u>aa</u>tayn<u>aa</u> muus<u>aa</u> <b>a</b>lkit<u>aa</u>ba la\'allahum yahtaduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2723, '23:50', 23, 50, ' waja\'aln<u>aa</u> ibna maryama waummahu <u>aa</u>yatan wa<u>aa</u>wayn<u>aa</u>hum<u>aa</u> il<u>aa</u> rabwatin <u>dzaa</u>ti qar<u>aa</u>rin wama\'iin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2724, '23:51', 23, 51, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>rrusulu kuluu mina <b>al</b><u>ththh</u>ayyib<u>aa</u>ti wa<b>i</b>\'maluu <u>shaa</u>li<u>h</u>an innii bim<u>aa</u> ta\'maluuna \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2725, '23:52', 23, 52, ' wa-inna h<u>aadz</u>ihi ummatukum ummatan w<u>aah</u>idatan wa-an<u>aa</u> rabbukum fa<b>i</b>ttaquun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2726, '23:53', 23, 53, ' fataqa<u>ththh</u>a\'uu amrahum baynahum zuburan kullu <u>h</u>izbin bim<u>aa</u> ladayhim fari<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2727, '23:54', 23, 54, ' fa<u>dz</u>arhum fii ghamratihim <u>h</u>att<u>aa</u> <u>h</u>iin<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2728, '23:55', 23, 55, ' aya<u>h</u>sabuuna annam<u>aa</u> numidduhum bihi min m<u>aa</u>lin wabaniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2729, '23:56', 23, 56, ' nus<u>aa</u>ri\'u lahum fii <b>a</b>lkhayr<u>aa</u>ti bal l<u>aa</u> yasy\'uruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2730, '23:57', 23, 57, ' inna <b>al</b>la<u>dz</u>iina hum min khasyyati rabbihim musyfiquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2731, '23:58', 23, 58, ' wa<b>a</b>lla<u>dz</u>iina hum bi-<u>aa</u>y<u>aa</u>ti rabbihim yu/minuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2732, '23:59', 23, 59, ' wa<b>a</b>lla<u>dz</u>iina hum birabbihim l<u>aa</u> yusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2733, '23:60', 23, 60, ' wa<b>a</b>lla<u>dz</u>iina yu/tuuna m<u>aa</u> <u>aa</u>taw waquluubuhum wajilatun annahum il<u>aa</u> rabbihim r<u>aa</u>ji\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2734, '23:61', 23, 61, ' ul<u>aa</u>-ika yus<u>aa</u>ri\'uuna fii <b>a</b>lkhayr<u>aa</u>ti wahum lah<u>aa</u> s<u>aa</u>biquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2735, '23:62', 23, 62, ' wal<u>aa</u> nukallifu nafsan ill<u>aa</u> wus\'ah<u>aa</u> waladayn<u>aa</u> kit<u>aa</u>bun yan<u>th</u>iqu bi<b>a</b>l<u>h</u>aqqi wahum l<u>aa</u> yu<u>zh</u>lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2736, '23:63', 23, 63, ' bal quluubuhum fii ghamratin min h<u>aadzaa</u> walahum a\'m<u>aa</u>lun min duuni <u>dzaa</u>lika hum lah<u>aa</u> \'<u>aa</u>miluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2737, '23:64', 23, 64, ' <u>h</u>att<u>aa</u> i<u>dzaa</u> akha<u>dz</u>n<u>aa</u> mutrafiihim bi<b>a</b>l\'a<u>dzaa</u>bi i<u>dzaa</u> hum yaj-aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2738, '23:65', 23, 65, ' l<u>aa</u> taj-aruu <b>a</b>lyawma innakum minn<u>aa</u> l<u>aa</u> tun<u>sh</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2739, '23:66', 23, 66, ' qad k<u>aa</u>nat <u>aa</u>y<u>aa</u>tii tutl<u>aa</u> \'alaykum fakuntum \'al<u>aa</u> a\'q<u>aa</u>bikum tanki<u>sh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2740, '23:67', 23, 67, ' mustakbiriina bihi s<u>aa</u>miran tahjuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2741, '23:68', 23, 68, ' afalam yaddabbaruu <b>a</b>lqawla am j<u>aa</u>-ahum m<u>aa</u> lam ya/ti <u>aa</u>b<u>aa</u>-ahumu <b>a</b>l-awwaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2742, '23:69', 23, 69, ' am lam ya\'rifuu rasuulahum fahum lahu munkiruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2743, '23:70', 23, 70, ' am yaquuluuna bihi jinnatun bal j<u>aa</u>-ahum bi<b>a</b>l<u>h</u>aqqi wa-aktsaruhum lil<u>h</u>aqqi k<u>aa</u>rihuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2744, '23:71', 23, 71, ' walawi ittaba\'a <b>a</b>l<u>h</u>aqqu ahw<u>aa</u>-ahum lafasadati <b>al</b>ssam<u>aa</u>w<u>aa</u>tu wa<b>a</b>l-ar<u>dh</u>u waman fiihinna bal atayn<u>aa</u>hum bi<u>dz</u>ikrihim fahum \'an <u>dz</u>ikrihim mu\'ri<u>dh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2745, '23:72', 23, 72, ' am tas-aluhum kharjan fakhar<u>aa</u>ju rabbika khayrun wahuwa khayru <b>al</b>rr<u>aa</u>ziqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2746, '23:73', 23, 73, ' wa-innaka latad\'uuhum il<u>aa</u> <u>sh</u>ir<u>aath</u>in mustaqiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2747, '23:74', 23, 74, ' wa-inna <b>al</b>la<u>dz</u>iina l<u>aa</u> yu/minuuna bi<b>a</b>l-<u>aa</u>khirati \'ani <b>al</b><u>shsh</u>ir<u>aath</u>i lan<u>aa</u>kibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2748, '23:75', 23, 75, ' walaw ra<u>h</u>imn<u>aa</u>hum wakasyafn<u>aa</u> m<u>aa</u> bihim min <u>dh</u>urrin lalajjuu fii <u>th</u>ughy<u>aa</u>nihim ya\'mahuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2749, '23:76', 23, 76, ' walaqad akha<u>dz</u>n<u>aa</u>hum bi<b>a</b>l\'a<u>dzaa</u>bi fam<u>aa</u> istak<u>aa</u>nuu lirabbihim wam<u>aa</u> yata<u>dh</u>arra\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2750, '23:77', 23, 77, ' <u>h</u>att<u>aa</u> i<u>dzaa</u> fata<u>h</u>n<u>aa</u> \'alayhim b<u>aa</u>ban <u>dzaa</u> \'a<u>dzaa</u>bin syadiidin i<u>dzaa</u> hum fiihi mublisuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2751, '23:78', 23, 78, ' wahuwa <b>al</b>la<u>dz</u>ii ansya-a lakumu <b>al</b>ssam\'a wa<b>a</b>l-ab<u>shaa</u>ra wa<b>a</b>l-af-idata qaliilan m<u>aa</u> tasykuruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2752, '23:79', 23, 79, ' wahuwa <b>al</b>la<u>dz</u>ii <u>dz</u>ara-akum fii <b>a</b>l-ar<u>dh</u>i wa-ilayhi tu<u>h</u>syaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2753, '23:80', 23, 80, ' wahuwa <b>al</b>la<u>dz</u>ii yu<u>h</u>yii wayumiitu walahu ikhtil<u>aa</u>fu <b>al</b>layli wa<b>al</b>nnah<u>aa</u>ri afal<u>aa</u> ta\'qiluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2754, '23:81', 23, 81, ' bal q<u>aa</u>luu mitsla m<u>aa</u> q<u>aa</u>la <b>a</b>l-awwaluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2755, '23:82', 23, 82, ' q<u>aa</u>luu a-i<u>dzaa</u> mitn<u>aa</u> wakunn<u>aa</u> tur<u>aa</u>ban wa\'i<u>zhaa</u>man a-inn<u>aa</u> lamab\'uutsuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2756, '23:83', 23, 83, ' laqad wu\'idn<u>aa</u> na<u>h</u>nu wa<u>aa</u>b<u>aa</u>un<u>aa</u> h<u>aadzaa</u> min qablu in h<u>aadzaa</u> ill<u>aa</u> as<u>aath</u>iiru <b>a</b>l-awwaliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2757, '23:84', 23, 84, ' qul limani <b>a</b>l-ar<u>dh</u>u waman fiih<u>aa</u> in kuntum ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2758, '23:85', 23, 85, ' sayaquuluuna lill<u>aa</u>hi qul afal<u>aa</u> ta<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2759, '23:86', 23, 86, ' qul man rabbu <b>al</b>ssam<u>aa</u>w<u>aa</u>ti <b>al</b>ssab\'i warabbu <b>a</b>l\'arsyi <b>a</b>l\'a<u>zh</u>iim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2760, '23:87', 23, 87, ' sayaquuluuna lill<u>aa</u>hi qul afal<u>aa</u> tattaquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2761, '23:88', 23, 88, ' qul man biyadihi malakuutu kulli syay-in wahuwa yujiiru wal<u>aa</u> yuj<u>aa</u>ru \'alayhi in kuntum ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2762, '23:89', 23, 89, ' sayaquuluuna lill<u>aa</u>hi qul fa-ann<u>aa</u> tus<u>h</u>aruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2763, '23:90', 23, 90, ' bal atayn<u>aa</u>hum bi<b>a</b>l<u>h</u>aqqi wa-innahum lak<u>aadz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2764, '23:91', 23, 91, ' m<u>aa</u> ittakha<u>dz</u>a <b>al</b>l<u>aa</u>hu min waladin wam<u>aa</u> k<u>aa</u>na ma\'ahu min il<u>aa</u>hin i<u>dz</u>an la<u>dz</u>ahaba kullu il<u>aa</u>hin bim<u>aa</u> khalaqa wala\'al<u>aa</u> ba\'<u>dh</u>uhum \'al<u>aa</u> ba\'<u>dh</u>in sub<u>haa</u>na <b>al</b>l<u>aa</u>hi \'amm<u>aa</u> ya<u>sh</u>ifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2765, '23:92', 23, 92, ' \'<u>aa</u>limi <b>a</b>lghaybi wa<b>al</b>sysyah<u>aa</u>dati fata\'<u>aa</u>l<u>aa</u> \'amm<u>aa</u> yusyrikuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2766, '23:93', 23, 93, ' qul rabbi imm<u>aa</u> turiyannii m<u>aa</u> yuu\'aduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2767, '23:94', 23, 94, ' rabbi fal<u>aa</u> taj\'alnii fii <b>a</b>lqawmi <b>al</b><u>zhzhaa</u>limiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2768, '23:95', 23, 95, ' wa-inn<u>aa</u> \'al<u>aa</u> an nuriyaka m<u>aa</u> na\'iduhum laq<u>aa</u>diruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2769, '23:96', 23, 96, ' idfa\' bi<b>a</b>llatii hiya a<u>h</u>sanu <b>al</b>ssayyi-ata na<u>h</u>nu a\'lamu bim<u>aa</u> ya<u>sh</u>ifuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2770, '23:97', 23, 97, ' waqul rabbi a\'uu<u>dz</u>u bika min hamaz<u>aa</u>ti <b>al</b>sysyay<u>aath</u>iin<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2771, '23:98', 23, 98, ' wa-a\'uu<u>dz</u>u bika rabbi an ya<u>hd</u>uruun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2772, '23:99', 23, 99, ' <u>h</u>att<u>aa</u> i<u>dzaa</u> j<u>aa</u>-a a<u>h</u>adahumu <b>a</b>lmawtu q<u>aa</u>la rabbi irji\'uun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2773, '23:100', 23, 100, ' la\'allii a\'malu <u>shaa</u>li<u>h</u>an fiim<u>aa</u> taraktu kall<u>aa</u> innah<u>aa</u> kalimatun huwa q<u>aa</u>-iluh<u>aa</u> wamin war<u>aa</u>-ihim barzakhun il<u>aa</u> yawmi yub\'atsuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2774, '23:101', 23, 101, ' fa-i<u>dzaa</u> nufikha fii <b>al</b><u>shsh</u>uuri fal<u>aa</u> ans<u>aa</u>ba baynahum yawma-i<u>dz</u>in wal<u>aa</u> yatas<u>aa</u>-aluun<b>a </b>\r\r\n');
INSERT INTO quranLatin VALUES (2775, '23:102', 23, 102, ' faman tsaqulat maw<u>aa</u>ziinuhu faul<u>aa</u>-ika humu <b>a</b>lmufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2776, '23:103', 23, 103, ' waman khaffat maw<u>aa</u>ziinuhu faul<u>aa</u>-ika <b>al</b>la<u>dz</u>iina khasiruu anfusahum fii jahannama kh<u>aa</u>liduun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2777, '23:104', 23, 104, ' talfa<u>h</u>u wujuuhahumu <b>al</b>nn<u>aa</u>ru wahum fiih<u>aa</u> k<u>aa</u>li<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2778, '23:105', 23, 105, ' alam takun <u>aa</u>y<u>aa</u>tii tutl<u>aa</u> \'alaykum fakuntum bih<u>aa</u> tuka<u>dzdz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2779, '23:106', 23, 106, ' q<u>aa</u>luu rabban<u>aa</u> ghalabat \'alayn<u>aa</u> syiqwatun<u>aa</u> wakunn<u>aa</u> qawman <u>daa</u>lliin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2780, '23:107', 23, 107, ' rabban<u>aa</u> akhrijn<u>aa</u> minh<u>aa</u> fa-in \'udn<u>aa</u> fa-inn<u>aa</u> <u>zhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2781, '23:108', 23, 108, ' q<u>aa</u>la ikhsauu fiih<u>aa</u> wal<u>aa</u> tukallimuun<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2782, '23:109', 23, 109, ' innahu k<u>aa</u>na fariiqun min \'ib<u>aa</u>dii yaquuluuna rabban<u>aa</u> <u>aa</u>mann<u>aa</u> fa<b>i</b>ghfir lan<u>aa</u> wa<b>i</b>r<u>h</u>amn<u>aa</u> wa-anta khayru <b>al</b>rr<u>aah</u>imiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2783, '23:110', 23, 110, ' fa<b>i</b>ttakha<u>dz</u>tumuuhum sikhriyyan <u>h</u>att<u>aa</u> ansawkum <u>dz</u>ikrii wakuntum minhum ta<u>dh</u>akuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2784, '23:111', 23, 111, ' innii jazaytuhumu <b>a</b>lyawma bim<u>aa</u> <u>sh</u>abaruu annahum humu <b>a</b>lf<u>aa</u>-izuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2785, '23:112', 23, 112, ' q<u>aa</u>la kam labitstum fii <b>a</b>l-ar<u>dh</u>i \'adada siniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2786, '23:113', 23, 113, ' q<u>aa</u>luu labitsn<u>aa</u> yawman aw ba\'<u>dh</u>a yawmin fa<b>i</b>s-ali <b>a</b>l\'<u>aa</u>ddiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2787, '23:114', 23, 114, ' q<u>aa</u>la in labitstum ill<u>aa</u> qaliilan law annakum kuntum ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2788, '23:115', 23, 115, ' afa<u>h</u>asibtum annam<u>aa</u> khalaqn<u>aa</u>kum \'abatsan wa-annakum ilayn<u>aa</u> l<u>aa</u> turja\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2789, '23:116', 23, 116, ' fata\'<u>aa</u>l<u>aa</u> <b>al</b>l<u>aa</u>hu <b>a</b>lmaliku <b>a</b>l<u>h</u>aqqu l<u>aa</u> il<u>aa</u>ha ill<u>aa</u> huwa rabbu <b>a</b>l\'arsyi <b>a</b>lkariim<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2790, '23:117', 23, 117, ' waman yad\'u ma\'a <b>al</b>l<u>aa</u>hi il<u>aa</u>han <u>aa</u>khara l<u>aa</u> burh<u>aa</u>na lahu bihi fa-innam<u>aa</u> <u>h</u>is<u>aa</u>buhu \'inda rabbihi innahu l<u>aa</u> yufli<u>h</u>u <b>a</b>lk<u>aa</u>firuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2791, '23:118', 23, 118, ' waqul rabbi ighfir wa<b>i</b>r<u>h</u>am wa-anta khayru <b>al</b>rr<u>aah</u>imiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2792, '24:1', 24, 1, ' suuratun anzaln<u>aa</u>h<u>aa</u> wafara<u>dh</u>n<u>aa</u>h<u>aa</u> wa-anzaln<u>aa</u> fiih<u>aa</u> <u>aa</u>y<u>aa</u>tin bayyin<u>aa</u>tin la\'allakum ta<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2793, '24:2', 24, 2, ' a<b>l</b>zz<u>aa</u>niyatu wa<b>al</b>zz<u>aa</u>nii fa<b>i</b>jliduu kulla w<u>aah</u>idin minhum<u>aa</u> mi-ata jaldatin wal<u>aa</u> ta/khu<u>dz</u>kum bihim<u>aa</u> ra/fatun fii diini <b>al</b>l<u>aa</u>hi in kuntum tu/minuuna bi<b>al</b>l<u>aa</u>hi wa<b>a</b>lyawmi <b>a</b>l-<u>aa</u>khiri walyasyhad \'a<u>dzaa</u>bahum<u>aa</u> <u>th</u><u>aa</u>-ifatun mina <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2794, '24:3', 24, 3, ' a<b>l</b>zz<u>aa</u>nii l<u>aa</u> yanki<u>h</u>u ill<u>aa</u> z<u>aa</u>niyatan aw musyrikatan wa<b>al</b>zz<u>aa</u>niyatu l<u>aa</u> yanki<u>h</u>uh<u>aa</u> ill<u>aa</u> z<u>aa</u>nin aw musyrikun wa<u>h</u>urrima <u>dzaa</u>lika \'al<u>aa</u> <b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2795, '24:4', 24, 4, ' wa<b>a</b>lla<u>dz</u>iina yarmuuna <b>a</b>lmu<u>hs</u>an<u>aa</u>ti tsumma lam ya/tuu bi-arba\'ati syuhad<u>aa</u>-a fa<b>i</b>jliduuhum tsam<u>aa</u>niina jaldatan wal<u>aa</u> taqbaluu lahum syah<u>aa</u>datan abadan waul<u>aa</u>-ika humu <b>a</b>lf<u>aa</u>siquun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2796, '24:5', 24, 5, ' ill<u>aa</u> <b>al</b>la<u>dz</u>iina t<u>aa</u>buu min ba\'di <u>dzaa</u>lika wa-a<u>sh</u>la<u>h</u>uu fa-inna <b>al</b>l<u>aa</u>ha ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2797, '24:6', 24, 6, ' wa<b>a</b>lla<u>dz</u>iina yarmuuna azw<u>aa</u>jahum walam yakun lahum syuhad<u>aa</u>u ill<u>aa</u> anfusuhum fasyah<u>aa</u>datu a<u>h</u>adihim arba\'u syah<u>aa</u>d<u>aa</u>tin bi<b>al</b>l<u>aa</u>hi innahu lamina <b>al</b><u>shshaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2798, '24:7', 24, 7, ' wa<b>a</b>lkh<u>aa</u>misatu anna la\'nata <b>al</b>l<u>aa</u>hi \'alayhi in k<u>aa</u>na mina <b>a</b>lk<u>aadz</u>ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2799, '24:8', 24, 8, ' wayadrau \'anh<u>aa</u> <b>a</b>l\'a<u>dzaa</u>ba an tasyhada arba\'a syah<u>aa</u>d<u>aa</u>tin bi<b>al</b>l<u>aa</u>hi innahu lamina <b>a</b>lk<u>aadz</u>ibiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2800, '24:9', 24, 9, ' wa<b>a</b>lkh<u>aa</u>misata anna gha<u>dh</u>aba <b>al</b>l<u>aa</u>hi \'alayh<u>aa</u> in k<u>aa</u>na mina <b>al</b><u>shshaa</u>diqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2801, '24:10', 24, 10, ' walawl<u>aa</u> fa<u>dh</u>lu <b>al</b>l<u>aa</u>hi \'alaykum wara<u>h</u>matuhu wa-anna <b>al</b>l<u>aa</u>ha taww<u>aa</u>bun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2802, '24:11', 24, 11, ' inna <b>al</b>la<u>dz</u>iina j<u>aa</u>uu bi<b>a</b>l-ifki \'u<u>sh</u>batun minkum l<u>aa</u> ta<u>h</u>sabuuhu syarran lakum bal huwa khayrun lakum likulli imri-in minhum m<u>aa</u> iktasaba mina <b>a</b>l-itsmi wa<b>a</b>lla<u>dz</u>ii tawall<u>aa</u> kibrahu minhum lahu \'a<u>dzaa</u>bun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2803, '24:12', 24, 12, ' lawl<u>aa</u> i<u>dz</u> sami\'tumuuhu <u>zh</u>anna <b>a</b>lmu/minuuna wa<b>a</b>lmu/min<u>aa</u>tu bi-anfusihim khayran waq<u>aa</u>luu h<u>aadzaa</u> ifkun mubiin<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2804, '24:13', 24, 13, ' lawl<u>aa</u> j<u>aa</u>uu \'alayhi bi-arba\'ati syuhad<u>aa</u>-a fa-i<u>dz</u> lam ya/tuu bi<b>al</b>sysyuhad<u>aa</u>-i faul<u>aa</u>-ika \'inda <b>al</b>l<u>aa</u>hi humu <b>a</b>lk<u>aadz</u>ibuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2805, '24:14', 24, 14, ' walawl<u>aa</u> fa<u>dh</u>lu <b>al</b>l<u>aa</u>hi \'alaykum wara<u>h</u>matuhu fii <b>al</b>dduny<u>aa</u> wa<b>a</b>l-<u>aa</u>khirati lamassakum fii m<u>aa</u> afa<u>dh</u>tum fiihi \'a<u>dzaa</u>bun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2806, '24:15', 24, 15, ' i<u>dz</u> talaqqawnahu bi-alsinatikum wataquuluuna bi-afw<u>aa</u>hikum m<u>aa</u> laysa lakum bihi \'ilmun wata<u>h</u>sabuunahu hayyinan wahuwa \'inda <b>al</b>l<u>aa</u>hi \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2807, '24:16', 24, 16, ' walawl<u>aa</u> i<u>dz</u> sami\'tumuuhu qultum m<u>aa</u> yakuunu lan<u>aa</u> an natakallama bih<u>aadzaa</u> sub<u>haa</u>naka h<u>aadzaa</u> buht<u>aa</u>nun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2808, '24:17', 24, 17, ' ya\'i<u>zh</u>ukumu <b>al</b>l<u>aa</u>hu an ta\'uuduu limitslihi abadan in kuntum mu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2809, '24:18', 24, 18, ' wayubayyinu <b>al</b>l<u>aa</u>hu lakumu <b>a</b>l-<u>aa</u>y<u>aa</u>ti wa<b>al</b>l<u>aa</u>hu \'aliimun <u>h</u>akiim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2810, '24:19', 24, 19, ' inna <b>al</b>la<u>dz</u>iina yu<u>h</u>ibbuuna an tasyii\'a <b>a</b>lf<u>aah</u>isyatu fii <b>al</b>la<u>dz</u>iina <u>aa</u>manuu lahum \'a<u>dzaa</u>bun <b>a</b>liimun fii <b>al</b>dduny<u>aa</u> wa<b>a</b>l-<u>aa</u>khirati wa<b>al</b>l<u>aa</u>hu ya\'lamu wa-antum l<u>aa</u> ta\'lamuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2811, '24:20', 24, 20, ' walawl<u>aa</u> fa<u>dh</u>lu <b>al</b>l<u>aa</u>hi \'alaykum wara<u>h</u>matuhu wa-anna <b>al</b>l<u>aa</u>ha rauufun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2812, '24:21', 24, 21, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tattabi\'uu khu<u>th</u>uw<u>aa</u>ti <b>al</b>sysyay<u>th</u><u>aa</u>ni waman yattabi\' khu<u>th</u>uw<u>aa</u>ti <b>al</b>sysyay<u>th</u><u>aa</u>ni fa-innahu ya/muru bi<b>a</b>lfa<u>h</u>sy<u>aa</u>-i wa<b>a</b>lmunkari walawl<u>aa</u> fa<u>dh</u>lu <b>al</b>l<u>aa</u>hi \'alaykum wara<u>h</u>matuhu m<u>aa</u> zak<u>aa</u> minkum min a<u>h</u>adin abadan wal<u>aa</u>kinna <b>al</b>l<u>aa</u>ha yuzakkii man yasy<u>aa</u>u wa<b>al</b>l<u>aa</u>hu samii\'un \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2813, '24:22', 24, 22, ' wal<u>aa</u> ya/tali uluu <b>a</b>lfa<u>dh</u>li minkum wa<b>al</b>ssa\'ati an yu/tuu ulii <b>a</b>lqurb<u>aa</u> wa<b>a</b>lmas<u>aa</u>kiina wa<b>a</b>lmuh<u>aa</u>jiriina fii sabiili <b>al</b>l<u>aa</u>hi walya\'fuu walya<u>sh</u>fa<u>h</u>uu <b>a</b>l<u>aa</u> tu<u>h</u>ibbuuna an yaghfira <b>al</b>l<u>aa</u>hu lakum wa<b>al</b>l<u>aa</u>hu ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2814, '24:23', 24, 23, ' inna <b>al</b>la<u>dz</u>iina yarmuuna <b>a</b>lmu<u>hs</u>an<u>aa</u>ti <b>a</b>lgh<u>aa</u>fil<u>aa</u>ti <b>a</b>lmu/min<u>aa</u>ti lu\'inuu fii <b>al</b>dduny<u>aa</u> wa<b>a</b>l-<u>aa</u>khirati walahum \'a<u>dzaa</u>bun \'a<u>zh</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2815, '24:24', 24, 24, ' yawma tasyhadu \'alayhim <b>a</b>lsinatuhum wa-aydiihim wa-arjuluhum bim<u>aa</u> k<u>aa</u>nuu ya\'maluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2816, '24:25', 24, 25, ' yawma-i<u>dz</u>in yuwaffiihimu <b>al</b>l<u>aa</u>hu diinahumu <b>a</b>l<u>h</u>aqqa waya\'lamuuna anna <b>al</b>l<u>aa</u>ha huwa <b>a</b>l<u>h</u>aqqu <b>a</b>lmubiin<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2817, '24:26', 24, 26, ' alkhabiits<u>aa</u>tu lilkhabiitsiina wa<b>a</b>lkhabiitsuuna lilkhabiits<u>aa</u>ti wa<b>al</b><u>ththh</u>ayyib<u>aa</u>tu li<b>l</b><u>ththh</u>ayyibiina wa<b>al</b><u>ththh</u>ayyibuuna li<b>l</b><u>ththh</u>ayyib<u>aa</u>ti ul<u>aa</u>-ika mubarrauuna mimm<u>aa</u> yaquuluuna lahum maghfiratun warizqun kariim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2818, '24:27', 24, 27, ' y<u>aa</u> ayyuh<u>aa</u> <b>al</b>la<u>dz</u>iina <u>aa</u>manuu l<u>aa</u> tadkhuluu buyuutan ghayra buyuutikum <u>h</u>att<u>aa</u> tasta/nisuu watusallimuu \'al<u>aa</u> ahlih<u>aa</u> <u>dzaa</u>likum khayrun lakum la\'allakum ta<u>dz</u>akkaruun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2819, '24:28', 24, 28, ' fa-in lam tajiduu fiih<u>aa</u> a<u>h</u>adan fal<u>aa</u> tadkhuluuh<u>aa</u> <u>h</u>att<u>aa</u> yu/<u>dz</u>ana lakum wa-in qiila lakumu irji\'uu fa<b>i</b>rji\'uu huwa azk<u>aa</u> lakum wa<b>al</b>l<u>aa</u>hu bim<u>aa</u> ta\'maluuna \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2820, '24:29', 24, 29, ' laysa \'alaykum jun<u>aah</u>un an tadkhuluu buyuutan ghayra maskuunatin fiih<u>aa</u> mat<u>aa</u>\'un lakum wa<b>al</b>l<u>aa</u>hu ya\'lamu m<u>aa</u> tubduuna wam<u>aa</u> taktumuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2821, '24:30', 24, 30, ' qul lilmu/miniina yaghu<u>dhdh</u>uu min ab<u>shaa</u>rihum waya<u>h</u>fa<u>zh</u>uu furuujahum <u>dzaa</u>lika azk<u>aa</u> lahum inna <b>al</b>l<u>aa</u>ha khabiirun bim<u>aa</u> ya<u>sh</u>na\'uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2822, '24:31', 24, 31, ' waqul lilmu/min<u>aa</u>ti yagh<u>dh</u>u<u>dh</u>na min ab<u>shaa</u>rihinna waya<u>h</u>fa<u>zh</u>na furuujahunna wal<u>aa</u> yubdiina ziinatahunna ill<u>aa</u> m<u>aa</u> <u>zh</u>ahara minh<u>aa</u> walya<u>dh</u>ribna bikhumurihinna \'al<u>aa</u> juyuubihinna wal<u>aa</u> yubdiina ziinatahunna ill<u>aa</u> libu\'uulatihinna aw <u>aa</u>b<u>aa</u>-ihinna aw <u>aa</u>b<u>aa</u>-i bu\'uulatihinna aw abn<u>aa</u>-ihinna aw abn<u>aa</u>-i bu\'uulatihinna aw ikhw<u>aa</u>nihinna aw banii ikhw<u>aa</u>nihinna aw banii akhaw<u>aa</u>tihinna aw nis<u>aa</u>-ihinna aw m<u>aa</u> malakat aym<u>aa</u>nuhunna awi <b>al</b>tt<u>aa</u>bi\'iina ghayri ulii <b>a</b>l-irbati mina <b>al</b>rrij<u>aa</u>li awi <b>al</b><u>ththh</u>ifli <b>al</b>la<u>dz</u>iina lam ya<u>zh</u>haruu \'al<u>aa</u> \'awr<u>aa</u>ti <b>al</b>nnis<u>aa</u>-i wal<u>aa</u> ya<u>dh</u>ribna bi-arjulihinna liyu\'lama m<u>aa</u> yukhfiina min ziinatihinna watuubuu il<u>aa</u> <b>al</b>l<u>aa</u>hi jamii\'an ayyuh<u>aa</u> <b>a</b>lmu/minuuna la\'allakum tufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2823, '24:32', 24, 32, ' wa-anki<u>h</u>uu <b>a</b>l-ay<u>aa</u>m<u>aa</u> minkum wa<b>al</b><u>shshaa</u>li<u>h</u>iina min \'ib<u>aa</u>dikum wa-im<u>aa</u>-ikum in yakuunuu fuqar<u>aa</u>-a yughnihimu <b>al</b>l<u>aa</u>hu min fa<u>dh</u>lihi wa<b>al</b>l<u>aa</u>hu w<u>aa</u>si\'un \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2824, '24:33', 24, 33, ' walyasta\'fifi <b>al</b>la<u>dz</u>iina l<u>aa</u> yajiduuna nik<u>aah</u>an <u>h</u>att<u>aa</u> yughniyahumu <b>al</b>l<u>aa</u>hu min fa<u>dh</u>lihi wa<b>a</b>lla<u>dz</u>iina yabtaghuuna <b>a</b>lkit<u>aa</u>ba mimm<u>aa</u> malakat aym<u>aa</u>nukum fak<u>aa</u>tibuuhum in \'alimtum fiihim khayran wa<u>aa</u>tuuhum min m<u>aa</u>li <b>al</b>l<u>aa</u>hi <b>al</b>la<u>dz</u>ii <u>aa</u>t<u>aa</u>kum wal<u>aa</u> tukrihuu fatay<u>aa</u>tikum \'al<u>aa</u> <b>a</b>lbigh<u>aa</u>-i in aradna ta<u>h</u>a<u>shsh</u>unan litabtaghuu \'ara<u>dh</u>a <b>a</b>l<u>h</u>ay<u>aa</u>ti <b>al</b>dduny<u>aa</u> waman yukrihhunna fa-inna <b>al</b>l<u>aa</u>ha min ba\'di ikr<u>aa</u>hihinna ghafuurun ra<u>h</u>iim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2825, '24:34', 24, 34, ' walaqad anzaln<u>aa</u> ilaykum <u>aa</u>y<u>aa</u>tin mubayyin<u>aa</u>tin wamatsalan mina <b>al</b>la<u>dz</u>iina khalaw min qablikum wamaw\'i<u>zh</u>atan lilmuttaqiin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2826, '24:35', 24, 35, ' <b>al</b>l<u>aa</u>hu nuuru <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i matsalu nuurihi kamisyk<u>aa</u>tin fiih<u>aa</u> mi<u>sh</u>b<u>aah</u>un <b>a</b>lmi<u>sh</u>b<u>aah</u>u fii zuj<u>aa</u>jatin <b>al</b>zzuj<u>aa</u>jatu ka-annah<u>aa</u> kawkabun durriyyun yuuqadu min syajaratin mub<u>aa</u>rakatin zaytuunatin l<u>aa</u> syarqiyyatin wal<u>aa</u> gharbiyyatin yak<u>aa</u>du zaytuh<u>aa</u> yu<u>dh</u>ii-u walaw lam tamsas-hu n<u>aa</u>run nuurun \'al<u>aa</u> nuurin yahdii <b>al</b>l<u>aa</u>hu linuurihi man yasy<u>aa</u>u waya<u>dh</u>ribu <b>al</b>l<u>aa</u>hu <b>a</b>l-amts<u>aa</u>la li<b>l</b>nn<u>aa</u>si wa<b>al</b>l<u>aa</u>hu bikulli syay-in \'aliim<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2827, '24:36', 24, 36, ' fii buyuutin a<u>dz</u>ina <b>al</b>l<u>aa</u>hu an turfa\'a wayu<u>dz</u>kara fiih<u>aa</u> ismuhu yusabbi<u>h</u>u lahu fiih<u>aa</u> bi<b>a</b>lghuduwwi wa<b>a</b>l-<u>aasaa</u>l<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2828, '24:37', 24, 37, ' rij<u>aa</u>lun l<u>aa</u> tulhiihim tij<u>aa</u>ratun wal<u>aa</u> bay\'un \'an <u>dz</u>ikri <b>al</b>l<u>aa</u>hi wa-iq<u>aa</u>mi <b>al</b><u>shsh</u>al<u>aa</u>ti wa-iit<u>aa</u>-i <b>al</b>zzak<u>aa</u>ti yakh<u>aa</u>fuuna yawman tataqallabu fiihi <b>a</b>lquluubu wa<b>a</b>l-ab<u>shaa</u>r<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2829, '24:38', 24, 38, ' liyajziyahumu <b>al</b>l<u>aa</u>hu a<u>h</u>sana m<u>aa</u> \'amiluu wayaziidahum min fa<u>dh</u>lihi wa<b>al</b>l<u>aa</u>hu yarzuqu man yasy<u>aa</u>u bighayri <u>h</u>is<u>aa</u>b<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2830, '24:39', 24, 39, ' wa<b>a</b>lla<u>dz</u>iina kafaruu a\'m<u>aa</u>luhum kasar<u>aa</u>bin biqii\'atin ya<u>h</u>sabuhu <b>al</b><u>zhzh</u>am-<u>aa</u>nu m<u>aa</u>-an <u>h</u>att<u>aa</u> i<u>dzaa</u> j<u>aa</u>-ahu lam yajidhu syay-an wawajada <b>al</b>l<u>aa</u>ha \'indahu fawaff<u>aa</u>hu <u>h</u>is<u>aa</u>bahu wa<b>al</b>l<u>aa</u>hu sarii\'u <b>a</b>l<u>h</u>is<u>aa</u>b<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2831, '24:40', 24, 40, ' aw ka<u>zh</u>ulum<u>aa</u>tin fii ba<u>h</u>rin lujjiyyin yaghsy<u>aa</u>hu mawjun min fawqihi mawjun min fawqihi sa<u>haa</u>bun <u>zh</u>ulum<u>aa</u>tun ba\'<u>dh</u>uh<u>aa</u> fawqa ba\'<u>dh</u>in i<u>dzaa</u> akhraja yadahu lam yakad yar<u>aa</u>h<u>aa</u> waman lam yaj\'ali <b>al</b>l<u>aa</u>hu lahu nuuran fam<u>aa</u> lahu min nuur<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2832, '24:41', 24, 41, ' alam tara anna <b>al</b>l<u>aa</u>ha yusabbi<u>h</u>u lahu man fii <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wa<b>al</b><u>ththh</u>ayru <u>shaa</u>ff<u>aa</u>tin kullun qad \'alima <u>sh</u>al<u>aa</u>tahu watasbii<u>h</u>ahu wa<b>al</b>l<u>aa</u>hu \'aliimun bim<u>aa</u> yaf\'aluun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2833, '24:42', 24, 42, ' walill<u>aa</u>hi mulku <b>al</b>ssam<u>aa</u>w<u>aa</u>ti wa<b>a</b>l-ar<u>dh</u>i wa-il<u>aa</u> <b>al</b>l<u>aa</u>hi <b>a</b>lma<u>sh</u>iir<b>u</b>\r\r\n');
INSERT INTO quranLatin VALUES (2834, '24:43', 24, 43, ' alam tara anna <b>al</b>l<u>aa</u>ha yuzjii sa<u>haa</u>ban tsumma yu-allifu baynahu tsumma yaj\'aluhu ruk<u>aa</u>man fatar<u>aa</u> <b>a</b>lwadqa yakhruju min khil<u>aa</u>lihi wayunazzilu mina <b>al</b>ssam<u>aa</u>-i min jib<u>aa</u>lin fiih<u>aa</u> min baradin fayu<u>sh</u>iibu bihi man yasy<u>aa</u>u waya<u>sh</u>rifuhu \'an man yasy<u>aa</u>u yak<u>aa</u>du san<u>aa</u> barqihi ya<u>dz</u>habu bi<b>a</b>l-ab<u>shaa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2835, '24:44', 24, 44, ' yuqallibu <b>al</b>l<u>aa</u>hu <b>al</b>layla wa<b>al</b>nnah<u>aa</u>ra inna fii <u>dzaa</u>lika la\'ibratan li-ulii <b>a</b>l-ab<u>shaa</u>r<b>i</b>\r\r\n');
INSERT INTO quranLatin VALUES (2836, '24:45', 24, 45, ' wa<b>al</b>l<u>aa</u>hu khalaqa kulla d<u>aa</u>bbatin min m<u>aa</u>-in faminhum man yamsyii \'al<u>aa</u> ba<u>th</u>nihi waminhum man yamsyii \'al<u>aa</u> rijlayni waminhum man yamsyii \'al<u>aa</u> arba\'in yakhluqu <b>al</b>l<u>aa</u>hu m<u>aa</u> yasy<u>aa</u>u inna <b>al</b>l<u>aa</u>ha \'al<u>aa</u> kulli syay-in qadiir<b>un</b>\r\r\n');
INSERT INTO quranLatin VALUES (2837, '24:46', 24, 46, ' laqad anzaln<u>aa</u> <u>aa</u>y<u>aa</u>tin mubayyin<u>aa</u>tin wa<b>al</b>l<u>aa</u>hu yahdii man yasy<u>aa</u>u il<u>aa</u> <u>sh</u>ir<u>aath</u>in mustaqiim<b>in</b>\r\r\n');
INSERT INTO quranLatin VALUES (2838, '24:47', 24, 47, ' wayaquuluuna <u>aa</u>mann<u>aa</u> bi<b>al</b>l<u>aa</u>hi wabi<b>al</b>rrasuuli wa-a<u>th</u>a\'n<u>aa</u> tsumma yatawall<u>aa</u> fariiqun minhum min ba\'di <u>dzaa</u>lika wam<u>aa</u> ul<u>aa</u>-ika bi<b>a</b>lmu/miniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2839, '24:48', 24, 48, ' wa-i<u>dzaa</u> du\'uu il<u>aa</u> <b>al</b>l<u>aa</u>hi warasuulihi liya<u>h</u>kuma baynahum i<u>dzaa</u> fariiqun minhum mu\'ri<u>dh</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2840, '24:49', 24, 49, ' wa-in yakun lahumu <b>a</b>l<u>h</u>aqqu ya/tuu ilayhi mu<u>dz</u>\'iniin<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2841, '24:50', 24, 50, ' afii quluubihim mara<u>dh</u>un ami irt<u>aa</u>buu am yakh<u>aa</u>fuuna an ya<u>h</u>iifa <b>al</b>l<u>aa</u>hu \'alayhim warasuuluhu bal ul<u>aa</u>-ika humu <b>al</b><u>zhzhaa</u>limuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2842, '24:51', 24, 51, ' innam<u>aa</u> k<u>aa</u>na qawla <b>a</b>lmu/miniina i<u>dzaa</u> du\'uu il<u>aa</u> <b>al</b>l<u>aa</u>hi warasuulihi liya<u>h</u>kuma baynahum an yaquuluu sami\'n<u>aa</u> wa-a<u>th</u>a\'n<u>aa</u> waul<u>aa</u>-ika humu <b>a</b>lmufli<u>h</u>uun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2843, '24:52', 24, 52, ' waman yu<u>th</u>i\'i <b>al</b>l<u>aa</u>ha warasuulahu wayakhsya <b>al</b>l<u>aa</u>ha wayattaqhi faul<u>aa</u>-ika humu <b>a</b>lf<u>aa</u>-izuun<b>a</b>\r\r\n');
INSERT INTO quranLatin VALUES (2844, '24:53', 24, 53, ' wa-aqsamuu bi<b>al</b>l<u>aa</u>hi jahda aym<u>aa</u>nihim la-in amartahum layakhrujunna qul l<u>aa</u> tuqsimuu <u>th</u><u>aa</u>\'atun ma\'ruufatun inna <b>al</b>l<u>aa</u>ha khabiirun bim<u>aa</u> ta\'maluun<b>a</b>\r\r\n');
